edg:GenerateLineageGramNodesAndLinksForFormItem
  a ui:ViewElement ;
  spin:constraint [
      a spl:Argument ;
      spl:predicate edg:viewFrameStructure ;
      spl:valueType rdfs:Resource ;
    ] ;
  spin:constraint [
      a spl:Argument ;
      spl:predicate qudt:quantityKind ;
