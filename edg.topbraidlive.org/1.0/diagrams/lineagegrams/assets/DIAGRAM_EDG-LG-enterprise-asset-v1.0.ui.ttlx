      a spl:Argument ;
      spl:predicate arg:focusNode ;
      spl:valueType xsd:string ;
    ] ;
  ui:prototype """
<ui:group let:mySchedule=\"{= spl:object(?focusNode, edg:scheduleItem) }\" let:viewFrameForFocusNode=\"{= edg:getViewFrameForResource(?focusNode, ?viewFrameStructure) }\">
    <edg:MakeSingleViewFrameResourceOccurrence edg:resource=\"{= ?focusNode }\" edg:viewFrame=\"{= ?viewFrameForFocusNode }\"/>
    <ui:if ui:condition=\"{= bound(?quantityKind) }\">
        <ui:group let:viewFrameForQuantityKind=\"{= edg:getViewFrameForResource(?quantityKind, ?viewFrameStructure) }\">
            <edg:MakeViewFrameResourceNodeLink arg:property=\"{= qudt:quantityKind }\" edg:dependencyType=\"{= edg:DEPTYPE_QUANTITYKIND }\" edg:explanation=\"form item data element\" edg:resource=\"{= ?quantityKind }\" edg:targetNode=\"{= ?focusNode }\" edg:targetViewFrame=\"{= ?viewFrameForFocusNode }\" edg:viewFrame=\"{= ?viewFrameForQuantityKind }\"/>
        </ui:group>
    </ui:if>
    <ui:group let:mySchedule=\"{= spl:object(?focusNode, edg:scheduleItem) }\">
        <ui:if ui:condition=\"{= bound(?mySchedule) }\">
            <ui:group let:viewFrameForSchedule=\"{= edg:getViewFrameForResource(?mySchedule, ?viewFrameStructure) }\">
                <edg:MakeSingleViewFrameResourceOccurrence edg:resource=\"{= ?mySchedule }\" edg:viewFrame=\"{= ?viewFrameForSchedule }\"/>
                <edg:MakeViewFrameResourceNodeLink arg:property=\"{= qudt:quantityKind }\" edg:dependencyType=\"{= edg:DEPTYPE_PART-OF }\" edg:explanation=\"form item data element\" edg:resource=\"{= ?focusNode }\" edg:targetNode=\"{= ?mySchedule }\" edg:targetViewFrame=\"{= ?viewFrameForSchedule }\" edg:viewFrame=\"{= ?viewFrameForFocusNode }\"/>
            </ui:group>
        </ui:if>
        <ui:group let:myForm=\"{= spl:object(?focusNode, edg:form) }\">
            <ui:if ui:condition=\"{= bound(?myForm) }\">
                <ui:group let:viewFrameForMyForm=\"{= edg:getViewFrameForResource(?myForm, ?viewFrameStructure) }\">
                    <edg:MakeSingleViewFrameResourceOccurrence edg:resource=\"{= ?myForm }\" edg:viewFrame=\"{= ?viewFrameForMyForm }\"/>
                    <edg:MakeViewFrameResourceNodeLink arg:property=\"{= qudt:quantityKind }\" edg:dependencyType=\"{= edg:DEPTYPE_PART-OF }\" edg:explanation=\"form item data element\" edg:resource=\"{= ?focusNode }\" edg:targetNode=\"{= ?myForm }\" edg:targetViewFrame=\"{= ?viewFrameForMyForm }\" edg:viewFrame=\"{= ?viewFrameForFocusNode }\"/>
                </ui:group>
            </ui:if>
        </ui:group>
    </ui:group>
</ui:group>
"""^^ui:Literal ;
