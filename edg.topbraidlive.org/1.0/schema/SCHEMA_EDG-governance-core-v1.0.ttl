edg:WorkflowParticipant
  a edg:AssetClass ;
  edg:acronym "WFPRTT" ;
  rdfs:comment "<p><em>Workflow Participant</em> is a resource that can be given a workflow participation role. This provides a way to refer, using role properties, to users and job roles and possibly other organizational constructs that  participate in workflows.</p>"^^rdf:HTML ;
