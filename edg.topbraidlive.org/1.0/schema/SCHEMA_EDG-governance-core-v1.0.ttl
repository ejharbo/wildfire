edg:WorkflowParticipantProperty
  a rdfs:Class ;
  a sh:NodeShape ;
  rdfs:comment "Instances of this class are roles that can be used in workflow steps as \"notification roles\"" ;
  rdfs:isDefinedBy <http://edg.topbraid.solutions/1.0/schema/governance/core> ;
  rdfs:label "Workflow property" ;
  rdfs:subClassOf owl:ObjectProperty ;
  skos:example """# Query to produce matching property shapes
CONSTRUCT {
	edg:Stewardship sh:property ?ps .
	?ps a sh:PropertyShape .
	?ps sh:nodeKind sh:IRI .  # Optional
	?ps sh:path ?property .
	?ps rdfs:isDefinedBy <http://edg.topbraid.solutions/1.0/schema/governance/core> .
}
WHERE {
	?property a edg:WorkflowParticipantProperty .
	BIND (IRI(CONCAT(str(edg:Stewardship), '-', afn:localname(?property))) AS ?ps) .
}""" ;
  sh:property [
      sh:path edg:abbreviation ;
      sh:datatype xsd:string ;
      sh:description "A short abbreviation, usually consisting of one or two upper-case letters." ;
      sh:maxCount 1 ;
      sh:minCount 1 ;
    ] ;
  sh:sparql [
      sh:message "edg:WorkflowParticipantProperty {$this} needs to be accompanied by a corresponding sh:property shape" ;
      sh:prefixes <http://edg.topbraid.solutions/1.0/schema/governance/core> ;
      sh:select """SELECT $this
WHERE {
	BIND (IRI(CONCAT(str(edg:Stewardship), '-', afn:localname($this))) AS ?ps) .
    FILTER NOT EXISTS {
		edg:Stewardship sh:property ?ps .
		?ps a sh:PropertyShape .
		?ps sh:path $this .
	}
}""" ;
    ] ;
