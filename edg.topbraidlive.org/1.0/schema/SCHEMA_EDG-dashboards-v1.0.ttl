<http://edg.topbraid.solutions/1.0/schema/dashboards>
  a owl:Ontology ;
  vaem:hasGraphMetadata <http://edg.topbraid.solutions/schema/governance/GMD_dashboards> ;
  rdfs:label "EDG Schema - Governance Assets" ;
  owl:imports <http://edg.topbraid.solutions/1.0/schema/core> ;
  owl:versionInfo "Created with TopBraid Composer" ;
.
edg:Dashboard
  a edg:AssetClass ;
  rdfs:comment "A dashboard is a container for 'dashboard tabs'. Each tab provides a grouping of dashboard data generated by a 'dashboard script'." ;
  rdfs:isDefinedBy <http://edg.topbraid.solutions/1.0/schema/governance> ;
  rdfs:label "Dashboard" ;
  rdfs:subClassOf edg:DashboardAsset ;
  sh:property edg:Dashboard-activated ;
  sh:property edg:Dashboard-belongsTo ;
  sh:property edg:Dashboard-ownedBy ;
  sh:property edg:Dashboard-replacedBy ;
.
edg:Dashboard-activated
  a sh:PropertyShape ;
  sh:path edg:activated ;
  rdfs:isDefinedBy <http://edg.topbraid.solutions/1.0/schema/governance> ;
  sh:datatype xsd:boolean ;
  sh:maxCount 1 ;
  sh:minCount 0 ;
.
edg:Dashboard-belongsTo
  a sh:PropertyShape ;
  sh:path [
      sh:inversePath edg:belongsTo ;
    ] ;
  rdfs:isDefinedBy <http://edg.topbraid.solutions/1.0/schema/governance> ;
  sh:class edg:DashboardGadgetTab ;
  sh:group edg:DashboardGadgetPropertiesGroup ;
.
edg:Dashboard-ownedBy
  a sh:PropertyShape ;
  sh:path edg:ownedBy ;
  rdfs:isDefinedBy <http://edg.topbraid.solutions/1.0/schema/governance> ;
  sh:class edg:Party ;
.
edg:Dashboard-replacedBy
  a sh:PropertyShape ;
  sh:path edg:replacedBy ;
  rdfs:isDefinedBy <http://edg.topbraid.solutions/1.0/schema/governance> ;
  sh:class edg:Dashboard ;
  sh:maxCount 1 ;
  sh:minCount 0 ;
.
edg:DashboardAsset
  a edg:AssetClass ;
  rdfs:isDefinedBy <http://edg.topbraid.solutions/1.0/schema/governance> ;
  rdfs:label "Dashboard Asset" ;
  rdfs:subClassOf edg:Asset ;
  rdfs:subClassOf edg:Identifiable ;
  rdfs:subClassOf edg:Narratable ;
.
edg:DashboardGadget
  a edg:AssetClass ;
  rdfs:isDefinedBy <http://edg.topbraid.solutions/1.0/schema/governance> ;
  rdfs:label "Dashboard Gadget" ;
  rdfs:subClassOf edg:DashboardAsset ;
  sh:property edg:DashboardGadget-belongsTo ;
  sh:property edg:DashboardGadget-metricQuery ;
  sh:property edg:DashboardGadget-order ;
.
edg:DashboardGadget-belongsTo
  a sh:PropertyShape ;
  sh:path edg:belongsTo ;
  rdfs:isDefinedBy <http://edg.topbraid.solutions/1.0/schema/governance> ;
  sh:class edg:DashboardGadgetTab ;
.
edg:DashboardGadget-metricQuery
  a sh:PropertyShape ;
  sh:path edg:metricQuery ;
  rdfs:isDefinedBy <http://edg.topbraid.solutions/1.0/schema/governance> ;
  sh:class edg:Metric ;
  sh:maxCount 1 ;
  sh:minCount 0 ;
.
edg:DashboardGadget-order
  a sh:PropertyShape ;
  sh:path sh:order ;
  rdfs:isDefinedBy <http://edg.topbraid.solutions/1.0/schema/governance> ;
  sh:datatype xsd:integer ;
  sh:maxCount 1 ;
  sh:minCount 0 ;
.
edg:DashboardGadgetTab
  a edg:AssetClass ;
  rdfs:comment "Groups one or more gadgets for dashboard data. The dashboard tab is generated by a 'dashboard script'." ;
  rdfs:isDefinedBy <http://edg.topbraid.solutions/1.0/schema/governance> ;
  rdfs:label "Dashboard Tab" ;
  rdfs:subClassOf edg:DashboardAsset ;
  sh:property edg:DashboardGadgetTab-activated ;
  sh:property edg:DashboardGadgetTab-belongsTo ;
  sh:property edg:DashboardGadgetTab-order ;
  sh:property edg:DashboardGadgetTab-scriptContainer ;
.
edg:DashboardGadgetTab-activated
  a sh:PropertyShape ;
  sh:path edg:activated ;
  rdfs:isDefinedBy <http://edg.topbraid.solutions/1.0/schema/governance> ;
  sh:datatype xsd:boolean ;
.
edg:DashboardGadgetTab-belongsTo
  a sh:PropertyShape ;
  sh:path edg:belongsTo ;
  rdfs:isDefinedBy <http://edg.topbraid.solutions/1.0/schema/governance> ;
  sh:class edg:Dashboard ;
.
edg:DashboardGadgetTab-order
  a sh:PropertyShape ;
  sh:path sh:order ;
  rdfs:isDefinedBy <http://edg.topbraid.solutions/1.0/schema/governance> ;
  sh:datatype xsd:integer ;
  sh:maxCount 1 ;
  sh:minCount 0 ;
.
edg:DashboardGadgetTab-scriptContainer
  a sh:PropertyShape ;
  sh:path edg:scriptContainer ;
  rdfs:isDefinedBy <http://edg.topbraid.solutions/1.0/schema/dashboards> ;
  sh:class edg:DashboardTabScriptContainer ;
  sh:group edg:DashboardPropertiesGroup ;
  sh:maxCount 1 ;
  sh:minCount 0 ;
  sh:order "30"^^xsd:decimal ;
.
edg:DashboardScriptClass
  a rdfs:Class ;
  a sh:NodeShape ;
  rdfs:comment "The class for scripts that generate a dashboard page for a given tab." ;
  rdfs:label "Dashboard Script" ;
  rdfs:subClassOf edg:ScriptClass ;
.
edg:DashboardScriptGraph
  a vaem:GraphRole ;
  rdfs:isDefinedBy <http://edg.topbraid.solutions/1.0/schema/governance> ;
  rdfs:label "Dashboard Script Graph" ;
.
edg:DashboardTabScript
  a edg:AssetClass ;
  rdfs:isDefinedBy <http://edg.topbraid.solutions/1.0/schema/governance> ;
  rdfs:label "Dashboard Tab Script" ;
  rdfs:subClassOf edg:DashboardAsset ;
.
edg:DashboardTabScriptContainer
  a edg:AssetClass ;
  rdfs:isDefinedBy <http://edg.topbraid.solutions/1.0/schema/governance> ;
  rdfs:label "Dashboard Tab Script Container" ;
  rdfs:subClassOf edg:DashboardAsset ;
  sh:property edg:DashboardTabScriptContainer-script ;
.
edg:DashboardTabScriptContainer-script
  a sh:PropertyShape ;
  sh:path edg:script ;
  rdfs:isDefinedBy <http://edg.topbraid.solutions/1.0/schema/governance> ;
  sh:class edg:DashboardScriptClass ;
  sh:maxCount 1 ;
  sh:minCount 0 ;
.
edg:PreconfiguredDashboardGadgetTab
  a edg:AssetClass ;
  rdfs:comment "A 'preconfigured dashboard tab' is always available for any dashboard'." ;
  rdfs:isDefinedBy <http://edg.topbraid.solutions/1.0/schema/dashboards> ;
  rdfs:label "Preconfigured Dashboard Tab" ;
  rdfs:subClassOf edg:DashboardGadgetTab ;
.
