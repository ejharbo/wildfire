      rdf:type owl:Restriction ;
      owl:onProperty fibo-bp-iss-prc:isPotentialIssuer ;
      owl:someValuesFrom fibo-bp-iss-prc:SecuritiesIssuanceProcess ;
    ] ;
.
fibo-bp-iss-prc:PotentialUnderwriter
  rdf:type owl:Class ;
  rdfs:label "potential underwriter"@en ;
  rdfs:subClassOf fibo-bp-iss-prc:SecuritiesIssuanceProcessActor ;
  skos:definition "The entity which will become the underwriter for an issue. Term origin:MBS PoC Reviews"@en ;
.
fibo-bp-iss-prc:PrimaryIdentifierIssuer
  rdf:type owl:Class ;
  rdfs:label "primary identifier issuer"@en ;
  rdfs:subClassOf fibo-fnd-agr-ctr:ContractThirdParty ;
  skos:definition "The party which formally issues the primary security identifier to the security. Term origin:MBS PoC Reviews"@en ;
.
fibo-bp-iss-prc:PrimarySecurityOfferingDistribution
  rdf:type owl:Class ;
  rdfs:label "primary security offering distribution"@en ;
  rdfs:subClassOf fibo-bp-iss-prc:SecurityOfferingDistributionMethod ;
  skos:definition "The original sale of a company's securities, in which the proceeds from the sale are received directly by the company."@en ;
.
fibo-bp-iss-prc:SecondarySecurityOfferingDistribution
  rdf:type owl:Class ;
  rdfs:label "secondary security offering distribution"@en ;
  rdfs:subClassOf fibo-bp-iss-prc:SecurityOfferingDistributionMethod ;
  skos:definition "An Offering of a security which has been previously issued."@en ;
.
fibo-bp-iss-prc:SecuritiesIssuanceProcess
  rdf:type owl:Class ;
  rdfs:label "securities issuance process"@en ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-bp-iss-prc:produces ;
      owl:someValuesFrom fibo-bp-iss-trm:TradedInstrumentIssuanceProcessInformation ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-bp-iss-muni:hasIssuanceGuarantor ;
      owl:someValuesFrom fibo-bp-iss-prc:SecurityIssuanceGuarantor ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-bp-iss-muni:hasPotentialIssuer ;
      owl:someValuesFrom fibo-bp-iss-prc:PotentialIssuer ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty <https://spec.edmcouncil.org/fibo/BP/SecuritiesIssuance/MuniIssuance/subscriber.1> ;
      owl:someValuesFrom fibo-bp-iss-trm:Subscriber ;
    ] ;
