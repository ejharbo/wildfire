fibo-loan-tem-bor:BorrowerOngoingDatedFacts
  rdf:type owl:Class ;
  rdfs:label "borrower ongoing dated facts  (please review)"@en ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
