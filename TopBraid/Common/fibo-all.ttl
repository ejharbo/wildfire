  rdfs:label "fx spot contract"@en ;
  rdfs:subClassOf fibo-der-der-oti:OTCInstrument ;
  rdfs:subClassOf fibo-der-der-sp:SpotContract ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-drc-bsc:hasUnderlier ;
      owl:someValuesFrom fibo-ind-fx-fx:CurrencySpotBuyRate ;
