      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-sec-dbt-ab-pbs:DebtCollateral ;
      owl:onProperty fibo-fnd-acc-dtt:collateralizedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-acc-dtt:collateralizedBy ;
      owl:someValuesFrom fibo-sec-dbt-ab-pbs:DebtCollateral ;
    ] ;
  skos:definition "Debt issued under some Security, with some Collateral."@en ;
.
fibo-sec-dbt-ab-pbs:CollateralizedObligationDeal
