      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-dt-bd:BusinessDayConvention ;
      owl:onProperty fibo-fnd-dt-bd:hasBusinessDayConvention ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-plc-cty:BusinessCenter ;
      owl:onProperty fibo-fnd-dt-bd:hasBusinessCenter ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "a convention that specifies what happens when a date falls on a day that is a weekend or a holiday in some business center" ;
