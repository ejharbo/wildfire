  sm:contentLanguage "http://www.omg.org/spec/ODM/"^^xsd:anyURI ;
  sm:contentLanguage "http://www.w3.org/standards/techs/owl#w3c_all"^^xsd:anyURI ;
  sm:copyright """Copyright (c) 2015-2016 EDM Council, Inc.
Copyright (c) 2015-2016 Object Management Group, Inc.""" ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Arrangements/Documents/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Arrangements/IdentifiersAndIndices/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/Occurrences/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Parties/Parties/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Parties/Roles/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Places/Facilities/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Places/Locations/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"^^xsd:anyURI ;
  sm:fileAbbreviation "fibo-fnd-pas-pas" ;
  sm:fileAbstract "This ontology defines fundamental concepts for buyers, sellers, clients, customers, products, goods and services for use in other FIBO ontologies." ;
  sm:filename "ProductsAndServices.rdf" ;
  rdfs:label "Products and Services Ontology" ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/AboutFND/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/ProductsAndServices/AboutProductsAndServices/"^^xsd:anyURI ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/FND/20160201/ProductsAndServices/ProductsAndServices/> ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/FND/20150801/ProductsAndServices/ProductsAndServices.rdf version of this ontology was modified per the issue resolutions identified in the FIBO FND 1.1 RTF report to replace MoneyAmount with AmountOfMoney." ;
.
fibo-fnd-pas-pas:Buyer
  rdf:type owl:Class ;
  rdfs:label "buyer" ;
  rdfs:subClassOf fibo-fnd-pty-pty:PartyInRole ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-pas-pas:Product ;
      owl:onProperty fibo-fnd-pas-pas:buys ;
    ] ;
  skos:altLabel "purchaser" ;
  skos:definition "a party that acquires, or agrees to acquire, ownership (in case of goods), or benefit or usage (in case of services), in exchange for money or other consideration under a contract of sale" ;
  skos:prefLabel "buyer" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.businessdictionary.com/definition/buyer.html"^^xsd:anyURI ;
.
fibo-fnd-pas-pas:Client
  rdf:type owl:Class ;
  rdfs:label "client" ;
  rdfs:subClassOf fibo-fnd-pty-pty:PartyInRole ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-pas-pas:ClientIdentifier ;
      owl:onProperty fibo-fnd-aap-agt:isIdentifiedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
      owl:someValuesFrom [
          rdf:type owl:Restriction ;
          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
          owl:onClass fibo-fnd-pas-pas:ServiceAgreement ;
          owl:onProperty fibo-fnd-pty-pty:isAPartyTo ;
        ] ;
    ] ;
  skos:definition "a party that purchases professional services from, or has a formal relationship to purchase services from another party" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fnd-pas-pas:ClientIdentifier
  rdf:type owl:Class ;
  rdfs:label "client identifier" ;
  rdfs:subClassOf fibo-fnd-arr-id:Identifier ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-pas-pas:Client ;
      owl:onProperty fibo-fnd-aap-agt:identifies ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "an identifier for a client" ;
.
fibo-fnd-pas-pas:Commodity
  rdf:type owl:Class ;
  rdfs:label "commodity" ;
  rdfs:subClassOf fibo-fnd-pas-pas:Good ;
  skos:definition "a basic good used in commerce that is interchangeable with other commodities of the same type" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.investopedia.com/terms/c/commodity.asp"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "Commodities are most often used as inputs in the production of other goods or services. The quality of a given commodity may differ slightly, but it is essentially uniform across producers." ;
.
fibo-fnd-pas-pas:Customer
  rdf:type owl:Class ;
  rdfs:label "customer" ;
  rdfs:subClassOf fibo-fnd-pas-pas:Buyer ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-pas-pas:CustomerIdentifier ;
      owl:onProperty fibo-fnd-aap-agt:isIdentifiedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-pas-pas:Supplier ;
      owl:onProperty fibo-fnd-pas-pas:buysFrom ;
    ] ;
  skos:definition "a buyer that receives or consumes products (goods or services) and has the ability to choose between different products and suppliers" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.businessdictionary.com/definition/customer.html"^^xsd:anyURI ;
.
fibo-fnd-pas-pas:CustomerIdentifier
  rdf:type owl:Class ;
  rdfs:label "customer identifier" ;
  rdfs:subClassOf fibo-fnd-arr-id:Identifier ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-pas-pas:Customer ;
      owl:onProperty fibo-fnd-aap-agt:identifies ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "an identifier for a customer" ;
.
fibo-fnd-pas-pas:Good
  rdf:type owl:Class ;
  rdfs:label "good" ;
  rdfs:subClassOf owl:Thing ;
  owl:disjointWith fibo-fnd-acc-cur:AmountOfMoney ;
  owl:disjointWith fibo-fnd-plc-loc:RealEstate ;
  skos:definition "any tangible thing that is not money or real estate" ;
  fibo-fnd-utl-av:definitionOrigin "http://www.businessdictionary.com/definition/good.html"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "An inherently useful and relatively scarce tangible item produced from agricultural, construction, manufacturing, or mining activities. According to the UN Convention On Contract For The International Sale Of Goods, the term 'good' does not include (1) items bought for personal use, (2) items bought at an auction or foreclosure sale, (3) aircraft or oceangoing vessels." ;
.
fibo-fnd-pas-pas:Producer
  rdf:type owl:Class ;
  rdfs:label "producer" ;
  rdfs:subClassOf fibo-fnd-pty-pty:PartyInRole ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-pas-pas:Product ;
      owl:onProperty fibo-fnd-pas-pas:produces ;
    ] ;
  skos:definition "the manufacturer of a product, also called maker" ;
  fibo-fnd-utl-av:definitionOrigin "http://www.investorwords.com/3872/producer.html"^^xsd:anyURI ;
.
fibo-fnd-pas-pas:Product
  rdf:type owl:Class ;
  rdfs:label "product" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Class ;
      owl:unionOf (
          fibo-fnd-pas-pas:Good
          fibo-fnd-pas-pas:Service
        ) ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-pas-pas:ProductIdentifier ;
      owl:onProperty fibo-fnd-aap-agt:isIdentifiedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fbc-pas-fpas:Catalog ;
      owl:onProperty fibo-be-fct-fct:isCharacterizedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-pas-pas:Producer ;
      owl:onProperty fibo-fnd-pas-pas:isProducedBy ;
    ] ;
  skos:definition "A commercially distributed good that is (1) tangible property, (2) the output or result of a fabrication, manufacturing, or production process, or (3) something that passes through a distribution channel before being consumed or used." ;
  fibo-fnd-utl-av:adaptedFrom "http://www.businessdictionary.com/definition/product.html"^^xsd:anyURI ;
.
fibo-fnd-pas-pas:ProductIdentifier
  rdf:type owl:Class ;
  rdfs:label "product identifier" ;
  rdfs:subClassOf fibo-fnd-arr-id:Identifier ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-pas-pas:Product ;
      owl:onProperty fibo-fnd-aap-agt:identifies ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "an identifier for a product" ;
.
fibo-fnd-pas-pas:Seller
  rdf:type owl:Class ;
  rdfs:label "seller" ;
  rdfs:subClassOf fibo-fnd-pty-pty:PartyInRole ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-pas-pas:Product ;
      owl:onProperty fibo-fnd-pas-pas:sells ;
    ] ;
  skos:altLabel "vendor" ;
  skos:definition "a party that makes, offers or contracts to make a sale to an actual or potential buyer" ;
  skos:prefLabel "seller" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.businessdictionary.com/definition/seller.html"^^xsd:anyURI ;
.
fibo-fnd-pas-pas:Service
  rdf:type owl:Class ;
  rdfs:label "service" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-pas-pas:ServiceProvider ;
      owl:onProperty fibo-fnd-pas-pas:isProvisionedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-pas-pas:ServiceProvider ;
      owl:onProperty fibo-fnd-rel-rel:isProvidedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-plc-fac:Capability ;
      owl:onProperty fibo-fnd-rel-rel:provides ;
    ] ;
  skos:definition "a type of economic activity that is intangible, is not stored and does not result in ownership; a service is consumed at the point of sale" ;
  skos:example "Services include intangible products, such as accounting, banking, cleaning, consultancy, education, insurance, expertise, medical treatment, or transportation services." ;
  fibo-fnd-utl-av:adaptedFrom "http://www.businessdictionary.com/definition/services.html"^^xsd:anyURI ;
  fibo-fnd-utl-av:adaptedFrom "http://www.investorwords.com/6664/service.html"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "Sometimes services are difficult to identify because they are closely associated with a good; such as the combination of a diagnosis with the administration of a medicine. No transfer of possession or ownership takes place when services are sold, and they (1) cannot be stored or transported, (2) are instantly perishable, and (3) come into existence at the time they are bought and consumed." ;
.
fibo-fnd-pas-pas:ServiceAgreement
  rdf:type owl:Class ;
  rdfs:label "service agreement" ;
  rdfs:subClassOf fibo-fnd-agr-ctr:MutualContractualAgreement ;
  rdfs:subClassOf fibo-fnd-agr-ctr:WrittenContract ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-pas-pas:ServiceProvider ;
      owl:onProperty fibo-fnd-agr-ctr:hasContractParty ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:governs ;
      owl:someValuesFrom fibo-fnd-pas-pas:Service ;
    ] ;
  skos:altLabel "service contract" ;
  skos:definition "a written contract between a client and service provider whereby the service provider supplies some service in the form of time, effort, and/or expertise in exchange for compensation" ;
  skos:prefLabel "service agreement" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.businessdictionary.com/definition/service-contract.html"^^xsd:anyURI ;
.
fibo-fnd-pas-pas:ServiceProvider
  rdf:type owl:Class ;
  rdfs:label "service provider" ;
  rdfs:subClassOf fibo-fnd-pty-pty:PartyInRole ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-plc-fac:Facility ;
      owl:onProperty fibo-fnd-pas-pas:provisions ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-pas-pas:Service ;
      owl:onProperty fibo-fnd-pas-pas:provisions ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-pas-pas:Service ;
      owl:onProperty fibo-fnd-rel-rel:provides ;
    ] ;
  skos:definition "a party that provides and typically provisions professional services, such as consulting, financial, legal, real estate, education, communications, storage, or processing services, to other parties, typically defined in a service agreement" ;
  fibo-fnd-utl-av:adaptedFrom "http://en.wikipedia.org/wiki/Service_provider"^^xsd:anyURI ;
.
fibo-fnd-pas-pas:Supplier
  rdf:type owl:Class ;
  rdfs:label "supplier" ;
  rdfs:subClassOf fibo-fnd-pty-pty:PartyInRole ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-pas-pas:Product ;
      owl:onProperty fibo-fnd-pas-pas:supplies ;
    ] ;
  skos:definition "a party that supplies goods or services" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.businessdictionary.com/definition/supplier.html"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "A supplier may be distinguished from a contractor or subcontractor, who commonly adds specialized input to deliverables." ;
.
fibo-fnd-pas-pas:TransactionConfirmation
  rdf:type owl:Class ;
  rdfs:label "transaction confirmation" ;
  rdfs:subClassOf fibo-fnd-arr-doc:LegalDocument ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-pas-pas:TransactionEvent ;
      owl:onProperty fibo-fnd-agr-ctr:isEvidenceFor ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "written communication from a seller or service provider reciting the relevant details of a transaction" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Banking Terms, Sixth Edition, 2012" ;
.
fibo-fnd-pas-pas:TransactionEvent
  rdf:type owl:Class ;
  rdfs:label "transaction event" ;
  rdfs:subClassOf fibo-fnd-dt-oc:OccurrenceKind ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-pas-pas:TransactionConfirmation ;
      owl:onProperty fibo-fnd-agr-ctr:isEvidencedBy ;
    ] ;
  skos:definition "any sale, assignment, lease, license, loan, advance, contribution, or other transfer of any interest in or right to use any property (tangible or intangible) or money, however that transaction is effected, and regardless of whether the terms of the transaction are formally documented" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fnd-pas-pas:buys
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-pas-pas:Buyer ;
  rdfs:label "buys" ;
  rdfs:range fibo-fnd-pas-pas:Product ;
.
fibo-fnd-pas-pas:buysFrom
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-pas-pas:Buyer ;
  rdfs:label "buys from" ;
  rdfs:range fibo-fnd-pas-pas:Seller ;
.
fibo-fnd-pas-pas:isProducedBy
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-pas-pas:Product ;
  rdfs:label "is produced by" ;
  rdfs:range fibo-fnd-pas-pas:Producer ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:isProvidedBy ;
  owl:inverseOf fibo-fnd-pas-pas:produces ;
  skos:definition "identifies the producer that fabricates, manufactures or creates the product through some production process" ;
.
fibo-fnd-pas-pas:isProvisionedBy
  rdf:type owl:ObjectProperty ;
  rdfs:label "is provisioned by" ;
  rdfs:range fibo-fnd-pas-pas:ServiceProvider ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:isProvidedBy ;
  owl:inverseOf fibo-fnd-pas-pas:provisions ;
  skos:definition "identifies the service provider that provisions the service or facility" ;
.
fibo-fnd-pas-pas:isSuppliedBy
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-pas-pas:Product ;
  rdfs:label "is supplied by" ;
  rdfs:range fibo-fnd-pas-pas:Supplier ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:isProvidedBy ;
  owl:inverseOf fibo-fnd-pas-pas:supplies ;
.
fibo-fnd-pas-pas:produces
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-pas-pas:Producer ;
  rdfs:label "produces" ;
  rdfs:range fibo-fnd-pas-pas:Product ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:provides ;
  skos:definition "identifies the product that is fabricated, manufactured or created via some production process" ;
.
fibo-fnd-pas-pas:provisions
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-pas-pas:ServiceProvider ;
  rdfs:label "provisions" ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:provides ;
  skos:definition "customizes, provides, or outfits something required for use in delivering a service" ;
.
fibo-fnd-pas-pas:sells
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-pas-pas:Seller ;
  rdfs:label "sells" ;
  rdfs:range fibo-fnd-pas-pas:Product ;
.
fibo-fnd-pas-pas:sellsTo
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-pas-pas:Seller ;
  rdfs:label "sells to" ;
  rdfs:range fibo-fnd-pas-pas:Buyer ;
  owl:inverseOf fibo-fnd-pas-pas:buysFrom ;
.
fibo-fnd-pas-pas:supplies
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-pas-pas:Supplier ;
  rdfs:label "supplies" ;
  rdfs:range fibo-fnd-pas-pas:Product ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:provides ;
.
fibo-fnd-pub-con:
  rdfs:label "Conventions"@en ;
.
fibo-fnd-pub-con:Convention
  rdf:type owl:Class ;
  rdfs:label "convention"@en ;
  rdfs:subClassOf fibo-fnd-pub-pub:PublishedInformation ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-pub-pub:Publisher ;
      owl:onProperty fibo-fnd-pub-pub:publishedBy ;
    ] ;
  skos:definition "The way something is usually done."@en ;
.
fibo-fnd-pub-con:ISOStandard
  rdf:type owl:Class ;
  rdfs:label "i s o standard"@en ;
  rdfs:subClassOf fibo-fnd-pub-std:TechnicalStandard ;
  skos:definition "A standard published and mandated by the International Standards Organisation (ISO)."@en ;
.
fibo-fnd-pub-con:IndustryModelConvention
  rdf:type owl:Class ;
  rdfs:label "industry model convention"@en ;
  rdfs:subClassOf fibo-fnd-pub-con:Convention ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pub-con:industryModelConventionDefinedBy ;
      owl:someValuesFrom fibo-fnd-pub-std:IndustryAssociation ;
    ] ;
  skos:definition "Standard conventions which exist in the public domain but are not enshrined in an existing ISO Standard are defined in this model and published as such."@en ;
.
fibo-fnd-pub-con:industryModelConventionDefinedBy
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-pub-con:IndustryModelConvention ;
  rdfs:label "industry model convention defined by"@en ;
  rdfs:range fibo-fnd-pub-std:IndustryAssociation ;
  skos:definition "Some Industry Association which defines the convention."@en ;
  fibo-fnd-utl-av:explanatoryNote "This relationship exists in order to be able to define conventions which do not have the formal standards but which are maintained in some way by some industry association, for example by publication of a website."@en ;
.
fibo-fnd-pub-pub:
  rdfs:label "Publications"@en ;
.
fibo-fnd-pub-pub:PublishedInformation
  rdf:type owl:Class ;
  rdfs:label "published information"@en ;
  rdfs:subClassOf fibo-fnd-inf-inf:Information ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-inf-inf:isAbout ;
      owl:someValuesFrom owl:Thing ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pub-pub:publishedBy ;
      owl:someValuesFrom fibo-fnd-pub-pub:Publisher ;
    ] ;
  skos:definition "Information put into the public domain by some party or sold at a profit."@en ;
.
fibo-fnd-pub-pub:Publisher
  rdf:type owl:Class ;
  rdfs:label "publisher"@en ;
  rdfs:subClassOf fibo-fnd-pty-rl:AgentInRole ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pub-pub:publisherIdentity ;
      owl:someValuesFrom fibo-be-le-fbo:FormallyConstitutedOrganization ;
    ] ;
  skos:definition "An organization (such as a publishing house) which makes information. available."@en ;
  fibo-fnd-utl-av:explanatoryNote "This is the role of publisher, whether or not the role is filled by a publishing house. For example it may also be the entity which publishes standard contract terms, e.g. an industry association or an exchange."@en ;
.
fibo-fnd-pub-pub:PublishingHouse
  rdf:type owl:Class ;
  rdfs:label "publishing house"@en ;
  rdfs:subClassOf fibo-be-le-fbo:FormallyConstitutedOrganization ;
  skos:definition "Some business entity which specializes in publishing information."@en ;
.
fibo-fnd-pub-pub:WebPage
  rdf:type owl:Class ;
  rdfs:label "web page"@en ;
  rdfs:subClassOf fibo-fnd-inf-doc:Document ;
  skos:definition "A document published on the World Wide Web and accessible via a Universal Resource Locator."@en ;
.
fibo-fnd-pub-pub:brandName
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-pub-pub:Publisher ;
  rdfs:label "brand name"@en ;
  rdfs:range fibo-fnd-utl-val:TextValue ;
  skos:definition "The name of marque by which the publisher is identified in its publications. This may or may not be identical to the legal name or the Trading As name, but represents a separate semantic. Usually to be found on book jackets, press releases etc."@en ;
.
fibo-fnd-pub-pub:publishedBy
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-pub-pub:PublishedInformation ;
  rdfs:label "is published by"@en ;
  rdfs:range fibo-fnd-pub-pub:Publisher ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:isProvidedBy ;
  skos:definition "The organization which makes this information available."@en ;
.
fibo-fnd-pub-pub:publisherIdentity
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-pub-pub:Publisher ;
  rdfs:label "publisher identity"@en ;
  rdfs:range fibo-be-le-fbo:FormallyConstitutedOrganization ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:hasIdentity ;
  skos:definition "A publisher of information may be identified as any Business Entity, including for example specialized publishing house, ratings agency, standards body and so on."@en ;
.
fibo-fnd-pub-pub:publishes
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-pub-pub:Publisher ;
  rdfs:label "publishes"@en ;
  rdfs:range fibo-fnd-pub-pub:PublishedInformation ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:provides ;
.
fibo-fnd-pub-pub:uRL
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-pub-pub:WebPage ;
  rdfs:label "u r l"@en ;
  rdfs:range fibo-fnd-utl-val:URIValue ;
  skos:definition "The URL of the web page."@en ;
.
fibo-fnd-pub-std:
  rdfs:label "Standards"@en ;
.
fibo-fnd-pub-std:IndustryAssociation
  rdf:type owl:Class ;
  rdfs:label "industry association"@en ;
  rdfs:subClassOf fibo-be-le-fbo:FormallyConstitutedOrganization ;
  skos:definition "An organization which has membership drawn from members of some industry and acts to serve the interests of those members."@en ;
.
fibo-fnd-pub-std:StandardsBody
  rdf:type owl:Class ;
  rdfs:label "standards body"@en ;
  rdfs:subClassOf fibo-be-le-fbo:FormallyConstitutedOrganization ;
  skos:definition "An organization dedicated exclusively to the propagation of standards."@en ;
.
fibo-fnd-pub-std:StandardsSetter
  rdf:type owl:Class ;
  rdfs:label "standards setter"@en ;
  rdfs:subClassOf fibo-fnd-pub-pub:Publisher ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pub-std:standardsSetterIdentity ;
      owl:someValuesFrom fibo-be-le-fbo:FormallyConstitutedOrganization ;
    ] ;
  skos:definition "An agent which acts to set standards such as message standards, programming standards and so on. This may in principal be any kind of business entity."@en ;
  fibo-fnd-utl-av:explanatoryNote "Note that by virtue of its activities the Standards Setter is also a Publisher, that is it publishes versions of the standards that it sets. However the activity of setting a standard is not itself a direct specialization of the activity of publishing, rather it is a by-product of that activity. This is why there is an apparently redundant parent relationship with Publisher."@en ;
.
fibo-fnd-pub-std:TechnicalStandard
  rdf:type owl:Class ;
  rdfs:label "technical standard"@en ;
  rdfs:subClassOf fibo-fnd-pub-pub:PublishedInformation ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pub-std:hasStandardSetter ;
      owl:someValuesFrom fibo-fnd-pub-std:StandardsSetter ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pub-std:standardSetBy ;
      owl:someValuesFrom fibo-fnd-pub-std:StandardsBody ;
    ] ;
  skos:definition "An established norm or requirement about technical systems; a published document formally setting out such established norm."@en ;
.
fibo-fnd-pub-std:acronym
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-pub-std:TechnicalStandard ;
  rdfs:label "acronym"@en ;
  rdfs:range fibo-fnd-utl-val:TextValue ;
  skos:definition "The abbreviation by which the Standard is referred."@en ;
.
fibo-fnd-pub-std:bodyAcronym
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-pub-std:StandardsSetter ;
  rdfs:label "body acronym"@en ;
  rdfs:range fibo-fnd-utl-val:TextValue ;
  skos:definition "The common acronym by which the standards body is referred in relation to the standards it sets e.g. FIX. Not necessarily the same as the legal name of the business entity that performs this role (e.g. FPL)."@en ;
.
fibo-fnd-pub-std:hasStandardSetter
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-pub-std:TechnicalStandard ;
  rdfs:label "has standard setter"@en ;
  rdfs:range fibo-fnd-pub-std:StandardsSetter ;
  rdfs:subPropertyOf fibo-fnd-pub-pub:publishedBy ;
  skos:definition "The agency by which the standard is set."@en ;
  fibo-fnd-utl-av:explanatoryNote "This may be one of a range of types of entity; this relationship is defined in terms of some entity standing in the role of Standards Setter."@en ;
.
fibo-fnd-pub-std:standardName
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-pub-std:TechnicalStandard ;
  rdfs:label "standard name"@en ;
  rdfs:range fibo-fnd-utl-val:TextValue ;
  skos:definition "The full formal name of the standard."@en ;
.
fibo-fnd-pub-std:standardSetBy
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-pub-std:TechnicalStandard ;
  rdfs:label "standard set by"@en ;
  rdfs:range fibo-fnd-pub-std:StandardsBody ;
  skos:definition "A Standards Body which acts as the setter of the Standard."@en ;
  fibo-fnd-utl-av:explanatoryNote "This is the case whereby the entity which sets the standard is one which is specifically set up for the purpose of setting standards."@en ;
.
fibo-fnd-pub-std:standardsSetterIdentity
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-pub-std:StandardsSetter ;
  rdfs:label "standards setter identity"@en ;
  rdfs:range fibo-be-le-fbo:FormallyConstitutedOrganization ;
  rdfs:subPropertyOf fibo-fnd-pub-pub:publisherIdentity ;
  skos:definition "The entity which performs the role of Standards Setter."@en ;
  fibo-fnd-utl-av:explanatoryNote "This could be any type of Formal Organization."@en ;
.
fibo-fnd-pub-stdi:
  rdfs:label "StandardsIndividuals"@en ;
.
fibo-fnd-pub-stdi:EDMCouncilConvention
  rdf:type owl:Class ;
  rdfs:label "e d m council convention"@en ;
  rdfs:subClassOf fibo-fnd-pub-con:IndustryModelConvention ;
.
fibo-fnd-qt-qtu:
  terms:license "http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense"^^xsd:anyURI ;
  sm:contentLanguage "http://www.omg.org/spec/ODM/"^^xsd:anyURI ;
  sm:contentLanguage "http://www.w3.org/standards/techs/owl#w3c_all"^^xsd:anyURI ;
  sm:copyright """Copyright (c) 2015-2016 EDM Council, Inc.
Copyright (c) 2015-2016 Object Management Group, Inc.""" ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Arrangements/Arrangements/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Arrangements/ClassificationSchemes/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Utilities/Analytics/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"^^xsd:anyURI ;
  sm:fileAbbreviation "fibo-fnd-qt-qtu" ;
  sm:fileAbstract "This ontology provides an initial set of concepts supporting the representation of quantities, units, systems of quantities, and systems of units for use in FIBO. It is compatible with and can be mapped directly to the OMG Date Time Vocabulary (DTV) Quantities Ontology, but has been integrated into FND to provide local coverage of quantities and measurements and eliminate the SBVR mark-up." ;
  sm:filename "QuantitiesAndUnits.rdf" ;
  rdfs:label "Quantities and Units Ontology" ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/AboutFND/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/Quantities/AboutQuantities/"^^xsd:anyURI ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/FND/20160201/Quantities/QuantitiesAndUnits/> ;
.
fibo-fnd-qt-qtu:BaseQuantity
  rdf:type owl:Class ;
  rdfs:label "base quantity" ;
  rdfs:subClassOf fibo-fnd-qt-qtu:QuantityKind ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-qt-qtu:SystemOfQuantities ;
      owl:onProperty fibo-fnd-rel-rel:hasDefinition ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "a quantity kind in a conventionally chosen subset of a given system of quantities, where no subset quantity can be expressed in terms of the others" ;
  skos:example "The International System of Quantities (ISQ) comprises these base quantities (with their SI base measurement units): length (meter), mass (kilogram), duration (second), electric current (ampere), thermodynamic temperature (kelvin), amount of substance (mole), and luminous intensity (candela).  These base quantities are not mutually comparable.  All quantities of any one of these kinds are, however, mutually comparable." ;
  fibo-fnd-utl-av:definitionOrigin "http://www.omg.org/spec/DTV/1.1/"^^xsd:anyURI ;
.
fibo-fnd-qt-qtu:BaseUnit
  rdf:type owl:Class ;
  rdfs:label "base unit" ;
  rdfs:subClassOf fibo-fnd-qt-qtu:MeasurementUnit ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-qt-qtu:SystemOfUnits ;
      owl:onProperty fibo-fnd-rel-rel:hasDefinition ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "a measurement unit that is defined by a system of units to be the reference measurement unit for a base quantity" ;
  fibo-fnd-utl-av:definitionOrigin "http://www.omg.org/spec/DTV/1.1/"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "Quantity units that are not base units are derived units." ;
.
fibo-fnd-qt-qtu:DerivedQuantity
  rdf:type owl:Class ;
  rdfs:label "derived quantity" ;
  rdfs:subClassOf fibo-fnd-qt-qtu:QuantityKind ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-qt-qtu:BaseQuantity ;
      owl:onProperty fibo-fnd-qt-qtu:isDerivedFrom ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "a quantity kind, in a system of quantities, that is not a base quantity of the system but may be defined in terms of base quantities of the system" ;
  skos:example "velocity (length/time), mass density (mass/length3)" ;
  fibo-fnd-utl-av:definitionOrigin "http://www.omg.org/spec/DTV/1.1/"^^xsd:anyURI ;
.
fibo-fnd-qt-qtu:DerivedUnit
  rdf:type owl:Class ;
  rdfs:label "derived unit" ;
  rdfs:subClassOf fibo-fnd-qt-qtu:MeasurementUnit ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-qt-qtu:BaseUnit ;
      owl:onProperty fibo-fnd-qt-qtu:isDerivedFrom ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "a measurement unit for a derived quantity" ;
  skos:example "1 minute = 60 seconds" ;
  fibo-fnd-utl-av:definitionOrigin "http://www.omg.org/spec/DTV/1.1/"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "Every derived unit is defined in terms of base units." ;
.
fibo-fnd-qt-qtu:MeasurementUnit
  rdf:type owl:Class ;
  rdfs:label "measurement unit" ;
  rdfs:subClassOf fibo-fnd-qt-qtu:Quantity ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-qt-qtu:QuantityKind ;
      owl:onProperty fibo-fnd-rel-rel:appliesTo ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-qt-qtu:SystemOfUnits ;
      owl:onProperty fibo-fnd-rel-rel:hasDefinition ;
    ] ;
  skos:definition "a quantity, defined and adopted by convention, with which any other quantity of the same kind can be compared to express the ratio of the two quantities as a number" ;
  skos:example "week, day, hour, minute, second, kilogram, joule, meter" ;
  fibo-fnd-utl-av:definitionOrigin "http://www.omg.org/spec/DTV/1.1/"^^xsd:anyURI ;
.
fibo-fnd-qt-qtu:ParticularQuantity
  rdf:type owl:Class ;
  rdfs:label "particular quantity" ;
  rdfs:subClassOf fibo-fnd-utl-alx:Measure ;
  skos:definition "a property that is of an individual thing and is quantifiable as an instance of some quantity kind" ;
  skos:example "The weight of a given person, the mass of the Earth, the speed of light, and the distance between and are said to be “particular quantities.”" ;
  fibo-fnd-utl-av:definitionOrigin "http://www.omg.org/spec/DTV/1.1/"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "A particular quantity is given by a definite description, which identifies the individual thing and the property. Particular quantities are properties of particular things and are generally expressed by a term for the property and a quantity value." ;
.
fibo-fnd-qt-qtu:Quantity
  rdf:type owl:Class ;
  rdfs:label "quantity" ;
  rdfs:subClassOf fibo-fnd-utl-alx:Measure ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-qt-qtu:hasQuantityKind ;
      owl:someValuesFrom fibo-fnd-qt-qtu:QuantityKind ;
    ] ;
  skos:definition "a property of a phenomenon, body, or substance, to which a number can be assigned with respect to a reference" ;
  skos:editorialNote "The term quantity is used here to refer to the abstraction of the properties - the amount of measurable stuff that can be compared between particular quantities. The height of the something refers to a particular quantity; 555 ft 5 inches refers to a quantity value." ;
  skos:example "second, kilogram, joule, meter. These are quantities in a general sense, which is what is meant here by quantity." ;
  fibo-fnd-utl-av:definitionOrigin "http://www.omg.org/spec/DTV/1.1/"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "A quantity as defined here is said to be a “scalar” as distinct from a “vector.” However, a vector or a tensor whose components are quantities is also considered to be a quantity." ;
.
fibo-fnd-qt-qtu:QuantityKind
  rdf:type owl:Class ;
  rdfs:label "quantity kind" ;
  rdfs:subClassOf fibo-fnd-arr-cls:Classifier ;
  skos:definition "a categorization type for ‘quantity’ that characterizes quantities as being mutually comparable" ;
  skos:example "duration, mass, energy, length" ;
  fibo-fnd-utl-av:definitionOrigin "http://www.omg.org/spec/DTV/1.1/"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "Every instance of ‘quantity kind’ is also a specialization of ‘quantity’. So the concept ‘duration’ is an instance of ‘quantity kind’ and it is a specialization of ‘quantity’, i.e., it is a classifier of actual quantities. But a given duration (i.e., the duration of something) is an instance of ‘duration’ and thus a ‘particular quantity,’ not an instance of ‘quantity kind’. For example, a ‘year’ is not an instance of quantity kind; it is an instance of quantity, but not a category of quantity." ;
.
fibo-fnd-qt-qtu:QuantityValue
  rdf:type owl:Class ;
  rdfs:label "quantity value" ;
  rdfs:subClassOf fibo-fnd-utl-alx:Measure ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-qt-qtu:MeasurementUnit ;
      owl:onProperty fibo-fnd-qt-qtu:hasMeasurementUnit ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onDataRange xsd:decimal ;
      owl:onProperty fibo-fnd-qt-qtu:hasNumericValue ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "number and measurement unit together giving magnitude of a quantity" ;
  skos:example "2 days, 3.5 hours, 150 lb, 45.5 miles" ;
  fibo-fnd-utl-av:definitionOrigin "http://www.omg.org/spec/DTV/1.1/"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "The quantity expressed by a quantity value is the quantity whose ratio to the measurement unit is the number." ;
.
fibo-fnd-qt-qtu:Rate
  rdf:type owl:Class ;
  rdfs:label "rate" ;
  rdfs:subClassOf fibo-fnd-qt-qtu:Quantity ;
  skos:definition "a quantity measured with respect to some other quantity" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.icoachmath.com/math_dictionary/rate.html"^^xsd:anyURI ;
  fibo-fnd-utl-av:adaptedFrom "http://www.thefreedictionary.com/rate"^^xsd:anyURI ;
.
fibo-fnd-qt-qtu:SystemOfQuantities
  rdf:type owl:Class ;
  rdfs:label "system of quantities" ;
  rdfs:subClassOf fibo-fnd-arr-cls:ClassificationScheme ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-qt-qtu:BaseQuantity ;
      owl:onProperty fibo-fnd-rel-rel:defines ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-qt-qtu:DerivedQuantity ;
      owl:onProperty fibo-fnd-rel-rel:involves ;
    ] ;
  skos:definition "set of quantities together with a set of non-contradictory equations relating those quantities" ;
  fibo-fnd-utl-av:definitionOrigin "http://www.omg.org/spec/DTV/1.1/"^^xsd:anyURI ;
.
fibo-fnd-qt-qtu:SystemOfUnits
  rdf:type owl:Class ;
  rdfs:label "system of units" ;
  rdfs:subClassOf fibo-fnd-arr-cls:ClassificationScheme ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-qt-qtu:BaseUnit ;
      owl:onProperty fibo-fnd-rel-rel:defines ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-qt-qtu:DerivedUnit ;
      owl:onProperty fibo-fnd-rel-rel:defines ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-qt-qtu:MeasurementUnit ;
      owl:onProperty fibo-fnd-rel-rel:defines ;
    ] ;
  owl:equivalentClass [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-qt-qtu:SystemOfQuantities ;
      owl:onProperty fibo-fnd-rel-rel:appliesTo ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "a set of measurement units associated with a system of quantities, together with a set of rules that assign one measurement unit to be the base unit for each base quantity in the system of quantities and a set of rules for the derivation of other units from the base units" ;
  skos:example "The International System of Units (SI) is a system of units." ;
  fibo-fnd-utl-av:definitionOrigin "http://www.omg.org/spec/DTV/1.1/"^^xsd:anyURI ;
.
fibo-fnd-qt-qtu:hasMeasurementUnit
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-qt-qtu:QuantityValue ;
  rdfs:label "has measurement unit" ;
  rdfs:range fibo-fnd-qt-qtu:MeasurementUnit ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "a quantity value is expressed in terms of a particular measurement unit" ;
  fibo-fnd-utl-av:definitionOrigin "http://www.omg.org/spec/DTV/1.1/"^^xsd:anyURI ;
.
fibo-fnd-qt-qtu:hasNumericValue
  rdf:type owl:DatatypeProperty ;
  rdfs:label "has numeric value" ;
  rdfs:range xsd:decimal ;
  skos:definition "a number associated with a quantity value" ;
  fibo-fnd-utl-av:definitionOrigin "http://www.omg.org/spec/DTV/1.1/"^^xsd:anyURI ;
.
fibo-fnd-qt-qtu:hasQuantityKind
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-qt-qtu:Quantity ;
  rdfs:label "has quantity kind" ;
  rdfs:range fibo-fnd-qt-qtu:QuantityKind ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "a quantity is an instance of the category of quantity that is the quantity kind" ;
  skos:example "hour (the duration) is an instance of ‘duration’ – a specific quantity of time. So the quantity kind of ‘hour’ is ‘duration’." ;
  fibo-fnd-utl-av:definitionOrigin "http://www.omg.org/spec/DTV/1.1/"^^xsd:anyURI ;
.
fibo-fnd-qt-qtu:isDerivedFrom
  rdf:type owl:ObjectProperty ;
  rdfs:label "is derived from" ;
  skos:definition "a derived quantity is derived from a base quantity; a derived unit is derived from a base unit" ;
  fibo-fnd-utl-av:definitionOrigin "http://www.omg.org/spec/DTV/1.1/"^^xsd:anyURI ;
.
fibo-fnd-rel-rel:
  terms:license "http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense"^^xsd:anyURI ;
  sm:contentLanguage "http://www.omg.org/spec/ODM/"^^xsd:anyURI ;
  sm:contentLanguage "http://www.w3.org/standards/techs/owl#w3c_all"^^xsd:anyURI ;
  sm:copyright """Copyright (c) 2013-2014 EDM Council, Inc.
Copyright (c) 2013-2014 Object Management Group, Inc.""" ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Utilities/BusinessFacingTypes/"^^xsd:anyURI ;
  sm:fileAbbreviation "fibo-fnd-rel-rel" ;
  sm:fileAbstract "This ontology defines a set of general purpose relations for use in other FIBO ontology elements.  These include a number of properties required for reuse across the foundations and business entities models." ;
  sm:filename "Relations.rdf" ;
  rdfs:label "Relations Ontology" ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/AboutFND/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/Relations/AboutRelations/"^^xsd:anyURI ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/FND/20141101/Relations/Relations/> ;
  skos:changeNote "The http://www.omg.org/spec/EDMC-FIBO/FND/20130801/Relations/Relations.rdf version of this ontology was revised to add the appliesTo object property in support of the IND RFC." ;
  skos:changeNote "The http://www.omg.org/spec/EDMC-FIBO/FND/20140501/Relations/Relations.rdf version of this ontology was modified per the issue resolutions identified in the FIBO FND 1.0 FTF report and in http://www.omg.org/spec/EDMC-FIBO/FND/1.0/AboutFND-1.0/. It was further revised in FTF 2 resulting in http://www.omg.org/spec/EDMC-FIBO/FND/20141101/Relations/Relations/." ;
  skos:changeNote """The http://www.omg.org/spec/FIBO/Foundations/20130601/Relations/Relations.owl version of the ontology submitted with 
the FIBO FND RFC, was revised in advance of the September 2013 New Brunswick, NJ meeting, as follows:
   (1) to use slash style URI/IRIss (also called 303 URIs, vs. hash style) as required to support server side processing 
   (2) to use version-independent IRIs for all definitions internally as opposed to version-specific IRIs
   (3) to change the file suffix from .owl to .rdf to increase usability in RDF tools
   (4) to use 4-level abbreviations and corresponding namespace prefixes for all FIBO ontologies, reflecting a family/specification/module/ontology structure
   (5) to incorporate changes to the specification metadata to support documentation at the family, specification, module, and ontology level, similar to the abbreviations
   (6) to move the ontology from the Utilities module to an independent Relations module
   (7) to revise a number of definitions, per discussion with various stakeholders.
   (8) to augment the definitions to include entity names from Business Entities.""" ;
.
fibo-fnd-rel-rel:Classifies
  rdf:type owl:ObjectProperty ;
.
fibo-fnd-rel-rel:Reference
  rdf:type owl:Class ;
  rdfs:label "reference" ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "a concept that refers to (or stands in for) another concept" ;
  fibo-fnd-utl-av:adaptedFrom "http://grammar.about.com/od/rs/g/referenceterm.htm"^^xsd:anyURI ;
.
fibo-fnd-rel-rel:Referent
  rdf:type owl:Class ;
  rdfs:label "referent" ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "the concept that another concept stands for or refers to" ;
  fibo-fnd-utl-av:adaptedFrom "http://grammar.about.com/od/rs/g/referentterm.htm"^^xsd:anyURI ;
.
fibo-fnd-rel-rel:appliesTo
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-rel-rel:Reference ;
  rdfs:label "applies to" ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:refersTo ;
  skos:definition "a relation indicating something that is pertinent or relevant to the concept" ;
.
fibo-fnd-rel-rel:appoints
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-aap-agt:AutonomousAgent ;
  rdfs:label "appoints" ;
  rdfs:range fibo-fnd-aap-agt:AutonomousAgent ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:designates ;
  skos:definition "assigns a job or role to someone, selects or designates to fill an office or a position, fixes or sets by authority or by mutual agreement" ;
  skos:scopeNote "typically requires having the capacity or authority to appoint" ;
  fibo-fnd-utl-av:adaptedFrom "Free Online Dictionary" ;
.
fibo-fnd-rel-rel:causes
  rdf:type owl:ObjectProperty ;
  rdfs:label "causes" ;
  skos:definition "the relationship between an event (the cause) and a second event (the effect), where the second event is understood as a consequence of the first; also, the relationship between a set of factors (causes) and a phenomenon (the effect)" ;
.
fibo-fnd-rel-rel:characterizes
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-rel-rel:Reference ;
  rdfs:label "characterizes" ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:refersTo ;
  skos:definition "describes the character or quality of" ;
.
fibo-fnd-rel-rel:classifies
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-rel-rel:Reference ;
  rdfs:label "classifies" ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:refersTo ;
  skos:definition "arranges in classes; assigns to a category" ;
  fibo-fnd-utl-av:definitionOrigin "Merriam-Webster Dictionary" ;
.
fibo-fnd-rel-rel:comprises
  rdf:type owl:ObjectProperty ;
  rdfs:label "comprises" ;
  skos:definition "includes, especially within a particular scope, is made up of" ;
.
fibo-fnd-rel-rel:confers
  rdf:type owl:ObjectProperty ;
  rdfs:label "confers" ;
  skos:definition "grants or bestows by virtue of some authority" ;
  fibo-fnd-utl-av:definitionOrigin "Merriam-Webster Online Dictionary" ;
  fibo-fnd-utl-av:explanatoryNote "This property should be read as describing the conferral of some legal power or duty, some commitment or some social construct, and is a property of some social construct such as an agreement or some legal authority. These concepts, which would describe the kind of thing of which this is a property, and the kinds of thing in terms of which this property is framed, are outside the scope of this mode land so are not shown." ;
  fibo-fnd-utl-av:synonym "invests with" ;
.
fibo-fnd-rel-rel:controls
  rdf:type owl:ObjectProperty ;
  rdfs:label "controls" ;
  skos:definition "exercises authoritative or dominating influence over; directs" ;
  fibo-fnd-utl-av:adaptedFrom "The American Heritage(R) Dictionary of the English Language, Fourth Edition" ;
.
fibo-fnd-rel-rel:defines
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-rel-rel:Reference ;
  rdfs:label "defines" ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:represents ;
  skos:definition "determines or identifies the essential qualities or meaning of, discovers and sets forth the meaning of, fixes or marks the limits of, demarcates" ;
.
fibo-fnd-rel-rel:denotes
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-rel-rel:Reference ;
  rdfs:label "denotes" ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:represents ;
  skos:definition "represents, calls by a distinctive title, term, or expression" ;
.
fibo-fnd-rel-rel:designates
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-aap-agt:AutonomousAgent ;
  rdfs:label "designates" ;
  skos:definition "to name something officially or appoint someone to a position officially" ;
  fibo-fnd-utl-av:definitionOrigin "http://www.dictionarycentral.com/definition/designate.html"^^xsd:anyURI ;
.
fibo-fnd-rel-rel:embodies
  rdf:type owl:ObjectProperty ;
  rdfs:label "embodies" ;
  skos:definition "is an expression of, or gives a tangible or visible form to (an idea, quality, or feeling), makes concrete and perceptible" ;
  fibo-fnd-utl-av:definitionOrigin "http://www.merriam-webster.com/dictionary/govern"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "This property should be interpreted as being the property of a union of concrete things and information constructs, and as referring to some abstract thing or to some mediating thing or both, in the informative ontology of conceptual abstractions." ;
.
fibo-fnd-rel-rel:governs
  rdf:type owl:ObjectProperty ;
  rdfs:label "governs" ;
  skos:definition "prevails or has decisive influence over; exercises authority" ;
  fibo-fnd-utl-av:definitionOrigin "http://www.merriam-webster.com/dictionary/govern"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "This property should be read as being the property of a logical union of social construct (in the informative abstractions ontology) and legal person, and as referring to 'thing'." ;
.
fibo-fnd-rel-rel:has
  rdf:type owl:ObjectProperty ;
  rdfs:label "has" ;
  skos:definition "indicates that someone (or something) possesses something, as a characteristic, attribute, feature, capability, and so forth" ;
  skos:editorialNote "As used in FIBO, this definition of has specifically excludes possession in the sense of ownership." ;
  fibo-fnd-utl-av:definitionOrigin "Encarta Webster's Dictionary of the English Language (2004)" ;
.
fibo-fnd-rel-rel:hasAcquisitionDate
  rdf:type owl:DatatypeProperty ;
  rdf:type owl:FunctionalProperty ;
  rdfs:label "has acquisition date" ;
  rdfs:range xsd:dateTime ;
  skos:definition "links an asset or owner/controller/controllee to the date of acquisition" ;
.
fibo-fnd-rel-rel:hasAlias
  rdf:type owl:DatatypeProperty ;
  rdfs:label "has alias" ;
  rdfs:range fibo-fnd-utl-bt:text ;
  rdfs:subPropertyOf fibo-fnd-aap-agt:hasName ;
  skos:definition "Any other name by which an individual or organization is known" ;
  skos:editorialNote "Added at SME Review, to meet AML requirements" ;
.
fibo-fnd-rel-rel:hasCommonName
  rdf:type owl:DatatypeProperty ;
  rdfs:label "has common name" ;
  rdfs:range fibo-fnd-utl-bt:text ;
  rdfs:subPropertyOf fibo-fnd-aap-agt:hasName ;
  skos:definition "a name by which something is frequently referred, without reference to any formal usage or structure" ;
.
fibo-fnd-rel-rel:hasContext
  rdf:type owl:ObjectProperty ;
  rdfs:label "has context" ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "provides a context in which something is defined, expressed, or represented" ;
  fibo-fnd-utl-av:explanatoryNote "This property should be read as referring to some context (known as 'mediating thing') in the informative upper ontology which is not included in this model. It should also be read as being the property of some contextually defined thing (known in the informative upper ontology as 'relative thing')." ;
.
fibo-fnd-rel-rel:hasDefinition
  rdf:type owl:ObjectProperty ;
  rdfs:label "has definition" ;
  rdfs:range fibo-fnd-rel-rel:Reference ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:hasRepresentation ;
  owl:inverseOf fibo-fnd-rel-rel:defines ;
  skos:definition "specifies a form of words that conveys the meaning associated with something" ;
.
fibo-fnd-rel-rel:hasDenotation
  rdf:type owl:ObjectProperty ;
  rdfs:label "has denotation" ;
  rdfs:range fibo-fnd-rel-rel:Reference ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:hasRepresentation ;
  owl:inverseOf fibo-fnd-rel-rel:denotes ;
  skos:definition "relates a concept (or something else, but typically a concept) to a representation or denotation for that concept" ;
.
fibo-fnd-rel-rel:hasDesignation
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-aap-agt:AutonomousAgent ;
  rdfs:label "has designation" ;
  skos:definition "relates an individual or organization to a position, role, or other designation" ;
.
fibo-fnd-rel-rel:hasDispositionDate
  rdf:type owl:DatatypeProperty ;
  rdf:type owl:FunctionalProperty ;
  rdfs:label "has disposition date" ;
  rdfs:range xsd:dateTime ;
  skos:definition "links something, such as an asset or its owner/controller/controllee to the date something was sold, transferred, destroyed, etc." ;
.
fibo-fnd-rel-rel:hasFormalName
  rdf:type owl:DatatypeProperty ;
  rdfs:label "has formal name" ;
  rdfs:range fibo-fnd-utl-bt:text ;
  rdfs:subPropertyOf fibo-fnd-aap-agt:hasName ;
  skos:definition "a name by which something is known for some official purpose or context, or which is structured in some way such as to always follow the same format regardless of usage" ;
.
fibo-fnd-rel-rel:hasIdentity
  rdf:type owl:ObjectProperty ;
  rdfs:label "has identity" ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "provides a means for identifying something that fills a particular role" ;
  fibo-fnd-utl-av:explanatoryNote "This property should be read as being a property of some kind of 'relative thing' as defined externality to this ontology. The property is usually but not exclusively framed with reference to some 'independent thing' but may take other forms and so should be regarded as having a target of 'thing'." ;
.
fibo-fnd-rel-rel:hasLegalName
  rdf:type owl:DatatypeProperty ;
  rdfs:label "has legal name" ;
  rdfs:range fibo-fnd-utl-bt:text ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:hasFormalName ;
  skos:definition "the name used to refer to an person or organization in legal communications" ;
.
fibo-fnd-rel-rel:hasMember
  rdf:type owl:ObjectProperty ;
  rdfs:label "has member" ;
  owl:inverseOf fibo-fnd-rel-rel:isMemberOf ;
  skos:definition "relates something, typically a group or organization, to some discrete thing identified as a part (member) of it" ;
  fibo-fnd-utl-av:explanatoryNote "This property should be read as being the property of a logical union of group and organization (not shown)." ;
.
fibo-fnd-rel-rel:hasPart
  rdf:type owl:ObjectProperty ;
  rdfs:label "has part" ;
  skos:definition "indicates any portion of a thing, regardless of whether the portion itself is attached to the remainder or detached; cognitively salient or arbitrarily demarcated; self-connected or disconnected; homogeneous or gerrymandered; material or immaterial; extended or unextended; spatial or temporal" ;
  fibo-fnd-utl-av:adaptedFrom "Stanford Encyclopedia of Philosophy at http://plato.stanford.edu/entries/mereology/" ;
  fibo-fnd-utl-av:explanatoryNote "This property relates a thing to anything which is a proper part of that thing. This is not parthood in the sense of a the role of part which may be played by interchangeable things such as wheels; instead this property relates an independent thing to something which makes up a part of it." ;
.
fibo-fnd-rel-rel:hasRepresentation
  rdf:type owl:ObjectProperty ;
  rdfs:label "has representation" ;
  rdfs:range fibo-fnd-rel-rel:Reference ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "relates a concept to some textual or other symbol which is intended to convey the sense of that concept or to some form of words which sets out the meaning of that concept" ;
.
fibo-fnd-rel-rel:hasUniqueIdentifier
  rdf:type owl:DatatypeProperty ;
  rdf:type owl:FunctionalProperty ;
  rdfs:label "has unique identifier" ;
  rdfs:range fibo-fnd-utl-bt:text ;
  skos:definition "has some textual or numeric information which when taken in combination with some associated scheme is unique to the thing and may be used to distinguish it from other things of the same or different type" ;
  fibo-fnd-utl-av:explanatoryNote "With reference to a given (possibly implicit) set of objects, a unique identifier (UID) is any identifier which is guaranteed to be unique among all identifiers used for those objects and for a specific purpose. The uniqueness requires and is guaranteed by the existence of a scheme associated with the identifier." ;
.
fibo-fnd-rel-rel:holds
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-aap-agt:AutonomousAgent ;
  rdfs:label "holds" ;
  skos:definition "is the relationship between an individual or organization and something it possesses, or over which it exercises some ownership or control or has at its discretion the ability to dispose of it as it sees fit" ;
.
fibo-fnd-rel-rel:involves
  rdf:type owl:ObjectProperty ;
  rdfs:label "involves" ;
  skos:definition "(of a situation or event) includes (something) as a necessary part or result" ;
.
fibo-fnd-rel-rel:isAppointedBy
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-aap-agt:AutonomousAgent ;
  rdfs:label "is appointed by" ;
  rdfs:range fibo-fnd-aap-agt:AutonomousAgent ;
  owl:inverseOf fibo-fnd-rel-rel:appoints ;
  skos:definition "indicates the individual or group that has assigned or appointed someone to an office or position" ;
.
fibo-fnd-rel-rel:isCausedBy
  rdf:type owl:ObjectProperty ;
  rdfs:label "is caused by" ;
  owl:inverseOf fibo-fnd-rel-rel:causes ;
  skos:definition "is the relationship between an event (the effect) and a second event (the cause), where the first event is understood as a consequence of the second; also, the relationship between a set of factors (causes) and a phenomenon (the effect)" ;
.
fibo-fnd-rel-rel:isClassifiedBy
  rdf:type owl:ObjectProperty ;
  rdfs:label "is classified by" ;
  rdfs:range fibo-fnd-rel-rel:Reference ;
  owl:inverseOf fibo-fnd-rel-rel:classifies ;
  skos:definition "indicates the classification scheme used to classify something" ;
.
fibo-fnd-rel-rel:isConferredBy
  rdf:type owl:ObjectProperty ;
  rdfs:label "is conferred by" ;
  owl:inverseOf fibo-fnd-rel-rel:confers ;
  skos:definition "a relationship between a right or obligation and the vehicle, such as an agreement or contract, that vests (or confers) said right or obligation" ;
  fibo-fnd-utl-av:explanatoryNote "This property should be read as describing some legal power or duty, some commitment or some social construct being conferred as a result of some social construct such as an agreement or some legal authority. These concepts, which would describe the kind of thing of which this is a property, and the kinds of thing in terms of which this property is framed, are outside the scope of this model and so are not shown." ;
.
fibo-fnd-rel-rel:isConferredOn
  rdf:type owl:ObjectProperty ;
  rdfs:label "is conferred on" ;
  rdfs:range fibo-fnd-aap-agt:AutonomousAgent ;
  skos:definition "that on which the conferred thing is conferred" ;
.
fibo-fnd-rel-rel:isControlledBy
  rdf:type owl:ObjectProperty ;
  rdfs:label "is controlled by" ;
  owl:inverseOf fibo-fnd-rel-rel:controls ;
  skos:definition "is influenced, managed, or directed by" ;
.
fibo-fnd-rel-rel:isDefinedBy
  rdf:type owl:ObjectProperty ;
  rdfs:label "is defined by" ;
.
fibo-fnd-rel-rel:isGovernedBy
  rdf:type owl:ObjectProperty ;
  rdfs:label "is governed by" ;
  owl:inverseOf fibo-fnd-rel-rel:governs ;
  skos:definition "a relationship between a contract, agreement, jurisdiction, or other legal construct and the regulation, policy, procedure, or legal person that regulates or oversees (governs) it" ;
  fibo-fnd-utl-av:explanatoryNote "This property should be read as being the property of some thing and as referring to a logical union of social construct (in the informative abstractions ontology) and legal person." ;
.
fibo-fnd-rel-rel:isHeldBy
  rdf:type owl:ObjectProperty ;
  rdfs:label "is held by" ;
  rdfs:range fibo-fnd-aap-agt:AutonomousAgent ;
  owl:inverseOf fibo-fnd-rel-rel:holds ;
  skos:definition "something that is possessed by and at least partially under the control of something which can be used or acted on by the holder, regardless of ownership" ;
.
fibo-fnd-rel-rel:isIssuedBy
  rdf:type owl:ObjectProperty ;
  rdfs:label "is issued by" ;
  rdfs:range fibo-fnd-aap-agt:AutonomousAgent ;
  skos:definition "identifies an office or organization responsible for circulating, distributing, or publishing something" ;
  fibo-fnd-utl-av:definitionOrigin "http://www.thefreedictionary.com/issue"^^xsd:anyURI ;
.
fibo-fnd-rel-rel:isManagedBy
  rdf:type owl:ObjectProperty ;
  rdfs:label "is managed by" ;
  skos:definition "relates something to another thing that has some role in directing its affairs" ;
  fibo-fnd-utl-av:explanatoryNote "The target or range of this property should be read as always being some kind of 'relative thing', that is a thing defined in some context. Generally this will be a 'party in role'. This property is not intended to be used to relate a thing to some independent thing which it is managed by, only to something in the role of being that which manages it." ;
.
fibo-fnd-rel-rel:isMandatedBy
  rdf:type owl:ObjectProperty ;
  rdfs:label "is mandated by" ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:isConferredBy ;
  skos:definition "relates a responsibility, capacity, or action to that which requires it" ;
  fibo-fnd-utl-av:explanatoryNote "This prooerty should be read as being a property of some social construct as defined in the informative ontology for conceptual abstractions, to some other social construct such as a legal instrument or an agreement." ;
.
fibo-fnd-rel-rel:isMemberOf
  rdf:type owl:ObjectProperty ;
  rdfs:label "is member of" ;
  skos:definition "belonging, either individually or collectively, to a group" ;
  fibo-fnd-utl-av:explanatoryNote "This property should be read as being framed in terms of a logical union of group and organization (not shown)." ;
.
fibo-fnd-rel-rel:isPartOf
  rdf:type owl:ObjectProperty ;
  rdf:type owl:TransitiveProperty ;
  rdfs:label "is a part of" ;
  owl:inverseOf fibo-fnd-rel-rel:hasPart ;
  skos:definition "relates something to a thing that it is some component or portion of, regardless of how that whole-part relationship is manifested, i.e., attached to the remainder or detached; cognitively salient or arbitrarily demarcated; self-connected or disconnected; homogeneous or gerrymandered; material or immaterial; extended or unextended; spatial or temporal; the most generic part relation, reflexive, asymmetric, and transitive" ;
  fibo-fnd-utl-av:adaptedFrom "Stanford Encyclopedia of Philosophy at http://plato.stanford.edu/entries/mereology/" ;
  fibo-fnd-utl-av:explanatoryNote "This property represents what is also known in the literature of 'proper parthood',that is the recursive (transitive) relationship whereby things have parts which have parts and so on. This is distinct from a separate meaning of 'has part' which would refer to an item playing the named role of a part such as a nearside front wheel. for the avoidance of doubt, this is not that relationship, and this property applies betwen independent things and other independent things which may make up their parts." ;
.
fibo-fnd-rel-rel:isProvidedBy
  rdf:type owl:ObjectProperty ;
  rdfs:label "is provided by" ;
  owl:inverseOf fibo-fnd-rel-rel:provides ;
  skos:definition "is made available by" ;
  fibo-fnd-utl-av:explanatoryNote "The target or range of this property should be read as always being some kind of 'relative thing', that is a thing defined in some context. Generally this will be a 'party in role'. This property is not intended to be used to relate a thing to some independent thing which it is provided by, only to something in the role of being that which provides it." ;
.
fibo-fnd-rel-rel:isUsedBy
  rdf:type owl:ObjectProperty ;
  rdfs:label "is used by" ;
  rdfs:range fibo-fnd-aap-agt:AutonomousAgent ;
  skos:definition "relates something to a thing that has the ability to employ or deploy it as appropriate" ;
.
fibo-fnd-rel-rel:manages
  rdf:type owl:ObjectProperty ;
  rdfs:label "manages" ;
  owl:inverseOf fibo-fnd-rel-rel:isManagedBy ;
  skos:definition "relates an autonomous agent to something that it directs in some way" ;
  fibo-fnd-utl-av:explanatoryNote "This property should be read as always being a property of some kind of 'relative thing', that is a thing defined in some context. Generally this will be a 'party in role'. This property is not intended to be used to relate some independent thing to that which it manages, instead it must only be a property of something in the role of being that which manages some thing." ;
.
fibo-fnd-rel-rel:provides
  rdf:type owl:ObjectProperty ;
  rdfs:label "provides" ;
  skos:definition "makes something available to" ;
  fibo-fnd-utl-av:explanatoryNote "This property should be read as always being a property of some kind of 'relative thing', that is a thing defined in some context. Generally this will be a 'party in role'. This property is not intended to be used to relate some independent thing to that which it provides, instead it must only be a property of something in the role of being that which provides some thing." ;
.
fibo-fnd-rel-rel:refersTo
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-rel-rel:Reference ;
  rdfs:label "refers to" ;
  skos:definition "the relationship between a reference and the concept it stands for or refers to, i.e., the referent for that reference" ;
.
fibo-fnd-rel-rel:represents
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-rel-rel:Reference ;
  rdfs:label "represents" ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:refersTo ;
  owl:inverseOf fibo-fnd-rel-rel:hasRepresentation ;
  skos:definition "relates some textual or other symbol or some set of words to some concept that has the sense or meaning the representation is intended to convey" ;
.
fibo-fnd-rel-rel:uses
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-aap-agt:AutonomousAgent ;
  rdfs:label "uses" ;
  owl:inverseOf fibo-fnd-rel-rel:isUsedBy ;
  skos:definition "relates an autonomous agent to something that it has the ability to employ in some way" ;
.
fibo-fnd-rel-rel:wasFormerlyKnownAs
  rdf:type owl:DatatypeProperty ;
  rdfs:label "was formerly known as" ;
  rdfs:range fibo-fnd-utl-bt:text ;
  rdfs:subPropertyOf fibo-fnd-aap-agt:hasName ;
  skos:definition "a name by which something was known in the past" ;
.
