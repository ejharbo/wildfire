.
fibo-sec-sec-sec:CashSecuritiesDelivery
  rdf:type owl:Class ;
  rdfs:label "cash securities delivery"@en ;
  rdfs:subClassOf fibo-sec-sec-sec:SecurityDeliveryType ;
  skos:definition "Delivery of a security in cash"@en ;
.
fibo-sec-sec-sec:ExchangeTradedSecurity
  rdf:type owl:Class ;
  rdfs:label "exchange traded security"@en ;
  rdfs:subClassOf fibo-sec-sec-sec:TradingRegisteredSecurity ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-sec-id:hasNationalIdentifier ;
      owl:someValuesFrom fibo-sec-sec-id:NationalSecuritiesIdentifyingNumber ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-sec-lst:hasPlaceOfListing ;
      owl:someValuesFrom fibo-sec-sec-lst:SecurityListing ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-sec-resx:hasRestrictions ;
      owl:someValuesFrom fibo-sec-sec-resx:InvestmentRestriction ;
    ] ;
  skos:definition "Tradable security traded on an exchange or other trading venue."@en ;
.
fibo-sec-sec-sec:NonCashSecuritiesDelivery
  rdf:type owl:Class ;
  rdfs:label "non cash securities delivery"@en ;
  rdfs:subClassOf fibo-sec-sec-sec:SecurityDeliveryType ;
  skos:definition "Delivery is by some other means."@en ;
  skos:editorialNote "Review: There may be more applicable values for this selection. Used in settlement terms and (if required) in settlement conventions for the security."@en ;
.
fibo-sec-sec-sec:Security
  rdf:type owl:Class ;
  rdfs:label "security"@en ;
  rdfs:subClassOf fibo-fnd-agr-ctr:TransferableContract ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-sec-sec-sec:SecurityStructuredName ;
      owl:onProperty fibo-sec-sec-sec:hasStructuredName ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-bp-iss-trm:hasIssuanceInformation ;
      owl:someValuesFrom fibo-bp-iss-trm:IssuedSecurityIssueInformation ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-bp-iss-trm:issuer ;
      owl:someValuesFrom fibo-bp-iss-trm:SecurityIssuer ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-inf-tem:hasStatus ;
      owl:someValuesFrom fibo-md-tem-trs:SecurityLifecycleStatus ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-md-tem-ins:hasPrice ;
      owl:someValuesFrom fibo-md-tem-ins:TradedSecurityPublishedPrice ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-sec-sec:denominationCurrency ;
      owl:someValuesFrom fibo-fnd-acc-cur:Currency ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-sec-sec:hasDefaultSettlementTerms ;
      owl:someValuesFrom fibo-sec-sec-sec:SecuritySettlementConvention ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-sec-sec:hasStructuredName ;
      owl:someValuesFrom fibo-sec-sec-sec:SecurityStructuredName ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-sec-sec:legallyRecordedIn ;
      owl:someValuesFrom fibo-fnd-law-jur:Jurisdiction ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-sec-lst:issuedIn ;
      owl:someValuesFrom fibo-sec-sec-lst:FinancialMarket ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-sec-tax:hasTaxTreatment ;
      owl:someValuesFrom fibo-sec-sec-tax:SecurityTaxTreatment ;
    ] ;
  skos:definition "A financial instrument which can be bought or sold. This may be traded either over the counter or through an exchange or other trading venue."@en ;
  fibo-fnd-utl-av:explanatoryNote "This is a publicly traded security (corresponding to the scope of \"Security\" in ISO 20022) and therefore does not include privately held equity or equity in companies that are not publicly traded."@en ;
.
fibo-sec-sec-sec:SecurityContractTerms
  rdf:type owl:Class ;
  rdfs:label "security contract terms"@en ;
  rdfs:subClassOf fibo-fnd-agr-ctr:ContractualCommitment ;
  skos:definition "Contractual terms of a negotiable financial security"@en ;
.
fibo-sec-sec-sec:SecurityDeliveryType
  rdf:type owl:Class ;
  rdfs:label "security delivery type  (please review)"@en ;
  rdfs:subClassOf fibo-fnd-arrx-meth:DeliveryMethod ;
  skos:definition "Type of delivery as used in settlement terms for a financial security."@en ;
.
fibo-sec-sec-sec:SecurityNote
  rdf:type owl:Class ;
  rdfs:label "security note  (please review)"@en ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "An individually denominated note within a security or tranche of a security, where this is issued in notes of different denominations."@en ;
  skos:editorialNote "Introduced for MBS Proof of Concept, to model MBS tranches where one tranche may have notes of multiple denominations. Other applications of this concept are not known at present. Trivially, all securities could be said to be in \"notes\" in this sense but the term is not normally referred to."@en ;
.
fibo-sec-sec-sec:SecuritySettlementConvention
  rdf:type owl:Class ;
  rdfs:label "security settlement convention"@en ;
  rdfs:subClassOf fibo-sec-sec-sec:SettlementConvention ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-sec-sec:hasDeliveryMethod ;
      owl:someValuesFrom fibo-sec-sec-sec:SecurityDeliveryType ;
    ] ;
  skos:definition "Default Terms for the settlement of a Security."@en ;
.
fibo-sec-sec-sec:SecurityStructuredName
  rdf:type owl:Class ;
  rdfs:label "security structured name"@en ;
  rdfs:subClassOf fibo-fnd-inf-inf:StructuredName ;
  skos:definition "The formal, structured name of a Security."@en ;
.
fibo-sec-sec-sec:SettlementConvention
  rdf:type owl:Class ;
  rdfs:label "settlement convention"@en ;
  rdfs:subClassOf fibo-fnd-pub-con:Convention ;
  skos:definition "A convention for settlement of a trade or transaction."@en ;
  fibo-fnd-utl-av:explanatoryNote "This is usually stated in the form 'T+n' where n is the Settlement Business Days. There are no date roll rules, the T+n simply refers to the next available business days and is specified as a whole number. Note that this term has been created for the OTC model, but on review of the existing terms for security settlement conventions in the common model for tradable securities, this term has been separated out as a more general term, with the securities-specific in a sub-type of this item, renamed to Security Settlement Convention."@en ;
.
fibo-sec-sec-sec:Stake
  rdf:type owl:Class ;
  rdfs:label "stake"@en ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "The holding of some portion of some venture or something of value, by some party. The stake may be represented by some form of formal document such as a share or some debt unit."@en ;
.
fibo-sec-sec-sec:TradingRegisteredSecurity
  rdf:type owl:Class ;
  rdfs:label "trading registered security"@en ;
  rdfs:subClassOf fibo-fbc-fi-fi:Security ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-sec-lst:hasListing ;
      owl:someValuesFrom fibo-sec-sec-lst:SecurityListing ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-sec-lst:issuedIn ;
      owl:someValuesFrom fibo-sec-sec-lst:SecuritiesTradingFacility ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-sec-lst:tradedOn ;
      owl:someValuesFrom fibo-sec-sec-lst:SecuritiesTradingFacility ;
    ] ;
  skos:definition "A security which is traded on some trading venue."@en ;
  skos:editorialNote "Define the three types of these as per the MIFID definitions of types of trading facility. Also need to allow for jurisdiction variation. Renamed March 2013 from Listed Security to Trading Registered Security (Citi review session). The rationale is that any security which is listed on some facility is registered, and the fact of it being registered is more significant. This is still to be reviewed more widely with the SME review group."@en ;
.
fibo-sec-sec-sec:allowsContractualSettlement
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-sec-sec:SecuritySettlementConvention ;
  rdfs:label "allows contractual settlement  (please review)"@en ;
  rdfs:range fibo-fnd-utl-val:TrueFalseValue ;
  skos:definition "Whether the market allows contractual settlement. This is a settlement term related to the market."@en ;
.
fibo-sec-sec-sec:bearer
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-sec-sec:Security ;
  rdfs:label "bearer  (please review)"@en ;
  rdfs:range fibo-fnd-utl-val:TrueFalseValue ;
  skos:definition "The owner is not registered in the books of the issuer or of the registrar."@en ;
.
fibo-sec-sec-sec:deemedRiskCurrency
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-sec-sec:Security ;
  rdfs:label "deemed risk currency  (please review)"@en ;
  rdfs:range fibo-fnd-acc-cur:Currency ;
  skos:definition "The currency in which any risks relating to the instrument are seen to take place. Further notes: This is a risk specific term identified during SME Reviews of common securities terms. It was agreed at a review with a risk firm that this term is applicable within the context of risk management only and is not a fundamental fact about the security. It therefore needs to be disposed in some way to show it in the context of risk."@en ;
.
fibo-sec-sec-sec:denominationCurrency
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-sec-sec:Security ;
  rdfs:label "denomination currency  (please review)"@en ;
  rdfs:range fibo-fnd-acc-cur:Currency ;
  skos:definition "The default currency in which units of the Security are denominated."@en ;
.
fibo-sec-sec-sec:denominationIncrement
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-sec-sec:Security ;
  rdfs:label "denomination increment  (please review)"@en ;
  rdfs:range fibo-fnd-utl-val:IntegerValue ;
  skos:definition "The multiples above the smallest possible denomination (Default Lot Size) in which the security can be held."@en ;
.
fibo-sec-sec-sec:fASTApplicable
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-sec-sec:Security ;
  rdfs:label "f a s t applicable  (please review)"@en ;
  rdfs:range fibo-fnd-utl-val:TrueFalseValue ;
  skos:definition "Whether the security is to be held at the transfer agent as part of the FAST (Fully Automated Securities Transfer) program. Further notes: Term is specific to DTCC / US context. Is there a more generic meaning similar to this which applies across different jurisdictions? If not this term should be defined with reference to this specific context."@en ;
.
fibo-sec-sec-sec:fractionalEligible
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-sec-sec:Security ;
  rdfs:label "fractional eligible  (please review)"@en ;
  rdfs:range fibo-fnd-utl-val:TrueFalseValue ;
  skos:definition "Whether the security can contain Fractional Share/amount component."@en ;
.
fibo-sec-sec-sec:fullName
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-sec-sec:SecurityStructuredName ;
  rdfs:label "full name  (please review)"@en ;
  rdfs:range fibo-fnd-utl-val:TextValue ;
  skos:definition "Full name of the security"@en ;
.
fibo-sec-sec-sec:hasDefaultSettlementTerms
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-sec-sec:Security ;
  rdfs:label "has default settlement terms  (please review)"@en ;
  rdfs:range fibo-sec-sec-sec:SecuritySettlementConvention ;
.
fibo-sec-sec-sec:hasDeliveryMethod
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-sec-sec:SecuritySettlementConvention ;
  rdfs:label "has delivery method  (please review)"@en ;
  rdfs:range fibo-sec-sec-sec:SecurityDeliveryType ;
  skos:definition "Has a method by which securities are delivered at settlement."@en ;
.
fibo-sec-sec-sec:hasStructuredName
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-sec-sec:Security ;
  rdfs:label "has structured name"@en ;
  rdfs:range fibo-sec-sec-sec:SecurityStructuredName ;
  rdfs:subPropertyOf fibo-fnd-inf-inf:hasName ;
.
fibo-sec-sec-sec:interchangeable
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-sec-sec:Security ;
  rdfs:label "interchangeable  (please review)"@en ;
  rdfs:range fibo-fnd-utl-val:TrueFalseValue ;
  skos:definition "Whether or not the security is freely interchangeable with other of the same securities. Yes indicates that the security is interchangeable; no indicates that it is not."@en ;
.
fibo-sec-sec-sec:isListed
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-sec-sec:TradingRegisteredSecurity ;
