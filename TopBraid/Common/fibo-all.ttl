fibo-fbc-fct-cafse:CreditUnion
  rdf:type owl:Class ;
  rdfs:label "credit union" ;
  rdfs:seeAlso "http://www.investopedia.com/terms/c/creditunion.asp"^^xsd:anyURI ;
  rdfs:subClassOf fibo-fbc-fct-fse:SavingsAssociation ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-be-le-cb:NotForProfitCorporation ;
      owl:onProperty fibo-fnd-rel-rel:hasIdentity ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "a not-for-profit financial institution, typically formed by the employees of a company, labor union, or religious group, operated as a cooperative association organized for the purpose of promoting thrift among its members and creating a source of credit for provident or productive purposes" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
