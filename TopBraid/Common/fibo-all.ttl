  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-ln-ln-up:hasString ;
      owl:someValuesFrom xsd:string ;
    ] ;
  skos:definition "A set of words and characters expressed in some language. E.g. a description, a message" ;
  skos:editorialNote "This probably belongs in a documents messages ontology" ;
.
fibo-ln-ln-up:TransactionEvent
