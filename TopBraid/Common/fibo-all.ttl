fibo-der-rgt-raw:
  rdfs:label "RightsAndWarrants"@en ;
.
fibo-der-rgt-raw:AllotmentRight
  rdf:type owl:Class ;
  rdfs:label "allotment right"@en ;
  rdfs:subClassOf fibo-der-rgt-raw:StockholdersRightsInstrument ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-rgt-raw:numberOfSecuritiesDeterminedBy ;
      owl:someValuesFrom fibo-der-rgt-raw:AllotmentRightFormula ;
    ] ;
  skos:definition "Privileges allotted to existing security holders, entitling them to receive new securities free of charge."@en ;
.
fibo-der-rgt-raw:AllotmentRightFormula
  rdf:type owl:Class ;
  rdfs:label "allotment right formula"@en ;
  rdfs:subClassOf fibo-fnd-math-math:Formula ;
  skos:definition "A formula used to calculate the number of securities for an Allotment Right, based on the number of these instruments that the holder holds. Conesnsus:Review"@en ;
.
fibo-der-rgt-raw:AllotmentRightHolder
  rdf:type owl:Class ;
  rdfs:label "allotment right holder"@en ;
  rdfs:subClassOf fibo-der-rgt-raw:StockholdersRightsInstrumentHolder ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-rgt-raw:isByVirtueOfBeing ;
      owl:someValuesFrom fibo-sec-sec-ast:SecurityHolder ;
    ] ;
  skos:definition "The party which is the holder of an allotment right. This is identified as being a party which is also an existing holder of the security identified as the underlying security."@en ;
.
fibo-der-rgt-raw:CommodityWarrant
  rdf:type owl:Class ;
  rdfs:label "commodity warrant"@en ;
  rdfs:subClassOf fibo-der-rgt-raw:Warrant ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-rgt-raw:hasExerciseTerms ;
      owl:someValuesFrom fibo-der-rgt-raw:CommodityWarrantExerciseTerms ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-rgt-raw:hasWarrantUnderlying ;
      owl:someValuesFrom fibo-fnd-bus-pasx:NegotiableCommodity ;
    ] ;
  skos:definition "A derivative based on commodities contracts traded on exchanges."@en ;
  fibo-fnd-utl-av:explanatoryNote "Example: Commodity Warrants Australia (CWA) is a locally owned outfit that has been in business for 18 months. It sells warrants based on 12 commodities and financial markets - crude oil, gold, silver, live cattle, corn, orange juice, soy, coffee, cocoa, the Dow Jones Industrial Average, the NASDAQ Composite Index and the S&P 500 Index"@en ;
.
fibo-der-rgt-raw:CommodityWarrantExerciseTerms
  rdf:type owl:Class ;
  rdfs:label "commodity warrant exercise terms"@en ;
  rdfs:subClassOf fibo-der-rgt-raw:WarrantExerciseTermsSet ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-rgt-raw:delivery ;
      owl:someValuesFrom fibo-fnd-acc-acc:CashCurrency ;
    ] ;
  skos:definition "Terms setting out how a commodity warrant may be exercised, including the form which delivery takes."@en ;
.
fibo-der-rgt-raw:CompanyWarrant
  rdf:type owl:Class ;
  rdfs:label "company warrant"@en ;
  rdfs:subClassOf fibo-der-rgt-raw:Warrant ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-rgt-raw:hasExerciseTerms ;
      owl:someValuesFrom fibo-der-rgt-raw:CompanyWarrantExerciseTermsSet ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-rgt-raw:hasWarrantUnderlying ;
      owl:someValuesFrom fibo-fbc-fi-fi:Security ;
    ] ;
  owl:disjointWith fibo-der-rgt-raw:CoveredWarrant ;
  skos:definition "Warrants which are issued by the issuer of the underlying."@en ;
.
fibo-der-rgt-raw:CompanyWarrantExerciseTermsSet
  rdf:type owl:Class ;
  rdfs:label "company warrant exercise terms set"@en ;
  rdfs:subClassOf fibo-der-rgt-raw:WarrantExerciseTermsSet ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-rgt-raw:delivery ;
      owl:someValuesFrom fibo-fbc-fi-fi:Security ;
    ] ;
  owl:disjointWith fibo-der-rgt-raw:CoveredWarrantExerciseTerms ;
  skos:definition "Terms setting out how a company warrant or naked warrant may be exercised, including the form which delivery takes."@en ;
.
fibo-der-rgt-raw:CoveredWarrant
  rdf:type owl:Class ;
  rdfs:label "covered warrant"@en ;
  rdfs:subClassOf fibo-der-rgt-raw:Warrant ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-rgt-raw:hasExerciseTerms ;
      owl:someValuesFrom fibo-der-rgt-raw:CoveredWarrantExerciseTerms ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-rgt-raw:hasWarrantUnderlying ;
      owl:someValuesFrom fibo-fbc-fi-fi:Security ;
    ] ;
.
fibo-der-rgt-raw:CoveredWarrantExerciseTerms
  rdf:type owl:Class ;
  rdfs:label "covered warrant exercise terms"@en ;
  rdfs:subClassOf fibo-der-rgt-raw:WarrantExerciseTermsSet ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-rgt-raw:delivery ;
      owl:someValuesFrom fibo-fbc-fi-fi:Security ;
    ] ;
.
fibo-der-rgt-raw:EntitlementRightsInstrument
  rdf:type owl:Class ;
  rdfs:label "entitlement rights instrument"@en ;
  rdfs:subClassOf fibo-fbc-fi-fi:DerivativeInstrument ;
  rdfs:subClassOf fibo-fbc-fi-fi:Security ;
  skos:definition "A security giving the holder of that security the entitlement to purchase or sell back to the issuer new securities or some other commodity or instrument at a predetermined price."@en ;
.
fibo-der-rgt-raw:ExchangeTradedWarrant
  rdf:type owl:Class ;
  rdfs:label "exchange traded warrant"@en ;
  rdfs:subClassOf fibo-der-rgt-raw:Warrant ;
  rdfs:subClassOf fibo-sec-sec-lst:ExchangeTradedSecurity ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-rgt-raw:hasListing ;
      owl:someValuesFrom fibo-der-rgt-raw:WarrantListing ;
    ] ;
  skos:definition "A warrant which is traded on a securities exchange."@en ;
  fibo-fnd-utl-av:explanatoryNote "This may be any kind of warrant except a (usually) Company Warrant. According to notes on Traditional Wwarrant: Warrants may be privately issued and may not necessarily be traded on an exchange. (Wikipedia)"@en ;
.
fibo-der-rgt-raw:PurchaseRight
  rdf:type owl:Class ;
  rdfs:label "purchase right"@en ;
  rdfs:subClassOf fibo-der-rgt-raw:StockholdersRightsInstrument ;
  skos:definition "Anti-takeover device that gives a prospective acquiree’s shareholders the right to buy usually shares of the firm or shares of anyone who acquires the firm at a deep discount to their fair market value."@en ;
  fibo-fnd-utl-av:explanatoryNote "The underlying is usually shares but this not necessarily the case. Also known as \"Poison Pill\"."@en ;
.
fibo-der-rgt-raw:StockholdersRightsInstrument
  rdf:type owl:Class ;
  rdfs:label "stockholders rights instrument"@en ;
  rdfs:subClassOf fibo-der-rgt-raw:EntitlementRightsInstrument ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-bp-iss-trm:issuer ;
      owl:someValuesFrom fibo-der-rgt-raw:StockholdersRightsInstrumentIssuer ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-rgt-raw:givesTheRightToBuy ;
      owl:someValuesFrom fibo-fbc-fi-fi:Security ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-sec-assx:hasHolder ;
      owl:someValuesFrom fibo-der-rgt-raw:StockholdersRightsInstrumentHolder ;
    ] ;
  skos:definition "A security giving stockholders entitlement to purchase new securities issued by the corporation at a predetermined price (normally less than the current market price) in proportion to the number of securities already owned."@en ;
  fibo-fnd-utl-av:explanatoryNote "Stockholders may sell these to non stockholders if they wish. Rights are issued only for a short period of time, after which they expire."@en ;
.
fibo-der-rgt-raw:StockholdersRightsInstrumentHolder
  rdf:type owl:Class ;
  rdfs:label "stockholders rights instrument holder"@en ;
  rdfs:subClassOf fibo-sec-sec-ast:SecurityHolder ;
  skos:definition "A party which is the holder of a Stockholders Rights instrument."@en ;
.
fibo-der-rgt-raw:StockholdersRightsInstrumentIssuer
  rdf:type owl:Class ;
  rdfs:label "stockholders rights instrument issuer"@en ;
  rdfs:subClassOf fibo-bp-iss-trm:SecurityIssuer ;
  skos:definition "The party which is the issuer of a Stockholders Rights instrument."@en ;
  skos:editorialNote "Specializes the term \"Issuer\" for different instrument classes; this specialization is not present in ISO FIBIM where the term is \"Issuer\" in each case."@en ;
.
fibo-der-rgt-raw:SubscriptionRight
  rdf:type owl:Class ;
  rdfs:label "subscription right"@en ;
  rdfs:subClassOf fibo-der-rgt-raw:StockholdersRightsInstrument ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-rgt-raw:givesRightToBuy ;
      owl:someValuesFrom fibo-sec-eq-eq:Share ;
    ] ;
  skos:definition "Privileges allotted to existing security holders, entitling them to subscribe to new securities at a price normally lower than the prevailing market price."@en ;
  skos:editorialNote "Why prevailing? It could be a new security of exactly the same class. Is this always new securities? Notes May 25: May be more an accounting or legal term for bonus issue, more accurate than scrip or bonus issue which reflects what happens in the books of the company. THis would mean that capitalization issue is seemingly synonymous with Bonus Issue / Scrip Issue (see moneytterms.co.uk which says that capitalization is less common but more correct, for scrip. \"The share capital on the balance sheet has to increate by the nominal value of the newly issued shares. This is balanced by an equal decrease in another part of hte shareholder's funds such as retained income of valuation reserves\""@en ;
.
fibo-der-rgt-raw:SubscriptionRightHolder
  rdf:type owl:Class ;
  rdfs:label "subscription right holder"@en ;
  rdfs:subClassOf fibo-der-rgt-raw:StockholdersRightsInstrumentHolder ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-rgt-raw:isByVirtueOfBeing ;
      owl:someValuesFrom fibo-sec-sec-ast:SecurityHolder ;
    ] ;
  skos:definition "The party which is the holder of a subscription right. This is identified as being a party which is also an existing holder of the security identified as the underlying security."@en ;
.
fibo-der-rgt-raw:ThirdPartyWarrantIssuer
  rdf:type owl:Class ;
  rdfs:label "third party warrant issuer"@en ;
  rdfs:subClassOf fibo-der-rgt-raw:WarrantIssuer ;
  owl:disjointWith fibo-der-rgt-raw:UnderlyingIssuer ;
.
fibo-der-rgt-raw:TraditionalWarrant
  rdf:type owl:Class ;
  rdfs:label "traditional warrant"@en ;
  rdfs:subClassOf fibo-der-rgt-raw:CompanyWarrant ;
  skos:definition "Financial instruments which permit the holder to purchase or sell back to the issuer a specified amount of a share."@en ;
  fibo-fnd-utl-av:explanatoryNote "Warrants may be privately issued and may not necessarily be traded on an exchange. From Wikipedia: A warrant issued with a bond gives the holder the right to purchase equity shares issued by the company that issued the bond with the warrant."@en ;
.
fibo-der-rgt-raw:UnderlyingIssuer
  rdf:type owl:Class ;
  rdfs:label "underlying issuer"@en ;
  rdfs:subClassOf fibo-der-rgt-raw:WarrantIssuer ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-rgt-raw:identifiedAs ;
      owl:someValuesFrom fibo-bp-iss-trm:SecurityIssuer ;
    ] ;
  skos:definition "A party which is the issuer of a Covered Warrant. This party is by definition the issuer of the underlying securities."@en ;
  skos:editorialNote "Specializes the term \"Issuer\" for different instrument classes; this specialization is not present in ISO FIBIM where the term is \"Issuer\" in each case."@en ;
.
fibo-der-rgt-raw:Warrant
  rdf:type owl:Class ;
  rdfs:label "warrant"@en ;
  rdfs:subClassOf fibo-der-rgt-raw:EntitlementRightsInstrument ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-bp-iss-trm:issuer ;
      owl:someValuesFrom fibo-der-rgt-raw:WarrantIssuer ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-rgt-raw:hasExerciseTerms ;
      owl:someValuesFrom fibo-der-rgt-raw:WarrantExerciseTermsSet ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-rgt-raw:hasWarrantUnderlying ;
      owl:someValuesFrom fibo-der-der-dbx:Underlier ;
    ] ;
  skos:definition "Financial instruments which permit the holder to purchase or sell back to the issuer a specified amount of a financial instrument, commodity, currency or other during a specified period at a specified price."@en ;
  fibo-fnd-utl-av:explanatoryNote "Warrants are for any kind of instrument. Warrants may be privately issued and may not necessarily be traded on an exchange."@en ;
.
fibo-der-rgt-raw:WarrantExerciseTermsSet
  rdf:type owl:Class ;
  rdfs:label "warrant exercise terms set"@en ;
  rdfs:subClassOf fibo-fnd-agr-ctr:ContractualCommitment ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-rgt-raw:delivery ;
      owl:someValuesFrom fibo-fbc-fi-fi:FinancialInstrument ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-rgt-raw:hasExerciseConvention ;
      owl:someValuesFrom fibo-der-der-ex:ExerciseDatesConvention ;
    ] ;
  skos:definition "Terms setting out how a warrant instrument may be exercised. Further Notes Exercise terms as defined here also cover the automatic exercise of the warrant in the event of its expiry. These terms include the exercise style (FIBIM = OptionStyle type OptionStyleCode) and the delivery."@en ;
.
fibo-der-rgt-raw:WarrantIssuer
  rdf:type owl:Class ;
  rdfs:label "warrant issuer"@en ;
  rdfs:subClassOf fibo-bp-iss-trm:SecurityIssuer ;
  skos:definition "The issuer of a Warrant. This is defined in terms of what individual entity is the issuer and also what other roles they may or may not play for a specific type of warranty, specifically whether or not they are also the issuer of the underlying, if the underlying is a security."@en ;
  skos:editorialNote "Specializes the term \"Issuer\" for different instrument classes; this specialization is not present in ISO FIBIM where the term is \"Issuer\" in each case."@en ;
.
fibo-der-rgt-raw:WarrantListing
  rdf:type owl:Class ;
  rdfs:label "warrant listing"@en ;
  rdfs:subClassOf fibo-sec-sec-lst:SecurityListing ;
  skos:definition "The listing of a warrant on a securities exchange. Action: confirm whether all the facts shown for listing of tradable securities generally, apply to the listing of a warrant. Also determine whether any of the terms in Listing need to be specialized or added to for Warrant Listing."@en ;
.
fibo-der-rgt-raw:delivery
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-der-rgt-raw:WarrantExerciseTermsSet ;
  rdfs:label "delivery"@en ;
  rdfs:range fibo-fbc-fi-fi:FinancialInstrument ;
.
fibo-der-rgt-raw:expiryDate
  rdf:type owl:DatatypeProperty ;
  rdfs:domain fibo-der-rgt-raw:EntitlementRightsInstrument ;
  rdfs:label "expiry date"@en ;
  skos:definition "Legal expiry of the Contract."@en ;
.
<https://spec.edmcouncil.org/fibo/DER/RightsInstruments/RightsAndWarrants/expiryDate.1>
  rdf:type owl:DatatypeProperty ;
  rdfs:domain fibo-der-rgt-raw:Warrant ;
  rdfs:label "expiry date"@en ;
  skos:definition "Expiry date of the warrant. This is the last date on which the holder may exercise the right to buy or sell back the underlying security."@en ;
.
fibo-der-rgt-raw:formulaText
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-der-rgt-raw:AllotmentRightFormula ;
  rdfs:label "formula text"@en ;
  rdfs:range fibo-fnd-utl-val:TextValue ;
  skos:definition "The formula stated as text."@en ;
.
fibo-der-rgt-raw:givesRightToBuy
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-der-rgt-raw:SubscriptionRight ;
  rdfs:label "gives right to buy"@en ;
  rdfs:range fibo-sec-eq-eq:Share ;
  rdfs:subPropertyOf fibo-der-rgt-raw:givesTheRightToBuy ;
.
fibo-der-rgt-raw:givesTheRightToBuy
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-der-rgt-raw:StockholdersRightsInstrument ;
  rdfs:label "gives the right to buy"@en ;
  rdfs:range fibo-fbc-fi-fi:Security ;
  rdfs:subPropertyOf fibo-der-drc-bsc:hasUnderlier ;
.
fibo-der-rgt-raw:hasExerciseConvention
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-der-rgt-raw:WarrantExerciseTermsSet ;
  rdfs:label "has exercise convention"@en ;
  rdfs:range fibo-der-der-ex:ExerciseDatesConvention ;
  rdfs:subPropertyOf fibo-fnd-agr-ctr:isQualifiedBy ;
.
fibo-der-rgt-raw:hasExerciseTerms
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-der-rgt-raw:Warrant ;
  rdfs:label "has exercise terms"@en ;
  rdfs:range fibo-der-rgt-raw:WarrantExerciseTermsSet ;
  rdfs:subPropertyOf fibo-fnd-agr-ctr:hasContractualElement ;
.
fibo-der-rgt-raw:hasListing
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-der-rgt-raw:ExchangeTradedWarrant ;
  rdfs:label "has listing"@en ;
  rdfs:range fibo-der-rgt-raw:WarrantListing ;
.
fibo-der-rgt-raw:hasWarrantUnderlying
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-der-rgt-raw:Warrant ;
  rdfs:label "has warrant underlying"@en ;
  rdfs:range fibo-der-der-dbx:Underlier ;
  rdfs:subPropertyOf fibo-der-drc-bsc:hasUnderlier ;
.
fibo-der-rgt-raw:identifiedAs
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-der-rgt-raw:UnderlyingIssuer ;
  rdfs:label "identified as"@en ;
  rdfs:range fibo-bp-iss-trm:SecurityIssuer ;
.
fibo-der-rgt-raw:isByVirtueOfBeing
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-der-rgt-raw:StockholdersRightsInstrumentHolder ;
  rdfs:label "is by virtue of being"@en ;
  rdfs:range fibo-sec-sec-ast:SecurityHolder ;
  rdfs:subPropertyOf fibo-fnd-ptyx-ptyx:hasRelatedParty ;
.
fibo-der-rgt-raw:numberOfSecurities
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-der-rgt-raw:AllotmentRight ;
  rdfs:label "number of securities"@en ;
  rdfs:range fibo-fnd-utl-val:DecimalValue ;
  skos:definition "The number of securities based on the number of these instruments that the holder holds."@en ;
.
fibo-der-rgt-raw:numberOfSecuritiesDeterminedBy
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-der-rgt-raw:AllotmentRight ;
  rdfs:label "number of securities determined by"@en ;
  rdfs:range fibo-der-rgt-raw:AllotmentRightFormula ;
  rdfs:subPropertyOf fibo-fnd-agr-ctr:hasContractualElement ;
.
fibo-der-rgt-raw:oversubscribeOption
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-der-rgt-raw:StockholdersRightsInstrument ;
  rdfs:label "oversubscribe option"@en ;
  rdfs:range fibo-fnd-utl-val:TrueFalseValue ;
  skos:definition "Option whereby the holders of the rights instrument may get securities in the event that other right holders choose not to subscribe to theirs."@en ;
.
fibo-der-rgt-raw:perpetualMaturity
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-der-rgt-raw:Warrant ;
  rdfs:label "perpetual maturity"@en ;
  rdfs:range fibo-fnd-utl-val:TrueFalseValue ;
  skos:definition "Whether this is a perpetual warrant, in other words it has no expiry date."@en ;
.
fibo-der-rgt-raw:purchasePrice
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-der-rgt-raw:PurchaseRight ;
  rdfs:label "purchase price"@en ;
  rdfs:range fibo-fnd-acc-acc:Price ;
  skos:definition "The price at which the holder of the Purchase Right may purchase the underlying security."@en ;
.
<https://spec.edmcouncil.org/fibo/DER/RightsInstruments/RightsAndWarrants/purchasePrice.1>
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-der-rgt-raw:SubscriptionRight ;
  rdfs:label "purchase price"@en ;
  rdfs:range fibo-fnd-acc-acc:Price ;
  skos:definition "The price at which the holder of the Subscription Right may purchase the underlying security."@en ;
.
fibo-der-rgt-raw:putWarrant
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-der-rgt-raw:Warrant ;
  rdfs:label "put warrant"@en ;
  rdfs:range fibo-fnd-utl-val:TrueFalseValue ;
  skos:definition "If Yes, this warrant gives the holder the right to sell the underlying security back to the issuer at a predetermined price; if No, the warrant gives the holder the right to purchase the instrument at the predetermined price (a call warrant)."@en ;
.
fibo-der-rgt-raw:ratio
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-der-rgt-raw:CompanyWarrant ;
  rdfs:domain fibo-der-rgt-raw:CoveredWarrant ;
  rdfs:label "ratio"@en ;
  rdfs:range fibo-fnd-utl-val:DecimalValue ;
  skos:definition "The ratio of warrant units to underlying security units for delivery when the warrant is exercised."@en ;
.
fibo-fbc-dae-dbt:
  terms:license sm:MITLicense ;
  sm:contentLanguage <http://www.omg.org/spec/ODM/> ;
  sm:contentLanguage <http://www.w3.org/standards/techs/owl#w3c_all> ;
  sm:copyright """Copyright (c) 2016-2017 EDM Council, Inc.
Copyright (c) 2016-2017 Object Management Group, Inc.""" ;
  sm:dependsOn <https://spec.edmcouncil.org/fibo/BE/> ;
  sm:dependsOn fibo-fbc-fi-fi: ;
  sm:dependsOn fibo-fbc-pas-fpas: ;
  sm:dependsOn <https://spec.edmcouncil.org/fibo/FND/> ;
  sm:fileAbbreviation "fibo-fbc-dae-dbt" ;
  sm:fileAbstract "This ontology defines concepts that are common to all debt instruments, such as debt, borrower, lender, debtor, creditor, interest, principal, and the like. It is designed to be used by various other FIBO specifications, including but not limited to SEC/Debt and LOAN." ;
  sm:filename "Debt.rdf" ;
  rdfs:label "Debt Ontology" ;
  rdfs:seeAlso <https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/> ;
  rdfs:seeAlso <https://spec.edmcouncil.org/fibo/FBC/AboutFBC/> ;
  rdfs:seeAlso <https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/AboutDebtAndEquities/> ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/FBC/20170201/DebtAndEquities/Debt/> ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/FBC/20170201/DebtAndEquities/Debt/ version of this ontology was added to the FBC specification via the SEC RFC and FBC 1.1 RTF in support of several FIBO debt-oriented initiatives." ;
  skos:editorialNote "Note that in the original 'red' version of this ontology, there were conflicting and confusing definitions related to credit and debt whereby accounting definitions were mixed in with instrument-related definitions.  The accounting terms, which conflated debt and debit, among other things, have been removed. For example, the concepts of credit and debit may specify balance sheet entries. Definitions for balance sheet credit and balance sheet debit have been eliminated from this ontology, however, as they belong somewhere else in an accounting ontology, in the context of defining a general ledger and balance sheet, rather than herein." ;
.
fibo-fbc-dae-dbt:Accrual
  rdf:type owl:Class ;
  rdfs:label "accrual" ;
  rdfs:subClassOf fibo-fnd-pty-rl:ThingInRole ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fbc-dae-dbt:Interest ;
      owl:onProperty fibo-fnd-rel-rel:appliesTo ;
    ] ;
  skos:definition "the process of accumulating interest or other income that has been earned but not paid" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
  fibo-fnd-utl-av:explanatoryNote "There are legal contractual terms for the accrual of interest, as distinct from the payment of interest." ;
.
fibo-fbc-dae-dbt:Amortization
  rdf:type owl:Class ;
  rdfs:label "amortization" ;
  rdfs:subClassOf fibo-fnd-pty-rl:ThingInRole ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fbc-dae-dbt:Debt ;
      owl:onProperty fibo-fbc-dae-dbt:isAmortizationOf ;
    ] ;
  skos:definition "the process of reduction of debt or other costs through periodic charges to assets or liabilities, such as through principal payments on mortgages" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fbc-dae-dbt:Borrower
  rdf:type owl:Class ;
  rdfs:label "borrower" ;
  rdfs:subClassOf fibo-fbc-dae-dbt:Debtor ;
  rdfs:subClassOf fibo-fnd-agr-ctr:ContractParty ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fbc-dae-dbt:owes ;
      owl:someValuesFrom fibo-fbc-dae-dbt:Debt ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
      owl:someValuesFrom [
          rdf:type owl:Restriction ;
          owl:onProperty fibo-fnd-pty-pty:isAPartyTo ;
          owl:someValuesFrom fibo-fbc-fi-fi:DebtInstrument ;
        ] ;
    ] ;
  skos:definition "a party to a debt instrument that is obligated to repay the amount borrowed (principal) with interest and other fees according to the terms of the instrument" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fbc-dae-dbt:BorrowingCapacity
  rdf:type owl:Class ;
  rdfs:label "borrowing capacity" ;
  rdfs:subClassOf fibo-fnd-law-lcap:LegalCapacity ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-be-le-lp:LegalPerson ;
      owl:onProperty fibo-fnd-rel-rel:appliesTo ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-acc-cur:MonetaryAmount ;
      owl:onProperty fibo-fbc-dae-dbt:hasAvailableAmount ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-dt-fd:Date ;
      owl:onProperty fibo-fnd-dt-fd:hasDate ;
    ] ;
  skos:definition "an amount of money that an individual or organization can borrow" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.investopedia.com/terms/c/credit.asp"^^xsd:anyURI ;
.
fibo-fbc-dae-dbt:Collateral
  rdf:type owl:Class ;
  rdfs:label "collateral" ;
  rdfs:subClassOf fibo-fnd-oac-own:Asset ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass [
          rdf:type owl:Class ;
          owl:unionOf (
              fibo-fbc-dae-dbt:CreditAgreement
              fibo-fbc-fi-fi:DebtInstrument
              fibo-fbc-fi-fi:Security
            ) ;
        ] ;
      owl:onProperty fibo-fbc-dae-dbt:isCollateralizationOf ;
    ] ;
  skos:definition "an asset offered as security, pledged as an inducement to another party, to lend money, extend credit, or provision securities" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fbc-dae-dbt:CreditAgreement
  rdf:type owl:Class ;
  rdfs:label "credit agreement" ;
  rdfs:subClassOf fibo-fnd-agr-ctr:Contract ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fbc-dae-dbt:Collateral ;
      owl:onProperty fibo-fbc-dae-dbt:isCollateralizedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-agr-ctr:hasContractParty ;
      owl:someValuesFrom fibo-fbc-dae-dbt:Creditor ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-agr-ctr:hasContractParty ;
      owl:someValuesFrom fibo-fbc-dae-dbt:Debtor ;
    ] ;
  skos:definition "a contractual agreement in which a debtor receives something of value now and agrees to repay the creditor at some date in the future, in some form (e.g., cash, securities, etc.), generally with interest" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.investopedia.com/terms/c/credit.asp"^^xsd:anyURI ;
.
fibo-fbc-dae-dbt:Creditor
  rdf:type owl:Class ;
  rdfs:label "creditor" ;
  rdfs:subClassOf fibo-fnd-pty-pty:PartyInRole ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-be-le-lp:LegalPerson ;
      owl:onProperty fibo-fnd-rel-rel:hasIdentity ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fbc-dae-dbt:isOwed ;
      owl:someValuesFrom fibo-fbc-dae-dbt:Debt ;
    ] ;
  skos:definition "a party to whom an obligation, such as an amount of money, or good, or performance of some service exists" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fbc-dae-dbt:DayCountConvention
  rdf:type owl:Class ;
  rdfs:label "day-count convention" ;
  rdfs:subClassOf fibo-fnd-dt-bd:BusinessRecurrenceIntervalConvention ;
  rdfs:subClassOf fibo-fnd-utl-alx:Expression ;
  skos:definition "a business recurrence interval convention that is used to calculate the number of days in an interest payment, which applies to the amount of accrued interest or the present value for debt instruments" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.investinginbonds.com/story.asp?id=3140"^^xsd:anyURI ;
  fibo-fnd-utl-av:adaptedFrom "http://www.investopedia.com/terms/d/daycount.asp"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "Day-count conventions apply to swaps, mortgages and forward rate agreements as well as bonds, each of which has its own day-count convention, which varies depending on the type of instrument, whether the interest rate is fixed or floating, and the country of issuance. Among the most common conventions are 30/360 or 365, actual/360 or 365, and actual/actual. A 30/360 convention assumes 30 days in a month and 360 days in a year. An actual/360 convention assumes the actual number of days in the given month and 360 days in the year. An actual/ actual convention uses the actual number of days in the given interest period and year." ;
.
fibo-fbc-dae-dbt:Debt
  rdf:type owl:Class ;
  rdfs:label "debt" ;
  rdfs:subClassOf fibo-fnd-agr-agr:Commitment ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fbc-dae-dbt:isOwedBy ;
      owl:someValuesFrom fibo-fbc-dae-dbt:Debtor ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fbc-dae-dbt:isOwedTo ;
      owl:someValuesFrom fibo-fbc-dae-dbt:Creditor ;
    ] ;
  skos:definition "an obligation to pay something, such as an amount of money, good, service, or instrument" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
  fibo-fnd-utl-av:explanatoryNote "In cases where the debtor and payer are the same legal person, then a debt is equivalent to a payment obligation." ;
.
fibo-fbc-dae-dbt:DebtTerms
  rdf:type owl:Class ;
  rdfs:label "debt terms" ;
  rdfs:subClassOf fibo-fnd-agr-ctr:ContractualCommitment ;
  skos:definition "contract terms that specify the formal rights and obligations of borrower and lender under a contract in which funds are lent from the one party to the other" ;
  fibo-fnd-utl-av:explanatoryNote "These may be terms in a loan contract (including for example a mortgage contract) or they may be the contractual terms of a debt security." ;
.
fibo-fbc-dae-dbt:Debtor
  rdf:type owl:Class ;
  rdfs:label "debtor" ;
  rdfs:subClassOf fibo-fnd-pty-pty:PartyInRole ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-be-le-lp:LegalPerson ;
      owl:onProperty fibo-fnd-rel-rel:hasIdentity ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fbc-dae-dbt:Debt ;
      owl:onProperty fibo-fbc-dae-dbt:owes ;
    ] ;
  skos:definition "a party that owes a debt" ;
  fibo-fnd-utl-av:adaptedFrom "http://legal-dictionary.thefreedictionary.com/Debtor+and+Creditor" ;
.
fibo-fbc-dae-dbt:ExtensionProvision
  rdf:type owl:Class ;
  rdfs:label "extension provision" ;
  rdfs:subClassOf fibo-fnd-agr-ctr:ContractualCommitment ;
  skos:definition "contract terms that specify the conditions under which a contract can be extended" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
  fibo-fnd-utl-av:explanatoryNote "In the case of a debt instrument, an extension may include extending the time allowed for repayment of the principal, the maturity date, and so forth." ;
.
fibo-fbc-dae-dbt:FixedInterestRate
  rdf:type owl:Class ;
  rdfs:label "fixed interest rate" ;
  rdfs:subClassOf fibo-fnd-acc-cur:InterestRate ;
  skos:definition "an interest rate that does not fluctuate over the lifetime of a loan or other debt instrument" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fbc-dae-dbt:FloatingInterestRate
  rdf:type owl:Class ;
  rdfs:label "floating interest rate" ;
  rdfs:seeAlso <http://recomparison.com/comparisons/100975/floating-vs-variable-vs-adjustable-interest-rate/> ;
  rdfs:subClassOf fibo-fbc-dae-dbt:VariableInterestRate ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-qt-qtu:Rate ;
      owl:onProperty fibo-fbc-dae-dbt:isBasedOn ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "a variable interest rate that is based on a specific index or benchmark rate" ;
  skos:example "Certain revolving credit, such as credit-card related debt, may adjust after a specified period of time to an absolute rate stated in the agreement (variable but not floating) rather than based on a benchmark rate (variable, floating)." ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
  fibo-fnd-utl-av:explanatoryNote "The index used to determine the specific interest rate is generally included in the terms of the loan. In most cases, lenders will also charge a spread, or added percentage points on top of the established index rate. If a loan is billed as prime plus 2.5 percent, for a prime rate of 3.5 percent, the terms of the loan will require the borrower to pay off a 6 percent interest. Floating interest rates typically involve periodic reset dates for the loan, particularly when the index rate changes. Resets may also occur online at market predetermined intervals, with yearly adjustments being a common arrangement." ;
.
fibo-fbc-dae-dbt:Interest
  rdf:type owl:Class ;
  rdfs:label "interest" ;
  rdfs:subClassOf fibo-fnd-acc-cur:MonetaryAmount ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-acc-cur:InterestRate ;
      owl:onProperty fibo-fbc-dae-dbt:hasInterestRate ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-dt-fd:DatePeriod ;
      owl:onProperty fibo-fnd-dt-fd:hasDatePeriod ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fbc-dae-dbt:isInterestOn ;
      owl:someValuesFrom fibo-fbc-dae-dbt:Debt ;
    ] ;
  skos:definition "the cost of using credit, or another's money, expressed as a rate per period of time, payable by a debtor to a creditor in consideration of the credit extended to the debtor" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fbc-dae-dbt:InterestPaymentTerms
  rdf:type owl:Class ;
  rdfs:label "interest payment terms" ;
  rdfs:subClassOf fibo-fbc-dae-dbt:DebtTerms ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-acc-cur:InterestRate ;
      owl:onProperty fibo-fbc-dae-dbt:hasInterestRate ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fbc-dae-dbt:governsPaymentOf ;
      owl:someValuesFrom fibo-fbc-dae-dbt:Interest ;
    ] ;
  skos:definition "contract terms for payment of interest on a debt" ;
  fibo-fnd-utl-av:explanatoryNote "Interest is usually payable on any outstanding principal amount, therefore Interest itself relates to the amount of debt at any given point of time, not to the principal amount advanced at the time that the loan was advanced or the debt security issued." ;
.
fibo-fbc-dae-dbt:Lender
  rdf:type owl:Class ;
  rdfs:label "lender" ;
  rdfs:subClassOf fibo-fbc-dae-dbt:Creditor ;
  rdfs:subClassOf fibo-fnd-agr-ctr:ContractParty ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
      owl:someValuesFrom [
          rdf:type owl:Restriction ;
          owl:onProperty fibo-fnd-pty-pty:isAPartyTo ;
          owl:someValuesFrom fibo-fbc-fi-fi:DebtInstrument ;
        ] ;
    ] ;
  skos:definition "a party that extends credit or money to a borrower with the expectation of being repaid, usually with interest" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fbc-dae-dbt:ManagedInterestRate
  rdf:type owl:Class ;
  rdfs:label "managed interest rate" ;
  rdfs:subClassOf fibo-fbc-dae-dbt:VariableInterestRate ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:isManagedBy ;
      owl:someValuesFrom fibo-fbc-pas-fpas:FinancialServiceProvider ;
    ] ;
  skos:definition "a variable interest rate charged by a financial institution for borrowing that is not prescribed as a margin over base rate but is set from time to time by the institution" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.investorguide.com/definition/managed-rate.html"^^xsd:anyURI ;
.
fibo-fbc-dae-dbt:Principal
  rdf:type owl:Class ;
  rdfs:label "principal" ;
  rdfs:subClassOf fibo-fnd-acc-cur:MonetaryAmount ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fbc-dae-dbt:isPrincipalOf ;
      owl:someValuesFrom fibo-fbc-dae-dbt:Debt ;
    ] ;
  skos:definition "the value (monetary amount) of an obligation, such as a bond or loan, raised and that must be repaid at maturity" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fbc-dae-dbt:PrincipalRepaymentTerms
  rdf:type owl:Class ;
  rdfs:label "principal repayment terms" ;
  rdfs:subClassOf fibo-fbc-dae-dbt:DebtTerms ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fbc-dae-dbt:ExtensionProvision ;
      owl:onProperty fibo-fbc-dae-dbt:hasExtensionProvision ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fbc-dae-dbt:governsPaymentOf ;
      owl:someValuesFrom fibo-fbc-dae-dbt:Principal ;
    ] ;
  skos:definition "contract terms that specify requirements for repayment of the principal" ;
.
fibo-fbc-dae-dbt:RedemptionProvision
  rdf:type owl:Class ;
  rdfs:label "redemption provision" ;
  rdfs:subClassOf fibo-fbc-dae-dbt:PrincipalRepaymentTerms ;
  skos:definition "a contract provision enabling the issuer (writer) to regain possession through repayment of some stipulated price" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Econonmics Terms, Fifth Edition, 2012" ;
  fibo-fnd-utl-av:explanatoryNote "In general, redemption is synonymous with 'buy back' or 'cash in', depending on the kind of instrument. Redemption provisions are commonly applicable to the process of annulling a defeasible title, such as for a mortgage or tax sale, by paying the debt or fulfilling an obligation." ;
.
fibo-fbc-dae-dbt:VariableInterestRate
  rdf:type owl:Class ;
  rdfs:label "variable interest rate" ;
  rdfs:subClassOf fibo-fnd-acc-cur:InterestRate ;
  owl:disjointWith fibo-fbc-dae-dbt:FixedInterestRate ;
  skos:definition "an interest rate that is allowed to vary over the maturity of a loan or other debt instrument" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
  fibo-fnd-utl-av:synonym "adjustable rate" ;
.
fibo-fbc-dae-dbt:governsPaymentOf
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fbc-dae-dbt:DebtTerms ;
  rdfs:label "governs payment of" ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:governs ;
.
fibo-fbc-dae-dbt:hasAccrualBasis
  rdf:type owl:ObjectProperty ;
  rdfs:label "has accrual basis" ;
  rdfs:range fibo-fbc-dae-dbt:DayCountConvention ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "identifies the convention that defines how interest accrues on something, that is the number of days in a month and days in a year that are counted when performing interest accrual calculations" ;
.
fibo-fbc-dae-dbt:hasAvailableAmount
  rdf:type owl:ObjectProperty ;
  rdfs:label "has available amount" ;
  rdfs:range fibo-fnd-acc-cur:MonetaryAmount ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "indicates an amount of money available for an individual or organization to borrow" ;
.
fibo-fbc-dae-dbt:hasDebtAmount
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fbc-fi-fi:DebtInstrument ;
  rdfs:label "has debt amount" ;
  rdfs:range fibo-fnd-acc-cur:MonetaryAmount ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "indicates the monetary amount of the debt" ;
  fibo-fnd-utl-av:explanatoryNote "Represents the total debt amount including principal and interest" ;
.
fibo-fbc-dae-dbt:hasDenomination
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fbc-fi-fi:Security ;
  rdfs:label "has denomination" ;
  rdfs:range fibo-fnd-acc-cur:MonetaryAmount ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "the face value of currency units, coins, or securities" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fbc-dae-dbt:hasExtensionProvision
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fbc-fi-fi:DebtInstrument ;
  rdfs:label "has an extension provision" ;
  rdfs:range fibo-fbc-dae-dbt:ExtensionProvision ;
  rdfs:subPropertyOf fibo-fnd-agr-ctr:hasContractualElement ;
  skos:definition "identifies a contract provision allowing extension of repayment or maturity dates" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fbc-dae-dbt:hasInterestRate
  rdf:type owl:ObjectProperty ;
  rdfs:label "has interest rate" ;
  rdfs:range fibo-fnd-acc-cur:InterestRate ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "relates something, such as an agreement, or debt instrument, to the rate (typically annual) of interest that is to be paid by the debtor to the creditor on the debt" ;
.
fibo-fbc-dae-dbt:hasOutstandingAmount
  rdf:type owl:ObjectProperty ;
  rdfs:label "has outstanding amount" ;
  rdfs:range fibo-fnd-acc-cur:MonetaryAmount ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "indicates an amount of money representing the principal, interest, or other amount owed at a specific point in time" ;
.
fibo-fbc-dae-dbt:hasPrincipal
  rdf:type owl:ObjectProperty ;
  rdfs:label "has principal" ;
  rdfs:range fibo-fbc-dae-dbt:Principal ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "indicates the face value of an obligation, such as a bond or loan, that must be repaid at maturity, i.e., the base amount raised by a mortgage or other debt instrument" ;
  skos:editorialNote "This is not the balance of the debt at a point in time, but the amount drawn down at a specific point in time, after which (for interest bearing debts) interest becomes payable. Semantic Modeling note This term is over-ridden for specific kinds of debt (securities, loans) and is therefore not needed in any data model. It is included here to define part of the meaning of the Debt Finance term. It is a Relationship Fact and strictly speaking should be regarded as being specialized and over-ridden by the terms in the Loan and Security classes, however it is modeled here as a \"Referenceable Archetype\" meaning that it appears in diagrams as a textual entry not a relationship line, and so it is not possible to formally show this specialization." ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fbc-dae-dbt:hasPrincipalRepaymentDate
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fbc-dae-dbt:PrincipalRepaymentTerms ;
  rdfs:label "has principal repayment date" ;
  rdfs:range fibo-fnd-dt-fd:Date ;
  rdfs:subPropertyOf fibo-fnd-dt-fd:hasEndDate ;
  skos:definition "relates an instrument to the date when the principal must be repaid in full" ;
  fibo-fnd-utl-av:explanatoryNote "Depending on the terms of the instrument (debt security, such as a bond, loan, etc.), this may be the date of a single payment of the debt principal or of the completion of scheduled partial redemption payments." ;
.
fibo-fbc-dae-dbt:isAmortizationOf
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fbc-dae-dbt:Amortization ;
  rdfs:label "is amortization of" ;
.
fibo-fbc-dae-dbt:isBasedOn
  rdf:type owl:ObjectProperty ;
  rdfs:label "is based on" ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:appliesTo ;
  skos:definition "relates something to something else on which it rests, or that supports it in some way" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.merriam-webster.com/dictionary/base"^^xsd:anyURI ;
.
fibo-fbc-dae-dbt:isCollateralizationOf
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fbc-dae-dbt:Collateral ;
  rdfs:label "is collateralization of" ;
  skos:definition "relates some collateral to a credit agreement or debt instrument for which the property has been pledged as security for the debt" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fbc-dae-dbt:isCollateralizedBy
  rdf:type owl:ObjectProperty ;
  rdfs:label "is collateralized by" ;
  rdfs:range fibo-fbc-dae-dbt:Collateral ;
  skos:definition "relates a credit agreement or debt instrument to property pledged as security for the debt" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fbc-dae-dbt:isInterestOn
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fbc-dae-dbt:Interest ;
  rdfs:label "is interest on" ;
.
fibo-fbc-dae-dbt:isOwed
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fbc-dae-dbt:Creditor ;
  rdfs:label "is owed" ;
  rdfs:range fibo-fbc-dae-dbt:Debt ;
.
fibo-fbc-dae-dbt:isOwedBy
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fbc-dae-dbt:Debt ;
  rdfs:label "is owed by" ;
  rdfs:range fibo-fbc-dae-dbt:Debtor ;
  owl:inverseOf fibo-fbc-dae-dbt:owes ;
.
fibo-fbc-dae-dbt:isOwedTo
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fbc-dae-dbt:Debt ;
  rdfs:label "is owed to" ;
  rdfs:range fibo-fbc-dae-dbt:Creditor ;
.
fibo-fbc-dae-dbt:isPrincipalOf
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fbc-dae-dbt:Principal ;
  rdfs:label "is principal of" ;
.
fibo-fbc-dae-dbt:owes
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fbc-dae-dbt:Debtor ;
  rdfs:label "owes" ;
  rdfs:range fibo-fbc-dae-dbt:Debt ;
.
fibo-fbc-dae-gty:
