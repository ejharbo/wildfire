  sm:fileAbbreviation "fibo-ind-ei-pub" ;
  sm:fileAbstract "This ontology provides concepts descriptive of the publishers of market indicators, such as gross domestic product, employment statistics, inflation rates and so on. These may include government or quasi-government bodies, international agencies, third parties and data providers." ;
  sm:filename "EconomicIndicatorPublishers.rdf" ;
  rdfs:label "Economic Indicator Publishers Ontology" ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/IND/AboutIND/"^^xsd:anyURI ;
