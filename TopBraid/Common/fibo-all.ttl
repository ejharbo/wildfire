  sm:normativeReference "http://www.businessdictionary.com/definition/report.html" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-aap-agt:AutonomousAgent ;
      owl:onProperty fibo-ln-ln-up:rel-isProducedBy_I ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-dt-fd:DatePeriod ;
      owl:onProperty fibo-ln-ln-up:dt-fd-coversPeriod ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-ln-ln-up:hasCreationDate ;
      owl:someValuesFrom fibo-fnd-dt-fd:Date ;
    ] ;
  skos:definition "A body of information organized in a narrative, graphic, or tabular form, prepared on ad hoc, periodic, recurring, regular, or as required basis. Reports may refer to specific periods, events, occurrences, or subjects, and may be communicated or presented in oral or written form." ;
.
fibo-ln-ln-up:RiskAssesssment
  rdf:type owl:Class ;
