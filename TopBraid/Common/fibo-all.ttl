  rdfs:label "individual tranche definitions"@en ;
  rdfs:subClassOf fibo-bp-iss-doc:SecurityTermSheet ;
  rdfs:subClassOf fibo-bp-iss-pmbs:TranchedMBSIssueProspectusPart ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-bp-iss-doc:definesTerms ;
      owl:someValuesFrom fibo-sec-dbt-bo-cft:BondAmortizationPaymentTerms ;
    ] ;
