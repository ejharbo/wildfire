  rdfs:subClassOf fibo-sec-dbt-dbt-cft:PutSchedule ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-sec-schx:listsScheduledEvent ;
      owl:someValuesFrom fibo-sec-dbt-dbt-cft:PutOfIssue ;
