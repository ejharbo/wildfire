.
fibo-sec-sec-clax:Security
  rdf:type owl:Class ;
  rdfs:label "security"@en ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:isClassifiedBy ;
      owl:someValuesFrom fibo-sec-sec-clax:SecurityClassification ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-sec-clax:hasSectorClassification ;
      owl:someValuesFrom fibo-fnd-arr-cls:IndustrySectorClassifier ;
    ] ;
.
fibo-sec-sec-clax:SecurityClassification
  rdf:type owl:Class ;
  rdfs:label "security classification"@en ;
  rdfs:subClassOf fibo-sec-sec-cls:FinancialInstrumentClassifier ;
  skos:definition "The classification of the type of security, e.g. Equity, Debt, Short Term Note etc."@en ;
  fibo-fnd-utl-av:explanatoryNote "Not the same as the Industry Classification identifying the industry sector associated with the Security."@en ;
.
fibo-sec-sec-clax:SecurityClassificationScheme
  rdf:type owl:Class ;
  rdfs:label "security classification scheme"@en ;
  rdfs:subClassOf fibo-sec-sec-cls:FinancialInstrumentClassificationScheme ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-sec-sec-clax:SecurityClassification ;
      owl:onProperty fibo-fnd-rel-rel:defines ;
    ] ;
  skos:definition "The Scheme defining the classification of a security i..e what type of security it is (for example the ISO 10962 CFI lassification Scheme). This may be referred to by means of a URI or some other means depending on the convention adopted on implementation."@en ;
.
fibo-sec-sec-clax:hasSectorClassification
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-sec-clax:Security ;
  rdfs:label "has sector classification"@en ;
  rdfs:range fibo-fnd-arr-cls:IndustrySectorClassifier ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:isClassifiedBy ;
.
