.
fibo-sec-dbt-dbti:AtADiscount
  rdf:type owl:NamedIndividual ;
  rdf:type fibo-sec-dbt-dbti:RelativePrice ;
  rdfs:label "at a discount" ;
  skos:definition "a selling price that is less than the face or nominal value" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.investopedia.com/terms/a/at-a-discount.asp" ;
  fibo-fnd-utl-av:synonym "below par" ;
.
fibo-sec-dbt-dbti:AtAPremium
  rdf:type owl:NamedIndividual ;
  rdf:type fibo-sec-dbt-dbti:RelativePrice ;
  rdfs:label "at a premium" ;
  skos:definition "a selling price significantly above the stated face or redemption value due to high demand or timing of redemption" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.investopedia.com/terms/a/at-a-premium.asp" ;
  fibo-fnd-utl-av:synonym "above par" ;
  fibo-fnd-utl-av:synonym "premium" ;
.
fibo-sec-dbt-dbti:CallEvent
  rdf:type owl:Class ;
  rdfs:label "call event" ;
  rdfs:subClassOf fibo-fnd-dt-oc:OccurrenceKind ;
  skos:definition "an event associated with the call schedule for a debt instrument" ;
.
fibo-sec-dbt-dbti:CallFeature
  rdf:type owl:Class ;
  rdfs:label "call feature" ;
  rdfs:subClassOf fibo-sec-dbt-dbti:TradableDebtInstrumentRedemptionProvision ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:comprises ;
      owl:someValuesFrom fibo-sec-dbt-dbti:CallNotificationProvision ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:comprises ;
      owl:someValuesFrom fibo-sec-dbt-dbti:CallSchedule ;
    ] ;
  skos:definition "a redemption provision defining the schedule and price of redemption prior to maturity" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
  fibo-fnd-utl-av:explanatoryNote "Most corporate and municipal bonds have ten-year call features (termed call protection by holders); government securities typically have none." ;
  fibo-fnd-utl-av:synonym "call provision" ;
.
fibo-sec-dbt-dbti:CallNotificationProvision
  rdf:type owl:Class ;
  rdfs:label "call notification provision" ;
  rdfs:subClassOf fibo-sec-dbt-dbti:TradableDebtInstrumentRedemptionProvision ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-sec-dbt-dbti:CallWindow ;
      owl:onProperty fibo-fnd-dt-fd:hasDatePeriod ;
    ] ;
  skos:definition "a provision of a call feature defining notification requirements" ;
.
fibo-sec-dbt-dbti:CallPremium
  rdf:type owl:Class ;
  rdfs:label "call premium" ;
  rdfs:subClassOf fibo-fnd-acc-cur:MonetaryPrice ;
  skos:definition "an amount over par that an issuer must pay an investor to redeem the security early" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-sec-dbt-dbti:CallPrice
  rdf:type owl:Class ;
  rdfs:label "call price" ;
  rdfs:subClassOf fibo-fnd-acc-cur:MonetaryPrice ;
  skos:definition "a price at which a debt instrument with a call feature may be redeemed by the issuer" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
  fibo-fnd-utl-av:synonym "redemption price" ;
  fibo-ind-ei-ei:actualExpression "par value + call premium" ;
.
fibo-sec-dbt-dbti:CallSchedule
  rdf:type owl:Class ;
  rdfs:label "call schedule" ;
  rdfs:subClassOf fibo-fnd-dt-fd:Schedule ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:comprises ;
      owl:someValuesFrom fibo-sec-dbt-dbti:CallEvent ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:comprises ;
      owl:someValuesFrom [
          rdf:type owl:Class ;
          owl:unionOf (
              fibo-sec-dbt-dbti:CallPremium
              fibo-sec-dbt-dbti:CallPrice
            ) ;
        ] ;
    ] ;
  skos:definition "a schedule that defines the events associated with the call feature of a debt instrument, i.e, the dates on which the debt instrument may be called (recalled) at what price by the issuer" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-sec-dbt-dbti:CallWindow
  rdf:type owl:Class ;
  rdfs:label "call window" ;
  rdfs:subClassOf fibo-fnd-dt-fd:DatePeriod ;
  skos:definition "an explicit period of time prior to a call date during which an issuer or agent must give notice to holders" ;
.
fibo-sec-dbt-dbti:DebtOffering
  rdf:type owl:Class ;
  rdfs:label "debt offering" ;
  rdfs:subClassOf fibo-sec-sec-iss:SecuritiesOffering ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-sec-dbt-dbti:CallFeature ;
      owl:onProperty fibo-sec-dbt-dbti:hasCallFeature ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fbc-dae-dbt:RedemptionProvision ;
      owl:onProperty fibo-sec-dbt-dbti:hasRedemptionProvision ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:appliesTo ;
