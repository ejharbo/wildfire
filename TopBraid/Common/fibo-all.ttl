.
loan:LoanContract
  rdf:type owl:Class ;
  rdfs:subClassOf fibo-fbc-fi-fi:FinancialInstrument ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-pty-pty:IndependentParty ;
      owl:onProperty fibo-ln-ln-up:opty-isGuaranteedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass loan:ObligationToPay ;
      owl:onProperty fibo-ln-ln-up:rel-produces_I ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass loan:ObligationToRepay ;
      owl:onProperty fibo-ln-ln-up:rel-produces_I ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass loan:SecurityAgreement ;
      owl:onProperty fibo-fnd-rel-rel:has ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass loan:UniversalLoanIdentifier ;
      owl:onProperty fibo-fnd-aap-agt:isIdentifiedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-agr-ctr:ContractTermsSet ;
      owl:onProperty fibo-fnd-agr-ctr:hasTerms ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:has ;
      owl:someValuesFrom loan:CreditRiskAssessment ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:isClassifiedBy ;
      owl:someValuesFrom loan:LenderLienPosition ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:isClassifiedBy ;
      owl:someValuesFrom loan:LoanMarketCategory ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty loan:hasBorrower ;
      owl:someValuesFrom fibo-fnd-pty-pty:IndependentParty ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty loan:hasLender ;
      owl:someValuesFrom fibo-fnd-pty-pty:IndependentParty ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty loan:hasOriginatorCompany ;
      owl:someValuesFrom fibo-fnd-org-fm:FormalOrganization ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty loan:hasOriginatorPerson ;
      owl:someValuesFrom fibo-fnd-aap-ppl:Person ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-ln-ln-up:conformsTo ;
      owl:someValuesFrom fibo-ln-ln-up:Specification ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-ln-ln-up:cur-hasAmount ;
      owl:someValuesFrom fibo-fnd-acc-cur:MonetaryAmount ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-ln-ln-up:instantiates ;
      owl:someValuesFrom loan:LoanOffer ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-ln-ln-up:rel-produces_I ;
      owl:someValuesFrom loan:ObligationToFund ;
    ] ;
