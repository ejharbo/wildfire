fibo-sec-dbt-dbt-cft:PutTerms
  rdf:type owl:Class ;
  rdfs:label "put terms" ;
  rdfs:subClassOf fibo-fnd-agr-ctr:ContractualCommitment ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-dbt-dbt-cft:includes ;
      owl:someValuesFrom fibo-sec-dbt-dbt-cft:PutSchedule ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty <https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtCashflowTerms/includesWindow.1> ;
      owl:someValuesFrom fibo-sec-dbt-dbt-cft:PutWindow ;
    ] ;
