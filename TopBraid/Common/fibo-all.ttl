    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-md-dbt-py:hasOutlookPeriod ;
      owl:someValuesFrom [
          rdf:type owl:Class ;
          owl:unionOf (
              fibo-md-dbt-aly:LifeAnalytic
              fibo-md-tem-ins:Maturity
              fibo-sec-dbt-dbt-cft:PrescriptiveEvent
              fibo-sec-dbt-dbt-cft:RedemptionEvent
            ) ;
        ] ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-md-tem-ins:derivedFromOutlay ;
      owl:someValuesFrom fibo-md-dbt-py:DebtSecurityPrice ;
    ] ;
  skos:definition "The return on the debt instrument at the stated price."@en ;
  fibo-fnd-utl-av:explanatoryNote "Yield has a relationship to the price."@en ;
.
