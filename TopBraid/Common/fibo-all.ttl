fibo-ind-ei-caei:CanadianConsumerPriceIndex
  rdf:type owl:Class ;
  rdfs:label "Canadian consumer price index" ;
  rdfs:subClassOf fibo-fnd-utl-alx:Expression ;
  rdfs:subClassOf fibo-ind-ei-ei:ConsumerPriceIndex ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:hasValue fibo-ind-ei-caei:CanadianStatisticsPublisher ;
      owl:onProperty fibo-be-fct-pub:hasPublisher ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:hasValue fibo-ind-ei-caei:StatisticsCanada ;
      owl:onProperty fibo-be-fct-pub:isPublishedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-ind-ei-caei:CanadianHouseholdsConsumersUniverse ;
      owl:onProperty fibo-fnd-utl-alx:hasArgument ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:appliesTo ;
      owl:someValuesFrom fibo-ind-ei-caei:CanadianHouseholdsConsumersUniverse ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-utl-alx:hasArgument ;
      owl:someValuesFrom fibo-ind-ei-ei:FixedBasket ;
    ] ;
  skos:definition "an economic indicator representing a measure of changes over time in the prices of a fixed basket of consumer goods and services that Canadian private households consume" ;
  fibo-fnd-utl-av:abbreviation "CPI" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.statcan.gc.ca/eng/start"^^xsd:anyURI ;
  fibo-fnd-utl-av:adaptedFrom "http://www5.statcan.gc.ca/olc-cel/olc.action?ObjId=62-553-X&ObjType=2&lang=en&limit=0"^^xsd:anyURI ;
.
fibo-ind-ei-caei:CanadianHouseholdsConsumersUniverse
  rdf:type owl:Class ;
  rdfs:label "Canadian households consumers universe" ;
  rdfs:subClassOf fibo-ind-ei-ei:CivilianNonInstitutionalPopulation ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:hasContext ;
      owl:someValuesFrom fibo-ind-ei-ei:Household ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:hasMember ;
      owl:someValuesFrom fibo-ind-ei-ei:Consumer ;
    ] ;
  skos:definition "a statistical universe consisting of all private households in Canada, with the exception of soldiers on military bases, people living on First Nations reserves, institutionalized persons, and households living in the rural areas of the three northern territories" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.statcan.gc.ca/pub/62-553-x/62-553-x2015001-eng.pdf" ;
.
fibo-ind-ei-caei:CanadianProducerPriceIndex
  rdf:type owl:Class ;
  rdfs:label "Canadian producer price index" ;
  rdfs:subClassOf fibo-ind-ei-ei:ProducerPriceIndex ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:hasValue fibo-ind-ei-caei:CanadianStatisticsPublisher ;
      owl:onProperty fibo-be-fct-pub:hasPublisher ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:hasValue fibo-ind-ei-caei:StatisticsCanada ;
      owl:onProperty fibo-be-fct-pub:isPublishedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:appliesTo ;
      owl:someValuesFrom [
          rdf:type owl:Class ;
          owl:unionOf (
              fibo-ind-ei-caei:IndustrialProductsSector
              fibo-ind-ei-caei:NewHousingSector
              fibo-ind-ei-caei:RawMaterialsSector
            ) ;
        ] ;
    ] ;
  skos:definition "an economic indicator representing a measure of the change over time in the prices of a fixed-basket of domestic producer goods and services" ;
  fibo-fnd-utl-av:adaptedFrom "http://www23.statcan.gc.ca/imdb-bmdi/pub/indexth-eng.htm"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "Note that Canada does not produce a high level, cross industry PPI per se. Canadian PPIs are published by industry sector. Three of the most important are captured in the union defined herein, which may be expanded over time to integrate others, as needed."^^xsd:anyURI ;
.
fibo-ind-ei-caei:CanadianStatisticsPublisher
  rdf:type owl:NamedIndividual ;
  rdf:type fibo-ind-ei-pub:StatisticalInformationPublisher ;
  rdfs:label "Canadian statistics publisher" ;
  skos:definition "individual representing the functional entity that is the primary publisher of statistical information for the Canadian Government" ;
  fibo-fnd-rel-rel:hasIdentity fibo-ind-ei-caei:StatisticsCanada ;
  fibo-fnd-utl-av:adaptedFrom "http://www.statcan.gc.ca/eng/about/mandate/"^^xsd:anyURI ;
.
fibo-ind-ei-caei:IndustrialProductsSector
  rdf:type owl:Class ;
  rdfs:label "industrial products sector" ;
  rdfs:subClassOf fibo-fnd-arr-cls:IndustrySectorClassifier ;
  skos:definition "a classifier for a sector of the economy used for price indices focused on major commodities sold by manufacturers in Canada" ;
  fibo-fnd-utl-av:adaptedFrom "http://www23.statcan.gc.ca/imdb/p2SV.pl?Function=getSurvey&SDDS=2318"^^xsd:anyURI ;
.
fibo-ind-ei-caei:NewHousingSector
  rdf:type owl:Class ;
  rdfs:label "new housing sector" ;
  rdfs:subClassOf fibo-fnd-arr-cls:IndustrySectorClassifier ;
  skos:definition "a classifier for a sector of the economy used for price indices focused on contractors' selling prices of new residential houses, where detailed specifications pertaining to each house remain the same between two consecutive periods" ;
  fibo-fnd-utl-av:adaptedFrom "http://www23.statcan.gc.ca/imdb/p2SV.pl?Function=getSurvey&SDDS=2310"^^xsd:anyURI ;
.
fibo-ind-ei-caei:RawMaterialsSector
  rdf:type owl:Class ;
  rdfs:label "raw materials sector" ;
  rdfs:subClassOf fibo-fnd-arr-cls:IndustrySectorClassifier ;
  skos:definition "a classifier used for price indices related to raw materials purchased by industries in Canada for further processing" ;
  fibo-fnd-utl-av:adaptedFrom "http://www23.statcan.gc.ca/imdb/p2SV.pl?Function=getSurvey&SDDS=2306"^^xsd:anyURI ;
.
fibo-ind-ei-caei:StatisticsCanada
  rdf:type owl:NamedIndividual ;
  rdf:type fibo-be-ge-ge:GovernmentAgency ;
  rdfs:label "Statistics Canada"@en ;
  rdfs:label "Statistique Canada"@en ;
  skos:definition "individual representing Statistics Canada, a government agency mandated to collect, compile, analyse, abstract and publish statistical information relating to the commercial, industrial, financial, social, economic and general activities and condition of the people of Canada" ;
  fibo-be-ge-ge:hasJurisdiction fibo-be-ge-caj:CanadianJurisdiction ;
  fibo-fnd-rel-rel:isPartOf fibo-be-ge-caj:GovernmentOfCanada ;
  fibo-fnd-utl-av:abbreviation "StatCan" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.statcan.gc.ca/eng/about/mandate"^^xsd:anyURI ;
.
fibo-ind-ei-usei:
  terms:license "http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense"^^xsd:anyURI ;
  sm:contentLanguage "http://www.omg.org/spec/ODM/"^^xsd:anyURI ;
  sm:contentLanguage "http://www.w3.org/standards/techs/owl#w3c_all"^^xsd:anyURI ;
  sm:copyright """Copyright (c) 2016 EDM Council, Inc.
Copyright (c) 2016 Object Management Group, Inc.""" ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/BE/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FBC/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/IND/EconomicIndicators/EconomicIndicatorPublishers/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/IND/EconomicIndicators/EconomicIndicators/"^^xsd:anyURI ;
  sm:fileAbbreviation "fibo-ind-ei-usei" ;
  sm:fileAbstract "This ontology provides specific parameters which make up the various types of market economic indicators applicable to the American economy." ;
  sm:filename "USEconomicIndicators.rdf" ;
  rdfs:label "American Economic Indicators Ontology" ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/IND/AboutIND/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/IND/EconomicIndicators/AboutEconomicIndicators/"^^xsd:anyURI ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/IND/20160801/EconomicIndicators/NorthAmericanIndicators/USEconomicIndicators/> ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/IND/20160801/EconomicIndicators/NorthAmericanIndicators/USEconomicIndicators.rdf version of this ontology was added to the IND specification per the issue resolutions identified in the FIBO IND 1.0 FTF 3 report." ;
.
fibo-ind-ei-usei:AmericanStatisticsPublisher
  rdf:type owl:Class ;
  rdfs:label "American statistics publisher" ;
  rdfs:subClassOf fibo-ind-ei-pub:StatisticalInformationPublisher ;
  skos:definition "a statistical information publisher comprised of the government agencies and departments that publish statistical information on behalf of the U.S. Federal Government" ;
  skos:example "Examples of American statistical information publishers include the Department of Commerce, Bureau of Economic Analysis (Commerce), Census Bureau (Commerce), Bureau of Labor Statistics (Labor), Department of the Treasury, and the Federal Reserve Board, among others." ;
.
fibo-ind-ei-usei:BureauOfLaborStatistics
  rdf:type owl:NamedIndividual ;
  rdf:type fibo-be-ge-ge:GovernmentAgency ;
  rdfs:label "Bureau of Labor Statistics" ;
  rdfs:seeAlso <http://www.bls.gov/> ;
  skos:definition "individual representing the Bureau of Labor Statistics, the principal Federal agency responsible for measuring labor market activity, working conditions, and price changes in the economy" ;
  fibo-be-ge-ge:hasJurisdiction fibo-be-ge-usj:UnitedStatesJurisdiction ;
  fibo-fnd-rel-rel:isPartOf fibo-ind-ei-usei:UnitedStatesDepartmentOfLabor ;
  fibo-fnd-utl-av:abbreviation "BLS" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.bls.gov/bls/infohome.htm"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "Its mission is to collect, analyze, and disseminate essential economic information to support public and private decision-making. As an independent statistical agency, BLS serves its diverse user communities by providing products and services that are objective, timely, accurate, and relevant." ;
.
fibo-ind-ei-usei:ConsumerExpenditureSurvey
  rdf:type owl:Class ;
  rdfs:label "consumer expenditure survey" ;
  rdfs:subClassOf fibo-ind-ei-ei:StatisticalProgram ;
  skos:definition "a program conducted on a regular basis that provides information on the buying habits of consumers, including data on their expenditures, income, and consumer unit (families and single consumers) characteristics" ;
  fibo-fnd-utl-av:adaptedFrom <http://www.bls.gov/cex/> ;
.
fibo-ind-ei-usei:CurrentEmploymentStatistics
  rdf:type owl:Class ;
  rdfs:label "current employment statistics" ;
  rdfs:subClassOf fibo-ind-ei-ei:StatisticalProgram ;
  skos:definition "a survey conducted on a regular basis that presents analytical information related to businesses and government agencies, in order to provide detailed industry data on employment, hours, and earnings of workers" ;
  fibo-fnd-utl-av:adaptedFrom <http://www.bls.gov/ces/> ;
.
fibo-ind-ei-usei:CurrentPopulationSurvey
  rdf:type owl:Class ;
  rdfs:label "current population survey" ;
  rdfs:subClassOf fibo-ind-ei-ei:StatisticalProgram ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fbc-fct-ra:specifies ;
      owl:someValuesFrom fibo-ind-ei-ei:CivilianNonInstitutionalPopulation ;
    ] ;
  skos:definition "a survey conducted on a regular basis that presents analytical information related to the general population of a given statistical area with respect to labor force, employment, unemployment, persons not in the labor force, hours of work, earnings, and other demographic and labor force characteristics" ;
  fibo-fnd-utl-av:adaptedFrom <http://www.bls.gov/cps/> ;
.
fibo-ind-ei-usei:EmploymentSituationEstablishmentSurvey
  rdf:type owl:Class ;
  rdfs:label "employment situation establishment survey" ;
  rdfs:seeAlso <http://www.bls.gov/web/empsit/ces_cps_trends.pdf> ;
  rdfs:subClassOf fibo-ind-ei-usei:EmploymentSituationSurvey ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:hasContext ;
      owl:someValuesFrom fibo-ind-ei-ei:Establishment ;
    ] ;
  skos:definition "a survey conducted on a regular basis that presents analytical information related to the labor force of a given statistical area, surveyed with respect to businesses, and is, for the most part, seasonally adjusted" ;
  fibo-fnd-utl-av:adaptedFrom "U.S. Bureau of Labor Statistics and Statistics Canada reference definitions - https://wiki.edmcouncil.org/pages/viewpage.action?pageId=6358041" ;
.
fibo-ind-ei-usei:EmploymentSituationHouseholdSurvey
  rdf:type owl:Class ;
  rdfs:label "employment situation household survey" ;
  rdfs:seeAlso <http://www.bls.gov/web/empsit/ces_cps_trends.pdf> ;
  rdfs:subClassOf fibo-ind-ei-usei:EmploymentSituationSurvey ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:hasContext ;
      owl:someValuesFrom fibo-ind-ei-ei:Household ;
    ] ;
  skos:definition "a survey conducted on a regular basis that presents analytical information related to the labor force of a given statistical area, surveyed with respect to households, and is, for the most part, seasonally adjusted" ;
  fibo-fnd-utl-av:adaptedFrom "U.S. Bureau of Labor Statistics and Statistics Canada reference definitions - https://wiki.edmcouncil.org/pages/viewpage.action?pageId=6358041" ;
.
fibo-ind-ei-usei:EmploymentSituationSurvey
  rdf:type owl:Class ;
  rdfs:label "employment situation survey" ;
  rdfs:subClassOf fibo-ind-ei-usei:CurrentEmploymentStatistics ;
  rdfs:subClassOf fibo-ind-ei-usei:CurrentPopulationSurvey ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onDataRange xsd:boolean ;
      owl:onProperty fibo-ind-ei-ei:isSeasonallyAdjusted ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "a survey conducted on a regular basis that presents analytical information focused on the employment characteristics of a given statistical area" ;
  fibo-fnd-utl-av:adaptedFrom <http://www.bls.gov/opub/reports/about.htm> ;
.
fibo-ind-ei-usei:PointOfPurchaseSurvey
  rdf:type owl:Class ;
  rdfs:label "point of purchase survey" ;
  rdfs:subClassOf fibo-ind-ei-ei:StatisticalProgram ;
  skos:definition "a program conducted on a regular basis that provides information on purchases of various items and services by consumers" ;
  fibo-fnd-utl-av:adaptedFrom <http://www.bls.gov/respondents/cpi/tpops/> ;
.
fibo-ind-ei-usei:USProducerPriceIndex
  rdf:type owl:Class ;
  rdfs:label "U.S. producer price index" ;
  rdfs:subClassOf fibo-ind-ei-ei:ProducerPriceIndex ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:hasValue fibo-ind-ei-usei:BureauOfLaborStatistics ;
      owl:onProperty fibo-be-fct-pub:isPublishedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-be-fct-pub:hasPublisher ;
      owl:someValuesFrom fibo-ind-ei-usei:AmericanStatisticsPublisher ;
    ] ;
  skos:definition "an economic indicator representing a measure of the change over time in the selling prices received by domestic producers for their output" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.bls.gov/ppi/"^^xsd:anyURI ;
.
fibo-ind-ei-usei:UnitedStatesDepartmentOfLabor
  rdf:type owl:NamedIndividual ;
  rdf:type fibo-be-ge-ge:GovernmentDepartment ;
  rdfs:label "United States Department of Labor" ;
  rdfs:seeAlso <https://www.dol.gov/> ;
  skos:definition "individual representing the U.S. Department of Labor, a government department whose mission is to foster, promote, and develop the welfare of the wage earners, job seekers, and retirees of the United States; improve working conditions; advance opportunities for profitable employment; and assure work-related benefits and rights" ;
  fibo-be-ge-ge:hasJurisdiction fibo-be-ge-usj:UnitedStatesJurisdiction ;
  fibo-fnd-rel-rel:isPartOf fibo-be-ge-usj:UnitedStatesGovernment ;
  fibo-fnd-utl-av:abbreviation "DOL" ;
  fibo-fnd-utl-av:adaptedFrom "https://www.dol.gov/general/aboutdol/mission"^^xsd:anyURI ;
.
fibo-ind-ei-usei:UrbanConsumerPriceIndex
  rdf:type owl:Class ;
  rdfs:label "consumer price index" ;
  rdfs:subClassOf fibo-fnd-utl-alx:Expression ;
  rdfs:subClassOf fibo-ind-ei-ei:ConsumerPriceIndex ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:hasValue fibo-ind-ei-usei:AmericanStatisticsPublisher ;
      owl:onProperty fibo-be-fct-pub:hasPublisher ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:hasValue fibo-ind-ei-usei:BureauOfLaborStatistics ;
      owl:onProperty fibo-be-fct-pub:isPublishedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-ind-ei-usei:UrbanConsumersUniverse ;
      owl:onProperty fibo-fnd-utl-alx:hasArgument ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:appliesTo ;
      owl:someValuesFrom fibo-ind-ei-usei:UrbanConsumersUniverse ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-utl-alx:hasArgument ;
      owl:someValuesFrom fibo-fbc-pas-fpas:Basket ;
    ] ;
  skos:definition "an economic indicator representing a measure of the average change over time in the prices paid by urban consumers for a market basket of consumer goods and services" ;
  fibo-fnd-utl-av:abbreviation "CPI-U" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.bls.gov/cpi/"^^xsd:anyURI ;
.
fibo-ind-ei-usei:UrbanConsumersUniverse
  rdf:type owl:Class ;
  rdfs:label "urban consumers universe" ;
  rdfs:subClassOf fibo-ind-ei-ei:CivilianNonInstitutionalPopulation ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:hasContext ;
      owl:someValuesFrom fibo-ind-ei-usei:ConsumerExpenditureSurvey ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:hasContext ;
      owl:someValuesFrom fibo-ind-ei-usei:PointOfPurchaseSurvey ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:hasMember ;
      owl:someValuesFrom fibo-ind-ei-ei:Consumer ;
    ] ;
  skos:definition "a statistical universe for consumer expenditure surveys consisting of people within a household that make joint expenditure decisions" ;
  fibo-fnd-utl-av:adaptedFrom "U.S. Bureau of Labor Statistics, http://www.bls.gov/cpi/" ;
  fibo-fnd-utl-av:explanatoryNote "In the United States, the CPI-U population, which covers about 88 percent of the U.S. population, covers households in all areas of the United States except people living in rural nonmetropolitan areas, in farm households, on military installations, in religious communities, and in institutions such as prisons and mental hospitals." ;
.
fibo-ind-fx-fx:
  terms:license sm:MITLicense ;
  sm:contentLanguage <http://www.omg.org/spec/ODM/> ;
  sm:contentLanguage <http://www.w3.org/standards/techs/owl#w3c_all> ;
  sm:copyright """Copyright (c) 2014-2017 EDM Council, Inc.
Copyright (c) 2014-2017 Object Management Group, Inc.""" ;
  sm:dependsOn <https://spec.edmcouncil.org/fibo/FBC/> ;
  sm:dependsOn <https://spec.edmcouncil.org/fibo/FND/> ;
  sm:dependsOn fibo-ind-ind-ind: ;
  sm:fileAbbreviation "fibo-ind-fx-fx" ;
  sm:fileAbstract "This ontology provides the parameters for foreign exchange rates, covering spot and forward rates, as well as Fx spot rate volatilities." ;
  sm:filename "ForeignExchange.rdf" ;
  rdfs:label "Foreign Exchange Ontology" ;
  rdfs:seeAlso <https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/> ;
  rdfs:seeAlso <https://spec.edmcouncil.org/fibo/IND/AboutIND/> ;
  rdfs:seeAlso <https://spec.edmcouncil.org/fibo/IND/ForeignExchange/AboutForeignExchange/> ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/IND/20170201/ForeignExchange/ForeignExchange/> ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/IND/20140601/ForeignExchange/ForeignExchange.rdf version of this ontology was modified per the issue resolutions identified in the FIBO IND 1.0 FTF 1 report." ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/IND/20150501/ForeignExchange/ForeignExchange.rdf version of this ontology was modified per the issue resolutions identified in the FIBO IND 1.0 FTF 2 report, namely, to take advantage of content added via the FIBO FND 1.1 with respect to higher-level concepts of Rate, ExchangeRate, and InterestRate." ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/IND/20160801/ForeignExchange/ForeignExchange.rdf version of this ontology was modified per the issue resolutions identified in the FIBO IND 1.1 RTF report." ;
.
fibo-ind-fx-fx:CurrencyForwardRate
  rdf:type owl:Class ;
  rdfs:label "currency forward rate" ;
  rdfs:subClassOf fibo-ind-fx-fx:QuotedExchangeRate ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-dt-fd:Date ;
      owl:onProperty fibo-fbc-pas-fpas:hasSettlementDate ;
    ] ;
  skos:definition "a rate of exchange between two currencies for settlement at some future point in time, expressed as a premium on the spot rate" ;
.
fibo-ind-fx-fx:CurrencySpotBuyRate
  rdf:type owl:Class ;
  rdfs:label "currency spot buy rate" ;
  rdfs:subClassOf fibo-ind-fx-fx:QuotedExchangeRate ;
  skos:definition "an indicative spot buying market rate as observed by the reporting source" ;
.
fibo-ind-fx-fx:CurrencySpotMidRate
  rdf:type owl:Class ;
  rdfs:label "currency spot mid rate" ;
  rdfs:subClassOf fibo-ind-fx-fx:QuotedExchangeRate ;
  skos:definition "an indicative middle market (mean of spot buying and selling) rate as observed by the reporting source" ;
.
fibo-ind-fx-fx:CurrencySpotSellRate
  rdf:type owl:Class ;
  rdfs:label "currency spot sell rate" ;
  rdfs:subClassOf fibo-ind-fx-fx:QuotedExchangeRate ;
  skos:definition "an indicative spot selling market rate as observed by the reporting source" ;
.
fibo-ind-fx-fx:FxSpotVolatility
  rdf:type owl:Class ;
  rdfs:label "FX spot volatility" ;
  rdfs:subClassOf fibo-ind-ind-ind:Volatility ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-acc-cur:ExchangeRate ;
      owl:onProperty fibo-ind-ind-ind:isVolatilityOf ;
    ] ;
  skos:definition "a measure of exchange rate fluctuation" ;
  fibo-fnd-utl-av:adaptedFrom "OptionsEducation.org"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "Mathematically, volatility is the annualized standard deviation of the daily changes in the exchange rate." ;
.
fibo-ind-fx-fx:QuotedExchangeRate
  rdf:type owl:Class ;
  rdfs:label "quoted exchange rate" ;
  rdfs:subClassOf fibo-fnd-acc-cur:ExchangeRate ;
  rdfs:subClassOf fibo-ind-ind-ind:MarketRate ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-acc-cur:Currency ;
      owl:onProperty fibo-ind-fx-fx:hasQuoteCurrency ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "an exchange rate quoted at a specific point in time, for a given block amount of currency as quoted against another (base) currency" ;
  fibo-fnd-utl-av:explanatoryNote "An exchange rate of R represents a rate of R units of the quoted currency to 1 unit of the base currency." ;
.
fibo-ind-fx-fx:hasQuotationBlockAmountBasis
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-ind-fx-fx:QuotedExchangeRate ;
  rdfs:label "has quotation block amount basis" ;
  rdfs:range fibo-fnd-acc-cur:MonetaryAmount ;
  skos:definition "indicates the amount of the dealt currency which would be exchanged in a trade for which the stated spot rate applies" ;
.
fibo-ind-fx-fx:hasQuotationSettlementBasis
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-ind-fx-fx:QuotedExchangeRate ;
  rdfs:label "has quotation settlement basis" ;
  rdfs:range fibo-fnd-dt-fd:Duration ;
  rdfs:subPropertyOf fibo-fnd-dt-fd:hasDuration ;
  skos:definition "indicates the settlement period for a trade for which the stated spot rate applies" ;
.
fibo-ind-fx-fx:hasQuotationSettlementBasisInDays
  rdf:type owl:DatatypeProperty ;
  rdfs:domain fibo-ind-fx-fx:QuotedExchangeRate ;
  rdfs:label "has quotation settlement basis in days" ;
  rdfs:range fibo-fnd-utl-bt:number ;
  owl:deprecated "true"^^xsd:boolean ;
  skos:definition "the settlement period in days for a trade for which the stated spot rate applies" ;
.
fibo-ind-fx-fx:hasQuoteCurrency
  rdf:type owl:ObjectProperty ;
  rdfs:label "has quote currency" ;
  rdfs:range fibo-fnd-acc-cur:Currency ;
  rdfs:subPropertyOf fibo-fnd-acc-cur:hasDealtCurrency ;
  skos:definition "indicates the quote currency in an exchange rate; R units of this currency represent one unit of the base currency" ;
.
fibo-ind-fx-fx:hasSettlementDate
  rdf:type owl:ObjectProperty ;
  rdfs:label "has settlement date" ;
  rdfs:range fibo-fnd-dt-fd:Date ;
  rdfs:subPropertyOf fibo-fnd-dt-fd:hasDate ;
  owl:deprecated "true"^^xsd:boolean ;
  skos:definition "a predicate indicating the settlement date of a given transaction" ;
.
fibo-ind-fx-fx:isPremiumOn
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-ind-fx-fx:CurrencyForwardRate ;
  rdfs:label "is premium on" ;
  rdfs:range fibo-ind-fx-fx:QuotedExchangeRate ;
  skos:definition "an exchange rate expressed as a premium on the spot rate for the currency pair" ;
.
fibo-ind-ind-ind:
  terms:license sm:MITLicense ;
  sm:contentLanguage <http://www.omg.org/spec/ODM/> ;
  sm:contentLanguage <http://www.w3.org/standards/techs/owl#w3c_all> ;
  sm:copyright """Copyright (c) 2014-2017 EDM Council, Inc.
Copyright (c) 2014-2017 Object Management Group, Inc.""" ;
  sm:dependsOn <https://spec.edmcouncil.org/fibo/BE/> ;
  sm:dependsOn <https://spec.edmcouncil.org/fibo/FND/> ;
  sm:fileAbbreviation "fibo-ind-ind-ind" ;
  sm:fileAbstract "This ontology provides the concepts common to all market rates, indices and indicators; that is concepts descriptive of the numeric parameters themselves. These are modeled independently of the values they may take over time." ;
  sm:filename "Indicators.rdf" ;
  rdfs:label "Indicators Ontology" ;
  rdfs:seeAlso <https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/> ;
  rdfs:seeAlso <https://spec.edmcouncil.org/fibo/IND/AboutIND/> ;
  rdfs:seeAlso <https://spec.edmcouncil.org/fibo/IND/Indicators/AboutIndicators/> ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/IND/20170201/Indicators/Indicators/> ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/IND/20140601/Indicators/Indicators.rdf version of this ontology was modified per the issue resolutions identified in the FIBO IND 1.0 FTF 1 report." ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/IND/20150501/Indicators/Indicators.rdf version of this ontology was modified per the issue resolutions identified in the FIBO IND 1.0 FTF 3 report, namely, to take advantage of content added via the FIBO FND 1.1 with respect to higher-level concepts of Rate, ExchangeRate, and InterestRate and revise the definition of TermStructure to better support concepts such as yield curves." ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/IND/20160801/Indicators/Indicators.rdf version of this ontology was modified per the issue resolutions identified in the FIBO IND 1.1 RTF report." ;
.
fibo-ind-ind-ind:DailyAverageMarketRate
  rdf:type owl:Class ;
  rdfs:label "daily average market rate" ;
  rdfs:subClassOf fibo-ind-ind-ind:MarketRate ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-dt-fd:ExplicitDate ;
      owl:onProperty fibo-ind-ind-ind:hasQuotationDate ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "a measure of the overall price level of a given rate, calculated as the sum of all values of the rates for a particular reference rate, foreign exchange rate, lending rate, or other market rate divided by the total number of rates collected over the course of a twenty-four (24) hour period for a specific date" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.investopedia.com/terms/m/marketaverage.asp"^^xsd:anyURI ;
.
fibo-ind-ind-ind:EndOfDayMarketRate
  rdf:type owl:Class ;
  rdfs:label "end-of-day market rate" ;
  rdfs:subClassOf fibo-ind-ind-ind:MarketRate ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-dt-fd:ExplicitDate ;
      owl:onProperty fibo-ind-ind-ind:hasQuotationDate ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "a measure of the price level (value) of a given market rate of the end of the business day for a specific date" ;
.
fibo-ind-ind-ind:FinancialInformationPublisher
  rdf:type owl:Class ;
  rdfs:label "financial information publisher" ;
  rdfs:subClassOf fibo-be-fct-pub:Publisher ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-ind-ind-ind:PublishedFinancialInformation ;
      owl:onProperty fibo-be-fct-pub:publishes ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:hasIdentity ;
      owl:someValuesFrom fibo-fnd-org-fm:FormalOrganization ;
    ] ;
  skos:definition "a formal organization acting as a publisher or provider of information related to the financial markets or of interest to financial market participants such as information on economies" ;
.
fibo-ind-ind-ind:MarketRate
  rdf:type owl:Class ;
  rdfs:label "market rate" ;
  rdfs:subClassOf fibo-fnd-qt-qtu:Rate ;
  rdfs:subClassOf fibo-ind-ind-ind:PublishedFinancialInformation ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-dt-fd:Date ;
      owl:onProperty fibo-ind-ind-ind:hasQuotationDate ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-dt-fd:DateTime ;
      owl:onProperty fibo-ind-ind-ind:hasQuotationDateTime ;
    ] ;
  owl:equivalentClass fibo-der-der-dbx:MarketRate ;
  skos:definition "a measure that is also published financial information providing a rate used to measure market trends for a set of instruments (S&P500, NASDAQ composite, 30 day CD) or that describes the economic climate for an industry (Dow Jones Industrial Average (DJIA), H&Q Growth Technologies) and/or political region (LIBOR, Prime)" ;
  skos:example "Financial market rates include, but are not limited to reference rates, foreign exchange rates, lending rates, bankers' acceptance rates, and so forth." ;
  skos:scopeNote """Market rates include but may not be limited to the following:
(1) Index: Statistical composite that measures changes in the economy or in financial markets, often expressed in percentage changes from a base year or from the previous month
(2) Money Rate: Benchmark or guideline for interest rates determined by central banks or economical climate as a whole
(3) Bankers' Acceptance Rate: Benchmark reflecting market fluctuations of Bankers' Acceptance issued instruments                
(4) Commercial Paper Rate: Benchmark reflecting market fluctuations of Commercial Paper issued instruments        
(5) Certificate of Deposit Rate: Benchmark reflecting market fluctuations of Certificate of Deposit issued instruments                
(6) Interbank Rate                                                
(7) Prime                        
(8) Time Deposit Rate: Benchmark reflecting market fluctuations of Deposit/Redeposit issued instruments""" ;
  skos:scopeNote "known collectively (in the CFI Standard) as referential instruments" ;
.
