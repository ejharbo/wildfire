      rdf:type owl:Restriction ;
      owl:onProperty fibo-ln-ln-up:hasInput ;
      owl:someValuesFrom loan:CreditReport ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-ln-ln-up:hasInput ;
      owl:someValuesFrom loan:PreApprovalContract ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-ln-ln-up:hasInput ;
      owl:someValuesFrom loan:PreApprovalRequest ;
    ] ;
