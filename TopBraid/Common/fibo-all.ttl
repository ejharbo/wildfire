  rdfs:subClassOf fibo-fnd-txn-txnx:CoveredTransaction ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-der-ma:embodies ;
      owl:someValuesFrom fibo-der-der-otc:OTCDerivativeContract ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-txn-txnx:governedBy ;
      owl:someValuesFrom fibo-der-der-ma:OTCDerivativeTransactionMasterAgreement ;
    ] ;
  skos:definition "A derivative transaction which is struck over the counter between two parties."@en ;
  fibo-fnd-utl-av:explanatoryNote "This is both a derivative transaction and a Covered Transaction, that is, the OTC transaction is transacted under some previously agreed Master Agreement between the two parties."@en ;
.
fibo-der-der-otc:OTCProduct
  rdf:type owl:Class ;
  rdfs:label "o t c product"@en ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "An Over the Counter Derivative. This is made up of one or more transactions, with a corresponding contract or contracts."@en ;
.
fibo-der-der-oti:
  rdfs:label "OTCInstruments"@en ;
.
fibo-der-der-oti:CashDeliveryCommitment
  rdf:type owl:Class ;
  rdfs:label "cash delivery commitment"@en ;
  rdfs:subClassOf fibo-der-drc-bsc:DeliveryCommitment ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-der-oti:commitsTo ;
      owl:someValuesFrom fibo-der-der-oti:DeliverCash ;
    ] ;
  skos:definition "Commitment to deliver cash at the earliest available date as per settlement convention. This is the same date as a settlement date when the corresponding cash is made available in the other currency, in settlement for this amount."@en ;
  fibo-fnd-utl-av:explanatoryNote "FpML exchangedCurrency1 referred to as a \"Currency Stream\""@en ;
.
fibo-der-der-oti:DeliverCash
  rdf:type owl:Class ;
  rdfs:label "deliver cash"@en ;
  rdfs:subClassOf fibo-der-drc-bsc:DerivativeDeliveryTermsSet ;
.
fibo-der-der-oti:OTCContractSettlementTerms
  rdf:type owl:Class ;
  rdfs:label "o t c contract settlement terms"@en ;
  rdfs:subClassOf fibo-fnd-agr-ctr:ContractTermsCompositeDescription ;
.
fibo-der-der-oti:OTCDerivativeAgreement
  rdf:type owl:Class ;
  rdfs:label "o t c derivative agreement"@en ;
  rdfs:subClassOf fibo-der-drc-bsc:DerivativeAgreement ;
  rdfs:subClassOf fibo-der-der-oti:OTCTransactionAgreement ;
.
fibo-der-der-oti:OTCInstrument
  rdf:type owl:Class ;
  rdfs:label "otcinstrument" ;
  rdfs:subClassOf owl:Thing ;
.
fibo-der-der-oti:OTCInstrumentConfirmation
  rdf:type owl:Class ;
  rdfs:label "o t c instrument confirmation"@en ;
  rdfs:subClassOf fibo-fnd-agr-ctr:WrittenContract ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-der-ma:governs ;
      owl:someValuesFrom fibo-der-der-otc:OTCDerivativeTransaction ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-agr-ctr:supersedes ;
      owl:someValuesFrom fibo-der-der-oti:OTCInstrumentContract ;
    ] ;
  owl:equivalentClass fibo-der-der-ma:OTCInstrumentConfirmation ;
  skos:definition "A formal confirmation document which constitutes a written contract between the parties to the transaction, setting out the terms and conditions of the transaction."@en ;
  fibo-fnd-utl-av:explanatoryNote "This effectively supersedes the (implied) contract that is brought into being by the transaction. It makes reference to the terms and conditions under which the transaction is deemed to have been conducted. The Confirmation references the Master Agreement under which the transaction has been conducted along with the ISDA standard terms referred to (if applicable), and adds terms and conditions specific to that one Transaction. This Confirmation generally supersedes all terms in the ISDA standard Definitions and the Master Agreement in the event of any inconsistency between the three."@en ;
.
fibo-der-der-oti:OTCInstrumentContract
  rdf:type owl:Class ;
  rdfs:label "o t c instrument contract"@en ;
  rdfs:subClassOf fibo-fnd-txn-rea:EconomicContract ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-der-oti:oTCInstrumentContractEmbodies ;
      owl:someValuesFrom fibo-der-der-oti:OTCTransactionAgreement ;
    ] ;
  owl:disjointWith fibo-sec-sec-sec:Security ;
.
fibo-der-der-oti:OTCTransactionAgreement
  rdf:type owl:Class ;
  rdfs:label "o t c transaction agreement"@en ;
  rdfs:subClassOf fibo-fnd-txn-rea:ContractualEconomicAgreement ;
  skos:definition "The agreement which forms the basis of any Over the Counter (OTC) transaction, including OTC derivatives and bilateral debt instruments."@en ;
.
fibo-der-der-oti:commitsTo
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-der-der-oti:CashDeliveryCommitment ;
  rdfs:label "commits to"@en ;
  rdfs:range fibo-der-der-oti:DeliverCash ;
.
fibo-der-der-oti:deliveryAmount
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-der-der-oti:DeliverCash ;
  rdfs:label "delivery amount"@en ;
  rdfs:range fibo-fnd-acc-cur:MonetaryAmount ;
.
fibo-der-der-oti:deliveryDate
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-der-der-oti:DeliverCash ;
  rdfs:label "delivery date"@en ;
  rdfs:range fibo-fnd-dt-fd:Date ;
  skos:definition "'The date on which both currencies traded will settle.'"@en ;
.
fibo-der-der-oti:oTCInstrumentContractEmbodies
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-der-der-oti:OTCInstrumentContract ;
  rdfs:label "o t c instrument contract embodies"@en ;
  rdfs:range fibo-der-der-oti:OTCTransactionAgreement ;
  skos:definition "The economic agreement (transaction related agreement) which the OTC Instrument Contract describes as obtaining between the parties to the Contract."@en ;
.
fibo-der-der-oti:paymentType
  rdf:type owl:DatatypeProperty ;
  rdfs:domain fibo-der-der-oti:DeliverCash ;
  rdfs:label "payment type"@en ;
  rdfs:range xsd:String ;
  skos:definition "FpML = paymentType 'A classification of the type of fee or additional payment, e.g. brokerage, upfront fee etc. FpML does not define domain values for this element.'"@en ;
.
fibo-der-der-cof:
  rdfs:label "OptionContractsOnFutures"@en ;
.
fibo-der-der-cof:FuturesContractUnderlying
  rdf:type owl:Class ;
  rdfs:label "futures contract underlying"@en ;
  rdfs:subClassOf fibo-der-drc-bsc:Underlier ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-der-cof:identity ;
      owl:someValuesFrom fibo-der-etd-fut:FutureInstrument ;
    ] ;
  skos:definition "An underlying which is a futures contract."@en ;
  fibo-fnd-utl-av:explanatoryNote "(not an underlying OF a future instrument)"@en ;
.
fibo-der-der-cof:OTCContractOnFuture
  rdf:type owl:Class ;
  rdfs:label "o t c contract on future"@en ;
  rdfs:subClassOf fibo-der-der-opt:OTCOptionContract ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-der-cof:hasUnderlying ;
      owl:someValuesFrom fibo-der-der-cof:FuturesContractUnderlying ;
    ] ;
  skos:definition "An option contract with an underlying futures contract."@en ;
.
fibo-der-der-cof:hasUnderlying
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-der-der-cof:OTCContractOnFuture ;
  rdfs:label "has underlying"@en ;
  rdfs:range fibo-der-der-cof:FuturesContractUnderlying ;
.
fibo-der-der-cof:identity
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-der-der-cof:FuturesContractUnderlying ;
  rdfs:label "identity"@en ;
  rdfs:range fibo-der-etd-fut:FutureInstrument ;
.
fibo-der-der-opt:
  rdfs:label "Options"@en ;
.
fibo-der-der-opt:AbsolutePriceStrikeSpecification
  rdf:type owl:Class ;
  rdfs:label "absolute price strike specification"@en ;
  rdfs:subClassOf fibo-der-der-opt:StrikePriceSpecification ;
  owl:disjointWith fibo-der-der-opt:PercentageStrikePriceSpecification ;
  skos:definition "Expression of the strike price as an absolute monetary price at which the option is to be struck."@en ;
.
fibo-der-der-opt:AssetMaximumOrMinimumPayoutCommitment
  rdf:type owl:Class ;
  rdfs:label "asset maximum or minimum payout commitment"@en ;
  rdfs:subClassOf fibo-der-der-opt:ConditionalPayoutCommitment ;
  skos:definition "A commitment to pay out on the maximum or minimum of a set of assets. Further Notes Maximum of n Assets This rainbow is similar to the best of n assets plus cash we referred to in part a, with the exception that no cash payoff is possible and there is a strike price for this type of option. The payoff of a call and put are given as: Rainbow-Call = max[0, max(S1, S2) - X] Rainbow-Put = max[0, X - max(S1, S2)] Minimum of n Assets The counterpart to a maximum of n assets, this rainbow pays out the value of the underperformer of the n assets. The payoff for minimum of 2 asset rainbow calls and puts are given as: Rainbow-Call = max[0, mainS1, S2) - X] Rainbow-Put = max[0, X - min(S1, S2)]"@en ;
.
fibo-der-der-opt:AssetOrNothingPayoutCommitment
  rdf:type owl:Class ;
  rdfs:label "asset or nothing payout commitment"@en ;
  rdfs:subClassOf fibo-der-der-opt:BinaryPayoutCommitment ;
  owl:disjointWith fibo-der-der-opt:CashOrNothingPayoutCommitment ;
.
fibo-der-der-opt:BestOfAssetsPlusCashPayoutCommitment
  rdf:type owl:Class ;
  rdfs:label "best of assets plus cash payout commitment"@en ;
  rdfs:subClassOf fibo-der-der-opt:ConditionalPayoutCommitment ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-der-opt:specifies ;
      owl:someValuesFrom fibo-der-der-opt:PayoffAssetsList ;
    ] ;
  skos:definition "A commitment to pay out on the best of a number of assets, plus cash."@en ;
  fibo-fnd-utl-av:explanatoryNote "This type of rainbow effectively has n + 1 payoff possibilities. If we consider a 2 asset \"best of plus cash\", the payoff at expiry is a choice between Asset 1, Asset 2, or the predetermined cash amount. There is no strike price and the payoff is given as: Rainbow = max(S1, S2, Cash;T)"@en ;
.
fibo-der-der-opt:BetterOrWorseOfAssetsPayoutCommitment
  rdf:type owl:Class ;
  rdfs:label "better or worse of assets payout commitment"@en ;
  rdfs:subClassOf fibo-der-der-opt:ConditionalPayoutCommitment ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-der-opt:references ;
      owl:someValuesFrom fibo-der-der-opt:PayoffAssetsList ;
    ] ;
  skos:definition "A commitment to pay out on the best or worst of a number of assets."@en ;
  fibo-fnd-utl-av:explanatoryNote "Better of n Assets This type of rainbow is similar to the best of n assets plus cash but with the exception that there is no possible cash payoff, and X is set to 0. With this in mind, a better of 2 assets rainbow is essentially a two-asset call option, with a payoff being: Rainbow = max[0, max(S1, S2)] Worse of n Assets Essentially the opposite to the better of n assets, with the payoff being on the asset with the lower value. We can give the payoff for this option on 2 assets as: Rainbow = max[0, min(S1, S2)]"@en ;
.
fibo-der-der-opt:BinaryPayoutCommitment
  rdf:type owl:Class ;
  rdfs:label "binary payout commitment"@en ;
  rdfs:subClassOf fibo-der-der-opt:OptionPayoutCommitment ;
  owl:disjointWith fibo-der-der-opt:ConditionalPayoutCommitment ;
.
fibo-der-der-opt:CalculatedPayoutCommitment
  rdf:type owl:Class ;
  rdfs:label "calculated payout commitment"@en ;
  rdfs:subClassOf fibo-der-der-opt:OptionPayoutCommitment ;
  owl:disjointWith fibo-der-der-opt:ConditionalPayoutCommitment ;
  owl:disjointWith fibo-der-der-opt:LinearPayoutCommitment ;
.
fibo-der-der-opt:CallOptionBuyer
  rdf:type owl:Class ;
  rdfs:label "call option buyer"@en ;
  rdfs:subClassOf fibo-der-der-opt:OptionBuyer ;
  skos:definition "The Party which is the buyer of the Call Option. This party has the option but not the obligation to transact as the Buyer of the Option Underlying."@en ;
.
fibo-der-der-opt:CallOptionContract
  rdf:type owl:Class ;
  rdfs:label "call option contract"@en ;
  rdfs:subClassOf fibo-der-der-opt:OptionContract ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-der-opt:hasBuyer ;
      owl:someValuesFrom fibo-der-der-opt:CallOptionBuyer ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-der-opt:hasOptionType ;
      owl:someValuesFrom fibo-der-der-opt:CallOptionType ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-der-opt:hasSeller ;
      owl:someValuesFrom fibo-der-der-opt:CallOptionWritingParty ;
    ] ;
  owl:disjointWith fibo-der-der-opt:ChooserOptionContract ;
  owl:disjointWith fibo-der-der-opt:PutOptionContract ;
  skos:definition "Contracts between a buyer and a seller giving the buyer (holder) the right, but not the obligation, to buy the assets specified at a fixed price or formula, on or before a specified date. The seller of the call option assumes the obligation of delivering the assets specified should the buyer exercise his option."@en ;
  skos:editorialNote "ISO FIBIM has same (ISO CFI-derived) written definition for the \"Call\" selection entry in the selection list \"OptionTypeCode\". Additional modeling Dec 2010: this is now deprecated for the former OTC Call Option. Combine the definition and consnsus from here, and the Simple Fact, befpre de;eting. the formerly OTC term has the required relationship facts."@en ;
.
fibo-der-der-opt:CallOptionType
  rdf:type owl:Class ;
  rdfs:label "call option type"@en ;
  rdfs:subClassOf fibo-der-der-opt:OptionType ;
.
fibo-der-der-opt:CallOptionWritingParty
  rdf:type owl:Class ;
  rdfs:label "call option writing party"@en ;
  rdfs:subClassOf fibo-der-der-opt:OptionWritingParty ;
  skos:definition "The Party which is the writer of the Call Option. This party has the obligation to transact as the Seller of the Option Underlying."@en ;
.
fibo-der-der-opt:CashOrNothingPayoutCommitment
  rdf:type owl:Class ;
  rdfs:label "cash or nothing payout commitment"@en ;
  rdfs:subClassOf fibo-der-der-opt:BinaryPayoutCommitment ;
  skos:definition "A commitment to pay out on a binary option, as cash or nothing."@en ;
.
fibo-der-der-opt:ChooserOptionContract
  rdf:type owl:Class ;
  rdfs:label "chooser option contract"@en ;
  rdfs:subClassOf fibo-der-der-opt:OptionContract ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-der-opt:definesOptional ;
      owl:someValuesFrom [
          rdf:type owl:Class ;
          owl:unionOf (
              fibo-der-der-opt:OptionCallTransaction
              fibo-der-der-opt:OptionPutTransaction
            ) ;
        ] ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-der-opt:hasOptionTypeElectionDate ;
      owl:someValuesFrom [
          rdf:type owl:Class ;
          owl:unionOf (
              fibo-fnd-tim-tim:DeterminedDate
              fibo-fnd-tim-tim:RelativeDate
            ) ;
        ] ;
    ] ;
  owl:disjointWith fibo-der-der-opt:PutOptionContract ;
.
fibo-der-der-opt:ConditionalPayoutCommitment
  rdf:type owl:Class ;
  rdfs:label "conditional payout commitment"@en ;
  rdfs:subClassOf fibo-der-der-opt:OptionPayoutCommitment ;
  owl:disjointWith fibo-der-der-opt:LinearPayoutCommitment ;
.
fibo-der-der-opt:DefinedConditionsPayoutCommitment
  rdf:type owl:Class ;
  rdfs:label "defined conditions payout commitment"@en ;
  rdfs:subClassOf fibo-der-der-opt:OptionPayoutCommitment ;
  skos:definition "A commitment which is only made when certain defined conditions are met. Further notes: Like all contractual commitments, the details of the commitment (and in this case, the conditions) are defined in the corresponding Contractual Terms. These set out what the commitment is and what the conditions are under which it is to be met. Term introduced in order to support digital and other exotic options. Note this is similar to but distinct from commitments where the specific nature of the commitment (typically a cashflow) is defined by resolving some mathematical formula i.e. where the commitment to do something is not conditional but the precise monetary amounts (payment, settlement) are."@en ;
.
fibo-der-der-opt:EarlyExerciseCommitment
  rdf:type owl:Class ;
  rdfs:label "early exercise commitment"@en ;
  rdfs:subClassOf fibo-der-der-opt:OptionWriterCommitment ;
  skos:definition "The commitment to exercise the option ahead of the expiry date under given conditions as laid out in the Barrier Feature terms of the Option contract."@en ;
.
fibo-der-der-opt:FixedLookbackStrikeFormula
  rdf:type owl:Class ;
  rdfs:label "fixed lookback strike formula"@en ;
  rdfs:subClassOf fibo-der-der-opt:StrikeFormula ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-der-opt:hasExpression ;
      owl:someValuesFrom fibo-der-der-opt:FixedLookbackStrikeFormulaExpression ;
    ] ;
.
fibo-der-der-opt:FixedLookbackStrikeFormulaExpression
  rdf:type owl:Class ;
  rdfs:label "fixed lookback strike formula expression"@en ;
  rdfs:subClassOf fibo-der-der-opt:LookbackFormulaExpression ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-der-opt:subtracts ;
      owl:someValuesFrom fibo-der-der-opt:ObservableBestValue ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-der-opt:subtractsFrom ;
      owl:someValuesFrom fibo-der-der-opt:FixedStrikeAmount ;
    ] ;
.
fibo-der-der-opt:FixedLookbackStrikeTermsSet
  rdf:type owl:Class ;
  rdfs:label "fixed lookback strike terms set"@en ;
  rdfs:subClassOf fibo-der-der-opt:LookbackStrikeTermsSet ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-der-opt:hasFormula ;
      owl:someValuesFrom fibo-der-der-opt:FixedLookbackStrikeFormula ;
    ] ;
  owl:disjointWith fibo-der-der-opt:FloatingLookbackStrikeTermsSet ;
  skos:definition "Strike terms which reflect the difference between a running maximum value of the observable during the lookback period, and the pre-agreed strike. The agreed settlement is based on the difference between these. Further Notes This is only settled in cash and the strike is predetermined at payoff and the payoff is the max diff between the optimal price and the strike price."@en ;
.
fibo-der-der-opt:FixedStrikeAmount
  rdf:type owl:Class ;
  rdfs:label "fixed strike amount"@en ;
  rdfs:subClassOf fibo-der-drc-bsc:ObservableValue ;
.
fibo-der-der-opt:FixedStrikeTermsSet
  rdf:type owl:Class ;
  rdfs:label "fixed strike terms set"@en ;
  rdfs:subClassOf fibo-der-der-opt:OptionStrikeTermsSet ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-der-opt:includesSpecificationTerm ;
      owl:someValuesFrom fibo-der-der-opt:StrikePriceSpecification ;
    ] ;
  owl:disjointWith fibo-der-der-opt:ResetableStrikeTermsSet ;
  skos:definition "Contractual terms setting out a fixed, pre-agreed price or level at which the option is to be struck."@en ;
  fibo-fnd-utl-av:explanatoryNote "This is similar to the FpML term for Strike, but there are two other types of Strike specification - look back and reset. FpML strike: 'Defines whether it is a price or level at which the option has been, or will be, struck.'"@en ;
.
fibo-der-der-opt:FloatingLookbackFormulaExpression
  rdf:type owl:Class ;
  rdfs:label "floating lookback formula expression"@en ;
  rdfs:subClassOf fibo-der-der-opt:LookbackFormulaExpression ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty <https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Options/subtracts.1> ;
      owl:someValuesFrom fibo-der-der-opt:ObservableBestValue ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty <https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Options/subtractsFrom.1> ;
      owl:someValuesFrom fibo-der-der-opt:ObservableFinalValue ;
    ] ;
.
fibo-der-der-opt:FloatingLookbackStrikeFormula
  rdf:type owl:Class ;
  rdfs:label "floating lookback strike formula"@en ;
  rdfs:subClassOf fibo-der-der-opt:StrikeFormula ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty <https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Options/hasExpression.1> ;
      owl:someValuesFrom fibo-der-der-opt:FloatingLookbackFormulaExpression ;
    ] ;
.
fibo-der-der-opt:FloatingLookbackStrikeSchedule
  rdf:type owl:Class ;
  rdfs:label "floating lookback strike schedule"@en ;
  rdfs:subClassOf fibo-fnd-dt-fd:Schedule ;
.
fibo-der-der-opt:FloatingLookbackStrikeTermsSet
  rdf:type owl:Class ;
  rdfs:label "floating lookback strike terms set"@en ;
  rdfs:subClassOf fibo-der-der-opt:LookbackStrikeTermsSet ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty <https://spec.edmcouncil.org/fibo/DER/DerivativesContracts/Options/hasFormula.1> ;
      owl:someValuesFrom fibo-der-der-opt:FloatingLookbackStrikeFormula ;
    ] ;
.
fibo-der-der-opt:KnockIn
  rdf:type owl:Class ;
  rdfs:label "knock in"@en ;
  rdfs:subClassOf fibo-der-der-opt:OptionFeature ;
  skos:definition "An option which comes into existence if the knock-in event happens. It works exactly the reverse of a knock-out. In a knock-in, an option comes into existence if a certain level is hit."@en ;
.
fibo-der-der-opt:KnockOut
  rdf:type owl:Class ;
  rdfs:label "knock out"@en ;
  rdfs:subClassOf fibo-der-der-opt:OptionFeature ;
  skos:definition "An option which ceases to exist if the knock-out event occurs. A knock out happens when a particular level is hit (like the Swiss franc touching the level of 1.10 against the dollar), when the option ceases to exist."@en ;
.
fibo-der-der-opt:LinearPayoutCommitment
  rdf:type owl:Class ;
  rdfs:label "linear payout commitment"@en ;
  rdfs:subClassOf fibo-der-der-opt:OptionPayoutCommitment ;
  skos:definition "The commitment to pay out on a straight vanilla option."@en ;
.
fibo-der-der-opt:LinearPayoutTermsSet
  rdf:type owl:Class ;
  rdfs:label "linear payout terms set"@en ;
  rdfs:subClassOf fibo-der-der-opt:OptionPayoutTermsSet ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-der-opt:setsOut ;
      owl:someValuesFrom fibo-der-der-opt:LinearPayoutCommitment ;
    ] ;
  skos:definition "Terms governing the payment commitment on a vanilla option."@en ;
.
fibo-der-der-opt:LookbackFormulaExpression
  rdf:type owl:Class ;
  rdfs:label "lookback formula expression"@en ;
  rdfs:subClassOf fibo-fnd-math-math:FormulaExpression ;
  skos:definition "An expression of a lookback formula"@en ;
.
fibo-der-der-opt:LookbackObservableValue
  rdf:type owl:Class ;
  rdfs:label "lookback observable value"@en ;
  rdfs:subClassOf fibo-fnd-math-math:PastValue ;
.
fibo-der-der-opt:LookbackObservableValueAtMaturity
  rdf:type owl:Class ;
  rdfs:label "lookback observable value at maturity"@en ;
  rdfs:subClassOf fibo-der-der-opt:LookbackObservableValue ;
.
fibo-der-der-opt:LookbackObservableValueDuringLookbackPeriod
  rdf:type owl:Class ;
