      owl:someValuesFrom fibo-ln-ln-up:pas-LineItem ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-ln-ln-up:hasPayee ;
      owl:someValuesFrom fibo-fnd-pty-pty:IndependentParty ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-ln-ln-up:hasPayor ;
      owl:someValuesFrom fibo-fnd-pty-pty:IndependentParty ;
    ] ;
