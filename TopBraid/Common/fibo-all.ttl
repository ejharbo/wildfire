  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/AboutFND/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/Places/AboutPlaces/"^^xsd:anyURI ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/FND/20141101/Places/VirtualPlaces/> ;
  skos:changeNote "This ontology was added to Foundations in advance of the December 2014 Long Beach meeting in support of the SEC specification." ;
.
fibo-fnd-plc-vrt:NetworkLocation
  rdf:type owl:Class ;
  rdfs:label "network location" ;
  rdfs:subClassOf fibo-fnd-plc-vrt:VirtualLocation ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-plc-adr:Address ;
      owl:onProperty fibo-fnd-aap-agt:isIdentifiedBy ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "A network address is location in a telecommunications network that may be identified by a network address (an identifier for a node or interface)" ;
.
fibo-fnd-plc-vrt:NotionalPlace
  rdf:type owl:Class ;
  rdfs:label "notional place" ;
  rdfs:subClassOf fibo-fnd-plc-loc:Location ;
  skos:definition "A place described in terms of some abstract description or as a list of commonly understood concepts such as domestic, Eurozone etc." ;
.
fibo-fnd-plc-vrt:VirtualLocation
  rdf:type owl:Class ;
  rdfs:label "virtual location" ;
  rdfs:subClassOf fibo-fnd-plc-loc:Location ;
  owl:equivalentClass fibo-fnd-plcx-vrtx:VirtualLocation ;
  skos:definition "A place which has no physical location." ;
.
fibo-fnd-plc-vrt:hasURL
  rdf:type owl:DatatypeProperty ;
  rdfs:label "has url" ;
  rdfs:range fibo-fnd-utl-bt:URI ;
  skos:altLabel "has uniform resource locator" ;
  skos:prefLabel "has URL" ;
.
fibo-fnd-plcx-apc:
  rdfs:label "AddressPropertyChain"@en ;
.
fibo-fnd-plcx-apc:tradingAtPostalAddress
  rdf:type owl:ObjectProperty ;
  rdfs:label "trading at postal address"@en ;
  rdfs:range fibo-fnd-plc-adr:PhysicalAddress ;
.
fibo-fnd-plcx-sit:
  rdfs:label "AddressSites"@en ;
.
fibo-fnd-plcx-sit:BusinessSite
  rdf:type owl:Class ;
  rdfs:label "business site"@en ;
  rdfs:subClassOf fibo-fnd-plc-fac:PhysicalSite ;
  skos:definition "An address from which a business entity carries out its business, such as a shop or offices. This is the address which is generally shown on advertising by that company."@en ;
  skos:editorialNote "Term agreed in reviews but written definition not re-reviewed. Defined as a relative thing called \"Business Address Relation\" in Proof of Concept works. Renamed to Business Site in line with W3C Site semantics ."@en ;
.
fibo-fnd-plcx-sit:ContractuallyConstitutedOrganization
  rdf:type owl:Class ;
  rdfs:label "contractually constituted organization"@en ;
  rdfs:subClassOf owl:Thing ;
.
fibo-fnd-plcx-sit:Corporation
  rdf:type owl:Class ;
  rdfs:label "corporation"@en ;
  rdfs:subClassOf owl:Thing ;
.
fibo-fnd-plcx-sit:OperatingBusinessAddressRelation
  rdf:type owl:Class ;
  rdfs:label "operating business address relation"@en ;
  rdfs:subClassOf fibo-fnd-plcx-sit:BusinessSite ;
.
fibo-fnd-plcx-sit:PrimaryBusinessAddressRelation
  rdf:type owl:Class ;
  rdfs:label "primary business address relation"@en ;
  rdfs:subClassOf fibo-fnd-plcx-sit:BusinessSite ;
.
fibo-fnd-plcx-sit:RegisteredAddressSite
  rdf:type owl:Class ;
  rdfs:label "registered address site"@en ;
  rdfs:subClassOf fibo-fnd-plc-fac:PhysicalSite ;
  skos:definition "An official Registered Address of a company, as registered with the appropriate Companies Registry for the jurisdiction in which the company is registered, and at which legal proceedings may be served upon the entity."@en ;
  fibo-fnd-utl-av:explanatoryNote "It is usually a requirement to display this on stationery and other correspondence. This is defined as the address at which legal papers may be served upon the entity. In many jurisdictions this therefore cannot be a Post Box number or Poste Restante, however some jurisdictions (e.g. South Africa) do allow this."@en ;
.
fibo-fnd-plcx-sit:hasOperatingAddress
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-plcx-sit:ContractuallyConstitutedOrganization ;
  rdfs:label "has operating address site"@en ;
  rdfs:range fibo-fnd-plcx-sit:OperatingBusinessAddressRelation ;
  skos:definition "The address at which the organization carries out its business and receives correspondence relating to its business activities."@en ;
.
fibo-fnd-plcx-sit:hasPrimaryAddressSite
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-plcx-sit:ContractuallyConstitutedOrganization ;
  rdfs:label "has primary address site"@en ;
  rdfs:range fibo-fnd-plcx-sit:PrimaryBusinessAddressRelation ;
  skos:definition "The primary address at which this organization does business and received correspondence."@en ;
.
fibo-fnd-plcx-sit:headquarteredAt
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-plcx-sit:Corporation ;
  rdfs:label "headquartered at"@en ;
  rdfs:range fibo-fnd-plcx-sit:PrimaryBusinessAddressRelation ;
  skos:definition "The address of the company's head office."@en ;
.
fibo-fnd-plcx-sit:isPrimaryAddress
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-plcx-sit:OperatingBusinessAddressRelation ;
  rdfs:label "is primary address"@en ;
  rdfs:range fibo-fnd-utl-val:TrueFalseValue ;
.
<https://spec.edmcouncil.org/fibo/FND/PlacesExt/AddressSites/isPrimaryAddress.1>
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-plcx-sit:PrimaryBusinessAddressRelation ;
  rdfs:label "is primary address"@en ;
  rdfs:range fibo-fnd-utl-val:TrueFalseValue ;
.
<https://spec.edmcouncil.org/fibo/FND/PlacesExt/AddressSites/isPrimaryAddress.2>
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-plcx-sit:BusinessSite ;
  rdfs:label "is primary address"@en ;
  rdfs:range fibo-fnd-utl-val:TrueFalseValue ;
  skos:definition "Whether the address is the primary or headquarters address of the entity."@en ;
.
fibo-fnd-plcx-sit:tradingAt
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-plcx-sit:Corporation ;
  rdfs:label "trading at"@en ;
  rdfs:range fibo-fnd-plcx-sit:OperatingBusinessAddressRelation ;
.
fibo-fnd-plcx-sit:tradingAtSite
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-plcx-sit:Corporation ;
  rdfs:label "trading at site"@en ;
  rdfs:range fibo-fnd-plcx-sit:BusinessSite ;
.
fibo-fnd-plcx-gph:
  rdfs:label "Geophysical"@en ;
.
fibo-fnd-plcx-gph:County
  rdf:type owl:Class ;
  rdfs:label "county"@en ;
  rdfs:subClassOf fibo-fnd-plcx-gph:GeographicalArea ;
  skos:definition "An area within a country or state, designated by that country or state as a County for its own descriptive and administrative purposes. Fiurther Notes: Specific properties of counties will vary from one country or state to another and are not erpresented here - this is purely a labelled area of land within a country, similar to a Region."@en ;
.
fibo-fnd-plcx-gph:GeographicalArea
  rdf:type owl:Class ;
  rdfs:label "geographical area"@en ;
  rdfs:subClassOf fibo-fnd-plcx-gph:GeographicalFeature ;
  rdfs:subClassOf fibo-fnd-plcx-gph:GeophysicalLocation ;
  skos:definition "A named and formally defined area of the world defined in terms of its physical extent. Scope Note: A Geographical Area is both a Geographical Feature (some named physical location on Earth) and a Geophysical Location (having two-dimensional extent on the Earth's surface)."@en ;
.
fibo-fnd-plcx-gph:GeographicalFeature
  rdf:type owl:Class ;
  rdfs:label "geographical feature"@en ;
  rdfs:subClassOf fibo-fnd-plc-loc:PhysicalLocation ;
  skos:definition "Some named physical location on Earth, identified geographically such as a specific land area, city or geographical feature. Scope Note: This is usually an alternative way of describing some geophysical extent or location, for example describing a city as a city as distinct from describing the physical extent of that city. Many concepts may therefore extend from this and from Geophysical or Geospatial concepts. For many purposes, modelers may ignore this class. Scope Note: This differs from Geophysical Location in that may include geographical features identified as points on the Earth's surface, or as linear features such as rivers and shorelines."@en ;
.
fibo-fnd-plcx-gph:GeophysicalLocation
  rdf:type owl:Class ;
  rdfs:label "geophysical location"@en ;
  rdfs:subClassOf fibo-fnd-plcx-gph:TopographicalLocation ;
  skos:definition "Some physical two dimensional extent on the surface of the Earth. Scope Note: The definition given in the FIBO Foundations specification for Physical Location defines some n- e.g. three dimensional space. This concept extends this to define a two dimensional extent on the Earth or other planets. Explanatory Note: Some of these locations may also be co-extensive with Geographical Features, the latter being named geophysical lications."@en ;
.
fibo-fnd-plcx-gph:LandParcel
  rdf:type owl:Class ;
  rdfs:label "land parcel"@en ;
  rdfs:subClassOf fibo-fnd-plcx-gph:GeophysicalLocation ;
  skos:definition "Some parcel or subdivision of land."@en ;
.
fibo-fnd-plcx-gph:Neighborhood
  rdf:type owl:Class ;
  rdfs:label "neighborhood"@en ;
  rdfs:subClassOf fibo-fnd-plcx-gph:GeophysicalLocation ;
  skos:definition "Some residential area."@en ;
.
fibo-fnd-plcx-gph:NeighborhoodSubdivision
  rdf:type owl:Class ;
  rdfs:label "neighborhood subdivision"@en ;
  rdfs:subClassOf fibo-fnd-plcx-gph:GeophysicalLocation ;
  skos:definition "A block or subdivision within some residential area, commonly designated sa such for the purposes of addressing and containing within it some streets and / or residential blocks."@en ;
.
fibo-fnd-plcx-gph:Region
  rdf:type owl:Class ;
  rdfs:label "region"@en ;
  rdfs:subClassOf fibo-fnd-plcx-gph:GeographicalArea ;
  skos:definition "Some area of land, either within a country or state or encompassing several countries and states, and referred to as a Region for some purpose or other."@en ;
.
fibo-fnd-plcx-gph:Street
  rdf:type owl:Class ;
  rdfs:label "street"@en ;
  rdfs:subClassOf fibo-fnd-plcx-gph:GeophysicalLocation ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-inf-padr:streetIdentifiedBy ;
      owl:someValuesFrom fibo-fnd-inf-padr:StreetAddressText ;
    ] ;
  skos:definition "Some traversable strip of land or water containing along it some buildings or land."@en ;
.
fibo-fnd-plcx-gph:Territory
  rdf:type owl:Class ;
  rdfs:label "territory"@en ;
  rdfs:subClassOf fibo-fnd-plcx-gph:GeographicalArea ;
  skos:definition "A physical extent of land and water generally corresponding to the area claimed or governmed by some geopolitical entity. ."@en ;
  fibo-fnd-utl-av:explanatoryNote "Corresponds to the physical extent of some Geopolitical Terirtory."@en ;
.
fibo-fnd-plcx-gph:TopographicalLocation
  rdf:type owl:Class ;
  rdfs:label "topographical location"@en ;
  rdfs:subClassOf fibo-fnd-plc-loc:PhysicalLocation ;
  skos:definition "Some physical two dimensional extent on the surface of some planet or natural satellite. Scope Note: The definition given in the FIBO Foundations specification for Physical Location defines some n- e.g. three dimensional space. This concept extends this to define a two dimensional extent on the Earth or other planets, moons etc. Scope Note: Potential specializations of this concept would include areographical locations (on Mars) and other planetographahical features. These are not in scope for FIBO but this abstraction has been included so that models which do explore that scope may be compatible with this model. Explanatory Note: Please refer to the discipline of topynymy for further details and possible extensions."@en ;
.
fibo-fnd-plcx-gph:name
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-plcx-gph:GeographicalFeature ;
  rdfs:label "name"@en ;
  rdfs:range fibo-fnd-utl-val:TextValue ;
  skos:definition "The name of the geographical thing, as text."@en ;
.
fibo-fnd-plcx-gpo:
  rdfs:label "Geopolitical"@en ;
.
fibo-fnd-plcx-gpo:City
  rdf:type owl:Class ;
  rdfs:label "city"@en ;
  rdfs:subClassOf fibo-fnd-plcx-gpo:MunicipalArea ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-utl-val:TextValue ;
      owl:onProperty fibo-fnd-plcx-gpo:officialName ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-plcx-gpo:cityCorrespondsTo ;
      owl:someValuesFrom fibo-fnd-plc-cty:PopulatedPlace ;
    ] ;
  skos:definition "An urban center defined as a geopolitical entity, that is one coming under one or more specific adminstrations"@en ;
  skos:editorialNote "Identified the need for a suitable formal definition of a City, in SME Reviews. Taking this further there are requirements for physical city or urban center concepts e.g. for addresses, risk, and requirements for geopolitical concepts of an urban center or cityt e.g. for their relationship to a municipality which issues debt instruments."@en ;
.
fibo-fnd-plcx-gpo:Country
  rdf:type owl:Class ;
  rdfs:label "country"@en ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-plcx-gpo:hasCapital ;
      owl:someValuesFrom fibo-fnd-plcx-gpo:City ;
    ] ;
.
fibo-fnd-plcx-gpo:GeopoliticalGroup
  rdf:type owl:Class ;
  rdfs:label "geopolitical group"@en ;
  rdfs:subClassOf fibo-fnd-plc-cty:GeopoliticalEntity ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-plcx-gpo:hasMember ;
      owl:someValuesFrom fibo-fnd-plcx-gpo:Country ;
    ] ;
.
fibo-fnd-plcx-gpo:MunicipalArea
  rdf:type owl:Class ;
  rdfs:label "municipal area"@en ;
  rdfs:subClassOf fibo-fnd-plc-cty:GeopoliticalEntity ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-plcx-gpo:municipalAreaCorrespondsTo ;
      owl:someValuesFrom fibo-fnd-plcx-gph:GeographicalArea ;
    ] ;
  skos:definition "An area which is identified as a municipality and governed by a municipal government. Scope Note: In most of the world this term or word corresponds to a city area. In the US, municipalities are also identified as coextensive with counties and some other geographical areas."@en ;
.
fibo-fnd-plcx-gpo:cityCorrespondsTo
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-plcx-gpo:City ;
  rdfs:label "city corresponds to"@en ;
  rdfs:range fibo-fnd-plc-cty:PopulatedPlace ;
  rdfs:subPropertyOf fibo-fnd-plcx-gpo:municipalAreaCorrespondsTo ;
  skos:definition "The geographical area which is a city, defined in terms of its geospatial extent."@en ;
  fibo-fnd-utl-av:explanatoryNote "This is distinct from a City described in geopolitical terms as an area of government. The names of most cities refer both to the Municipal Area and the City; in other cases they do not."@en ;
.
fibo-fnd-plcx-gpo:hasBorderWith
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-plcx-gpo:Country ;
  rdfs:label "has border with"@en ;
  rdfs:range fibo-fnd-plc-cty:Country ;
.
fibo-fnd-plcx-gpo:hasCapital
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-plcx-gpo:Country ;
  rdfs:label "has capital"@en ;
  rdfs:range fibo-fnd-plcx-gpo:City ;
.
fibo-fnd-plcx-gpo:hasMember
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-plcx-gpo:GeopoliticalGroup ;
  rdfs:label "has member"@en ;
  rdfs:range fibo-fnd-plcx-gpo:Country ;
  owl:inverseOf fibo-fnd-plcx-gpo:isInGroup ;
  skos:editorialNote "Note: inspired by geopolitical ontology (FAO)"@en ;
.
fibo-fnd-plcx-gpo:isInGroup
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-plcx-gpo:Country ;
  rdfs:label "is in group"@en ;
  rdfs:range fibo-fnd-plcx-gpo:GeopoliticalGroup ;
.
fibo-fnd-plcx-gpo:municipalAreaCorrespondsTo
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-plcx-gpo:MunicipalArea ;
  rdfs:label "municipal area corresponds to"@en ;
  rdfs:range fibo-fnd-plcx-gph:GeographicalArea ;
  skos:definition "The geographical area which is a municipality defined in terms of its geospatial extent."@en ;
  fibo-fnd-utl-av:explanatoryNote "This is distinct from a municipality described in geopolitical terms as an area of government. This term exists to support the US situation in which municipalities may govern areas other than cities, with cities being specializations of these concepts."@en ;
.
fibo-fnd-plcx-gpo:officialName
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-plcx-gpo:City ;
  rdfs:label "official name"@en ;
  rdfs:range fibo-fnd-utl-val:TextValue ;
  skos:definition "The Official Name by which the City is formally referred, if different from the Common Name. Also usually the name of the State Actor."@en ;
.
fibo-fnd-plcx-gpo:recognizedBy
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-plcx-gpo:Country ;
  rdfs:label "recognized by"@en ;
  rdfs:range fibo-fnd-plc-cty:Country ;
  skos:definition "Countries which recognize the existence of the Country."@en ;
  skos:editorialNote "Work in progress: relationship between Country and ISO Country (which is a sub-class of Country). when a country first exists, it is recognized by some country - otherwise it's dubious to call it a country. Only once the UN recognizes a country e.g. South Sudan, it is on the road to becoming an ISO Country (via first being a UN-FAO Territory). Then disputed territories (a UN-FAO term), are recognized by some and not others, e..g Kosovo."@en ;
.
fibo-fnd-plcx-usa:
  rdfs:label "GeopoliticalUSA"@en ;
.
fibo-fnd-plcx-usa:USState
  rdf:type owl:Class ;
  rdfs:label "u s state"@en ;
  rdfs:subClassOf fibo-fnd-plc-cty:FederalState ;
  skos:definition "A Federated State within the United States of America. Editorial Note: Flagged for Action in order to review the use of type versus instance here."@en ;
.
fibo-fnd-plcx-usa:abbreviation
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-plcx-usa:USState ;
  rdfs:label "abbreviation"@en ;
  rdfs:range fibo-fnd-utl-val:TextValue ;
  skos:definition "The two-letter standard abbreviation for the US State."@en ;
.
fibo-fnd-plcx-usa:designation
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-plcx-usa:USState ;
  rdfs:label "designation"@en ;
  rdfs:range fibo-fnd-utl-val:TextValue ;
  skos:definition "What the federal unit is called in this country, e.g. State, Province, Canton."@en ;
.
fibo-fnd-plcx-re:
  rdfs:label "RealEstate"@en ;
.
fibo-fnd-plcx-re:Apartment
  rdf:type owl:Class ;
  rdfs:label "apartment"@en ;
  rdfs:subClassOf fibo-fnd-plcx-re:BuildingSubUnit ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-inf-padr:apartmentIdentifiedBy ;
      owl:someValuesFrom fibo-fnd-inf-padr:BuildingSubUnitAddressFragment ;
    ] ;
  skos:definition "A building subbdivision intended for habitation."@en ;
.
fibo-fnd-plcx-re:Building
  rdf:type owl:Class ;
  rdfs:label "building"@en ;
  rdfs:subClassOf fibo-fnd-plcx-re:BuiltProperty ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-inf-padr:buildingIdentifiedBy ;
      owl:someValuesFrom fibo-fnd-inf-padr:StreetAddressText ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:hasPart ;
      owl:someValuesFrom fibo-fnd-plcx-re:BuildingSubUnit ;
    ] ;
  skos:definition "A discrete construction on its own foundations or pilings."@en ;
.
fibo-fnd-plcx-re:BuildingSubUnit
  rdf:type owl:Class ;
  rdfs:label "building sub unit"@en ;
  rdfs:subClassOf fibo-fnd-plcx-re:BuiltProperty ;
  skos:definition "A discrete subdivision of some Building."@en ;
.
fibo-fnd-plcx-re:BuiltProperty
  rdf:type owl:Class ;
  rdfs:label "built property"@en ;
  rdfs:subClassOf fibo-fnd-phy-phy:DiscretePhysicalThing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-plcx-re:hasLocation ;
      owl:someValuesFrom fibo-fnd-plcx-gph:GeophysicalLocation ;
    ] ;
  skos:definition "Something which is constructed via construction methods; bricks and mortar."@en ;
.
fibo-fnd-plcx-re:Floor
  rdf:type owl:Class ;
  rdfs:label "floor"@en ;
  rdfs:subClassOf fibo-fnd-plcx-re:BuildingSubUnit ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-inf-padr:floorIdentifiedBy ;
      owl:someValuesFrom fibo-fnd-inf-padr:BuildingSubUnitAddressFragment ;
    ] ;
  skos:definition "A building subdivision whose extent is the horizontal extent of the building at that height."@en ;
.
fibo-fnd-plcx-re:PhysicalAddress
  rdf:type owl:Class ;
  rdfs:label "physical address"@en ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-inf-padr:isOf ;
      owl:someValuesFrom fibo-fnd-plcx-re:RealEstate ;
    ] ;
.
fibo-fnd-plcx-re:PropertyOwner
  rdf:type owl:Class ;
  rdfs:label "property owner"@en ;
  rdfs:subClassOf fibo-fnd-oac-own:Owner ;
.
fibo-fnd-plcx-re:RealEstate
  rdf:type owl:Class ;
  rdfs:label "real estate"@en ;
  rdfs:subClassOf fibo-fnd-plc-loc:PhysicalLocation ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-plcx-re:realEstateCoextensiveWith ;
      owl:someValuesFrom [
          rdf:type owl:Class ;
          owl:unionOf (
              fibo-fnd-plcx-gph:LandParcel
              fibo-fnd-plcx-re:BuiltProperty
            ) ;
        ] ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-plcx-re:realEstateOwnedBy ;
      owl:someValuesFrom fibo-fnd-plcx-re:PropertyOwner ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:hasPart ;
      owl:someValuesFrom fibo-fnd-plcx-re:RealEstate ;
    ] ;
  skos:definition "Land plus anything permanently fixed to it, including buildings, sheds and other items attached to the structure. Explanatory Note: Although, media often refers to the \"real estate market\" from the perspective of residential living, real estate can be grouped into three broad categories based on its use, namely residential, commercial and industrial. Explanatory Note: Examples of real estate include undeveloped land, houses, condominiums, townhomes, office buildings, retail store buildings and factories. Scope Note: Bricks and mortar or land which can be owned."@en ;
.
fibo-fnd-plcx-re:Suite
  rdf:type owl:Class ;
  rdfs:label "suite"@en ;
  rdfs:subClassOf fibo-fnd-plcx-re:BuildingSubUnit ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-inf-padr:suiteIdentifiedBy ;
      owl:someValuesFrom fibo-fnd-inf-padr:BuildingSubUnitAddressFragment ;
    ] ;
  skos:definition "A subdivision of a building for which the purposes are not clearly defined or may be variable, generally consisting of a number of contiguous rooms on one or more floors."@en ;
.
fibo-fnd-plcx-re:hasLocation
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-plcx-re:BuiltProperty ;
  rdfs:label "has location"@en ;
  rdfs:range fibo-fnd-plcx-gph:GeophysicalLocation ;
  skos:definition "The physical location of the built item or property."@en ;
.
fibo-fnd-plcx-re:identifiedBy
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-plcx-re:RealEstate ;
  rdfs:label "identified by"@en ;
  rdfs:range fibo-fnd-plcx-re:PhysicalAddress ;
  rdfs:subPropertyOf fibo-fnd-inf-inf:details ;
.
fibo-fnd-plcx-re:leasehold
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-plcx-re:RealEstate ;
  rdfs:label "leasehold"@en ;
  rdfs:range fibo-fnd-utl-val:TrueFalseValue ;
  skos:definition "Typically a leasehold is where the land is not owned by the borrower, rather it is leased from a third party for a term that exceeds the life of the mortgage."@en ;
.
fibo-fnd-plcx-re:realEstateCoextensiveWith
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-plcx-re:RealEstate ;
  rdfs:label "real estate coextensive with"@en ;
  rdfs:range [
      rdf:type owl:Class ;
      owl:unionOf (
          fibo-fnd-plcx-gph:LandParcel
          fibo-fnd-plcx-re:BuiltProperty
        ) ;
    ] ;
  skos:definition "Real Estate corresponds to either some built item or some parcel of land."@en ;
  skos:scopeNote "This is represented as a logical union of Built Item and Land Parcel. Scope Note: Future changes in Real Estate ontology alignment may well change or elaborate on this."@en ;
.
fibo-fnd-plcx-re:realEstateOwnedBy
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-plcx-re:RealEstate ;
  rdfs:label "real estate owned by"@en ;
  rdfs:range fibo-fnd-plcx-re:PropertyOwner ;
.
fibo-fnd-plcx-vrtx:
  rdfs:label "VirtualPlacesExt"@en ;
.
fibo-fnd-plcx-vrtx:NotionalPlace
  rdf:type owl:Class ;
  rdfs:label "notional place"@en ;
  rdfs:subClassOf fibo-fnd-plc-vrt:VirtualLocation ;
  skos:definition "A place described in terms of some abstract description or as a list of commonly understood concepts such as domestic, Eurozone etc."@en ;
.
fibo-fnd-plcx-vrtx:VirtualLocation
  rdf:type owl:Class ;
  rdfs:label "virtual location"@en ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "A place which has no physical location."@en ;
.
fibo-fnd-plcx-vrtx:knownAs
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-plcx-vrtx:VirtualLocation ;
  rdfs:label "known as"@en ;
  rdfs:range fibo-fnd-utl-val:TextValue ;
  skos:definition "How the place is commonly referred to."@en ;
.
fibo-fnd-plcx-vrtx:uRL
  rdf:type owl:DatatypeProperty ;
  rdfs:label "u r l"@en ;
.
fibo-fnd-pas-psch:
  terms:license "http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense"^^xsd:anyURI ;
  sm:contentLanguage "http://www.omg.org/spec/ODM/"^^xsd:anyURI ;
  sm:contentLanguage "http://www.w3.org/standards/techs/owl#w3c_all"^^xsd:anyURI ;
  sm:copyright """Copyright (c) 2015-2016 EDM Council, Inc.
Copyright (c) 2015-2016 Object Management Group, Inc.""" ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Agreements/Agreements/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/Occurrences/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Law/LegalCapacity/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Parties/Parties/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Parties/Roles/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/ProductsAndServices/ProductsAndServices/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"^^xsd:anyURI ;
  sm:fileAbbreviation "fibo-fnd-pas-psch" ;
  sm:fileAbstract "This ontology defines basic concepts such as payment, payee, payer, and payment schedule, extending the scheduling concepts from the Dates and Times module, among others." ;
  sm:filename "PaymentsAndSchedules.rdf" ;
  rdfs:label "Payments and Schedules Ontology" ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/AboutFND/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/ProductsAndServices/AboutProductsAndServices/"^^xsd:anyURI ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/FND/20160201/ProductsAndServices/PaymentsAndSchedules/> ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/FND/20150801/ProductsAndServices/PaymentsAndSchedules.rdf version of this ontology was modified per the issue resolutions identified in the FIBO FND 1.1 RTF report to replace MoneyAmount with MonetaryAmount." ;
.
fibo-fnd-pas-psch:Payee
  rdf:type owl:Class ;
  rdfs:label "payee" ;
  rdfs:subClassOf fibo-fnd-pty-pty:PartyInRole ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
      owl:someValuesFrom [
          rdf:type owl:Restriction ;
          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
          owl:onClass fibo-fnd-pas-psch:PaymentObligation ;
          owl:onProperty fibo-fnd-pty-pty:isAPartyTo ;
        ] ;
    ] ;
  skos:definition "a party to whom a debt should be paid, or to whose order a bill of exchange, note, or check is made payable" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fnd-pas-psch:Payer
  rdf:type owl:Class ;
  rdfs:label "payer" ;
  rdfs:subClassOf fibo-fnd-pty-pty:PartyInRole ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-pas-psch:PaymentObligation ;
      owl:onProperty fibo-fnd-pas-psch:hasObligation ;
    ] ;
  skos:definition "a party who pays a bill or fees" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fnd-pas-psch:Payment
  rdf:type owl:Class ;
  rdfs:label "payment" ;
  rdfs:subClassOf fibo-fnd-dt-oc:Occurrence ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-acc-cur:MonetaryAmount ;
      owl:onProperty fibo-fnd-pas-psch:hasPaymentAmount ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-pas-psch:PaymentObligation ;
      owl:onProperty fibo-fnd-pas-psch:fulfillsObligation ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pty-pty:hasPartyInRole ;
      owl:someValuesFrom fibo-fnd-pas-psch:Payee ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pty-pty:hasPartyInRole ;
      owl:someValuesFrom fibo-fnd-pas-psch:Payer ;
    ] ;
  skos:definition "delivery of money in fulfillment of an obligation, such as to satisfy a claim or debt" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fnd-pas-psch:PaymentEvent
  rdf:type owl:Class ;
  rdfs:label "payment event" ;
  rdfs:subClassOf fibo-fnd-pas-pas:TransactionEvent ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-pas-psch:Payment ;
      owl:onProperty fibo-fnd-rel-rel:involves ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-pas-psch:PaymentObligation ;
      owl:onProperty fibo-fnd-rel-rel:appliesTo ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "an event that involves delivery of money in fulfillment of an obligation" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fnd-pas-psch:PaymentObligation
  rdf:type owl:Class ;
  rdfs:label "payment obligation" ;
  rdfs:subClassOf fibo-fnd-agr-agr:Commitment ;
  rdfs:subClassOf fibo-fnd-law-lcap:Duty ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-agr-ctr:Contract ;
      owl:onProperty fibo-fnd-rel-rel:isConferredBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-pas-psch:Payer ;
      owl:onProperty fibo-fnd-pas-psch:isObligationOf ;
    ] ;
  skos:definition "a legally enforceable duty to pay a sum of money, or agree to do something (or not to do something), according to the terms stated in a contract" ;
  skos:example "the duty of a borrower to repay a loan, and the legal right of a lender to enforce payment" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fnd-pas-psch:PaymentSchedule
  rdf:type owl:Class ;
  rdfs:label "payment schedule" ;
  rdfs:subClassOf fibo-fnd-dt-fd:Schedule ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-pas-psch:PaymentEvent ;
      owl:onProperty fibo-fnd-rel-rel:comprises ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "schedule for delivery of money in fulfillment of an obligation, such as a coupon payment schedule, loan payment schedule, interest payment schedule" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fnd-pas-psch:fulfillsObligation
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-pas-psch:Payment ;
  rdfs:label "fulfills obligation" ;
  rdfs:range fibo-fnd-pas-psch:PaymentObligation ;
  skos:definition "satisfies a requirement for payment of some claim, debt, or other obligation" ;
.
fibo-fnd-pas-psch:hasObligation
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-pas-psch:Payer ;
  rdfs:label "has obligation" ;
  rdfs:range fibo-fnd-pas-psch:PaymentObligation ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  owl:inverseOf fibo-fnd-pas-psch:isObligationOf ;
  skos:definition "identifies a duty or obligation that a given party has taken on" ;
.
fibo-fnd-pas-psch:hasPaymentAmount
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-pas-psch:Payment ;
  rdfs:label "has payment amount" ;
  rdfs:range fibo-fnd-acc-cur:MonetaryAmount ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "specifies the amount of money involved in a payment" ;
.
fibo-fnd-pas-psch:isObligationOf
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-pas-psch:PaymentObligation ;
  rdfs:label "is obligation of" ;
  rdfs:range fibo-fnd-pas-psch:Payer ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:isConferredOn ;
  skos:definition "identifies a party that has incurred a given obligation" ;
.
fibo-fnd-pas-pas:
