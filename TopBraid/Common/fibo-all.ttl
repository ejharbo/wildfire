.
loan:CreditScoreCategory
  rdf:type owl:Class ;
  rdfs:subClassOf fibo-fnd-arr-cls:Classifier ;
  skos:definition "A CreditScore that is a qualitative measure." ;
.
loan:CreditScoreFactor
  rdf:type owl:Class ;
  rdfs:label "credit score factor" ;
  rdfs:subClassOf owl:Thing ;
.
loan:CreditScoreModel
  rdf:type owl:Class ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Class ;
      owl:intersectionOf (
          fibo-ln-ln-up:Specification
          [
            rdf:type owl:Restriction ;
            owl:onProperty fibo-fnd-rel-rel:isClassifiedBy ;
            owl:someValuesFrom loan:CreditScoreModelType ;
          ]
        ) ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-aap-agt:hasName ;
      owl:someValuesFrom xsd:string ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-ln-ln-up:rel-hasVersion ;
      owl:someValuesFrom xsd:string ;
    ] ;
  skos:definition "An algorithm for computing the credit score of e.g. a credit applicant." ;
.
loan:CreditScoreModelType
  rdf:type owl:Class ;
  rdfs:subClassOf fibo-fnd-arr-cls:Classifier ;
  skos:definition "A family of credit scoring algorithms sharing common characteristics." ;
.
loan:CreditScoreNumber
  rdf:type owl:Class ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-qt-qtu:hasNumericValue ;
      owl:someValuesFrom xsd:decimal ;
    ] ;
  skos:definition "A CreditScore that is a quantitative measure." ;
.
loan:CreditTradeLine
  rdf:type owl:Class ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-qt-qtu:isDerivedFrom ;
      owl:someValuesFrom fibo-ln-ln-up:TransactionHistory ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:isPartOf ;
      owl:someValuesFrom loan:CreditReport ;
    ] ;
