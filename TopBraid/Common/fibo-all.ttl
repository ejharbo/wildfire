  rdf:type owl:Class ;
  rdfs:label "business day convention" ;
  rdfs:subClassOf fibo-fnd-dt-bd:Convention ;
  skos:definition "a convention that enumerates the possible ways to handle a date that falls on a weekend or holiday" ;
.
fibo-fnd-dt-bd:BusinessDayFollowing
  rdf:type owl:NamedIndividual ;
  rdf:type fibo-fnd-dt-bd:BusinessDayConvention ;
  sm:normativeReference "FPML 5.1 \"BusinessDayConventionEnum\"" ;
  rdfs:label "business day following" ;
  skos:definition "BusinessDayFollowing is a BusinessDayConvention that means a non-business date will be adjusted to the first following day that is a business day." ;
.
fibo-fnd-dt-bd:BusinessDayModifiedFollowing
  rdf:type owl:NamedIndividual ;
  rdf:type fibo-fnd-dt-bd:BusinessDayConvention ;
  sm:normativeReference "FPML 5.1 \"BusinessDayConventionEnum\"" ;
  rdfs:label "business day modified following" ;
  skos:definition "BusinessDayModifiedFollowing is a BusinessDayConvenion that means a non-business date will be adjusted to the first following day that is a business day unless that day falls in the next calendar month, in which case that date will be the first preceding day that is a calendar date." ;
.
fibo-fnd-dt-bd:BusinessDayModifiedPreceding
  rdf:type owl:NamedIndividual ;
  rdf:type fibo-fnd-dt-bd:BusinessDayConvention ;
  sm:normativeReference "FPML 5.1 \"BusinessDayConventionEnum\"" ;
  rdfs:label "business day modified preceding" ;
  skos:definition "BusinessDayModifiedPreceding is a BusinessDayConvention that means that a non-business date will be adjusted to the first preceding day that is a business day unless that day falls in the previous month, in which case that date will be the first following day that is a business day." ;
.
fibo-fnd-dt-bd:BusinessDayNearest
  rdf:type owl:NamedIndividual ;
  rdf:type fibo-fnd-dt-bd:BusinessDayConvention ;
  sm:normativeReference "FPML 5.1 \"BusinessDayConventionEnum\"" ;
  rdfs:label "business day nearest" ;
  skos:definition "BusinessDayNearest is a BusinessDayConvention that means that a non-business date will be adjusted to the nearest day that is a business day -- i.e. if the non-business day falls on any day other than a Sunday or a Monday, it will be the first preceding day that is a business day, and will be the first following business day if it falls on a Sunday or a Monday." ;
.
fibo-fnd-dt-bd:BusinessDayNone
  rdf:type owl:NamedIndividual ;
  rdf:type fibo-fnd-dt-bd:BusinessDayConvention ;
  sm:normativeReference "FPML 5.1 \"BusinessDayConventionEnum\"" ;
  rdfs:label "business day none" ;
  skos:definition "BusinessDayNone is a BusinessDayConvention that means that a date will not be adjusted if it falls on a day that is not a business day." ;
.
fibo-fnd-dt-bd:BusinessDayPreceding
  rdf:type owl:NamedIndividual ;
  rdf:type fibo-fnd-dt-bd:BusinessDayConvention ;
  rdfs:label "business day preceding" ;
.
fibo-fnd-dt-bd:BusinessRecurrenceInterval
  rdf:type owl:Class ;
  rdfs:label "business recurrence interval" ;
  rdfs:subClassOf fibo-fnd-dt-fd:RecurrenceInterval ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-dt-bd:BusinessRecurrenceIntervalConvention ;
      owl:onProperty fibo-fnd-dt-bd:hasBusinessRecurrenceIntervalConvention ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "a recurrence interval that is specified using a business recurrence interval convention" ;
.
fibo-fnd-dt-bd:BusinessRecurrenceIntervalConvention
  rdf:type owl:Class ;
  rdfs:label "business recurrence interval convention" ;
  rdfs:subClassOf fibo-fnd-dt-bd:Convention ;
  skos:definition "a convention for modeling recurring days, such as the end of the month, a particular day of the month, a day of the week, or more specifically, a t-bill auction date" ;
.
fibo-fnd-dt-bd:BusinessRecurrenceIntervalEndOfMonth
  rdf:type owl:NamedIndividual ;
  rdf:type fibo-fnd-dt-bd:BusinessRecurrenceIntervalConvention ;
  sm:normativeReference "FIX 5.0SP2 \"LegDateRollConvention/EOM\"" ;
  sm:normativeReference "FPML 5.1 'RollConventionEnum/EOM'" ;
  rdfs:label "business recurrence interval end-of-month" ;
  skos:definition "The last day of a calendar month irrespective of the length of the calendar month." ;
.
fibo-fnd-dt-bd:Convention
  rdf:type owl:Class ;
  rdfs:label "convention" ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "a widely accepted or established way of doing 'something' within some community of practice" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
  fibo-fnd-utl-av:adaptedFrom "https://en.oxforddictionaries.com/definition/convention"^^xsd:anyURI ;
.
fibo-fnd-dt-bd:DayOfMonth
  rdf:type owl:Class ;
  rdfs:label "day of the month" ;
  rdfs:subClassOf fibo-fnd-dt-bd:BusinessRecurrenceInterval ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onDataRange xsd:nonNegativeInteger ;
      owl:onProperty fibo-fnd-dt-fd:hasOrdinalNumber ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "a business recurrence interval that indicates a specific, recurring day of the month" ;
  skos:scopeNote "This class is only used or meaningful when a regular period in a schedule is monthly, i.e., period is 1 month." ;
.
fibo-fnd-dt-bd:DayOfWeek
  rdf:type owl:Class ;
  rdfs:label "day of the week" ;
  rdfs:subClassOf fibo-fnd-dt-bd:BusinessRecurrenceInterval ;
  owl:equivalentClass [
      rdf:type owl:Class ;
      owl:oneOf (
          fibo-fnd-dt-fd:Monday
          fibo-fnd-dt-fd:Tuesday
          fibo-fnd-dt-fd:Wednesday
          fibo-fnd-dt-fd:Thursday
          fibo-fnd-dt-fd:Friday
          fibo-fnd-dt-fd:Saturday
          fibo-fnd-dt-fd:Sunday
        ) ;
    ] ;
  skos:definition "a business recurrence interval that indicates a specific, recurring day of the week" ;
  skos:scopeNote "This class is only used or meaningful when a regular period in a schedule is weekly, i.e., period is 1 week." ;
.
fibo-fnd-dt-bd:EndOfMonth
  rdf:type owl:Class ;
  rdfs:label "end of the month" ;
  rdfs:subClassOf fibo-fnd-dt-bd:BusinessRecurrenceInterval ;
  skos:definition "a business recurrence interval that indicates the last day of a calendar month, regardless of the length of that month" ;
.
fibo-fnd-dt-bd:hasBusinessCenter
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-dt-bd:BusinessDayAdjustment ;
  rdfs:label "has business center" ;
  rdfs:range fibo-fnd-plc-cty:BusinessCenter ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "identifies a location where business is conducted, and hence the business calendar used to adjust dates" ;
.
fibo-fnd-dt-bd:hasBusinessDayAdjustment
  rdf:type owl:ObjectProperty ;
  rdfs:label "has business day adjustment" ;
  rdfs:range fibo-fnd-dt-bd:BusinessDayAdjustment ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "identifies a convention for adjustment of the business day for handling weekends and holidays" ;
.
fibo-fnd-dt-bd:hasBusinessDayConvention
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-dt-bd:BusinessDayAdjustment ;
  rdfs:label "has business day convention" ;
  rdfs:range fibo-fnd-dt-bd:BusinessDayConvention ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "identifies a convention regarding how a date should be handled when it falls on a day that is not a business day" ;
.
fibo-fnd-dt-bd:hasBusinessRecurrenceIntervalConvention
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-dt-bd:BusinessRecurrenceInterval ;
  rdfs:label "has business recurrence interval convention" ;
  rdfs:range fibo-fnd-dt-bd:BusinessRecurrenceIntervalConvention ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "identifies a convention regarding how certain recurring dates should be handled with respect to a given schedule, such as the end of the month" ;
.
fibo-fnd-dt-bd:holdsDuring
  rdf:type owl:ObjectProperty ;
  rdfs:label "holds during" ;
  rdfs:subPropertyOf fibo-fnd-dt-fd:hasDatePeriod ;
  skos:definition "a relationship that states that some condition or state holds during a specified date period" ;
.
fibo-fnd-dt-fd:
  terms:license sm:MITLicense ;
  sm:contentLanguage <http://www.omg.org/spec/ODM/> ;
  sm:contentLanguage <http://www.w3.org/standards/techs/owl#w3c_all> ;
  sm:copyright """Copyright (c) 2014-2017 EDM Council, Inc.
Copyright (c) 2014-2017 Object Management Group, Inc.""" ;
  sm:dependsOn fibo-fnd-arr-arr: ;
  sm:dependsOn fibo-fnd-plc-loc: ;
  sm:dependsOn fibo-fnd-qt-qtu: ;
  sm:dependsOn fibo-fnd-rel-rel: ;
  sm:dependsOn fibo-fnd-utl-av: ;
  sm:fileAbbreviation "fibo-fnd-dt-fd" ;
  sm:fileAbstract "This ontology provides definitions of date and schedule concepts for use in other FIBO ontologies." ;
  sm:filename "FinancialDates.rdf" ;
  rdfs:label "Financial Dates Ontology" ;
  rdfs:seeAlso <https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/> ;
  rdfs:seeAlso <https://spec.edmcouncil.org/fibo/FND/AboutFND/> ;
  rdfs:seeAlso <https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/AboutDatesAndTimes/> ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/FND/20170201/DatesAndTimes/FinancialDates/> ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/FND/20141101/DatesAndTimes/FinancialDates/ version of this ontology was revised by the FIBO SEC RFC in order to introduce the definition of a time interval, which is a location, to ground some of the concepts such as a date period, and duration as well as to support the definition of business recurrence intervals for use in ISDA schedules." ;
  skos:changeNote """This ontology was added to Foundations in advance of the December 2014 Long Beach meeting in support of the SEC specification.  It is also needed to provide temporal relationships for Ownership and Control.

These three ontologies are designed for use together:

   * FinancialDates  -- financial Dates and Schedules
   * BusinessDates -- business day adjustments
   * Occurrences    -- occurrences (events) and kinds of occurrences

They are modularized this way to minimize the ontological committments that are imposed upon ontologies that rely upon them.  Ontologies can import FinancialDates alone, or FinancialDates + BusinessDates, or FinancialDates + Occurrences, or all three together.""" ;
.
fibo-fnd-dt-fd:AdHocSchedule
  rdf:type owl:Class ;
  rdfs:label "ad hoc schedule" ;
  rdfs:subClassOf fibo-fnd-dt-fd:Schedule ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:comprises ;
      owl:someValuesFrom fibo-fnd-dt-fd:AdHocScheduleEntry ;
    ] ;
  owl:equivalentClass fibo-sec-sec-schx:AdHocSschedule ;
  skos:definition "a schedule that consists of (comprises) AdHocScheduleEntries, each of which specifies a date" ;
  fibo-fnd-utl-av:usageNote "Other ontologies can extend AdHocSchedule and/or AdHocScheduleEntry as needed to relate the date to something. In particular, the Occurrences ontology extends AdHocScheduleEntry to associate an OccurrenceKind with each entry. The intended meaning is that an Occurrence of the OccurrenceKind happens on the corresponding Date." ;
.
fibo-fnd-dt-fd:AdHocScheduleEntry
  rdf:type owl:Class ;
  rdfs:label "ad hoc schedule entry" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-dt-fd:Date ;
      owl:onProperty fibo-fnd-dt-fd:hasDate ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-dt-oc:OccurrenceKind ;
      owl:onProperty fibo-fnd-rel-rel:comprises ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "an entry, including a date, among multiple ad hoc schedule entries that jointly make up an AdHocSchedule" ;
  fibo-fnd-utl-av:usageNote "Other ontologies can extend AdHocScheduleEntry as needed. In particular, the Occurrences ontology extends AdHocScheduleEntry to comprise an OccurrenceKind. The meaning is that an ad hoc schedule entry comprises a date and an event which is scheduled to occur on that date; in other words Occurrence of the OccurrenceKind should happen on the Date of the OccurrenceKind." ;
  fibo-fnd-utl-av:usageNote """The Date of an AdHocScheduleEntry can be an ExplicitDate or any kind of CalculatedDate, such as:

* An OccurrenceBasedDate -- a Date that itself is defined by an Occurrence (see the Occurrences ontology)
* A RelativeDate - a Date relative to another Date, such as T+3
* A SpecifiedDate - a Date that is defined by an arbitrary rule""" ;
.
fibo-fnd-dt-fd:CalculatedDate
  rdf:type owl:Class ;
  rdfs:label "calculated date" ;
  rdfs:subClassOf fibo-fnd-dt-fd:Date ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-dt-bd:BusinessDayAdjustment ;
      owl:onProperty fibo-fnd-dt-bd:hasBusinessDayAdjustment ;
    ] ;
  owl:disjointWith fibo-fnd-dt-fd:ExplicitDate ;
  skos:definition "a date that is or will be calculated in some way" ;
  fibo-fnd-utl-av:explanatoryNote "The hasDateValue property of a CalculatedDate is not set until the Date is calculated.  Since the calculation may depend upon future events that may or may not ever happen, the hasDateValue property may never be set." ;
.
fibo-fnd-dt-fd:CalendarMonth
  rdf:type owl:NamedIndividual ;
  rdf:type fibo-fnd-dt-fd:CalendarPeriod ;
  sm:normativeReference "ISO 8601, clause 2.2.11" ;
  rdfs:label "calendar month" ;
  skos:definition "time interval resulting from the division of a calendar year in 12 time intervals, each with a specific name and containing a specific number of calendar days" ;
.
fibo-fnd-dt-fd:CalendarPeriod
  rdf:type owl:Class ;
  rdfs:label "calendar period" ;
  rdfs:subClassOf fibo-fnd-dt-fd:TimeInterval ;
  skos:definition "an enumeration that indicates whether a CalendarSpecifiedDate is figured with respect to a calendar week, a calendar month, a calendar quarter, or a calendar year" ;
  skos:editorialNote """The terms 'calendar xxx' are intended to reinforce that these are periods on a calendar, not durations. 

For example, a calendar year always starts on a January 1 and ends on a December 31. The term 'calendar year' does not mean the same thing as  a duration (an amount of time) of 1 year, nor can a calendar year start on any arbitrary day of a year.  For example, a calendar year never starts on September 1.

Similar points apply to other kinds of calendar periods, such as calendar week, calendar month, and calendar quarter.""" ;
.
fibo-fnd-dt-fd:CalendarQuarter
  rdf:type owl:NamedIndividual ;
  rdf:type fibo-fnd-dt-fd:CalendarPeriod ;
  rdfs:label "calendar quarter" ;
  skos:definition "time interval of 3 months, starting on January 1, April 1, July 1, or October 1" ;
.
fibo-fnd-dt-fd:CalendarSpecifiedInterval
  rdf:type owl:Class ;
  rdfs:label "calendar specified interval" ;
  rdfs:subClassOf fibo-fnd-dt-fd:RecurrenceInterval ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-dt-fd:CalendarPeriod ;
      owl:onProperty fibo-fnd-dt-fd:hasCalendarPeriod ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-dt-fd:TimeDirection ;
      owl:onProperty fibo-fnd-dt-fd:hasTimeDirection ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onDataRange xsd:integer ;
      owl:onProperty fibo-fnd-dt-fd:hasOrdinalNumber ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  owl:disjointWith fibo-fnd-dt-fd:ExplicitRecurrenceInterval ;
  skos:definition """a RecurrenceInterval that is specified as the nth day of some CalendarPeriod (such as a calendar month), and a TimeDirection (forward from the beginning of the month, or backwards from the end).

The nth day is an ordinal number, not a cardinal number. '1' means the first day of the calendar period.""" ;
  skos:example "The 15th day of each calendar month." ;
  skos:example "The last day of each quarter, specified as RelativeDay 1, and TimeDirection set to FromEnd." ;
.
fibo-fnd-dt-fd:CalendarWeek
  rdf:type owl:NamedIndividual ;
  rdf:type fibo-fnd-dt-fd:CalendarPeriod ;
  sm:normativeReference "ISO 8601, clause 2.2.8" ;
  rdfs:label "calendar week" ;
  skos:definition "time interval of seven calendar days starting on a Monday" ;
.
fibo-fnd-dt-fd:CalendarYear
  rdf:type owl:NamedIndividual ;
  rdf:type fibo-fnd-dt-fd:CalendarPeriod ;
  sm:normativeReference "ISO 8601 clause 2.2.13" ;
  rdfs:label "calendar year" ;
  skos:definition "cyclic time interval in a calendar which is required for one revolution of the Earth around the Sun and approximated to an integral number of calendar days; a year in the Gregorian calendar" ;
.
fibo-fnd-dt-fd:Date
  rdf:type owl:Class ;
  rdfs:label "date" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange xsd:string ;
      owl:onProperty fibo-fnd-dt-fd:hasDateValue ;
    ] ;
  skos:definition "a calendar day on some calendar" ;
  fibo-fnd-utl-av:usageNote "A Date may or may not have a value, and may be explicit or calculated. A Date that has a value is one that is either explicitly set as a literal when it is created, or is some form of CalculatedDate.  In an instance of Date, the existence of the 'hasDateValue' property both indicates that the Date is known, and gives the value of the Date. A Date that does not have a value is one that is some form of CalculatedDate, in which the actual date has not (yet) been established." ;
.
fibo-fnd-dt-fd:DatePeriod
  rdf:type owl:Class ;
  rdfs:label "date period" ;
  rdfs:subClassOf fibo-fnd-dt-fd:TimeInterval ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-dt-fd:Date ;
      owl:onProperty fibo-fnd-dt-fd:hasEndDate ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-dt-fd:Date ;
      owl:onProperty fibo-fnd-dt-fd:hasStartDate ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-dt-fd:Duration ;
      owl:onProperty fibo-fnd-dt-fd:hasDuration ;
    ] ;
  skos:definition "a time span over one or more calendar days, defined by at least two of three properties: (1) a start date, (2) an end date, and (3) a date period (duration); if more than one of these properties is missing, the DatePeriod is invalid" ;
  fibo-fnd-utl-av:usageNote "A DatePeriod is unknown if either the startDate or the endDate has no value. If a DatePeriod is unknown, then the periodDuration should either be omitted or unknown (have no value)." ;
.
fibo-fnd-dt-fd:DateTime
  rdf:type owl:Class ;
  rdfs:label "date time" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange xsd:dateTime ;
      owl:onProperty fibo-fnd-dt-fd:hasDateTimeValue ;
    ] ;
  skos:definition "a combination of a date and a time, without a time zone" ;
  fibo-fnd-utl-av:usageNote "'hasDateTimeValue' is omitted if the DateTime is not (yet) known." ;
  fibo-fnd-utl-av:usageNote "Use DateTime for the combination of a Date and a time, without a time zone. Use DateTimeStamp for the combination of a Date and a time and a time zone." ;
.
fibo-fnd-dt-fd:DateTimeStamp
  rdf:type owl:Class ;
  rdfs:label "date time stamp" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange xsd:dateTimeStamp ;
      owl:onProperty fibo-fnd-dt-fd:hasDateTimeStampValue ;
    ] ;
  skos:definition "a combination of a date, a time, and a time zone" ;
  fibo-fnd-utl-av:synonym "time stamp" ;
  fibo-fnd-utl-av:usageNote "'hasDateTimeStampValue' is omitted if the DateTimeStamp is not (yet) established." ;
  fibo-fnd-utl-av:usageNote "Use DateTimeStamp when the time stamp includes a time zone field. Use DateTime for the combination of a Date and a time, without a time zone." ;
.
fibo-fnd-dt-fd:Duration
  rdf:type owl:Class ;
  rdfs:label "duration" ;
  rdfs:subClassOf fibo-fnd-dt-fd:TimeInterval ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange xsd:string ;
      owl:onProperty fibo-fnd-dt-fd:hasDurationValue ;
    ] ;
  skos:definition "an interval of time of some specific length" ;
  fibo-fnd-utl-av:usageNote "The 'hasDurationValue' property is absent if the duration is not (yet) known." ;
.
fibo-fnd-dt-fd:ExplicitDate
  rdf:type owl:Class ;
  rdfs:label "explicit date" ;
  rdfs:subClassOf fibo-fnd-dt-fd:Date ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onDataRange xsd:string ;
      owl:onProperty fibo-fnd-dt-fd:hasDateValue ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "a date in which the 'hasDateValue' property is required" ;
.
fibo-fnd-dt-fd:ExplicitDatePeriod
  rdf:type owl:Class ;
  rdfs:label "explicit date period" ;
  rdfs:subClassOf fibo-fnd-dt-fd:DatePeriod ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-dt-fd:ExplicitDate ;
      owl:onProperty fibo-fnd-dt-fd:hasEndDate ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-dt-fd:ExplicitDate ;
      owl:onProperty fibo-fnd-dt-fd:hasStartDate ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-dt-fd:ExplicitDuration ;
      owl:onProperty fibo-fnd-dt-fd:hasDuration ;
    ] ;
  skos:definition "a date period for which the start date, end date, and duration are all explicit" ;
  fibo-fnd-utl-av:usageNote "As with DatePeriod, any one of {start date, end date, duration} may be omitted because the missing property can be inferred from the other two" ;
.
fibo-fnd-dt-fd:ExplicitDuration
  rdf:type owl:Class ;
  rdfs:label "explicit duration" ;
  rdfs:subClassOf fibo-fnd-dt-fd:Duration ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onDataRange xsd:string ;
      owl:onProperty fibo-fnd-dt-fd:hasDurationValue ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "a duration for which the 'hasDurationValue' property must be set" ;
  fibo-fnd-utl-av:explanatoryNote "This class is used when a duration is guaranteed to be known when it is created." ;
.
fibo-fnd-dt-fd:ExplicitRecurrenceInterval
  rdf:type owl:Class ;
  rdfs:label "explicit recurrence interval" ;
  rdfs:subClassOf fibo-fnd-dt-fd:RecurrenceInterval ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onDataRange xsd:string ;
      owl:onProperty fibo-fnd-dt-fd:hasDurationValue ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "a recurrence interval defined via an explicit duration" ;
.
fibo-fnd-dt-fd:Friday
  rdf:type owl:NamedIndividual ;
  rdf:type fibo-fnd-dt-fd:TimeInterval ;
  rdfs:label "Friday" ;
  skos:definition "time interval that has duration 1 day and that meets a Saturday" ;
.
fibo-fnd-dt-fd:FromEnd
  rdf:type owl:NamedIndividual ;
  rdf:type fibo-fnd-dt-fd:TimeDirection ;
  rdfs:label "from the end" ;
  skos:definition "The CalendarSpecifiedDate is the nth date from the end of a calendar period." ;
  skos:example "Two days before the end of a calendar month." ;
.
fibo-fnd-dt-fd:FromStart
  rdf:type owl:NamedIndividual ;
  rdf:type fibo-fnd-dt-fd:TimeDirection ;
  rdfs:label "from the start" ;
  skos:definition "The CalendarSpecifiedDate is the nth day from the beginning of a calendar period." ;
  skos:example "The first day of a calendar quarter. (The offsetDay is zero.)" ;
.
fibo-fnd-dt-fd:Monday
  rdf:type owl:NamedIndividual ;
  rdf:type fibo-fnd-dt-fd:TimeInterval ;
  rdfs:label "Monday" ;
  skos:definition "time interval that has duration 1 day and that meets a Tuesday" ;
.
fibo-fnd-dt-fd:RecurrenceInterval
  rdf:type owl:Class ;
  rdfs:label "recurrence interval" ;
  rdfs:subClassOf fibo-fnd-dt-fd:TimeInterval ;
  skos:definition "a time interval that is consistent between elements of a regular schedule" ;
  fibo-fnd-utl-av:synonym "frequency" ;
.
fibo-fnd-dt-fd:RegularSchedule
  rdf:type owl:Class ;
  rdfs:label "regular schedule" ;
  rdfs:subClassOf fibo-fnd-dt-fd:Schedule ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-dt-bd:BusinessDayAdjustment ;
      owl:onProperty fibo-fnd-dt-bd:hasBusinessDayAdjustment ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-dt-fd:ScheduleStub ;
      owl:onProperty fibo-fnd-dt-fd:hasFinalStub ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-dt-fd:ScheduleStub ;
      owl:onProperty fibo-fnd-dt-fd:hasInitialStub ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange xsd:positiveInteger ;
      owl:onProperty fibo-fnd-dt-fd:hasCount ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-dt-fd:Date ;
      owl:onProperty fibo-fnd-dt-fd:hasRecurrenceStartDate ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-dt-fd:RecurrenceInterval ;
      owl:onProperty fibo-fnd-dt-fd:hasRecurrenceInterval ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-dt-oc:OccurrenceKind ;
      owl:onProperty fibo-fnd-rel-rel:comprises ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  owl:equivalentClass fibo-sec-sec-schx:RegularSchedule ;
  skos:definition "a schedule whose time intervals recur regularly" ;
  skos:editorialNote "The BusinessDates ontology extends 'RegularSchedule' with an optional BusinessDayAdjustment that specifies what should happen if a scheduled date falls on a weekend or a holiday." ;
  skos:example """A 30 year mortgage is payable monthly on the 10th of the month, starting July 2015. The mortgage is issued on June 15, 2015 so the first payment is for the period June 15-June 30, and the last payment is for June 1-14 2045.

The payment schedule is a RegularSchedule with these properties:

* comprises: regular payment OccurrenceKind (with payment details)  (see the 'comprises' property of the Occurrences ontology)
* hasInitialStub: June 15-30, 2015 for initial payment
* hasFinalStub: June 1-14, 2045 for final payment
* hasCount: 358
* hasOverallPeriod starting Date: June 15, 2015 with a duration of 30 years
* hasRecurrenceInterval: specifies 10th day of each calendar month
* hasRecurrenceStartDate: July 1, 2015""" ;
  skos:example """A corporate bond pays interest for 10 years starting on the first day of 2015.  Interest payments are due 15 days after the expiration of each 6 month period: on July 15 and January 16.

The payment schedule is a RegularSchedule, wiith these properties:

* comprises: identifies the interest payment details
* overall DatePeriod starting date is '2015-01-01', ending date is '2025-01-15', and duration is 'P10Y15D'
* hasCount is 20 (2 payments per year for 10 years)
* hasRecurrenceInterval is 'P6M'
* hasRecurrenceStartDate is '2015-01-15'""" ;
  fibo-fnd-utl-av:explanatoryNote """A RegularSchedule is a Schedule defined as a set of Dates that start on a recurrence start date and repeat after each recurrence interval.  The size of this set is defined by a count.

The 'initial ScheduleStub' associated with a RegularSchedule identifies any special treatment applied before the recurrence start date.  Simiilarly, a 'final ScheduleStub' identifies any special handling at the end of the recurrences.  For example, a mortgage loan that is due each calendar month may have an initial payment due before the first calendar month, or a final payment due after the last monthly payment.""" ;
  fibo-fnd-utl-av:usageNote """Other ontologies can extend RegularSchedule as needed.

In particular, the Occurrences ontology extends RegularSchedule to 'comprise' an 'OccurrenceKind'.  The intended meaning is that a regular schedule comprises a number of scheduled dates and an event which is scheduled to occur on each of those dates – in other words an Occurrence of the OccurrenceKind should happen on each Date defined by the RegularSchedule.""" ;
  fibo-fnd-utl-av:usageNote """The recurrence start date can be an ExplicitDate or any kind of CalculatedDate.  Hence, the starting date could be relative to another Date (e.g. T+3) or triggered by the Occurrence of an OccurrenceKind, etc.

The recurrence start date can also be relative to the starting Date of the overall DatePeriod of the Schedule.""" ;
.
fibo-fnd-dt-fd:RelativeDate
  rdf:type owl:Class ;
  rdfs:label "relative date" ;
  rdfs:subClassOf fibo-fnd-dt-fd:CalculatedDate ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-dt-fd:Date ;
      owl:onProperty fibo-fnd-dt-fd:isRelativeTo ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onDataRange xsd:string ;
      owl:onProperty fibo-fnd-dt-fd:hasRelativeDuration ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  owl:disjointWith fibo-fnd-dt-fd:SpecifiedDate ;
  owl:disjointWith fibo-fnd-dt-oc:OccurrenceBasedDate ;
  skos:definition "a calculated date that is some duration before or after another date" ;
  skos:example "A settlement date, defined as T+3: three days after the trade date.  The 'hasRelativeDayNumber' property is set to '3'." ;
  fibo-fnd-utl-av:explanatoryNote "When the 'hasRelativeDuration' property is negative, the RelativeDate is before the 'isRelativeTo' Date; otherwise the RelativeDate is after the 'isRelativeTo' Date." ;
.
fibo-fnd-dt-fd:Saturday
  rdf:type owl:NamedIndividual ;
  rdf:type fibo-fnd-dt-fd:TimeInterval ;
  rdfs:label "Saturday" ;
  skos:definition "time interval that has duration 1 day and that meets a Sunday" ;
  fibo-fnd-utl-av:explanatoryNote "One Saturday is the time interval that has duration 1 day and that starts Gregorian year 2000. This requirement anchors the repeating sequence of days of week to specific Gregorian days. It requires that January 1, 2000 is a Saturday. It follows that January 2, 2000 must be the Sunday that it meets, and so on." ;
.
fibo-fnd-dt-fd:Schedule
  rdf:type owl:Class ;
  rdfs:label "schedule" ;
  rdfs:subClassOf fibo-fnd-arr-arr:Collection ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-dt-fd:DatePeriod ;
      owl:onProperty fibo-fnd-dt-fd:hasOverallPeriod ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "a collection of events or things that will be done and the dates and/or times when they will be done" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.merriam-webster.com/dictionary/schedule"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "The overall period covers the entire DatePeriod of the Schedule, from the earliest Date to the final Date of the Schedule. Schedules may be ad hoc, essentially a list of dates and events without any consistency in the durations between events, regular, in which case there is a consistently recurring interval between events, or a combination of the two." ;
.
fibo-fnd-dt-fd:ScheduleStub
  rdf:type owl:Class ;
  rdfs:label "schedule stub" ;
  rdfs:subClassOf fibo-fnd-arr-arr:Collection ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-dt-fd:DatePeriod ;
      owl:onProperty fibo-fnd-dt-fd:hasDatePeriod ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-dt-oc:OccurrenceKind ;
      owl:onProperty fibo-fnd-rel-rel:comprises ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "a date period before the start of the recurring part of a schedule or after the end of the recurring part, which may be associated with a specific occurrence kind" ;
  skos:editorialNote "The Occurrences ontology extends ScheduleStub to 'comprise' an OccurrenceKind. The meaning is that a schedule stub comprises a date period and an event which is scheduled to occur during that date period; in other words that an Occurrence of the OccurrenceKind should happen during the DatePeriod of the ScheduleStub." ;
  skos:example "A 30 year mortgage calls for monthly payments on the first day of each month, according to a RegularSchedule.  If the mortgage does not start on the first day of a calendar month, then an initial ScheduleStub specifies the payment due for the DatePeriod up to the first day of the next calendar month.  Similarly, a final ScheduleStub specifies the last payment due for the DatePeriod after the end of the last full calendar month." ;
.
fibo-fnd-dt-fd:SpecifiedDate
  rdf:type owl:Class ;
  rdfs:label "specified date" ;
  rdfs:subClassOf fibo-fnd-dt-fd:CalculatedDate ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onDataRange xsd:string ;
      owl:onProperty fibo-fnd-dt-fd:hasDateSpecification ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  owl:disjointWith fibo-fnd-dt-oc:OccurrenceBasedDate ;
  skos:definition "a calculated date that is defined by a rule, which is captured as a string by the 'hasDateSpecification' property" ;
.
fibo-fnd-dt-fd:Sunday
  rdf:type owl:NamedIndividual ;
  rdf:type fibo-fnd-dt-fd:TimeInterval ;
  rdfs:label "Sunday" ;
  skos:definition "time interval that has duration 1 day and that meets a Monday" ;
.
fibo-fnd-dt-fd:Thursday
  rdf:type owl:NamedIndividual ;
  rdf:type fibo-fnd-dt-fd:TimeInterval ;
  rdfs:label "Thursday" ;
  skos:definition "time interval that has duration 1 day and that meets a Friday" ;
.
fibo-fnd-dt-fd:TimeDirection
  rdf:type owl:Class ;
  rdfs:label "time direction" ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "an enumeration that indicates whether a calendar specified date is figured from the start or the end of a calendar period" ;
.
fibo-fnd-dt-fd:TimeInterval
  rdf:type owl:Class ;
  rdfs:label "time interval" ;
  rdfs:subClassOf fibo-fnd-plc-loc:Location ;
  skos:definition "a segment of the time axis, a location in time" ;
  skos:example "the day whose Gregorian calendar date is September 11, 2001" ;
  skos:example "the lifetime of Henry V" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.omg.org/spec/DTV/"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "Every time interval has a beginning, an end, and a duration, even if not known. Every time interval is 'finite', a bounded segment of the time axis. The beginning or end of a time interval may be defined by reference to events that occur for a time interval that is not known." ;
  fibo-fnd-utl-av:explanatoryNote "Time intervals may be indefinite, meaning that their beginning is primordiality or their end is perpetuity, or both (eternity). This vocabulary assumes that indefinite time intervals exist and have some duration, but their duration is unknown." ;
.
fibo-fnd-dt-fd:Tuesday
  rdf:type owl:NamedIndividual ;
  rdf:type fibo-fnd-dt-fd:TimeInterval ;
  rdfs:label "Tuesday" ;
  skos:definition "time interval that has duration 1 day and that meets a Wednesday" ;
.
fibo-fnd-dt-fd:Wednesday
  rdf:type owl:NamedIndividual ;
  rdf:type fibo-fnd-dt-fd:TimeInterval ;
  rdfs:label "Wednesday" ;
  skos:definition "time interval that has duration 1 day and that meets a Thursday" ;
.
fibo-fnd-dt-fd:dateValue
  rdf:type rdfs:Datatype ;
  rdfs:label "date value" ;
  rdfs:subClassOf rdfs:Resource ;
  owl:deprecated "true"^^xsd:boolean ;
  owl:equivalentClass xsd:string ;
  skos:definition "a literal (explicit) date captured in the format specified for xsd:date (i.e., ISO 8601 format), WITHOUT a timezone field; the semantics are identical to those of xsd:date" ;
  skos:editorialNote "This datatype substitutes for xsd:date, which OWL 2 does not support." ;
  skos:example "2002-10-10 means October 10, 2002" ;
  fibo-fnd-utl-av:usageNote "For consistency with FPML (reference FPML Coding Schemes 30 June 2014, Version 1.56, section 2.1.1), the year MUST be specified as 4 digits, and the month and day MUST be specified as 2 digits with a leading zero if needed. Timezones should NOT be specified." ;
.
fibo-fnd-dt-fd:durationValue
  rdf:type rdfs:Datatype ;
  rdfs:label "duration value" ;
  rdfs:subClassOf rdfs:Resource ;
  owl:deprecated "true"^^xsd:boolean ;
  owl:equivalentClass xsd:string ;
  skos:definition "a literal (explicit) duration (amount of time) captured in the format specified for xsd:duration (i.e., ISO 8601 format); the semantics are identical to those of xsd:duration" ;
  skos:editorialNote "This datatype substitutes for xsd:duration, which OWL2 does not support." ;
  skos:example "-P3D means negative 3 days duration. This is used with OffsetDates to specify 3 days before (prior) to some other Date." ;
  skos:example "P1Y means 1 year" ;
  skos:example "P1Y2M3DT4H5M6S means 1 year, 2 months, 3 days, 4 hours, 5 minutes, 6 seconds" ;
  skos:example "P2M means 2 months" ;
  skos:example "P3D means 3 days" ;
  skos:example "PT4H means 4 hours" ;
  skos:example "PT5M means 5 minutes" ;
  skos:example "PT6S means 6 seconds" ;
  fibo-fnd-utl-av:usageNote "Negative durations are used to indicate RelativeDates that are before (rather than after) some other Date." ;
.
fibo-fnd-dt-fd:hasCalendarPeriod
  rdf:type owl:ObjectProperty ;
  rdfs:label "has calendar period" ;
  rdfs:range fibo-fnd-dt-fd:CalendarPeriod ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "identifies a calendar period used in computing a CalendarSpecifiedDate, such as a calendar week, calendar month, calendar quarter, or calendar year" ;
.
fibo-fnd-dt-fd:hasCount
  rdf:type owl:DatatypeProperty ;
  rdfs:label "has count" ;
  rdfs:range xsd:positiveInteger ;
  rdfs:subPropertyOf fibo-fnd-qt-qtu:hasNumericValue ;
  skos:definition "indicates the number of entries in a RegularSchedule" ;
.
fibo-fnd-dt-fd:hasDate
  rdf:type owl:ObjectProperty ;
  rdfs:label "has date" ;
  rdfs:range fibo-fnd-dt-fd:Date ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "identifies a specific date" ;
.
fibo-fnd-dt-fd:hasDatePeriod
  rdf:type owl:ObjectProperty ;
  rdfs:label "has date period" ;
  rdfs:range fibo-fnd-dt-fd:DatePeriod ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "identifies a specific period of time, including a start date, end date and or duration" ;
.
fibo-fnd-dt-fd:hasDateSpecification
  rdf:type owl:DatatypeProperty ;
  rdfs:domain fibo-fnd-dt-fd:SpecifiedDate ;
  rdfs:label "has date specification" ;
  rdfs:range xsd:string ;
  skos:definition "A rule that specifies how a SpecifiedDate is computed." ;
  skos:editorialNote "The rule is modeled as a simple String because OWL2 provides no way to model the semantics of such a rule." ;
.
fibo-fnd-dt-fd:hasDateTime
  rdf:type owl:ObjectProperty ;
  rdfs:label "has date time" ;
  rdfs:range fibo-fnd-dt-fd:DateTime ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "identifies a specific date and time of day, excluding the time zone" ;
.
fibo-fnd-dt-fd:hasDateTimeStamp
  rdf:type owl:ObjectProperty ;
  rdfs:label "has date time stamp" ;
  rdfs:range fibo-fnd-dt-fd:DateTimeStamp ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "identifies a specific date and time of day, including the time zone" ;
.
fibo-fnd-dt-fd:hasDateTimeStampValue
  rdf:type owl:DatatypeProperty ;
  rdfs:label "has date time stamp value" ;
  rdfs:range xsd:dateTimeStamp ;
  skos:definition "specifies an actual literal (explicit) date and time, including the time zone" ;
.
fibo-fnd-dt-fd:hasDateTimeValue
  rdf:type owl:DatatypeProperty ;
  rdfs:label "has date time value" ;
  rdfs:range xsd:dateTime ;
  skos:definition "specifies an actual literal (explicit) date and time" ;
.
fibo-fnd-dt-fd:hasDateValue
  rdf:type owl:DatatypeProperty ;
  rdfs:label "has date value" ;
  rdfs:range xsd:string ;
  skos:definition "specifies an actual literal (explicit) date captured in the format specified for xsd:date (i.e., ISO 8601 format), WITHOUT the time or timezone information; the semantics are identical to those of xsd:date" ;
  skos:example "2002-10-10 means October 10, 2002" ;
  fibo-fnd-utl-av:usageNote "For consistency with FPML (reference FPML Coding Schemes 30 June 2014, Version 1.56, section 2.1.1), the year MUST be specified as 4 digits, and the month and day MUST be specified as 2 digits with a leading zero if needed. Times and timezones should NOT be specified." ;
.
fibo-fnd-dt-fd:hasDuration
  rdf:type owl:FunctionalProperty ;
  rdf:type owl:ObjectProperty ;
  rdfs:label "has duration" ;
  rdfs:range fibo-fnd-dt-fd:Duration ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "the duration of a date period or similar concept, such as the tenor of a contract" ;
  fibo-fnd-utl-av:usageNote "This duration may be omitted or unknown (hasSettledDuration missing) if either the start or end Date of the DatePeriod is a CalculatedDate or a DefinedDate." ;
.
fibo-fnd-dt-fd:hasDurationValue
  rdf:type owl:DatatypeProperty ;
  rdfs:domain fibo-fnd-dt-fd:Duration ;
  rdfs:label "has duration value" ;
  rdfs:range xsd:string ;
  skos:definition "specifies a literal (explicit) duration (amount of time) captured in the format specified for xsd:duration (i.e., ISO 8601 format); the semantics are identical to those of xsd:duration" ;
  skos:example "-P3D means negative 3 days duration. This is used with OffsetDates to specify 3 days before (prior) to some other Date." ;
  skos:example "P1Y means 1 year" ;
  skos:example "P1Y2M3DT4H5M6S means 1 year, 2 months, 3 days, 4 hours, 5 minutes, 6 seconds" ;
  skos:example "P2M means 2 months" ;
  skos:example "P3D means 3 days" ;
  skos:example "PT4H means 4 hours" ;
  skos:example "PT5M means 5 minutes" ;
  skos:example "PT6S means 6 seconds" ;
  fibo-fnd-utl-av:usageNote "Negative durations are used to indicate relative dates that are before (rather than after) some other Date." ;
.
fibo-fnd-dt-fd:hasEndDate
  rdf:type owl:FunctionalProperty ;
  rdf:type owl:ObjectProperty ;
  rdfs:label "has end date" ;
  rdfs:subPropertyOf fibo-fnd-dt-fd:hasDate ;
  skos:definition "the ending date of some Schedule or DatePeriod" ;
.
fibo-fnd-dt-fd:hasExplicitDate
  rdf:type owl:FunctionalProperty ;
  rdf:type owl:ObjectProperty ;
  rdfs:label "has explicit date" ;
  rdfs:range fibo-fnd-dt-fd:ExplicitDate ;
  rdfs:subPropertyOf fibo-fnd-dt-fd:hasDate ;
.
fibo-fnd-dt-fd:hasFinalStub
  rdf:type owl:ObjectProperty ;
  rdfs:label "has final stub" ;
  rdfs:subPropertyOf fibo-fnd-dt-fd:hasStub ;
  skos:definition "identifies any special period at the end of a regular schedule" ;
.
fibo-fnd-dt-fd:hasInitialStub
  rdf:type owl:ObjectProperty ;
  rdfs:label "has initial stub" ;
  rdfs:subPropertyOf fibo-fnd-dt-fd:hasStub ;
  skos:definition "identifies any special period at the start of a regular schedule" ;
.
fibo-fnd-dt-fd:hasOrdinalNumber
  rdf:type owl:DatatypeProperty ;
  rdfs:label "has ordinal number" ;
  rdfs:range xsd:integer ;
  skos:definition "An ordinal number meaning 1st, 2nd, 3rd, etc." ;
  fibo-fnd-utl-av:explanatoryNote "Negative ordinal numbers mean 1st before, 2nd before, etc." ;
.
fibo-fnd-dt-fd:hasOverallPeriod
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-dt-fd:Schedule ;
  rdfs:label "has overall period" ;
  rdfs:subPropertyOf fibo-fnd-dt-fd:hasDatePeriod ;
  skos:definition "identifies a DatePeriod that includes all the Dates of a Schedule, including any ScheduleStubs" ;
.
fibo-fnd-dt-fd:hasRecurrenceInterval
  rdf:type owl:ObjectProperty ;
  rdfs:label "has recurrence interval" ;
  rdfs:range fibo-fnd-dt-fd:RecurrenceInterval ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
.
fibo-fnd-dt-fd:hasRecurrenceStartDate
  rdf:type owl:ObjectProperty ;
  rdfs:label "has recurrence start date" ;
  rdfs:subPropertyOf fibo-fnd-dt-fd:hasStartDate ;
  skos:definition "the starting Date of the first recurrence of a RegularSchedule" ;
.
fibo-fnd-dt-fd:hasRelativeDuration
  rdf:type owl:DatatypeProperty ;
  rdfs:label "has relative duration" ;
  rdfs:range xsd:string ;
  rdfs:subPropertyOf fibo-fnd-dt-fd:hasDurationValue ;
  skos:definition "a duration between two explicit dates, specified in the form described for the hasDurationValue property" ;
  fibo-fnd-utl-av:explanatoryNote "A relative duration may be negative." ;
.
fibo-fnd-dt-fd:hasSchedule
  rdf:type owl:ObjectProperty ;
  rdfs:label "has schedule" ;
  rdfs:range fibo-fnd-dt-fd:Schedule ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "a schedule for something" ;
.
fibo-fnd-dt-fd:hasStartDate
  rdf:type owl:FunctionalProperty ;
  rdf:type owl:ObjectProperty ;
  rdfs:label "has start date" ;
  rdfs:subPropertyOf fibo-fnd-dt-fd:hasDate ;
  skos:definition "the starting Date of something" ;
.
fibo-fnd-dt-fd:hasStub
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-dt-fd:RegularSchedule ;
  rdfs:label "has stub" ;
  rdfs:range fibo-fnd-dt-fd:ScheduleStub ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
.
fibo-fnd-dt-fd:hasTimeDirection
  rdf:type owl:ObjectProperty ;
  rdfs:label "has time direction" ;
  rdfs:range fibo-fnd-dt-fd:TimeDirection ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "A TimeDirection indicates whether a CalendarSpecifiedDate is figured from the beginning or end of a calendar period." ;
.
fibo-fnd-dt-fd:isRelativeTo
  rdf:type owl:ObjectProperty ;
  rdfs:label "is relative to" ;
  rdfs:range fibo-fnd-dt-fd:Date ;
  skos:definition "identifies a RelativeDate or RelativeDatePeriod defined relative to this Date" ;
.
