fibo-der-rat-isw:
  rdfs:label "InflationSwaps"@en ;
.
fibo-der-rat-isw:InflationIndexFallBackBond
  rdf:type owl:Class ;
  rdfs:label "inflation index fall back bond"@en ;
  rdfs:subClassOf fibo-fnd-math-math:ParametricReferenceEntity ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-rat-isw:identity ;
      owl:someValuesFrom fibo-sec-dbt-bo-cfv:InflationBond ;
    ] ;
  skos:definition "A fallback bond as defined in the 2006 ISDA Inflation Derivatives Definitions, sections 1.3 and 1.8."@en ;
  skos:editorialNote "From FpML definition of \"Fallback Bond applicable\" related term: 'The applicability of a fallback bond as defined in the 2006 ISDA Inflation Derivatives Definitions, sections 1.3 and 1.8. Omission of this element implies a value of true.'"@en ;
.
fibo-der-rat-isw:InflationIndexUnderlying
  rdf:type owl:Class ;
  rdfs:label "inflation index underlying"@en ;
  rdfs:subClassOf fibo-der-rtd-rtd:EconomicRateObservable ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-der-rat-isw:InflationIndexFallBackBond ;
      owl:onProperty fibo-der-rat-isw:specifies ;
    ] ;
.
fibo-der-rat-isw:InflationLeg
  rdf:type owl:Class ;
  rdfs:label "inflation leg"@en ;
  rdfs:subClassOf fibo-der-drc-swp:SwapLeg ;
.
fibo-der-rat-isw:InflationSwapContract
  rdf:type owl:Class ;
  rdfs:label "inflation swap contract"@en ;
  rdfs:subClassOf fibo-der-drc-swp:Swap ;
  rdfs:subClassOf fibo-der-rtd-rtd:IndexDerivativeContract ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-rat-isw:underlying ;
      owl:someValuesFrom fibo-der-rat-isw:InflationIndexUnderlying ;
    ] ;
  skos:definition "Draft definition: This is a swap instrument which makes reference to an inflation index."@en ;
  fibo-fnd-utl-av:explanatoryNote "It is also possible to make reference to an individual bond interest rate under some circumstances or instances. Not a specific schema in FpML. FpML details on fall-back bond (in definition for a yes/no field defining the presence of such a thing): 'The applicability of a fallback bond as defined in the 2006 ISDA Inflation Derivatives Definitions, sections 1.3 and 1.8. Omission of this element imples a value of true.'"@en ;
.
fibo-der-rat-isw:InflationSwapTransaction
  rdf:type owl:Class ;
  rdfs:label "inflation swap transaction"@en ;
  rdfs:subClassOf fibo-der-drc-swp:SwapTransaction ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-drc-swp:hasTransactionLeg ;
      owl:someValuesFrom fibo-der-rtd-irswp:FixedInterestRateLeg ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-drc-swp:hasTransactionLeg ;
      owl:someValuesFrom fibo-der-rat-isw:InflationLeg ;
    ] ;
  skos:definition "A transaction in which one leg is based on an inflation rate and the other leg is based on an interest rate (usually or always fixed interest)."@en ;
.
fibo-der-rat-isw:fallbackBondApplicable
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-der-rat-isw:InflationSwapContract ;
  rdfs:label "fallback bond applicable"@en ;
  rdfs:range fibo-fnd-utl-val:TrueFalseValue ;
  skos:definition "The applicability of a fallback bond as defined in the 2006 ISDA Inflation Derivatives Definitions, sections 1.3 and 1.8. Yes implies a value of true."@en ;
.
fibo-der-rat-isw:identity
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-der-rat-isw:InflationIndexFallBackBond ;
  rdfs:label "identity"@en ;
  rdfs:range fibo-sec-dbt-bo-cfv:InflationBond ;
.
fibo-der-rat-isw:lag
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-der-rat-isw:InflationIndexUnderlying ;
  rdfs:label "lag"@en ;
  rdfs:range fibo-fnd-tim-tim:DurationDescriptionInCalendarTemporalUnit ;
  skos:definition "Details needed. Inserted per telecon (Derivatives PoC) from Andrew Jacobs. Identify the corresponding term in FpML and use that here."@en ;
.
fibo-der-rat-isw:specifies
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-der-rat-isw:InflationIndexUnderlying ;
  rdfs:label "specifies"@en ;
  rdfs:range fibo-der-rat-isw:InflationIndexFallBackBond ;
.
fibo-der-rat-isw:underlying
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-der-rat-isw:InflationSwapContract ;
  rdfs:label "underlying"@en ;
  rdfs:range fibo-der-rat-isw:InflationIndexUnderlying ;
.
