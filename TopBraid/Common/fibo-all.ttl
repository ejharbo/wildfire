  rdfs:label "mortgage indemnity insurance policy  (please review)"@en ;
  rdfs:subClassOf fibo-fnd-inf-doc:InsurancePolicy ;
  skos:definition "The physical policy for the Mortgage Indemnity Guaranty."@en ;
