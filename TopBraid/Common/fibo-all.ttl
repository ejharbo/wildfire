fibo-der-cr-cds:CreditDefaultSwapContract
  rdf:type owl:Class ;
  rdfs:label "credit default swap contract"@en ;
  rdfs:subClassOf fibo-der-cr-cds:CreditDerivativeContract ;
  rdfs:subClassOf fibo-der-drc-swp:Swap ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-der-cr-cds:ProtectionBuyer ;
      owl:onProperty fibo-der-cr-cds:hasBuyer ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-der-cr-cds:ProtectionSeller ;
      owl:onProperty fibo-der-cr-cds:hasSeller ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-cr-cds:establishes ;
      owl:someValuesFrom fibo-der-cr-cds:ContingentTransactionCommitment ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-cr-cds:hasBuyer ;
      owl:someValuesFrom fibo-der-cr-cds:ProtectionBuyer ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-cr-cds:hasReferenceObligation ;
      owl:someValuesFrom fibo-der-cr-cds:ReferenceObligation ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-cr-cds:hasSeller ;
      owl:someValuesFrom fibo-der-cr-cds:ProtectionSeller ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-agr-ctr:hasContractualElement ;
      owl:someValuesFrom fibo-der-cr-cds:CDSFeePaymentCommitment ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-agr-ctr:hasContractualElement ;
      owl:someValuesFrom fibo-der-cr-cds:CreditProtectionCommitment ;
    ] ;
  skos:definition "In a credit default swap one party (the protection seller) agrees to compensate another party (the protection buyer) if a specified company or Sovereign (the reference entity) experiences a credit event, indicating it is or may be unable to service its debts. The protection seller is typically paid a fee and/or premium, expressed as an annualized percent of the notional in basis points, regularly over the life of the transaction or otherwise as agreed by the parties."@en ;
  skos:editorialNote "Earlier draft definition: A swap instrument in which one party underwrites the risk of credit default of the other party or of an identified third party."@en ;
.
fibo-der-cr-cds:CreditDefaultSwapTransaction
  rdf:type owl:Class ;
  rdfs:label "credit default swap transaction"@en ;
