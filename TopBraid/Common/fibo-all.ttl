  sm:copyright """Copyright (c) 2013-2014 EDM Council, Inc.
Copyright (c) 2013-2014 Object Management Group, Inc.""" ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/BusinessDates/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Law/LegalCapacity/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Parties/Parties/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Parties/Roles/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"^^xsd:anyURI ;
  sm:fileAbbreviation "fibo-fnd-oac-ctl" ;
  sm:fileAbstract "This ontology defines high-level, control-related concepts for use in other FIBO ontology elements.  The ontology covers basic concepts around control, along with a distinction between de jure and de facto control, the former being derived with reference to terms in the LegalCapacity ontology." ;
  sm:filename "Control.rdf" ;
  rdfs:label "Control Ontology" ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"^^xsd:anyURI ;
