fibo-sec-sec-id:NationalSecurityIdentificationScheme
  rdf:type owl:Class ;
  rdfs:label "national security identification scheme" ;
  rdfs:subClassOf fibo-fbc-fct-ra:RegistrationScheme ;
  rdfs:subClassOf fibo-sec-sec-id:SecurityIdentificationScheme ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-sec-sec-id:NationalSecuritiesIdentifyingNumber ;
      owl:onProperty fibo-fnd-rel-rel:defines ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-sec-sec-id:NationalSecuritiesIdentifyingNumberRegistry ;
      owl:onProperty fibo-fnd-rel-rel:characterizes ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:appliesTo ;
      owl:someValuesFrom fibo-fnd-law-jur:Jurisdiction ;
    ] ;
