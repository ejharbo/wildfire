fibo-der-der-opt:OptionContract
  rdf:type owl:Class ;
  rdfs:label "option contract"@en ;
  rdfs:subClassOf fibo-fbc-fi-fi:DerivativeInstrument ;
  rdfs:subClassOf fibo-fnd-txn-sec:SecuritiesTransactionContract ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-der-der-opt:OptionBarrierFeatureTerms ;
      owl:onProperty fibo-der-der-opt:hasFeature ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-drc-bsc:hasUnderlier ;
      owl:someValuesFrom fibo-der-der-opt:UnderlyingOnOption ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-der-opt:definesOptional ;
      owl:someValuesFrom fibo-der-der-opt:OptionalUnderlyingTransaction ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-der-opt:hasBuyer ;
      owl:someValuesFrom fibo-der-der-opt:OptionBuyer ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-der-opt:hasPremium ;
      owl:someValuesFrom fibo-der-der-opt:OptionPremium ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-der-opt:hasSeller ;
      owl:someValuesFrom fibo-der-der-opt:OptionWritingParty ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-der-opt:hasStrikeTerms ;
      owl:someValuesFrom fibo-der-der-opt:OptionStrikeTermsSet ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-der-opt:hasValuationTerms ;
      owl:someValuesFrom fibo-der-der-opt:OptionValuationTermsSet ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-agrx-con:confers ;
      owl:someValuesFrom fibo-der-der-opt:SecuritiesTransactionOption ;
    ] ;
  skos:definition "A financial derivative which represents a contract sold by one party (option writer) to another party (option holder). The contract offers the buyer the right, but not the obligation, to buy (call) or sell (put) a security or other financial asset at an agreed-upon price (the strike price) during a certain period of time or on a specific date (exercise date)."@en ;
  fibo-fnd-utl-av:explanatoryNote "Options may be created Over the Counter, or through an Exchange (in which case the Option Holder may transfer the Option to another party)."@en ;
.
