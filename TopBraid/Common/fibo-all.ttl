  rdfs:label "municipal bond issue information"@en ;
  rdfs:subClassOf fibo-sec-dbt-dbt-iss:BondIssueInformation ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-utl-val:TextValue ;
      owl:onProperty fibo-sec-dbt-dbt-iss:debtIssuancePurpose ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-dbt-dbt-iss:debtIssuancePurpose ;
      owl:someValuesFrom fibo-fnd-utl-val:TextValue ;
    ] ;
  skos:definition "Information about the Issuance of a Municipal Bond, which is maintained throughout the life of the Security."@en ;
.
fibo-sec-dbt-dbt-iss:NewMoneyPurpose
  rdf:type owl:Class ;
  rdfs:label "new money purpose"@en ;
  rdfs:subClassOf fibo-sec-dbt-dbt-iss:DebtIssuancePurpose ;
  skos:definition "Securities Issued for new Project or Purpose."@en ;
.
fibo-sec-dbt-dbt-iss:RefundingPurpose
  rdf:type owl:Class ;
  rdfs:label "refunding purpose"@en ;
  rdfs:subClassOf fibo-sec-dbt-dbt-iss:DebtIssuancePurpose ;
  skos:definition "A procedure whereby an issuer refinances outstanding bonds by issuing new bonds."@en ;
.
fibo-sec-dbt-dbt-iss:RemarketingPurpose
  rdf:type owl:Class ;
  rdfs:label "remarketing purpose"@en ;
  rdfs:subClassOf fibo-sec-dbt-dbt-iss:DebtIssuancePurpose ;
  skos:definition "The process of reselling securities to the public that have been tendered for purchase by the previous owners thereof."@en ;
.
fibo-sec-dbt-dbt-iss:SecuritiesIssuance
  rdf:type owl:Class ;
  rdfs:label "securities issuance"@en ;
  rdfs:subClassOf fibo-be-fctx-spv:SPVPurpose ;
  skos:definition "The SPV is set up to issue securities."@en ;
.
fibo-sec-dbt-dbt-iss:SecuritiesIssuanceSPV
  rdf:type owl:Class ;
  rdfs:label "securities issuance s p v"@en ;
  rdfs:subClassOf fibo-be-fctx-spv:SpecialPurposeVehicle ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-be-fctx-spv:isSetUpFor ;
      owl:someValuesFrom fibo-sec-dbt-dbt-iss:SecuritiesIssuance ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-dbt-dbt-iss:holdsPool ;
      owl:someValuesFrom fibo-sec-sec-pls:DebtPool ;
    ] ;
  skos:definition "An SPV set up specifically to issue securities."@en ;
.
fibo-sec-dbt-dbt-iss:UnderwriterTakedownForDebt
  rdf:type owl:Class ;
  rdfs:label "underwriter takedown for debt"@en ;
  rdfs:subClassOf fibo-bp-iss-trm:UnderwriterTakedown ;
  skos:definition "Infomation on Takedown quantity of the Debt security handled by the underwriter (that will be brought into DTC)."@en ;
.
<https://spec.edmcouncil.org/fibo/SEC/Debt/DebtFoundations/DebtIssuanceTerms/bankQualified.1>
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-dbt-dbt-iss:MunicipalBondIssueInformation ;
  rdfs:label "bank qualified"@en ;
  rdfs:range fibo-fnd-utl-val:TrueFalseValue ;
  skos:definition "Designation given to a public purpose bond offering by the issuer if it reasonably expects to issue in the calendar year of such offering no more than $10 million par amount of bonds of the type required to be included in making such calculation under the Internal Revenue Code.  When purchased by a commercial bank for its portfolio, the bank may receive an 80% tax deduction for the interest cost of carry for the issue.  A bond that is bank qualified is also known as a qualified tax-exempt obligation."@en ;
.
fibo-sec-dbt-dbt-iss:becomesPartOfTerms
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-dbt-dbt-iss:DebtIssuanceProcessInformation ;
  rdfs:label "becomes part of terms"@en ;
  rdfs:range fibo-sec-dbt-dbt-iss:DebtInstrumentIssueInformation ;
  rdfs:subPropertyOf fibo-bp-iss-trm:becomesPartOf ;
  skos:definition "The information required throughout the life of the Security becomes the Issuance information maintained for that security."@en ;
.
fibo-sec-dbt-dbt-iss:debtIssuancePurpose
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-dbt-dbt-iss:DebtInstrumentIssueInformation ;
  rdfs:label "debt issuance purpose"@en ;
  rdfs:range fibo-fnd-utl-val:TextValue ;
  skos:definition "Purpose of the Issue of a Debt Security."@en ;
.
fibo-sec-dbt-dbt-iss:describedIn
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-dbt-dbt-iss:DebtOfferIssue ;
  rdfs:label "described in"@en ;
  rdfs:range fibo-sec-dbt-dbt-iss:DebtIssueProspectus ;
  rdfs:subPropertyOf fibo-bp-iss-trm:finalStateDescribedIn ;
.
fibo-sec-dbt-dbt-iss:hasDebtIssuancePurpose
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-dbt-dbt-iss:DebtIssuanceProcessInformation ;
  rdfs:label "has debt issuance purpose"@en ;
  rdfs:range fibo-sec-dbt-dbt-iss:DebtIssuancePurpose ;
.
fibo-sec-dbt-dbt-iss:holdsPool
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-dbt-dbt-iss:SecuritiesIssuanceSPV ;
  rdfs:label "holds pool"@en ;
  rdfs:range fibo-sec-sec-pls:DebtPool ;
  skos:definition "The Issuance Special Purpose Vehicle holds assets in the form of a pool of securities."@en ;
  fibo-fnd-utl-av:explanatoryNote "Securities are purchased by an SPV or other entity for the purpose of issuing notes against those securities. Term Implied by Pool v SPV structure as applied to Participation Notes"@en ;
.
fibo-sec-dbt-dbt-iss:isANumberOf
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-dbt-dbt-iss:DebtIssuanceProgram ;
  rdfs:label "is a number of"@en ;
  rdfs:range fibo-sec-dbt-dbt-iss:DebtOfferIssue ;
  rdfs:subPropertyOf fibo-bp-iss-trm:isCollectionOf ;
.
fibo-sec-dbt-dbt-iss:issueNominalAmount
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-dbt-dbt-iss:DebtInstrumentIssueInformation ;
  rdfs:label "issue nominal amount"@en ;
  rdfs:range fibo-fnd-acc-cur:MonetaryAmount ;
  skos:definition "Total original amount issued, expressed as a monetary amount."@en ;
.
fibo-sec-dbt-dbt-iss:issues
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-dbt-dbt-iss:MTNOfferProgram ;
  rdfs:label "issues"@en ;
  rdfs:range fibo-sec-dbt-dbt-iss:MTNIssue ;
.
fibo-sec-dbt-dbt-iss:oID
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-dbt-dbt-iss:BondIssueInformation ;
  rdfs:label "o i d"@en ;
  rdfs:range fibo-fnd-utl-val:PercentageValue ;
  skos:definition "Discount from par value at the time a bond or other debt instrument is issued."@en ;
.
fibo-sec-dbt-dbt-iss:toBeBankQualified
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-dbt-dbt-iss:MunicipalBondIssuanceProcessInformation ;
  rdfs:label "to be bank qualified"@en ;
  rdfs:range fibo-fnd-utl-val:TrueFalseValue ;
  skos:definition "Designation given to a public purpose bond offering by the issuer if it reasonably expects to issue in the calendar year of such offering no more than $10 million par amount of bonds of the type required to be included in making such calculation under the Internal Revenue Code.  When purchased by a commercial bank for its portfolio, the bank may receive an 80% tax deduction for the interest cost of carry for the issue.  A bond that is bank qualified is also known as a qualified tax-exempt obligation."@en ;
.
fibo-sec-dbt-dbt-iss:underwriterTakedownAmount
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-dbt-dbt-iss:UnderwriterTakedownForDebt ;
  rdfs:label "underwriter takedown amount"@en ;
  rdfs:range fibo-fnd-acc-cur:MonetaryAmount ;
  skos:definition "Takedown amount of the security handled by the underwriter(that will be brought into DTC)."@en ;
.
fibo-sec-dbt-dbt-gtyx:
  rdfs:label "GuarantyExt"@en ;
.
fibo-sec-dbt-dbt-gtyx:Contract
  rdf:type owl:Class ;
  rdfs:label "contract"@en ;
  rdfs:subClassOf owl:Thing ;
.
fibo-sec-dbt-dbt-gtyx:ContractGuarantor
  rdf:type owl:Class ;
  rdfs:label "contract guarantor"@en ;
  rdfs:subClassOf fibo-fbc-dae-gty:Guarantor ;
  rdfs:subClassOf fibo-fnd-agr-ctr:ContractThirdParty ;
  skos:definition "A Party which guarantees to underwrite some cash flow obligation on behalf of some other party."@en ;
.
fibo-sec-dbt-dbt-gtyx:Guaranty
  rdf:type owl:Class ;
  rdfs:label "guaranty"@en ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom owl:Thing ;
      owl:onProperty fibo-sec-dbt-dbt-gtyx:guarantyTakesFormOf ;
    ] ;
  skos:definition "Arrangement whereby cash flows on the debt security, e.g., interest payment, are guaranteed by a firm other than the issuer."@en ;
.
fibo-sec-dbt-dbt-gtyx:guarantorHasIdentity
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-dbt-dbt-gtyx:ContractGuarantor ;
  rdfs:label "guarantor has identity"@en ;
  rdfs:range fibo-be-le-fbo:FormallyConstitutedOrganization ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:hasIdentity ;
.
fibo-sec-dbt-dbt-gtyx:guarantyTakesFormOf
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-dbt-dbt-gtyx:Guaranty ;
  rdfs:label "guaranty takes form of"@en ;
  rdfs:range owl:Thing ;
  rdfs:subPropertyOf fibo-fnd-pty-rl:isPlayedBy ;
.
fibo-sec-dbt-dbt-gtyx:hasContractGuarantor
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-dbt-dbt-gtyx:Contract ;
  rdfs:label "has contract guarantor"@en ;
  rdfs:range fibo-sec-dbt-dbt-gtyx:ContractGuarantor ;
  rdfs:subPropertyOf fibo-fbc-dae-gty:hasGuarantor ;
.
fibo-sec-dbt-dbt-par:
  rdfs:label "ParityVariants"@en ;
.
fibo-sec-dbt-dbt-par:DebtRelativeValueSelection
  rdf:type owl:Class ;
  rdfs:label "DebtRelativeValueSelection"@en ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "A relative value of a debt instrument at some point in time. This is exhaustively either par value, a discount or a premium."@en ;
.
fibo-sec-dbt-dbt-par:DiscountValue
  rdf:type owl:Class ;
  terms:description "A discount to the face (or par) value i.e. a value which is less than the par value of the instrument."@en ;
  rdfs:label "discount value"@en ;
  rdfs:subClassOf fibo-sec-dbt-dbt-par:DebtRelativeValueSelection ;
.
fibo-sec-dbt-dbt-par:DiscountedInstrument
  rdf:type owl:Class ;
  rdfs:label "discounted instrument"@en ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-md-dbt-py:hasYield ;
      owl:someValuesFrom fibo-md-dbt-py:DiscountedInstrumentYield ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-dbt-dbt-dbt:hasValueAtIssue ;
      owl:someValuesFrom fibo-sec-dbt-dbt-par:DiscountValue ;
    ] ;
  owl:disjointWith fibo-sec-dbt-dbt-par:InstrumentIssuedAtPar ;
  owl:disjointWith fibo-sec-dbt-dbt-par:PremiumInstrument ;
.
fibo-sec-dbt-dbt-par:InstrumentIssuedAtPar
  rdf:type owl:Class ;
  rdfs:label "instrument issued at par"@en ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-dbt-dbt-dbt:hasValueAtIssue ;
      owl:someValuesFrom fibo-sec-dbt-dbt-par:ParValue ;
    ] ;
  owl:disjointWith fibo-sec-dbt-dbt-par:PremiumInstrument ;
  skos:definition "Instrument is issued at par. These instruments are issued at some face value and mature for a value equal to that face value plus interest."@en ;
  fibo-fnd-utl-av:explanatoryNote "This classification of debt instruments is stated in terms of how an investor buys it. A debt instrument is bought either at discount, par or premium."@en ;
.
fibo-sec-dbt-dbt-par:ParValue
  rdf:type owl:Class ;
  terms:description "The same value as the face value of the instrument, also known as parity."@en ;
  rdfs:label "par value"@en ;
  rdfs:subClassOf fibo-sec-dbt-dbt-par:DebtRelativeValueSelection ;
  fibo-fnd-utl-av:explanatoryNote "For an instrument which redeems at par, the par or face value is repaid upon redemption.  Par value is synonymous with face value and nominal value for bonds.  Par value is an amount in a currency.  Each asset class has a standard for par value.  The standard par value of a U.S. corporate bond is $1000."@en ;
.
fibo-sec-dbt-dbt-par:PremiumInstrument
  rdf:type owl:Class ;
  rdfs:label "premium instrument"@en ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-dbt-dbt-dbt:hasValueAtIssue ;
      owl:someValuesFrom fibo-sec-dbt-dbt-par:PremiumValue ;
    ] ;
.
fibo-sec-dbt-dbt-par:PremiumValue
  rdf:type owl:Class ;
  terms:description "A premium over the face (or par) value, i.e. some amount greater than the par value of the instrument."@en ;
  rdfs:label "premium value"@en ;
  rdfs:subClassOf fibo-sec-dbt-dbt-par:DebtRelativeValueSelection ;
.
fibo-sec-dbt-dbt-pn:
  rdfs:label "ParticipationNotes"@en ;
.
fibo-sec-dbt-dbt-pn:ParticipationNote
  rdf:type owl:Class ;
  rdfs:label "participation note"@en ;
  rdfs:subClassOf fibo-sec-dbt-dbti:TradableDebtInstrument ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-bp-iss-trm:issuer ;
      owl:someValuesFrom fibo-sec-dbt-dbt-pn:ParticipationNoteIssuer ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-dbt-dbt-pn:hasUnderlying ;
      owl:someValuesFrom fibo-sec-dbt-dbt-dbt:TradableDebtInstrument ;
    ] ;
  skos:definition "A tradable note issued by an entity for sale in a jurisdiction other than the one in which the entity is registered, and based on the holding by that entity of underlying securities issued in the jurisdiction in which that entity is registered."@en ;
  fibo-fnd-utl-av:explanatoryNote "Typically P-Notes are SPVs that are created to allow participation from outside that market. The SPV purchases a security on shore and issues a note that represents that security to offshore investors. Similar to an ADR but is always a debt security. All underlying securities come from another market from the one where the PN is issued."@en ;
.
fibo-sec-dbt-dbt-pn:ParticipationNoteIssuer
  rdf:type owl:Class ;
  rdfs:label "participation note issuer"@en ;
  rdfs:subClassOf fibo-bp-iss-trm:SecurityIssuer ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-dbt-dbt-pn:describedAs ;
      owl:someValuesFrom fibo-sec-dbt-dbt-iss:SecuritiesIssuanceSPV ;
    ] ;
  skos:definition "The party which is the issuer of a Participation Note."@en ;
.
fibo-sec-dbt-dbt-pn:describedAs
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-dbt-dbt-pn:ParticipationNoteIssuer ;
  rdfs:label "described as"@en ;
  rdfs:range fibo-sec-dbt-dbt-iss:SecuritiesIssuanceSPV ;
  rdfs:subPropertyOf fibo-bp-iss-trm:securityIssuerDescribedAs ;
.
fibo-sec-dbt-dbt-pn:hasUnderlying
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-dbt-dbt-pn:ParticipationNote ;
  rdfs:label "has underlying"@en ;
  rdfs:range fibo-sec-dbt-dbt-dbt:TradableDebtInstrument ;
  skos:definition "THe underlying for the Participation Note."@en ;
.
