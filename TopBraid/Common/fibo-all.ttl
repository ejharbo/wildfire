fibo-md-dbt-py:YieldCalculationFormula
  rdf:type owl:Class ;
  rdfs:label "yield calculation formula"@en ;
  rdfs:subClassOf fibo-fnd-math-math:Formula ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-math-math:hasSubject ;
      owl:someValuesFrom fibo-md-dbt-py:DebtInstrumentYield ;
    ] ;
  skos:definition "The formula used in determining the Yield."@en ;
  fibo-fnd-utl-av:explanatoryNote "The subject of this Formula is the Yield. The formula has an expression which can be defined either in tectual terms or by further local extension of the term \"Formula Expression\" to define the parameters used."@en ;
