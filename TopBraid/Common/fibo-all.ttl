Copyright (c) 2016 Object Management Group, Inc.""" ;
  sm:dependsOn "http://www.omg.org/spec/LCC/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/BE/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FBC/FunctionalEntities/FinancialServicesEntities/"^^xsd:anyURI ;
