      owl:onProperty fibo-der-der-sp:refersTo ;
      owl:someValuesFrom fibo-sec-sec-sec:SettlementConvention ;
    ] ;
  skos:definition "A commitment for settlement at the earliest possible trading date, in accordance with the applicable settlement convention."@en ;
.
fibo-der-der-sp:SpotTransaction
