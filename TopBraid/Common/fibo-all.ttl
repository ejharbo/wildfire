      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fbc-pas-caa:Account ;
      owl:onProperty fibo-fnd-rel-rel:refersTo ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-arr-doc:LegalDocument ;
      owl:onProperty fibo-ln-ln-up:isEvidencedBy_g ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:isClassifiedBy ;
      owl:someValuesFrom loan:PublicRecordCategory ;
    ] ;
