    ] ;
.
fibo-sec-eq-iss:becomesPartOfTerms
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-iss:EquityIssuanceProcessInformation ;
  rdfs:label "becomes part of terms"@en ;
  rdfs:range fibo-sec-eq-iss:EquityInstrumentIssueInformation ;
  rdfs:subPropertyOf fibo-bp-iss-trm:becomesPartOf ;
.
fibo-sec-eq-lpe:
  rdfs:label "LimitedPartnershipEquities"@en ;
.
fibo-sec-eq-lpe:LimitedPartnershipUnit
  rdf:type owl:Class ;
  rdfs:label "limited partnership unit"@en ;
  rdfs:subClassOf fibo-fnd-agr-ctr:TransferableContract ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-agrx-con:confers ;
      owl:someValuesFrom fibo-sec-eq-eq:VotingRight ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-eq-eq:hasPaymentStatus ;
      owl:someValuesFrom fibo-sec-eq-eq:SecurityPaymentStatus ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-eq-lpe:definesRestrictions ;
      owl:someValuesFrom fibo-sec-eq-lpe:LimitedPartnershipUnitRestriction ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-eq-lpe:limitedPartnerhipUnitHasHolderLimitedPartner ;
      owl:someValuesFrom fibo-be-ptr-ptr:LimitedPartner ;
    ] ;
  skos:definition "A limited partnership is a form of partnership similar to a general partnership, except that in addition to one or more general partners (GPs) there are one or more limited partners (LPs)."@en ;
  fibo-fnd-utl-av:explanatoryNote "Like shareholders in a corporation, the LPs have limited liability, i.e. they are only liable on debts incurred by the firm to the extent of their registered investment, and they have no management authority. The GPs pay the LPs the equivalent of a dividend on their investment, the nature and extent of which is usually defined in the partnership agreement."@en ;
.
fibo-sec-eq-lpe:LimitedPartnershipUnitRestriction
  rdf:type owl:Class ;
  rdfs:label "limited partnership unit restriction"@en ;
  rdfs:subClassOf fibo-sec-sec-resx:SecurityContractualRestriction ;
  skos:definition "Restrictions on ownership or transfer of the Units."@en ;
.
fibo-sec-eq-lpe:definesRestrictions
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-lpe:LimitedPartnershipUnit ;
  rdfs:label "defines restrictions"@en ;
  rdfs:range fibo-sec-eq-lpe:LimitedPartnershipUnitRestriction ;
.
fibo-sec-eq-lpe:investorType
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-lpe:LimitedPartnershipUnitRestriction ;
  rdfs:label "defines investor type"@en ;
  rdfs:label "investor type"@en ;
  rdfs:range fibo-sec-sec-resx:InvestorType ;
  skos:definition "Type of investor that is allowed to hold the security."@en ;
.
fibo-sec-eq-lpe:limitedPartnerhipUnitHasHolderLimitedPartner
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-lpe:LimitedPartnershipUnit ;
  rdfs:label "limited partnerhip unit has holder limited partner"@en ;
  rdfs:range fibo-be-ptr-ptr:LimitedPartner ;
  rdfs:subPropertyOf fibo-fnd-agr-ctr:hasCounterparty ;
  skos:definition "The holder of the Limited Partnership Unit is a Limited Partner."@en ;
.
fibo-sec-eq-lpe:restricted
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-lpe:LimitedPartnershipUnitRestriction ;
  rdfs:label "restricted"@en ;
  rdfs:range fibo-fnd-utl-val:TrueFalseValue ;
  skos:definition "Yes = Restricted; No = Free."@en ;
.
fibo-sec-eq-sha:
  rdfs:label "ShareTerms"@en ;
.
fibo-sec-eq-sha:ArticlesOfIncorporation
  rdf:type owl:Class ;
  rdfs:label "articles of incorporation"@en ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-eq-sha:setsOut ;
      owl:someValuesFrom fibo-sec-eq-sha:CompanyEquityTermsSet ;
    ] ;
  skos:definition "proxy"@en ;
.
fibo-sec-eq-sha:CompanyEquityTermsSet
  rdf:type owl:Class ;
  rdfs:label "company equity terms"@en ;
  rdfs:subClassOf fibo-fnd-agr-ctr:ContractualCommitment ;
  skos:definition "Terms setting out the equity in a company, including the various kinds of rights, the shares that embody those rights etc."@en ;
.
fibo-sec-eq-sha:ConvertibleCommonShare
  rdf:type owl:Class ;
  rdfs:label "convertible common share"@en ;
  rdfs:subClassOf fibo-sec-eq-sha:ConvertibleShare ;
  rdfs:subClassOf fibo-sec-eq-sha:OrdinaryShare ;
  owl:disjointWith fibo-sec-eq-sha:ConvertiblePreferenceShare ;
.
fibo-sec-eq-sha:ConvertibleIntoFacet
  rdf:type owl:Class ;
  rdfs:label "convertible into facet"@en ;
  rdfs:subClassOf owl:Thing ;
.
<https://spec.edmcouncil.org/fibo/SEC/Equities/ShareTerms/ConvertibleIntoFacet.1>
  rdf:type owl:Class ;
  rdfs:label "ConvertibleIntoFacet.1"@en ;
  rdfs:subClassOf owl:Thing ;
.
fibo-sec-eq-sha:ConvertibleParticipatingPreferenceShare
  rdf:type owl:Class ;
  rdfs:label "convertible participating preference share"@en ;
  rdfs:subClassOf fibo-sec-eq-sha:ConvertiblePreferenceShare ;
  rdfs:subClassOf fibo-sec-eq-sha:ParticipatingPreferenceShare ;
  skos:definition "A preference share that can be converted into common shares at a fixed conversion price. Preference or preferred shares entitle a holder to a prior claim on any dividend paid by the company before payment is made on ordinary shares."@en ;
.
fibo-sec-eq-sha:ConvertiblePreferenceShare
  rdf:type owl:Class ;
  rdfs:label "convertible preference share"@en ;
  rdfs:subClassOf fibo-sec-eq-sha:ConvertibleShare ;
  rdfs:subClassOf fibo-sec-eq-sha:PreferenceShare ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-eq-sha:convertibleInto ;
      owl:someValuesFrom fibo-sec-eq-eq:PubliclyTradedShare ;
    ] ;
  skos:definition "A Preferred Share which is convertible into another security."@en ;
.
fibo-sec-eq-sha:ConvertibleShare
  rdf:type owl:Class ;
  rdfs:label "convertible share"@en ;
  rdfs:subClassOf fibo-sec-eq-eq:Share ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-eq-sha:convertibleInto ;
      owl:someValuesFrom fibo-fbc-fi-fi:Security ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-eq-sha:hasConversionTerms ;
      owl:someValuesFrom fibo-sec-eq-sha:EquityConversionTerms ;
    ] ;
  skos:definition "A Share which is convertible into another security, usually a share in the same company."@en ;
.
fibo-sec-eq-sha:CumulativeFacet
  rdf:type owl:Class ;
  rdfs:label "cumulative facet"@en ;
  rdfs:subClassOf owl:Thing ;
.
<https://spec.edmcouncil.org/fibo/SEC/Equities/ShareTerms/CumulativeFacet.1>
  rdf:type owl:Class ;
  rdfs:label "CumulativeFacet.1"@en ;
  rdfs:subClassOf owl:Thing ;
.
fibo-sec-eq-sha:CumulativePreferenceShare
  rdf:type owl:Class ;
  rdfs:label "cumulative preference share"@en ;
  rdfs:subClassOf fibo-sec-eq-sha:PreferenceShare ;
  owl:disjointWith fibo-sec-eq-sha:NonCumulativePreferenceShare ;
.
fibo-sec-eq-sha:DividendTerms
  rdf:type owl:Class ;
  rdfs:label "dividend terms"@en ;
  rdfs:subClassOf fibo-sec-eq-sha:CompanyEquityTermsSet ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-sec-eq-eq:DividendsDistributionMethod ;
      owl:onProperty fibo-sec-eq-sha:hasDistributionMethod ;
    ] ;
.
fibo-sec-eq-sha:EquityConversionTerms
  rdf:type owl:Class ;
  rdfs:label "equity conversion terms"@en ;
  rdfs:subClassOf fibo-sec-eq-sha:CompanyEquityTermsSet ;
  rdfs:subClassOf fibo-sec-sec-con:ConversionTerms ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-sec-con:specifiesConversionInto ;
      owl:someValuesFrom fibo-fbc-fi-fi:Security ;
    ] ;
  skos:definition "Contractual specifications when an Equity security can be converted to another security (usually of the same issuer)."@en ;
.
fibo-sec-eq-sha:EquityListing
  rdf:type owl:Class ;
  rdfs:label "equity listing"@en ;
  rdfs:subClassOf fibo-sec-sec-lst:SecurityListing ;
  skos:definition "The listing of an equity security (stock) on an equities exchange or multilateral trading facility."@en ;
  fibo-fnd-utl-av:explanatoryNote "This is not the security or the exchange but the listing of the security on the exchange. Facts defined for Equity Listing are facts about that security listed on that exchange or trading facility."@en ;
.
fibo-sec-eq-sha:NonCumulativePreferenceShare
  rdf:type owl:Class ;
  rdfs:label "non cumulative preference share"@en ;
  rdfs:subClassOf fibo-sec-eq-sha:PreferenceShare ;
  skos:definition "Non Cumulative Preference Shares are preference shares where the payment of the fixed dividend, if not made in a given trading year, is not carried forward to the next trading year."@en ;
  fibo-fnd-utl-av:explanatoryNote "All preference shares are either cumulative or non cumulative."@en ;
.
fibo-sec-eq-sha:NonParticipatingPreferenceShare
  rdf:type owl:Class ;
  rdfs:label "non participating preference share"@en ;
  rdfs:subClassOf fibo-sec-eq-sha:NonParticipatingShare ;
  rdfs:subClassOf fibo-sec-eq-sha:PreferenceShare ;
  skos:definition "The usual type of preference share, which is not a Participating Preference Share"@en ;
.
fibo-sec-eq-sha:NonParticipatingShare
  rdf:type owl:Class ;
  rdfs:label "non participating share"@en ;
  rdfs:subClassOf fibo-sec-eq-eq:Share ;
  owl:disjointWith fibo-sec-eq-sha:ParticipatingShare ;
.
fibo-sec-eq-sha:OneVotePerHolder
  rdf:type owl:Class ;
  rdfs:label "one vote per holder"@en ;
  rdfs:subClassOf fibo-sec-eq-sha:RestrictedVotingRight ;
  skos:definition "Voting rights limited rights to one vote per person, or institution, to avoid any party taking control. Further notes: Not in ISO FIBIM selection. Identified from new article on UK building societies. Note that further types of voting rights can be defined any time in the prospectus for a new security. Original Article quotation: \"Adrian Coles, director general of the Building Societies Association, said it would be helpful if building societies could issue shares to investors to raise funds, but limit voting rights to one vote per person, or institution, to avoid any party taking control. Article Source:article on Profit Participating Deferred Shares in relation to West Bromwich Building society By Philip Aldrick,[citation missing], 12 June 2009"@en ;
.
fibo-sec-eq-sha:OrdinaryDividendTerms
  rdf:type owl:Class ;
  rdfs:label "ordinary dividend terms"@en ;
  rdfs:subClassOf fibo-fnd-agr-ctr:NonBindingTerms ;
  rdfs:subClassOf fibo-sec-eq-sha:DividendTerms ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxQualifiedCardinality "12"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-utl-val:DayMonthValue ;
      owl:onProperty fibo-sec-eq-sha:dividendPaymentDate ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-utl-val:DayMonthValue ;
      owl:onProperty fibo-sec-eq-sha:dividendPaymentDate ;
    ] ;
  skos:definition "Formal terms about the Dividend payments on an Ordinary Share."@en ;
  fibo-fnd-utl-av:explanatoryNote "These are terms that are not contractually binding on the company."@en ;
.
fibo-sec-eq-sha:OrdinaryShare
  rdf:type owl:Class ;
  rdfs:label "ordinary share"@en ;
  rdfs:subClassOf fibo-sec-eq-eq:Share ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-eq-sha:pays ;
      owl:someValuesFrom fibo-md-eq-pri:OrdinaryShareDividend ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-eq-she:confersOwnershipOf ;
      owl:someValuesFrom fibo-sec-eq-she:JuniorEquity ;
    ] ;
  owl:disjointWith fibo-sec-eq-sha:PreferenceShare ;
  skos:definition "A security that represents ownership in a corporation. Holders of common share exercise control by electing a board of directors and voting on corporate policy."@en ;
  fibo-fnd-utl-av:explanatoryNote "Common stockholders are on the bottom of the priority ladder for ownership structure. In the event of liquidation, common shareholders have rights to a company's assets only after bondholders, preferred shareholders and other debtholders have been paid in full."@en ;
.
fibo-sec-eq-sha:ParticipatingFacet
  rdf:type owl:Class ;
  rdfs:label "participating facet"@en ;
  rdfs:subClassOf owl:Thing ;
.
<https://spec.edmcouncil.org/fibo/SEC/Equities/ShareTerms/ParticipatingFacet.1>
  rdf:type owl:Class ;
  rdfs:label "ParticipatingFacet.1"@en ;
  rdfs:subClassOf owl:Thing ;
.
fibo-sec-eq-sha:ParticipatingPreferenceShare
  rdf:type owl:Class ;
  rdfs:label "participating preference share"@en ;
  rdfs:subClassOf fibo-sec-eq-sha:ParticipatingShare ;
  rdfs:subClassOf fibo-sec-eq-sha:PreferenceShare ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-md-eq-pri:OrdinaryShareDividend ;
      owl:onProperty fibo-sec-eq-sha:optionallyPays ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-eq-sha:participatingPreferenceShareSeniorToPreferenceShare ;
      owl:someValuesFrom fibo-sec-eq-sha:PreferenceShare ;
    ] ;
  skos:definition "Like the preferred shares, participating preference shares have a prior claim on dividends, and on assets in an event of corporate liquidation or dissolution. But preferred stock would take precedence over preference stock in respect of dividends and assets that may be available for distribution."@en ;
.
fibo-sec-eq-sha:ParticipatingShare
  rdf:type owl:Class ;
  rdfs:label "participating share"@en ;
  rdfs:subClassOf fibo-sec-eq-eq:Share ;
.
fibo-sec-eq-sha:PrecedenceRight
  rdf:type owl:Class ;
  rdfs:label "precedence right"@en ;
  rdfs:subClassOf fibo-fnd-agrx-con:ContractualRight ;
  skos:definition "The rights of various parties in respect of the assets of some entity in the event of that entity being wound up."@en ;
  fibo-fnd-utl-av:explanatoryNote "Some of these rights are enshrined in law. A special case of these is shareholder rights which are defined, for different classes of equity, by the Company Articles of Association or equivalent Documentation. These are the rights defined in the Equity Priority Terms Set."@en ;
.
fibo-sec-eq-sha:PreferenceShare
  rdf:type owl:Class ;
  rdfs:label "preference share"@en ;
  rdfs:subClassOf fibo-sec-eq-eq:Share ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-eq-sha:hasCoupon ;
      owl:someValuesFrom [
          rdf:type owl:Class ;
          owl:unionOf (
              fibo-md-eq-pri:OrdinaryShareDividend
              fibo-md-eq-pri:PreferenceShareCoupon
            ) ;
        ] ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-eq-sha:mayConfer ;
      owl:someValuesFrom fibo-sec-eq-shr:ExtraordinaryVotingRight ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-eq-sha:preferenceShareSeniorToOrdinaryShare ;
      owl:someValuesFrom fibo-sec-eq-sha:OrdinaryShare ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty <https://spec.edmcouncil.org/fibo/SEC/Equities/ShareholderEquity/confersOwnershipOf.1> ;
      owl:someValuesFrom fibo-sec-eq-she:SeniorEquity ;
    ] ;
.
fibo-sec-eq-sha:PreferredCouponPaymentTermSet
  rdf:type owl:Class ;
  rdfs:label "preferred coupon payment term set"@en ;
  rdfs:subClassOf fibo-sec-eq-sha:DividendTerms ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-eq-sha:includesScheduleTerm ;
      owl:someValuesFrom fibo-sec-eq-sha:ShareCouponSchedule ;
    ] ;
  skos:definition "Terms for payment of a Dividend on a Preferred Stock."@en ;
  fibo-fnd-utl-av:explanatoryNote "These, like the Dividend terms, are not contractually binding terms; the dividend is paid at the discretion of the company but takes precedence over the ordinary share dividend."@en ;
.
fibo-sec-eq-sha:RestrictedVotingRight
  rdf:type owl:Class ;
  rdfs:label "restricted voting right"@en ;
  rdfs:subClassOf fibo-sec-eq-eq:VotingRight ;
  skos:definition "Restricted Voting. The holder has the right to vote but there are restrictions on these rights."@en ;
  fibo-fnd-utl-av:explanatoryNote "The restrictions are described in a separate term about the Voting rights."@en ;
.
fibo-sec-eq-sha:ShareCouponSchedule
  rdf:type owl:Class ;
  rdfs:label "share coupon schedule"@en ;
  rdfs:subClassOf owl:Thing ;
.
fibo-sec-eq-sha:ShareCouponScheduleSpecification
  rdf:type owl:Class ;
  rdfs:label "share coupon schedule specification"@en ;
  rdfs:subClassOf fibo-sec-dbt-bo-cft:CouponScheduleSpecification ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-md-eq-pri:scheduledDividendPayment ;
      owl:someValuesFrom fibo-md-eq-pri:EquityDividendPayment ;
    ] ;
  skos:definition "Schedule of dates on which a Share Coupon Payment is to take place."@en ;
  skos:editorialNote "At present it is assumed that this has all and only the same sceduleing specification terms as for debt coupon payment events."@en ;
.
fibo-sec-eq-sha:ShareholderPrecedenceRightsFacet
  rdf:type owl:Class ;
  rdfs:label "shareholder precedence rights facet"@en ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "Facet based on the class or seniority of the equity represented by the shares i.e. ordinary, preference and so on."@en ;
.
<https://spec.edmcouncil.org/fibo/SEC/Equities/ShareTerms/ShareholderPrecedenceRightsFacet.1>
  rdf:type owl:Class ;
  rdfs:label "ShareholderPrecedenceRightsFacet.1"@en ;
  rdfs:subClassOf owl:Thing ;
.
fibo-sec-eq-sha:SubordinatedVotingRight
  rdf:type owl:Class ;
  rdfs:label "subordinated voting right"@en ;
  rdfs:subClassOf fibo-sec-eq-eq:VotingRight ;
  skos:definition "Subordinated Voting. Action: this is a self-referential non definition, from ISO FIBIM."@en ;
.
fibo-sec-eq-sha:autoReinvestment
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:DividendTerms ;
  rdfs:label "auto reinvestment"@en ;
  rdfs:range fibo-fnd-utl-val:TrueFalseValue ;
  skos:definition "Whether the security provides for mandatorily &amp; automatically re-investing the dividend from that security towards buying more of the same security."@en ;
.
fibo-sec-eq-sha:conversionDate
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:EquityConversionTerms ;
  rdfs:label "conversion date"@en ;
  rdfs:range fibo-fnd-dt-fd:Date ;
  skos:definition "Date on or after which the specified conversion may take place."@en ;
.
fibo-sec-eq-sha:convertibleInto
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:ConvertibleShare ;
  rdfs:label "convertible into"@en ;
  rdfs:range fibo-fbc-fi-fi:Security ;
  skos:definition "The security into which the convertible share can be converted. It is assumed this can be any tradable security."@en ;
.
fibo-sec-eq-sha:cumulative
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:CumulativePreferenceShare ;
  rdfs:label "cumulative"@en ;
  rdfs:range fibo-fnd-utl-val:TrueFalseValue ;
  skos:definition "Whether or not the payment of the fixed dividend, if not made in a given trading year, is carried forward to the next trading year. This is no: skipped payments are not carried forward."@en ;
.
<https://spec.edmcouncil.org/fibo/SEC/Equities/ShareTerms/cumulative.1>
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:NonCumulativePreferenceShare ;
  rdfs:label "cumulative"@en ;
  rdfs:range fibo-fnd-utl-val:TrueFalseValue ;
  skos:definition "Whether or not the payment of the fixed dividend, if not made in a given trading year, is carried forward to the next trading year. This is yes: skipped dividend payments are accumulated until they are finally paid."@en ;
.
<https://spec.edmcouncil.org/fibo/SEC/Equities/ShareTerms/cumulative.2>
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:PreferenceShare ;
  rdfs:label "cumulative"@en ;
  rdfs:range fibo-fnd-utl-val:TrueFalseValue ;
  skos:definition "Whether or not the payment of the fixed dividend, if not made in a given trading year, is carried forward to the next trading year. If yes, skipped dividend payments are accumulated until they are finally paid. If no, skipped payments are not carried forward."@en ;
.
fibo-sec-eq-sha:dividendAmount
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:PreferredCouponPaymentTermSet ;
  rdfs:label "dividend amount"@en ;
  rdfs:range fibo-fnd-acc-cur:MonetaryAmount ;
  skos:definition "Planned dividend amount for preferred shares"@en ;
.
fibo-sec-eq-sha:dividendDate
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:PreferredCouponPaymentTermSet ;
  rdfs:label "dividend date"@en ;
  rdfs:range fibo-fnd-utl-val:DayMonthValue ;
  skos:definition "Planned dividend date (day and month) for regular dividend payment commitment."@en ;
.
fibo-sec-eq-sha:dividendExDate
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:DividendTerms ;
  rdfs:label "dividend ex date"@en ;
  rdfs:range fibo-fnd-utl-val:DayMonthValue ;
  skos:definition "The day and month on which the security is traded \"Ex Dividend\"."@en ;
.
fibo-sec-eq-sha:dividendPaymentDate
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:OrdinaryDividendTerms ;
  rdfs:label "dividend payment date"@en ;
  rdfs:range fibo-fnd-utl-val:DayMonthValue ;
  rdfs:subPropertyOf fibo-fnd-agr-ctr:isQualifiedBy ;
  skos:definition "The day and month on which dividends are to be paid."@en ;
.
fibo-sec-eq-sha:dividendPaymentFrequency
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:DividendTerms ;
  rdfs:label "dividend payment frequency"@en ;
  rdfs:range fibo-fnd-tim-tim:Frequency ;
  skos:definition "The frequency with which dividends are expected to be paid out."@en ;
.
fibo-sec-eq-sha:dividendRate
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:PreferredCouponPaymentTermSet ;
  rdfs:label "dividend rate"@en ;
  rdfs:range fibo-fnd-utl-val:PercentageValue ;
  skos:definition "Planned dividend rate, for preferred shares."@en ;
.
fibo-sec-eq-sha:gracePeriod
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:PreferredCouponPaymentTermSet ;
  rdfs:label "grace period"@en ;
  rdfs:range fibo-fnd-tim-tim:Duration ;
.
fibo-sec-eq-sha:hasConversionTerms
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:ConvertibleShare ;
  rdfs:label "has conversion terms"@en ;
  rdfs:range fibo-sec-eq-sha:EquityConversionTerms ;
  rdfs:subPropertyOf fibo-fnd-agr-ctr:hasContractualElement ;
.
fibo-sec-eq-sha:hasCoupon
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:PreferenceShare ;
  rdfs:label "has coupon"@en ;
  rdfs:range [
      rdf:type owl:Class ;
      owl:unionOf (
          fibo-md-eq-pri:OrdinaryShareDividend
          fibo-md-eq-pri:PreferenceShareCoupon
        ) ;
    ] ;
.
fibo-sec-eq-sha:hasDistributionMethod
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:DividendTerms ;
  rdfs:label "has distribution method"@en ;
  rdfs:range fibo-sec-eq-eq:DividendsDistributionMethod ;
  skos:definition "Method by which Dividend payments are to be distributed."@en ;
.
fibo-sec-eq-sha:hasListing
  rdf:type owl:ObjectProperty ;
  rdfs:label "has listing" ;
.
fibo-sec-eq-sha:includesScheduleTerm
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:PreferredCouponPaymentTermSet ;
  rdfs:label "includes schedule term"@en ;
  rdfs:range fibo-sec-eq-sha:ShareCouponSchedule ;
.
fibo-sec-eq-sha:isSeniorToAClassOfOrdinaryShare
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:OrdinaryShare ;
  rdfs:label "is senior to a class of ordinary share"@en ;
  rdfs:range fibo-sec-eq-sha:OrdinaryShare ;
  rdfs:subPropertyOf fibo-sec-eq-eq:isSeniorTo ;
.
fibo-sec-eq-sha:isSeniorToAClassOfPreferenceShare
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:PreferenceShare ;
  rdfs:label "is senior to a class of preference share"@en ;
  rdfs:range fibo-sec-eq-sha:PreferenceShare ;
  rdfs:subPropertyOf fibo-sec-eq-eq:isSeniorTo ;
.
fibo-sec-eq-sha:maturityDate
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:ParticipatingShare ;
  rdfs:label "maturity date"@en ;
  rdfs:range fibo-fnd-dt-fd:Date ;
  skos:definition "Date/time at which the security will no longer exist, eg, redeemable preference shares. Definition:FIBIM"@en ;
.
<https://spec.edmcouncil.org/fibo/SEC/Equities/ShareTerms/maturityDate.1>
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:PreferenceShare ;
  rdfs:label "maturity date"@en ;
  rdfs:range fibo-fnd-dt-fd:Date ;
  skos:definition "Date/time at which the security will no longer exist, eg, redeemable preference shares. Definition:FIBIM"@en ;
.
fibo-sec-eq-sha:mayConfer
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:PreferenceShare ;
  rdfs:label "may confer"@en ;
  rdfs:range fibo-sec-eq-shr:ExtraordinaryVotingRight ;
  skos:definition "The right of the holder of the Preference Shares to vote on matters affecting the business of the issuing company. A preference share may confer some voting rights whereby the holder may vote in extraordinary general meetings. These are not the same as the ordinary voting rights conferred by ordinary shares."@en ;
.
fibo-sec-eq-sha:numberOfSecurities
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:EquityConversionTerms ;
  rdfs:label "number of securities"@en ;
  rdfs:range fibo-fnd-utl-val:IntegerValue ;
  skos:definition "The number of units of the target security into which this security is converted upon conversion."@en ;
.
fibo-sec-eq-sha:optionallyPays
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:ParticipatingPreferenceShare ;
  rdfs:label "optionally pays"@en ;
  rdfs:range fibo-md-eq-pri:OrdinaryShareDividend ;
.
fibo-sec-eq-sha:participatingPreferenceShareSeniorToPreferenceShare
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:ParticipatingPreferenceShare ;
  rdfs:label "participating preference share senior to preference share"@en ;
  rdfs:range fibo-sec-eq-sha:PreferenceShare ;
  rdfs:subPropertyOf fibo-sec-eq-sha:isSeniorToAClassOfPreferenceShare ;
.
fibo-sec-eq-sha:payInKind
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:PreferredCouponPaymentTermSet ;
  rdfs:label "pay in kind"@en ;
  rdfs:range fibo-fnd-utl-val:TrueFalseValue ;
  skos:definition "The issuer will pay additional security as part of interest disbursement."@en ;
.
fibo-sec-eq-sha:pays
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:OrdinaryShare ;
  rdfs:label "pays"@en ;
  rdfs:range fibo-md-eq-pri:OrdinaryShareDividend ;
.
fibo-sec-eq-sha:perpetual
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:PreferenceShare ;
  rdfs:label "perpetual"@en ;
  rdfs:range fibo-fnd-utl-val:TrueFalseValue ;
  skos:definition "Signifies that there is NO set term involved. It pays its stated dividend forever or \"in perpetuity\". For a fixed-rate a set dividend upon issue is agreed to, usually declared and paid quarterly (fixed dollar or a percent of the par value). For a floating-rate provide for a dividend that is paid by reference to a market interest rate."@en ;
.
fibo-sec-eq-sha:preferenceShareSeniorToOrdinaryShare
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:PreferenceShare ;
  rdfs:label "preference share senior to ordinary share"@en ;
  rdfs:range fibo-sec-eq-sha:OrdinaryShare ;
  rdfs:subPropertyOf fibo-sec-eq-eq:isSeniorTo ;
.
fibo-sec-eq-sha:setsOut
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:ArticlesOfIncorporation ;
  rdfs:label "sets out"@en ;
  rdfs:range fibo-sec-eq-sha:CompanyEquityTermsSet ;
.
fibo-sec-eq-sha:softRetractable
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-sha:PreferenceShare ;
  rdfs:label "soft retractable"@en ;
  rdfs:range fibo-fnd-utl-val:TrueFalseValue ;
  skos:definition "A soft retraction value means it is payable in either \"hard cash\" or in an equal value of common stock of the issuer, at the choice of the issuer."@en ;
.
fibo-sec-eq-she:
  rdfs:label "ShareholderEquity"@en ;
.
fibo-sec-eq-she:EquityPriorityTerms
  rdf:type owl:Class ;
  rdfs:label "equity priority terms"@en ;
  rdfs:subClassOf fibo-sec-eq-sha:CompanyEquityTermsSet ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-eq-she:confers ;
      owl:someValuesFrom fibo-sec-eq-she:ShareholderPrecedenceRight ;
    ] ;
  skos:definition "Terms setting out the priority of a given class of Equity. These are the terms under which the holder of that class of equity has a call on the equity in the event of the issuing entity being wound up."@en ;
  fibo-fnd-utl-av:explanatoryNote "this is one of the three kinds of term conferring three kinds of rights to the holder."@en ;
.
fibo-sec-eq-she:JuniorEquity
  rdf:type owl:Class ;
  rdfs:label "junior equity"@en ;
  rdfs:subClassOf fibo-sec-eq-eq:ShareholderEquity ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-eq-she:hasPriorityAsDefinedIn ;
      owl:someValuesFrom fibo-sec-eq-she:JuniorEquityPriorityTermsSet ;
    ] ;
  skos:definition "The most junior class of equity issued by a company. On the event of the company being wound up, holders of this class of equity have a call on the assets of the company only after the more senior classes of equity."@en ;
.
fibo-sec-eq-she:JuniorEquityPriorityTermsSet
  rdf:type owl:Class ;
  rdfs:label "junior equity priority terms set"@en ;
  rdfs:subClassOf fibo-sec-eq-she:EquityPriorityTerms ;
  skos:definition "The precise terms which define the seniority of this class of Equity. This is equity which has the lowest call on the assets of the issuer in the event of it being wound up, i.e. after Senior Equity and (if issued) other classes of equity."@en ;
.
fibo-sec-eq-she:OtherEquityPriorityTermsSet
  rdf:type owl:Class ;
  rdfs:label "other equity priority terms set"@en ;
  rdfs:subClassOf fibo-sec-eq-she:EquityPriorityTerms ;
  skos:definition "The precise terms which define the seniority of this class of Equity. These will set it apart from the highest of lowest seniority (senior or junior debt) and may be defined for more than one additional class of equity. These terms are defined by the issuer."@en ;
.
fibo-sec-eq-she:OtherSeniorityEquity
  rdf:type owl:Class ;
  rdfs:label "other seniority equity"@en ;
  rdfs:subClassOf fibo-sec-eq-eq:ShareholderEquity ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-eq-she:hasPriorityAsDefinedIn ;
      owl:someValuesFrom fibo-sec-eq-she:OtherEquityPriorityTermsSet ;
    ] ;
  skos:definition "Equity which is neither the most senior nor the most junior. The precise terms under which this equity is held, and the definition of the call on the assets of the company in the event of it winding up, will be defined by the issuing company and are not modelled here as they may vary from one company to another. There may be more than one class of such equity issued by a company."@en ;
.
fibo-sec-eq-she:SeniorEquity
  rdf:type owl:Class ;
  rdfs:label "senior equity"@en ;
  rdfs:subClassOf fibo-sec-eq-eq:ShareholderEquity ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-eq-she:hasPriorityAsDefinedIn ;
      owl:someValuesFrom fibo-sec-eq-she:SeniorEquityPriorityTermsSet ;
    ] ;
  skos:definition "The most senior class of equity issued in the company. Holders of this class of equity will receive first call on the assets of the company in the event of it being wound up."@en ;
.
fibo-sec-eq-she:SeniorEquityPriorityTermsSet
  rdf:type owl:Class ;
  rdfs:label "senior equity priority terms set"@en ;
  rdfs:subClassOf fibo-sec-eq-she:EquityPriorityTerms ;
  skos:definition "The precise terms which define the seniority of this class of Equity. These terms set out that the holder has the first call among equity holders, on the assets of the issuing entity in the event of it being wound up, ahead of the holders of other classes of equity."@en ;
.
fibo-sec-eq-she:ShareholderPrecedenceRight
  rdf:type owl:Class ;
  rdfs:label "shareholder precedence right"@en ;
  rdfs:subClassOf fibo-sec-eq-sha:PrecedenceRight ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:isConferredBy ;
      owl:someValuesFrom fibo-be-oac-exec:ArticlesOfIncorporation ;
    ] ;
  skos:definition "The rights of shareholderes in respect of the assets of the limited company in which they hold those shares, in the event of that company being wound up."@en ;
  fibo-fnd-utl-av:explanatoryNote "These are defined, for different classes of equity, by the Company Articles of Association or equivalent Documentation. These are the rights defined in the Equity Priority Terms Set."@en ;
.
fibo-sec-eq-she:confers
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-she:EquityPriorityTerms ;
  rdfs:label "confers"@en ;
  rdfs:range fibo-sec-eq-she:ShareholderPrecedenceRight ;
.
fibo-sec-eq-she:confersOwnershipOf
  rdf:type owl:ObjectProperty ;
  rdfs:label "confers ownership of" ;
.
<https://spec.edmcouncil.org/fibo/SEC/Equities/ShareholderEquity/confersOwnershipOf.1>
  rdf:type owl:ObjectProperty ;
  rdfs:label "confers ownership of" ;
.
fibo-sec-eq-she:hasPriorityAsDefinedIn
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-eq:ShareholderEquity ;
  rdfs:label "has priority as defined in"@en ;
  rdfs:range fibo-sec-eq-she:EquityPriorityTerms ;
.
fibo-sec-eq-she:juniorEquityPriority
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-she:JuniorEquity ;
  rdfs:label "junior equity priority"@en ;
  rdfs:range fibo-sec-eq-eq:Junior ;
  rdfs:subPropertyOf fibo-sec-eq-eq:priority ;
  skos:definition "The seniority of the equity. This is the order of precedence in which holders of different classes of equity will have a call on the assets of the company in the event of it winding up. In the case of ordinary equity this is the lowest seniority i.e. it has the last call on the assets of the company."@en ;
.
fibo-sec-eq-she:otherEquityPriority
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-she:OtherSeniorityEquity ;
  rdfs:label "other equity priority"@en ;
  rdfs:range fibo-sec-eq-eq:OtherSeniority ;
  rdfs:subPropertyOf fibo-sec-eq-eq:priority ;
  skos:definition "The seniority of the equity. This is the order of precedence in which holders of different classes of equity will have a call on the assets of the company in the event of it winding up. In this class of equity this is defined as being \"Other\" than highest or lowest, with the precise terms and order of precedence being defined in the priority terms."@en ;
.
fibo-sec-eq-she:seniorEquityPriority
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-she:SeniorEquity ;
  rdfs:label "senior equity priority"@en ;
  rdfs:range fibo-sec-eq-eq:Senior ;
  rdfs:subPropertyOf fibo-sec-eq-eq:priority ;
  skos:definition "The seniority of the equity. This is the order of precedence in which holders of different classes of equity will have a call on the assets of the company in the event of it winding up. In this class of equity this is the highest in senioity."@en ;
.
fibo-sec-eq-shr:
  rdfs:label "ShareholderRights"@en ;
.
fibo-sec-eq-shr:EquityVotingRightsTerms
  rdf:type owl:Class ;
  rdfs:label "equity voting rights terms"@en ;
  rdfs:subClassOf fibo-sec-eq-sha:CompanyEquityTermsSet ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-eq-shr:setsOut ;
      owl:someValuesFrom fibo-sec-eq-eq:VotingRight ;
    ] ;
  skos:definition "Terms setting out the voting rights of the Holder."@en ;
.
fibo-sec-eq-shr:ExtraordinaryVotingRight
  rdf:type owl:Class ;
  rdfs:label "extraordinary voting right"@en ;
  rdfs:subClassOf fibo-sec-eq-eq:VotingRight ;
  skos:definition "The right to vote at extraordinary meetings, as opposed to being able to vote at annual general meetings."@en ;
.
fibo-sec-eq-shr:OrdinaryVotingRight
  rdf:type owl:Class ;
  rdfs:label "ordinary voting right"@en ;
  rdfs:subClassOf fibo-sec-eq-eq:VotingRight ;
  skos:definition "The voting rights conferred on holders of Ordinary Shares in the company. The precise details of these rights such as what shareholders may vote on and how their voting right is exercised, are defined in the Company Constitution or Articles of Incorporation / Articles of Association."@en ;
.
fibo-sec-eq-shr:setsOut
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-eq-shr:EquityVotingRightsTerms ;
  rdfs:label "sets out"@en ;
  rdfs:range fibo-sec-eq-eq:VotingRight ;
.
