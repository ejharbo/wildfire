fibo-fbc-fct-breg:BusinessRegistrationAuthority
  rdf:type owl:Class ;
  rdfs:label "business registration authority" ;
  rdfs:subClassOf fibo-fbc-fct-ra:RegistrationAuthority ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fbc-fct-breg:BusinessRegistry ;
      owl:onProperty fibo-fnd-rel-rel:manages ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-law-jur:Jurisdiction ;
      owl:onProperty fibo-fnd-rel-rel:isGovernedBy ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "a governmental registration authority that is responsible for maintaining a registry of business entities for a particular jurisdiction" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.leiroc.org/publications/gls/lou_20140620.pdf"^^xsd:anyURI ;
.
fibo-fbc-fct-breg:BusinessRegistry
  rdf:type owl:Class ;
  rdfs:label "business registry" ;
  rdfs:subClassOf fibo-fbc-fct-ra:Registry ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fbc-fct-breg:BusinessRegistrationAuthority ;
      owl:onProperty fibo-fnd-rel-rel:isManagedBy ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-law-jur:Jurisdiction ;
      owl:onProperty fibo-fnd-rel-rel:isGovernedBy ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "a registry for registering and maintaining information about business entities for a particular jurisdiction" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.leiroc.org/publications/gls/lou_20140620.pdf"^^xsd:anyURI ;
.
fibo-fbc-fct-breg:BusinessRegistryEntry
  rdf:type owl:Class ;
  rdfs:label "business registry entry" ;
  rdfs:subClassOf fibo-fbc-fct-ra:RegistryEntry ;
  skos:definition "an entry in a business registry" ;
.
fibo-fbc-fct-breg:RegistrationAddress
  rdf:type owl:Class ;
  rdfs:label "registration address" ;
  rdfs:subClassOf fibo-fnd-plc-adr:Address ;
  skos:definition "an address for a business, such as its legal address, headquarters address, address registered for obtaining a license, and so forth" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.leiroc.org/publications/gls/lou_20140620.pdf"^^xsd:anyURI ;
.
fibo-fbc-fct-breg:hasAddressLine1
  rdf:type owl:DatatypeProperty ;
  rdfs:domain fibo-fbc-fct-breg:RegistrationAddress ;
  rdfs:label "has address line 1" ;
  rdfs:range fibo-fnd-utl-bt:text ;
  skos:definition "the first line of the street address" ;
  fibo-fnd-utl-av:definitionOrigin "http://www.leiroc.org/publications/gls/lou_20140620.pdf"^^xsd:anyURI ;
.
fibo-fbc-fct-breg:hasAddressLine2
  rdf:type owl:DatatypeProperty ;
  rdfs:domain fibo-fbc-fct-breg:RegistrationAddress ;
  rdfs:label "has address line 2" ;
  rdfs:range fibo-fnd-utl-bt:text ;
  skos:definition "the second line of the street address" ;
  fibo-fnd-utl-av:definitionOrigin "http://www.leiroc.org/publications/gls/lou_20140620.pdf"^^xsd:anyURI ;
.
fibo-fbc-fct-breg:hasAddressLine3
  rdf:type owl:DatatypeProperty ;
  rdfs:domain fibo-fbc-fct-breg:RegistrationAddress ;
  rdfs:label "has address line 3" ;
  rdfs:range fibo-fnd-utl-bt:text ;
  skos:definition "the third line of the street address" ;
  fibo-fnd-utl-av:definitionOrigin "http://www.leiroc.org/publications/gls/lou_20140620.pdf"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "This element SHALL be omitted if address line 2 is omitted." ;
.
fibo-fbc-fct-breg:hasAddressLine4
  rdf:type owl:DatatypeProperty ;
  rdfs:domain fibo-fbc-fct-breg:RegistrationAddress ;
  rdfs:label "has address line 4" ;
  rdfs:range fibo-fnd-utl-bt:text ;
  skos:definition "the fourth line of the street address" ;
  fibo-fnd-utl-av:definitionOrigin "http://www.leiroc.org/publications/gls/lou_20140620.pdf"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "This element SHALL be omitted if address line 3 is omitted." ;
.
fibo-fbc-fct-breg:hasCity
  rdf:type owl:DatatypeProperty ;
  rdfs:label "has city" ;
  rdfs:range fibo-fnd-utl-bt:text ;
  skos:definition "the name of the city" ;
  fibo-fnd-utl-av:definitionOrigin "http://www.leiroc.org/publications/gls/lou_20140620.pdf"^^xsd:anyURI ;
.
fibo-fbc-fct-breg:hasCountry
  rdf:type owl:ObjectProperty ;
  rdfs:label "has country" ;
  rdfs:range lcc-cr:Country ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "identifies the country according to ISO 3166-1" ;
.
fibo-fbc-fct-breg:hasHeadquartersAddress
  rdf:type owl:ObjectProperty ;
  rdfs:label "has headquarters address" ;
  rdfs:range fibo-fbc-fct-breg:RegistrationAddress ;
  rdfs:subPropertyOf fibo-be-le-fbo:hasPrimaryAddress ;
  skos:definition "identifies the headquarters address for the registered entity" ;
.
fibo-fbc-fct-breg:hasPostalCode
  rdf:type owl:DatatypeProperty ;
  rdfs:domain fibo-fbc-fct-breg:RegistrationAddress ;
  rdfs:label "has postal code" ;
  rdfs:range fibo-fnd-utl-bt:text ;
  skos:definition "the postal code of this address as specified by the local postal service" ;
  fibo-fnd-utl-av:definitionOrigin "http://www.leiroc.org/publications/gls/lou_20140620.pdf"^^xsd:anyURI ;
.
fibo-fbc-fct-breg:hasRegistrationStatus
  rdf:type owl:DatatypeProperty ;
  rdfs:label "has registration status" ;
  rdfs:range fibo-fnd-utl-bt:text ;
  skos:definition "the status of a specific registration, such as a registered identifier or license" ;
.
fibo-fbc-fct-breg:hasRegistrationStatusRevisionDate
  rdf:type owl:ObjectProperty ;
  rdfs:label "has registration status revision date" ;
  rdfs:range fibo-fnd-dt-fd:Date ;
  rdfs:subPropertyOf fibo-fnd-dt-fd:hasDate ;
  skos:definition "the date that the status of a specific registration in the registry was revised" ;
.
fibo-fbc-fct-breg:hasSubdivision
  rdf:type owl:ObjectProperty ;
  rdfs:label "has subdivision" ;
  rdfs:range lcc-cr:CountrySubdivision ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "identifies the country subdivision (state, province, region, etc.) according to ISO 3166-2" ;
.
