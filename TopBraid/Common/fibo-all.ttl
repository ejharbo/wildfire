fibo-sec-dbt-mm-lpn:LPNTerms
  rdf:type owl:Class ;
  rdfs:label "l p n terms"@en ;
  rdfs:subClassOf fibo-fnd-agr-ctr:ContractualCommitment ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
