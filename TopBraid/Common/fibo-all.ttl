fibo-bp-iss-doc:
  rdfs:label "IssuanceDocuments"@en ;
.
fibo-bp-iss-doc:Indenture
  rdf:type owl:Class ;
  rdfs:label "indenture"@en ;
  rdfs:subClassOf fibo-bp-iss-doc:SecuritiesIssuanceAgreement ;
  rdfs:subClassOf fibo-fnd-agr-ctr:WrittenContract ;
  skos:definition "A written contract, also known as a \"Deed of Trust\", under which bonds and debentures are issued, setting forth maturity date, interest rate, redemption rights, call privileges and other terms. Under the rules of the Trust Indenture Act of 1939, the contract is executed by the issuer and a trustee who acts on behalf of the bondholders."@en ;
.
fibo-bp-iss-doc:IssuanceTrustAgreement
  rdf:type owl:Class ;
  rdfs:label "issuance trust agreement"@en ;
  rdfs:subClassOf fibo-bp-iss-doc:SecuritiesIssuanceAgreement ;
  rdfs:subClassOf fibo-fnd-agr-ctr:WrittenContract ;
.
fibo-bp-iss-doc:OfferingDocument
  rdf:type owl:Class ;
  rdfs:label "offering document"@en ;
  rdfs:subClassOf fibo-fnd-inf-doc:ControlDocument ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom [
          rdf:type owl:Class ;
          owl:unionOf (
              fibo-bp-iss-doc:SecuritiesIssuanceAgreement
              fibo-bp-iss-doc:SecuritiesIssuanceCommunication
              fibo-bp-iss-doc:SecurityDraftProspectus
              fibo-bp-iss-doc:SecurityFinalProspectus
              fibo-bp-iss-doc:SecurityIssuanceCertificate
              fibo-bp-iss-doc:SecurityIssueOrdinance
              fibo-bp-iss-doc:SecurityTermSheet
            ) ;
        ] ;
      owl:onProperty fibo-fnd-inf-doc:identifiedAs ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass [
          rdf:type owl:Class ;
          owl:unionOf (
              fibo-bp-iss-doc:SecuritiesIssuanceAgreement
              fibo-bp-iss-doc:SecuritiesIssuanceCommunication
              fibo-bp-iss-doc:SecurityDraftProspectus
              fibo-bp-iss-doc:SecurityFinalProspectus
              fibo-bp-iss-doc:SecurityIssuanceCertificate
              fibo-bp-iss-doc:SecurityIssueOrdinance
              fibo-bp-iss-doc:SecurityTermSheet
            ) ;
        ] ;
      owl:onProperty fibo-fnd-inf-doc:identifiedAs ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-bp-iss-doc:containsTerms ;
      owl:someValuesFrom fibo-bp-iss-doc:OfferingDocumentTerms ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-bp-iss-doc:hasDocumentDate ;
      owl:someValuesFrom fibo-fnd-tim-tim:Date ;
    ] ;
  skos:definition "The type of document that is required to be sent in an Offering to make the securities in the Offering eligible at (for example) DTC."@en ;
.
fibo-bp-iss-doc:OfferingDocumentTerms
  rdf:type owl:Class ;
  rdfs:label "offering document terms"@en ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "Terms which are contained in the identified IOffering Document and are considered legally binding within the Issuance and Offering process, if they exist."@en ;
.
fibo-bp-iss-doc:OfferingMemorandum
  rdf:type owl:Class ;
  rdfs:label "offering memorandum"@en ;
  rdfs:subClassOf fibo-bp-iss-doc:SecuritiesIssueMemorandum ;
.
fibo-bp-iss-doc:PlacementAgreement
  rdf:type owl:Class ;
  rdfs:label "placement agreement"@en ;
  rdfs:subClassOf fibo-bp-iss-doc:SecuritiesIssuanceAgreement ;
  rdfs:subClassOf fibo-fnd-agr-ctr:WrittenContract ;
.
fibo-bp-iss-doc:PrivatePlacementMemorandum
  rdf:type owl:Class ;
  rdfs:label "private placement memorandum"@en ;
  rdfs:subClassOf fibo-bp-iss-doc:SecuritiesIssueMemorandum ;
.
fibo-bp-iss-doc:ProspectusTermsSet
  rdf:type owl:Class ;
  rdfs:label "prospectus terms set"@en ;
  rdfs:subClassOf fibo-bp-iss-doc:ProvisionalTermsDescription ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-bp-iss-doc:becomesOnIssue ;
      owl:someValuesFrom fibo-sec-sec-sec:SecurityContractTerms ;
    ] ;
  skos:definition "A set of terms in a Prospectus which on issue will become the binding Contractual Terms of the Security. For example, Call Terms, Interest Payment Terms."@en ;
  skos:editorialNote "becomes: The terms in the Prospectus become legally binding contractual terms of the Security, at issue. (former property, now a restriction)"@en ;
.
fibo-bp-iss-doc:ProvisionalTermsDescription
  rdf:type owl:Class ;
  rdfs:label "provisional terms description"@en ;
  rdfs:subClassOf fibo-fnd-inf-inf:WrittenInformation ;
.
fibo-bp-iss-doc:SecuritiesIssuanceAgreement
  rdf:type owl:Class ;
  rdfs:label "securities issuance agreement"@en ;
  rdfs:subClassOf fibo-fnd-agr-ctr:WrittenContract ;
  skos:definition "A formal agreement, deed or contract in respect of some intended securities issuance."@en ;
.
fibo-bp-iss-doc:SecuritiesIssuanceCommunication
  rdf:type owl:Class ;
  rdfs:label "securities issuance communication"@en ;
  rdfs:subClassOf fibo-fnd-inf-doc:WrittenCommunication ;
  skos:definition "A formal memorandum in respect of some securities issuance."@en ;
  skos:definition "A notice, statement, declaration, memorandum or other formal written communication in respect of some securities issue, sent out by or on behalf of the issuers of some intended securities issue."@en ;
.
fibo-bp-iss-doc:SecuritiesIssuanceNoticeOfSale
  rdf:type owl:Class ;
  rdfs:label "securities issuance notice of sale"@en ;
  rdfs:subClassOf fibo-bp-iss-doc:SecuritiesIssuanceCommunication ;
  rdfs:subClassOf fibo-fnd-inf-doc:Notice ;
.
fibo-bp-iss-doc:SecuritiesIssuanceOfficialStatement
  rdf:type owl:Class ;
  rdfs:label "securities issuance official statement"@en ;
  rdfs:subClassOf fibo-bp-iss-doc:SecuritiesIssuanceCommunication ;
  rdfs:subClassOf fibo-fnd-inf-doc:FormalStatement ;
.
fibo-bp-iss-doc:SecuritiesIssueMemorandum
  rdf:type owl:Class ;
  rdfs:label "securities issue memorandum"@en ;
  rdfs:subClassOf fibo-bp-iss-doc:SecuritiesIssuanceCommunication ;
  rdfs:subClassOf fibo-fnd-inf-doc:Memorandum ;
  skos:definition "A formal memorandum in respect of some securities issuance."@en ;
.
fibo-bp-iss-doc:SecurityDraftProspectus
  rdf:type owl:Class ;
  rdfs:label "security draft prospectus"@en ;
  rdfs:subClassOf fibo-fnd-inf-doc:DraftContract ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-bp-iss-doc:becomesFinal ;
      owl:someValuesFrom fibo-bp-iss-doc:SecurityFinalProspectus ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-bp-iss-doc:hasDraftTerms ;
      owl:someValuesFrom fibo-bp-iss-doc:ProspectusTermsSet ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-inf-doc:comesIntoEffectAs ;
      owl:someValuesFrom fibo-fbc-fi-fi:Security ;
    ] ;
  skos:definition "The draft prospectus for the issue, as determined prior to marketing the issue. Certain terms in the draft prospectus will be finalized later in the issuance process to become the actual Prospectus. Term origin:DTCC issuance Reviews"@en ;
.
fibo-bp-iss-doc:SecurityFinalProspectus
  rdf:type owl:Class ;
  rdfs:label "security final prospectus"@en ;
  rdfs:subClassOf fibo-fnd-inf-doc:Prospectus ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-bp-iss-doc:hasProspectiveTerms ;
      owl:someValuesFrom fibo-bp-iss-doc:ProspectusTermsSet ;
    ] ;
  skos:definition "The final Prospectus for an issue describes the Offer Issue, including facts about the issue itself such as closing dates, and known facts about the securities that will form part of that issue."@en ;
  fibo-fnd-utl-av:explanatoryNote "Anticipated facts might include expected issue price, yields, pool parameters as appropriate for the type of instrument being issued. Term origin:SMER"@en ;
.
fibo-bp-iss-doc:SecurityIssuanceCertificate
  rdf:type owl:Class ;
  rdfs:label "security issuance certificate"@en ;
  rdfs:subClassOf fibo-fnd-inf-doc:Certificate ;
  skos:definition "A formal agreement, deed or contract in respect of some intended securities issuance."@en ;
  skos:definition "A formal memorandum in respect of some securities issuance."@en ;
  skos:definition "A notice, statement, declaration, memorandum or other formal written communication in respect of some securities issue, sent out by or on behalf of the issuers of some intended securities issue."@en ;
  skos:definition "Certificate as defined in DTCC terms for type of Offering Document. REVIEW: It is not known whether this is the security certificate itself, or a certificate relating to the Offering of a new Security or issue."@en ;
.
fibo-bp-iss-doc:SecurityIssueOrdinance
  rdf:type owl:Class ;
  rdfs:label "security issue ordinance"@en ;
  rdfs:subClassOf fibo-fnd-law-cor:Ordinance ;
.
fibo-bp-iss-doc:SecurityTermSheet
  rdf:type owl:Class ;
  rdfs:label "security term sheet"@en ;
  rdfs:subClassOf fibo-fnd-inf-doc:TermsSheet ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-bp-iss-doc:hasProvisionalTerms ;
      owl:someValuesFrom fibo-bp-iss-doc:TermsSheetTermsSet ;
    ] ;
  skos:definition "A set of written terms for a future financial security setting out legal or contractual rights, obligations and the like, but not (or not yet) forming part of a set of Contractual Terms of the security contract."@en ;
  fibo-fnd-utl-av:explanatoryNote "A typical case would be a termsheet in a prospectus, informing potential investors in something, what terms they will expect when the prospectus becomes a formal contract. Note that Termsheet as defined here, being part of the terms of a future contract, refers to the termsheet for the or each individual tranche (i.e. individual contract). In issues with more than one tranche there may well be one document referred to as \"the termsheet\" but this would legally be several termsheets in the sense used here. Therefore more than one Termsheet may be included in one physical document. Term origin:MBS PoC Reviews"@en ;
.
fibo-bp-iss-doc:SeriesDeclaration
  rdf:type owl:Class ;
  rdfs:label "series declaration"@en ;
  rdfs:subClassOf fibo-bp-iss-doc:SecuritiesIssuanceCommunication ;
  rdfs:subClassOf fibo-fnd-inf-doc:Declaration ;
.
fibo-bp-iss-doc:ShareUnderwriter
  rdf:type owl:Class ;
  rdfs:label "share underwriter"@en ;
  rdfs:subClassOf fibo-bp-iss-trm:SecurityUnderwriter ;
  skos:definition "The party which agrees to buy any certificates that are not bought by investors Term origin:SR Modeling"@en ;
.
fibo-bp-iss-doc:TermsSheetTermsSet
  rdf:type owl:Class ;
  rdfs:label "terms sheet terms set"@en ;
  rdfs:subClassOf fibo-bp-iss-doc:ProvisionalTermsDescription ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-bp-iss-doc:becomesOnIssue ;
      owl:someValuesFrom fibo-sec-sec-sec:SecurityContractTerms ;
    ] ;
.
fibo-bp-iss-doc:becomesFinal
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-bp-iss-doc:SecurityDraftProspectus ;
  rdfs:label "becomes final"@en ;
  rdfs:range fibo-bp-iss-doc:SecurityFinalProspectus ;
  rdfs:subPropertyOf fibo-fnd-inf-doc:becomesFormalizedAs ;
.
fibo-bp-iss-doc:becomesOnIssue
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-bp-iss-doc:ProvisionalTermsDescription ;
  rdfs:label "becomes on issue"@en ;
  rdfs:range fibo-sec-sec-sec:SecurityContractTerms ;
  skos:definition "An individual set of terms in a Terms Sheet may become a corresponding set of Terms in an Security. This would happen as part of the Issuance process."@en ;
.
fibo-bp-iss-doc:containsTerms
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-bp-iss-doc:OfferingDocument ;
  rdfs:label "contains terms"@en ;
  rdfs:range fibo-bp-iss-doc:OfferingDocumentTerms ;
.
fibo-bp-iss-doc:definesTerms
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-bp-iss-doc:SecurityTermSheet ;
  rdfs:label "defines terms"@en ;
  rdfs:range fibo-sec-sec-sec:SecurityContractTerms ;
  rdfs:subPropertyOf fibo-fnd-inf-inf:isAbout ;
.
fibo-bp-iss-doc:documentType
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-bp-iss-doc:OfferingDocument ;
  rdfs:label "document type"@en ;
  rdfs:range fibo-fnd-utl-val:TextValue ;
  skos:definition "The type of document that is required to be sent in an Offering to make the securities in the Offering eligible at DTC."@en ;
.
fibo-bp-iss-doc:hasDocumentDate
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-bp-iss-doc:OfferingDocument ;
  rdfs:label "has document date"@en ;
  rdfs:range fibo-fnd-tim-tim:Date ;
  skos:definition "The date stated in the document, from which the document is considered 'Official' or Released."@en ;
.
fibo-bp-iss-doc:hasDraftTerms
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-bp-iss-doc:SecurityDraftProspectus ;
  rdfs:label "has draft terms"@en ;
  rdfs:range fibo-bp-iss-doc:ProspectusTermsSet ;
.
fibo-bp-iss-doc:hasProspectiveTerms
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-bp-iss-doc:SecurityFinalProspectus ;
  rdfs:label "has prospective terms"@en ;
  rdfs:range fibo-bp-iss-doc:ProspectusTermsSet ;
  skos:definition "The final Security Prospectus contains all the Terms which upon issue will become the binding contractual Terms of the Security."@en ;
.
fibo-bp-iss-doc:hasProvisionalTerms
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-bp-iss-doc:SecurityTermSheet ;
  rdfs:label "has provisional terms"@en ;
  rdfs:range fibo-bp-iss-doc:TermsSheetTermsSet ;
.
fibo-bp-iss-doc:isFinalVersion
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-bp-iss-doc:OfferingDocument ;
  rdfs:label "is final version"@en ;
  rdfs:range fibo-fnd-utl-val:TrueFalseValue ;
  skos:definition "The version of the document, i.e. whether it is a Draft or a Final version. Note: This is not the revision number, if that exists."@en ;
.
fibo-bp-iss-prc:
  rdfs:label "IssuanceProcess"@en ;
.
fibo-bp-iss-prc:Announcement
  rdf:type owl:Class ;
  rdfs:label "announcement"@en ;
  rdfs:subClassOf fibo-bp-iss-dbti:IssuanceProcessActivity ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-bp-iss-muni:offeringRequestor ;
      owl:someValuesFrom fibo-bp-iss-prc:PotentialIssuer ;
    ] ;
.
fibo-bp-iss-prc:CompetitiveSaleMethod
  rdf:type owl:Class ;
  rdfs:label "competitive sale method"@en ;
  rdfs:subClassOf fibo-bp-iss-prc:SecurityOfferingSaleMethod ;
  skos:definition "No definition"@en ;
.
fibo-bp-iss-prc:IssuanceProcessActivity
  rdf:type owl:Class ;
  rdfs:label "issuance process activity"@en ;
  rdfs:subClassOf owl:Thing ;
.
fibo-bp-iss-prc:NegotiatedSaleMethod
  rdf:type owl:Class ;
  rdfs:label "negotiated sale method"@en ;
  rdfs:subClassOf fibo-bp-iss-prc:SecurityOfferingSaleMethod ;
  skos:definition "No definition"@en ;
.
fibo-bp-iss-prc:Offering
  rdf:type owl:Class ;
  rdfs:label "offering"@en ;
  rdfs:subClassOf fibo-bp-iss-dbti:IssuanceProcessActivity ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-bp-iss-prc:hasSaleMethod ;
      owl:someValuesFrom fibo-bp-iss-prc:SecurityOfferingSaleMethod ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-bp-iss-prc:requiredToMakeEligible ;
      owl:someValuesFrom fibo-bp-iss-doc:OfferingDocument ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-bp-iss-trm:hasDistributionType ;
      owl:someValuesFrom fibo-bp-iss-prc:SecurityOfferingDistributionMethod ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-bp-iss-muni:givesRiseTo ;
      owl:someValuesFrom fibo-bp-iss-trm:IssuedSecurityIssueInformation ;
    ] ;
  skos:definition "The process step of offering a security for issue. This is the making available of a new securities issue through an underwriting."@en ;
  fibo-fnd-utl-av:explanatoryNote "It is assumed that this exists for all security types as the precise issuance process is defined among the Offering terms. Terms which only exist for specific types of instrument are given as specialized variants of this class of Thing."@en ;
.
fibo-bp-iss-prc:OfferingDocumentInformation
  rdf:type owl:Class ;
  rdfs:label "offering document information"@en ;
  rdfs:subClassOf fibo-fnd-inf-doc:DocumentInformation ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-bp-iss-prc:pertainsTo ;
      owl:someValuesFrom fibo-bp-iss-doc:OfferingDocument ;
    ] ;
  skos:definition "Information pertaining to the Offering Document but not forming part of it."@en ;
.
fibo-bp-iss-prc:PotentialIssuer
