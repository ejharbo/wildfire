  rdfs:label "Formal Business Organizations Ontology" ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/BE/AboutBE/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/BE/LegalEntities/AboutLegalEntities/"^^xsd:anyURI ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/BE/20160201/LegalEntities/FormalBusinessOrganizations/> ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/BE/20131101/LegalEntities/FormalBusinessOrganizations.rdf version of this ontology was modified per the issue resolutions identified in the FIBO BE 1.0 FTF report." ;
.
fibo-be-le-fbo:Branch
  rdf:type owl:Class ;
  rdfs:label "branch" ;
  rdfs:subClassOf fibo-be-le-fbo:OrganizationalSubUnit ;
  skos:definition "A part of a business organization or company, identified as a branch." ;
  skos:editorialNote "This is not a separate legal entity in its own right, but a functional part of the entity." ;
.
fibo-be-le-fbo:Division
  rdf:type owl:Class ;
  rdfs:label "division" ;
  rdfs:subClassOf fibo-be-le-fbo:OrganizationalSubUnit ;
  skos:definition "a part of a company, such as a line of business, that may have separate accounting or reporting requirements" ;
.
fibo-be-le-fbo:FormallyConstitutedOrganization
  rdf:type owl:Class ;
  rdfs:label "formally constituted organization" ;
  rdfs:subClassOf fibo-fnd-org-fm:FormalOrganization ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-be-le-fbo:OrganizationIdentifier ;
      owl:onProperty fibo-fnd-aap-agt:isIdentifiedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-be-le-fbo:OrganizationIndustrySectorClassification ;
      owl:onProperty fibo-fnd-rel-rel:isClassifiedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-be-le-fbo:OrganizationCoveringAgreement ;
      owl:onProperty fibo-fnd-rel-rel:isGovernedBy ;
    ] ;
  owl:equivalentClass fibo-be-lex-fox:FormallyConstitutedOrganization ;
  owl:equivalentClass fibo-fnd-inf-doc:FormallyConstitutedOrganization ;
  owl:equivalentClass fibo-fnd-plcx-sit:ContractuallyConstitutedOrganization ;
  owl:equivalentClass fibo-loan-typ-com:FormallyConstitutedOrganization ;
  owl:equivalentClass fibo-sec-dbt-mm-trd:FormallyConstitutedOrganization ;
  skos:definition "Any organization with some formal contractual agreement among its principals" ;
.
fibo-be-le-fbo:FormallyConstitutedOrganizationMember
  rdf:type owl:Class ;
  rdfs:label "formally constituted organization member" ;
  rdfs:subClassOf fibo-fnd-pty-pty:OrganizationMember ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
      owl:someValuesFrom [
          rdf:type owl:Restriction ;
          owl:allValuesFrom fibo-be-le-fbo:FormallyConstitutedOrganization ;
          owl:onProperty fibo-fnd-rel-rel:isMemberOf ;
        ] ;
    ] ;
  skos:definition "A member of some formally constituted organization." ;
  skos:editorialNote "This party may hold some equity in the formally constituted organization and may or may not have some responsibilities or potential liabilitiy in respect of the activities of that organization. Terms setting out the position of the member in these and other matters are defined in the organization covering agreement, to which the member may be a signatory." ;
.
fibo-be-le-fbo:JointVenture
  rdf:type owl:Class ;
  rdfs:label "joint venture" ;
  rdfs:subClassOf fibo-be-le-fbo:FormallyConstitutedOrganization ;
  skos:definition "A formal venture between two or more business entities" ;
  skos:editorialNote "Detailed properties still to be modeled. This will be similar to Partnership in that it will have two or more venture partners (need to determine best label for these), and some formal standing. Also to research: whether JVs are only instituted via mutual share ownership and therefore may only be between limited companies (or may only be a limited company but may have other types of legal person and/or legal entity as venture partners)." ;
.
fibo-be-le-fbo:NonGovernmentalOrganization
  rdf:type owl:Class ;
  rdfs:label "non-governmental organization" ;
  rdfs:subClassOf fibo-be-le-fbo:NotForProfitOrganization ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-be-le-lp:hasObjective ;
      owl:someValuesFrom fibo-be-le-lp:PublicPurpose ;
    ] ;
  skos:definition "a not for profit organization that is a citizen-based group that functions independently of government" ;
  fibo-fnd-utl-av:abbreviation "NGO" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.investopedia.com/ask/answers/13/what-is-non-government-organization.asp"^^xsd:anyURI ;
  fibo-fnd-utl-av:adaptedFrom "https://en.wikipedia.org/wiki/Non-governmental_organization"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "NGOs are neither a part of a government nor a conventional for-profit business.  Usually set up by ordinary citizens, NGOs may be funded by governments, foundations, businesses, or private persons." ;
  fibo-fnd-utl-av:explanatoryNote "NGOs, sometimes called civil societies, are organized on community, national and international levels to serve specific social or political purposes, and are cooperative, rather than commercial, in nature." ;
  fibo-fnd-utl-av:explanatoryNote "Some NGOs avoid formal funding altogether and are run primarily by volunteers. NGOs are highly diverse groups of organizations engaged in a wide range of activities, and take different forms in different parts of the world. Some may have charitable status, while others may be registered for tax exemption based on recognition of social purposes. Others may be fronts for political, religious, or other interests." ;
.
fibo-be-le-fbo:NotForProfitOrganization
  rdf:type owl:Class ;
  rdfs:label "not for profit organization" ;
  rdfs:subClassOf fibo-fnd-org-fm:FormalOrganization ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-be-le-lp:hasObjective ;
      owl:someValuesFrom fibo-be-le-lp:NotForProfitObjective ;
    ] ;
  owl:disjointWith [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-be-le-lp:hasObjective ;
      owl:someValuesFrom fibo-be-le-lp:ProfitObjective ;
    ] ;
  skos:definition "an organization that uses its surplus revenues to further achieve its purpose or mission, rather than distributing its surplus income to the organization's owners (directors, investors, or equivalents) as profit or dividends" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
  fibo-fnd-utl-av:adaptedFrom "https://en.wikipedia.org/wiki/Nonprofit_organization"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "In the US, a nonprofit organization is an association that explicitly is not required to pay taxes on its income.  Such organizations are qualified for this exemption due to their socially desirable objective (e.g. hospitals, charitable organizations, etc., or because they meet some set of requirements as determined by the US Internal Revenue Service." ;
  fibo-fnd-utl-av:explanatoryNote "The nonprofit landscape is highly varied, although many people have come to associate NPOs with charitable organizations. Although charities do comprise an often high profile or visible aspect of the sector, there are many other types of nonprofits. Overall, they tend to be either member-serving or community-serving. Member-serving organizations include mutual societies, cooperatives, trade unions, credit unions, industry associations, sports clubs, retired serviceman's clubs and other organizations that benefit a particular group of people - the members of the organization. Typically, community-serving organizations are focused on providing services to the community in general, either globally or locally: organizations delivering human services programs or projects, aid and development programs, medical research, education and health services, and so on." ;
  fibo-fnd-utl-av:synonym "non-profit organization" ;
.
fibo-be-le-fbo:OrganizationCoveringAgreement
  rdf:type owl:Class ;
  rdfs:label "organization covering agreement" ;
  rdfs:subClassOf fibo-fnd-agr-ctr:WrittenContract ;
  skos:definition "A formal agreement between the principals in a formal organization which covers the relationship between the principals, and between the principals and the entity." ;
  skos:editorialNote "Also covers the aims and purposes of the Entity." ;
.
fibo-be-le-fbo:OrganizationIdentificationScheme
  rdf:type owl:Class ;
  rdfs:label "organization identification scheme" ;
  rdfs:subClassOf fibo-fnd-arr-id:IdentificationScheme ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-be-le-fbo:OrganizationIdentifier ;
      owl:onProperty fibo-fnd-rel-rel:defines ;
    ] ;
  skos:definition "A formal scheme defining identification codes for a business entity." ;
.
fibo-be-le-fbo:OrganizationIdentifier
  rdf:type owl:Class ;
  rdfs:label "organization identifier" ;
  rdfs:subClassOf fibo-fnd-arr-id:Identifier ;
  skos:definition "A textual code which uniquely identifies a formal organization; Further Notes: Things which may identified by this kind of code Include but are not restricted to Legal Entity. At this most general level, this may be the identifier for a Fund, for a Legal Entity, for a Trust, or for the LEI scope of Artificial Person plus Trust." ;
.
fibo-be-le-fbo:OrganizationIndustrySectorClassification
  rdf:type owl:Class ;
  rdfs:label "organization industry sector classification" ;
  rdfs:subClassOf fibo-fnd-rel-rel:Reference ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-be-le-fbo:FormallyConstitutedOrganization ;
      owl:onProperty fibo-fnd-rel-rel:classifies ;
    ] ;
  skos:definition "The classification of an organization in terms of the industry sector in which it carries out business." ;
.
fibo-be-le-fbo:OrganizationalSubUnit
  rdf:type owl:Class ;
  rdfs:label "organizational sub-unit" ;
  rdfs:subClassOf fibo-fnd-org-fm:FormalOrganization ;
  owl:equivalentClass fibo-be-fctx-rep:OrganizationalSubUnit ;
  skos:definition "an organization that is part of a larger formal organization and only has full recognition within the context of that formal organization; In other words, it is not a legal entity in its own right." ;
.
fibo-be-le-fbo:RegisteredAddress
  rdf:type owl:Class ;
  rdfs:label "registered address" ;
  rdfs:subClassOf fibo-fnd-plc-adr:PostalAddress ;
  skos:definition "The address at which an entity is registered and at which legal papers may be served." ;
  skos:editorialNote "This usually has to not be a postal address (except in some jurisdictions). So there should be a common superclass." ;
.
fibo-be-le-fbo:hasEquity
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-be-le-fbo:FormallyConstitutedOrganization ;
  rdfs:label "has equity" ;
  rdfs:range fibo-fnd-acc-aeq:StockholdersEquity ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "the formally constituted organization has some form of stockholders equity" ;
.
fibo-be-le-fbo:hasInternalEntity
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-be-le-fbo:FormallyConstitutedOrganization ;
  rdfs:label "has internal entity" ;
  rdfs:range fibo-be-le-fbo:OrganizationalSubUnit ;
  rdfs:subPropertyOf fibo-be-le-fbo:hasSubUnit ;
  skos:definition "the organization has some part which is formally defined as an Internal Entity, for example for reporting, management or accounting purposes" ;
.
fibo-be-le-fbo:hasOperatingAddress
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-be-le-fbo:FormallyConstitutedOrganization ;
  rdfs:label "has operating address" ;
  rdfs:range fibo-fnd-plc-adr:Address ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "address at which the organization carries out its operations" ;
.
fibo-be-le-fbo:hasPrimaryAddress
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-be-le-fbo:FormallyConstitutedOrganization ;
  rdfs:label "has primary address" ;
  rdfs:range fibo-fnd-plc-adr:Address ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "The main address at which communications to the organization may be addressed; its headquarters address." ;
.
fibo-be-le-fbo:hasRegisteredAddress
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-be-le-fbo:FormallyConstitutedOrganization ;
  rdfs:label "has registered address" ;
  rdfs:range fibo-be-le-fbo:RegisteredAddress ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "The organization has an address at which it is registered and at which legal papers may be served on it." ;
.
fibo-be-le-fbo:hasSignatory
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-be-le-fbo:FormallyConstitutedOrganization ;
  rdfs:label "has signatory" ;
  rdfs:range fibo-be-le-lp:Signatory ;
  rdfs:subPropertyOf fibo-fnd-pty-pty:hasPartyInRole ;
  skos:definition "has a party which is authorized to sign contracts on behalf of the entity" ;
.
fibo-be-le-fbo:hasSubUnit
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-be-le-fbo:FormallyConstitutedOrganization ;
  rdfs:label "has sub-unit" ;
  rdfs:range fibo-be-le-fbo:OrganizationalSubUnit ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:hasPart ;
  owl:inverseOf fibo-be-le-fbo:isSubUnitOf ;
  skos:definition "The company has a branch, which is part of that company. This may be in another country or in the country in which that company is registered (incorporated)." ;
.
fibo-be-le-fbo:isSubUnitOf
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-be-le-fbo:OrganizationalSubUnit ;
  rdfs:label "is sub-unit of" ;
  rdfs:range fibo-be-le-fbo:FormallyConstitutedOrganization ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:isPartOf ;
  skos:definition "The organization is a sub-unit of a formally constituted organization and has the role and function of being a formal part of that organization, such as a branch." ;
.
fibo-be-le-lei:
  terms:license "http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense"^^xsd:anyURI ;
  sm:contentLanguage "http://www.omg.org/spec/ODM/"^^xsd:anyURI ;
  sm:contentLanguage "http://www.w3.org/standards/techs/owl#w3c_all"^^xsd:anyURI ;
  sm:copyright """Copyright (c) 2013-2016 EDM Council, Inc.
Copyright (c) 2013-2016 Object Management Group, Inc.""" ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/BE/GovernmentEntities/GovernmentEntities/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/BE/LegalEntities/FormalBusinessOrganizations/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/BE/LegalEntities/LegalPersons/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/"^^xsd:anyURI ;
  sm:fileAbbreviation "fibo-be-le-lei" ;
  sm:fileAbstract "This ontology defines concepts around contractually capable business entities. The terms defined here are those which are relevant to the Legal Entity Identifier (LEI) work. The term known as legal entity in that work is identified as a formal organization which is recognized in some jurisdiction as being capable of incurring some liability, whether or not is a legal person as understood by the legal community. This is labeled as contractually capable entity, to avoid confusion with the accepted legal term for Legal Entity. Such entities are recognized as requiring an LEI, but the identifier itself is allocated to the formal organization which is recognized as being contractually capable." ;
  sm:filename "LEIEntities.rdf" ;
  rdfs:label "Legal Entity Identifier (LEI) Entities Ontology" ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/BE/AboutBE/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/BE/LegalEntities/AboutLegalEntities/"^^xsd:anyURI ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/BE/20160801/LegalEntities/LEIEntities/> ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/BE/20131101/LegalEntities/LEIEntities.rdf version of this ontology was modified per the issue resolutions identified in the FIBO BE 1.0 FTF report." ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/BE/20150201/LegalEntities/LEIEntities.rdf version of this ontology was modified per the issue resolutions identified in the FIBO BE 1.1 RTF report. Changes include deprecation of the MunicipalEntity, Sovereign, and SupranationalEntity classes and making them equivalent to classes in the Government Entities ontology." ;
.
fibo-be-le-lei:ContractuallyCapableEntity
  rdf:type owl:Class ;
  rdfs:label "contractually capable entity" ;
  rdfs:subClassOf fibo-fnd-pty-rl:AgentInRole ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-be-le-lp:LegalEntity ;
      owl:onProperty fibo-fnd-rel-rel:hasIdentity ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  owl:equivalentClass fibo-be-lex-lec:ContractuallyCapableEntity ;
  skos:definition "A unique entity that is legally or financially responsible for the performance of financial transactions, or has the legal right in its jurisdiction to enter independently into legal contracts, regardless of whether it is incorporated or constituted in some other way (e.g. trust, partnership, contractual). This excludes natural persons, but includes governmental organizations and supranationals." ;
  fibo-fnd-utl-av:adaptedFrom "ISO 17442" ;
.
fibo-be-le-lei:LEICapableEntity
  rdf:type owl:Class ;
  rdfs:subClassOf owl:Thing ;
.
fibo-be-le-lei:LegalEntityIdentifier
  rdf:type owl:Class ;
  rdfs:label "legal entity identifier" ;
  rdfs:subClassOf fibo-be-le-fbo:OrganizationIdentifier ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-be-le-lp:LegalEntity ;
      owl:onProperty fibo-fnd-aap-agt:identifies ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "an organization identifier that uniquely identifies a legal entity as defined in ISO 17442" ;
.
fibo-be-le-lei:LegalEntityIdentifierScheme
  rdf:type owl:Class ;
  rdfs:label "legal entity identifier scheme" ;
  rdfs:subClassOf fibo-be-le-fbo:OrganizationIdentificationScheme ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-be-le-lei:LegalEntityIdentifier ;
      owl:onProperty fibo-fnd-rel-rel:defines ;
    ] ;
  skos:definition "the scheme that defines the LEI" ;
.
fibo-be-le-lei:MunicipalEntity
  rdf:type owl:Class ;
  rdfs:label "municipal entity" ;
  rdfs:subClassOf fibo-be-ge-ge:Polity ;
  owl:deprecated "true"^^xsd:boolean ;
  owl:equivalentClass fibo-be-ge-ge:MunicipalEntity ;
  skos:definition "Municipal entity in the sense of a legal entity, that is, what it is that incurs debt for a municipality, i.e. the Municipality in the sense of the legal entity, as distinct from the Municipal government. Draft definition: The legal entity which is a city or municipality. This has a Government which sets laws applicable within the geographical area corresponding to this legal entity. It is identified as a legal entity as well as the setter of laws because it is able to incur debt in its own right." ;
.
fibo-be-le-lei:Sovereign
  rdf:type owl:Class ;
  rdfs:label "sovereign" ;
  rdfs:subClassOf fibo-be-ge-ge:Polity ;
  owl:deprecated "true"^^xsd:boolean ;
  owl:equivalentClass fibo-be-ge-ge:SovereignState ;
  skos:definition "the legal person corresponding to a state or country" ;
.
fibo-be-le-lei:SupranationalEntity
  rdf:type owl:Class ;
  rdfs:label "supranational entity" ;
  rdfs:subClassOf fibo-be-ge-ge:Polity ;
  owl:deprecated "true"^^xsd:boolean ;
  owl:equivalentClass fibo-be-ge-ge:SupranationalEntity ;
  skos:definition "a governmental or non-governmental entity that is established by international law or treaty or incorporated at an international level; an international organization, or union, whereby member states transcend national boundaries or interests to share in the decision-making and vote on issues pertaining to the wider grouping" ;
  fibo-fnd-utl-av:adaptedFrom "ISO 17442, Financial services - Legal Entity Identifier (LEI), section 3.2" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.investopedia.com/terms/s/supranational.asp"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "The European Union and the World Trade Organization are both supranationals. In the EU, each member votes on policy that will affect each member nation. The benefits of this construct for the EU are the synergies derived from social and economic policies along with a stronger presence on the international stage." ;
.
fibo-be-le-lei:hasAddressOfLegalFormation
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-be-le-lp:LegalEntity ;
  rdfs:label "has address of legal formation" ;
  rdfs:range fibo-be-le-fbo:RegisteredAddress ;
  rdfs:subPropertyOf fibo-be-le-fbo:hasRegisteredAddress ;
  skos:definition "indicates the address of legal formation for the entity, in the jurisdiction in which the entity is established, used for registration purposes with respect to obtaining an LEI" ;
.
fibo-be-le-lp:
  terms:license "http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense"^^xsd:anyURI ;
  sm:contentLanguage "http://www.omg.org/spec/ODM/"^^xsd:anyURI ;
  sm:contentLanguage "http://www.w3.org/standards/techs/owl#w3c_all"^^xsd:anyURI ;
  sm:copyright """Copyright (c) 2013-2016 EDM Council, Inc.
Copyright (c) 2013-2016 Object Management Group, Inc.""" ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/"^^xsd:anyURI ;
  sm:fileAbbreviation "fibo-be-le-lp" ;
  sm:fileAbstract "This ontology defines legal personhood concepts. A legal person as defined here is any natural person or organization which is capable of accruing liability on its own part." ;
  sm:filename "LegalPersons.rdf" ;
  rdfs:label "Legal Persons Ontology" ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/BE/AboutBE/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/BE/LegalEntities/AboutLegalEntities/"^^xsd:anyURI ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/BE/20160201/LegalEntities/LegalPersons/> ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/BE/20131101/LegalEntities/LegalPersons.rdf version of this ontology was modified per the issue resolutions identified in the FIBO BE 1.0 FTF report." ;
.
fibo-be-le-lp:BusinessEntity
  rdf:type owl:Class ;
  rdfs:label "business entity" ;
  rdfs:subClassOf fibo-fnd-org-fm:FormalOrganization ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-law-lcap:License ;
      owl:onProperty fibo-fnd-rel-rel:holds ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-be-le-lp:hasObjective ;
      owl:someValuesFrom fibo-be-le-lp:BusinessObjective ;
    ] ;
  skos:definition "an entity that is formed and administered as per commercial law in order to engage in business activities" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
  fibo-fnd-utl-av:adaptedFrom "http://en.wikipedia.org/wiki/Types_of_business_entity"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "There are many types of business entities defined in the legal systems of various countries. These include corporations, cooperatives, partnerships, sole proprietorships, sole traders, limited liability companies, certain trusts and trust companies, and so forth. The rules vary by country and by state or province. Some of the more widely recognized types in the US, UK, and EU are defined in FIBO, by region. However, the regulations governing particular types of entity, even those described as roughly equivalent, differ from jurisdiction to jurisdiction." ;
.
fibo-be-le-lp:BusinessLicense
  rdf:type owl:Class ;
  rdfs:label "business license" ;
  rdfs:subClassOf fibo-fnd-law-lcap:License ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-be-le-lp:BusinessEntity ;
      owl:onProperty fibo-fnd-rel-rel:appliesTo ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-dt-fd:DatePeriod ;
      owl:onProperty fibo-fnd-dt-bd:holdsDuring ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-be-le-lp:isRecognizedIn ;
      owl:someValuesFrom fibo-fnd-law-jur:Jurisdiction ;
    ] ;
  skos:definition "a license that allows the holder to conduct business or carry out a specific profession within some jurisdiction for some period of time" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-be-le-lp:BusinessObjective
  rdf:type owl:Class ;
  rdfs:label "business objective" ;
  rdfs:subClassOf fibo-fnd-gao-obj:Objective ;
  skos:definition "an objective that reflects the strategic goals and direction of a business" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
  fibo-fnd-utl-av:explanatoryNote "Business objectives allow an organization to define its goals and direction. A company uses strategy and tactics at every level of its operation to achieve its objectives. These define the way a company allocates its resources and the strengths, weaknesses and opportunities it may have. Companies usually do not alter their objectives once they are implemented, unless changes in circumstances arise. Setting a clear course for the organization is key to its success." ;
.
fibo-be-le-lp:CharteredLegalPerson
  rdf:type owl:Class ;
  rdfs:label "chartered legal person" ;
  rdfs:subClassOf fibo-be-le-lp:JuridicalPerson ;
  skos:definition "a legal person created by a royal charter or decree" ;
  skos:editorialNote "In a monarchy or principality, the monarch typically vests the power to create such bodies, in an entity called (for example) the Privy Council." ;
  skos:example "Anything with 'Royal Institute' in the name. Also universities are generally set up by royal charter in a monarchy or principality, (often pre-dating any Privy Council i.e. directly be the monarch in the case of older universities). The Bank of England and the British Broadcasting Council (BBC) are also incorporated through Royal Charter." ;
.
fibo-be-le-lp:JuridicalPerson
  rdf:type owl:Class ;
  rdfs:label "juridical person" ;
  rdfs:subClassOf fibo-be-le-lp:LegalEntity ;
  rdfs:subClassOf fibo-fnd-org-fm:FormalOrganization ;
  skos:definition "an entity, as a firm, that is not a single natural person, as a human being, authorized by law with duties and rights, recognized as a legal authority having a distinct identity, a legal personality" ;
  fibo-fnd-utl-av:definitionOrigin "Black's Law Dictionary Free Online, see http://thelawdictionary.org/juridical-person/" ;
  fibo-fnd-utl-av:synonym "artificial person" ;
  fibo-fnd-utl-av:synonym "juridical entity" ;
  fibo-fnd-utl-av:synonym "juristic person" ;
.
fibo-be-le-lp:LegalEntity
  rdf:type owl:Class ;
  rdfs:label "legal entity" ;
  rdfs:subClassOf fibo-be-le-lp:LegalPerson ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-be-le-lei:LegalEntityIdentifier ;
      owl:onProperty fibo-fnd-aap-agt:isIdentifiedBy ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-law-jur:Jurisdiction ;
      owl:onProperty fibo-be-le-lp:isOrganizedIn ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  owl:disjointWith fibo-be-le-lp:NaturalPerson ;
  skos:definition "a partnership, corporation, or other organization having the capacity to negotiate contracts, assume financial obligations, and pay off debts, organized under the laws of some jurisdiction" ;
  skos:example """Examples of eligible legal entities include, without limitation:
-   all financial intermediaries;
-   banks and finance companies;
-   all entities that issue equity, debt or other securities for other capital structures;
-   all entities listed on an exchange;
-   all entities that trade stock or debt, investment vehicles, including mutual funds, pension funds and alternative investment vehicles constituted as corporate entities or collective investment agreements (including umbrella funds as well as funds under an umbrella structure, hedge funds, private equity funds, etc.);
-   all entities under the purview of a financial regulator and their affiliates, subsidiaries and holding companies;
-   counterparties to financial transactions.""" ;
  skos:scopeNote "The term 'legal entities' includes, but is not limited to, unique parties that are legally or financially responsible for the performance of financial transactions or have the legal right in their jurisdiction to enter independently into legal contracts, regardless of whether they are incorporated or constituted in some other way (e.g. trust, partnership, contractual). It excludes natural persons, but includes governmental organizations and supranationals." ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Banking Terms, Sixth Edition, 2012, definition of legal entity" ;
  fibo-fnd-utl-av:adaptedFrom "ISO 17442, Financial services - Legal Entity Identifier (LEI), first edition, 2012-06-01, section 3.1" ;
.
fibo-be-le-lp:LegalPerson
  rdf:type owl:Class ;
  rdfs:label "legal person" ;
  rdfs:subClassOf fibo-fnd-aap-agt:AutonomousAgent ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-law-jur:Jurisdiction ;
      owl:onProperty fibo-be-le-lp:isRecognizedIn ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-law-lcap:LiabilityCapacity ;
      owl:onProperty fibo-fnd-law-lcap:hasCapacity ;
    ] ;
  owl:equivalentClass fibo-fnd-soc-asp:LegalPerson ;
  skos:definition "any entity which can incur legal obligation and can be sued at law" ;
  fibo-fnd-utl-av:adaptedFrom "http://en.wikipedia.org/wiki/Legal_personality"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "To have legal personality means to be capable of having legal rights and duties within a certain legal system, such as to enter into contracts, sue, and be sued. Legal personality is a prerequisite to legal capacity, the ability of any legal person to amend (enter into, transfer, etc.) rights and obligations." ;
.
fibo-be-le-lp:NaturalPerson
  rdf:type owl:Class ;
  rdfs:label "natural person" ;
  rdfs:subClassOf fibo-be-le-lp:LegalPerson ;
  rdfs:subClassOf fibo-fnd-aap-ppl:Adult ;
  owl:disjointWith fibo-fnd-aap-ppl:IncapacitatedAdult ;
  owl:equivalentClass fibo-loan-typ-com:NaturalPerson ;
  skos:definition "A person of maturity who in the eyes of the law is able to assume obligations." ;
.
fibo-be-le-lp:NotForProfitObjective
  rdf:type owl:Class ;
  rdfs:label "not for profit objective" ;
  rdfs:subClassOf fibo-fnd-gao-obj:Objective ;
  owl:disjointWith fibo-be-le-lp:ProfitObjective ;
  skos:definition "an objective that reflects the charitable, educational, religious, humanitarian, public services, or other not for profit goals of an organization" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.investopedia.com/terms/n/not-for-profit.asp"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "The objective of all business activities is not to earn profits for its owners. All of the money earned by or donated to a not for profit organization is used in pursuing the organization's objectives." ;
  fibo-fnd-utl-av:synonym "nonprofit objective" ;
.
fibo-be-le-lp:PowerOfAttorney
  rdf:type owl:Class ;
  rdfs:label "power of attorney" ;
  rdfs:subClassOf fibo-fnd-law-lcap:LegalCapacity ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-be-le-lp:NaturalPerson ;
      owl:onProperty fibo-fnd-rel-rel:isConferredOn ;
    ] ;
  skos:definition "Written legal authority given by one party (the principal) to another (the agent or attorney-in-fact) to act on the principals behalf. It may be a (1) General power Of attorney that authorizes the agent to act generally on behalf of the principal, or (2) Special power of attorney that is limited to a specific act or situation. Decisions made and actions taken by an attorney in fact (within the scope of his or her authority) are legally binding on the principal. A power of attorney can remain in force only so long as the principal is alive and does not become disabled or incapacitated. Although it is often conferred on the principals attorney, one does not need to be an attorney at law to be an attorney-in-fact." ;
  fibo-fnd-utl-av:definitionOrigin "http://www.businessdictionary.com/definition/power-of-attorney.html"^^xsd:anyURI ;
.
fibo-be-le-lp:ProfitObjective
  rdf:type owl:Class ;
  rdfs:label "profit objective" ;
  rdfs:subClassOf fibo-be-le-lp:BusinessObjective ;
  skos:definition "an objective that reflects pursuit of a financial benefit that may be realized when the amount of revenue gained from a business activity exceeds the expenses, costs and taxes needed to sustain that activity" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.investopedia.com/terms/p/profit.asp"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "Any profit that is gained goes to the business's owners, who may or may not decide to spend it on the business." ;
  fibo-fnd-utl-av:synonym "for profit objective" ;
  fibo-fnd-utl-av:synonym "profit motive" ;
.
fibo-be-le-lp:PublicPurpose
  rdf:type owl:Class ;
  rdfs:label "public purpose" ;
  rdfs:subClassOf fibo-fnd-gao-obj:Objective ;
  skos:definition "an objective that reflects values generally thought to be shared by and that is intended to benefit the populace as a whole" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.yourdictionary.com/public-purpose"^^xsd:anyURI ;
  fibo-fnd-utl-av:synonym "public interest" ;
.
fibo-be-le-lp:ReligiousObjective
  rdf:type owl:Class ;
  rdfs:label "religious objective" ;
  rdfs:subClassOf fibo-be-le-lp:NotForProfitObjective ;
  skos:definition "a not for profit objective that reflects the religious goals of an organization" ;
.
fibo-be-le-lp:Signatory
  rdf:type owl:Class ;
  rdfs:label "signatory" ;
  rdfs:subClassOf fibo-fnd-pty-pty:PartyInRole ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-be-le-lp:NaturalPerson ;
      owl:onProperty fibo-fnd-rel-rel:hasIdentity ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
      owl:someValuesFrom [
          rdf:type owl:Restriction ;
          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
          owl:onClass fibo-fnd-law-lcap:SignatoryCapacity ;
          owl:onProperty fibo-fnd-law-lcap:hasCapacity ;
        ] ;
    ] ;
  skos:definition "Some agent who has the capacity to sign contracts on the part of some legal person" ;
.
fibo-be-le-lp:StatutoryBody
  rdf:type owl:Class ;
  rdfs:label "statutory body" ;
  rdfs:subClassOf fibo-be-le-lp:JuridicalPerson ;
  skos:definition "a body set up by a government to consider evidence and make judgements in some field of activity" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.collinsdictionary.com/dictionary/english/statutory-body"^^xsd:anyURI ;
.
fibo-be-le-lp:designatesSignatory
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-be-le-lp:LegalPerson ;
  rdfs:label "designates signatory" ;
  rdfs:range fibo-be-le-lp:Signatory ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:designates ;
  skos:definition "designates a party able to sign contracts on the part of the legal person" ;
.
fibo-be-le-lp:hasObjective
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-aap-agt:AutonomousAgent ;
  rdfs:label "has objective" ;
  rdfs:range fibo-fnd-gao-obj:Objective ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "relates an agent to a specific objective (result) that the agent aims to achieve within a time frame and with available resources" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.businessdictionary.com/definition/objective.html"^^xsd:anyURI ;
.
fibo-be-le-lp:isOrganizedIn
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-be-le-lp:LegalEntity ;
  rdfs:label "is organized in" ;
  rdfs:range fibo-fnd-law-jur:Jurisdiction ;
  rdfs:subPropertyOf fibo-be-le-lp:isRecognizedIn ;
  skos:definition "indicates the juridiction whose laws a legal entity is organized under" ;
.
fibo-be-le-lp:isRecognizedIn
  rdf:type owl:ObjectProperty ;
  rdfs:domain [
      rdf:type owl:Class ;
      owl:unionOf (
          fibo-be-le-lp:LegalPerson
          fibo-fnd-agr-agr:Agreement
        ) ;
    ] ;
  rdfs:label "is recognized in" ;
  rdfs:range fibo-fnd-law-jur:Jurisdiction ;
  skos:definition "indicates the jurisdiction in which a legal person is authorized to conduct business or an agreement may be acknowledged and possibly enforceable" ;
.
