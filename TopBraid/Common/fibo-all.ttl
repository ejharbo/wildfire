  rdfs:label "interest rate swap" ;
  rdfs:subClassOf fibo-der-drc-swp:Swap ;
  rdfs:subClassOf fibo-der-rtd-rtd:InterestRateDerivativeInstrument ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-der-rtd-irswp:InterestRateSwapLeg ;
      owl:onProperty fibo-der-drc-swp:exchanges ;
      owl:qualifiedCardinality "2"^^xsd:nonNegativeInteger ;
    ] ;
  owl:equivalentClass [
      rdf:type owl:Class ;
      owl:intersectionOf (
          fibo-der-drc-swp:Swap
          [
            rdf:type owl:Restriction ;
            owl:onClass fibo-der-rtd-irswp:InterestRateSwapLeg ;
            owl:onProperty fibo-der-drc-swp:hasLeg ;
