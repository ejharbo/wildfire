.
fibo-sec-sec-lst:ExchangeAndListingService
  rdf:type owl:Class ;
  rdfs:label "exchange and listing service" ;
  rdfs:subClassOf fibo-fbc-pas-fpas:FinancialService ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-sec-sec-lst:SecuritiesExchange ;
      owl:onProperty fibo-fnd-pas-pas:isProvisionedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-sec-sec-lst:SecuritiesExchange ;
      owl:onProperty fibo-fnd-rel-rel:isProvidedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-sec-sec-lst:SecurityListing ;
      owl:onProperty fibo-fnd-rel-rel:provides ;
    ] ;
  skos:definition "a trading and (optionally) listing service for the purposes of securities trading" ;
.
fibo-sec-sec-lst:ExchangeIdentifier
  rdf:type owl:Class ;
  rdfs:label "exchange identifier" ;
  rdfs:subClassOf fibo-fbc-fct-mkt:MarketIdentifier ;
  skos:definition "an identifier allocated to a securities exchange" ;
.
fibo-sec-sec-lst:ExchangeTradedSecurity
  rdf:type owl:Class ;
  rdfs:label "exchange traded security" ;
  rdfs:subClassOf fibo-sec-sec-lst:ListedSecurity ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-sec-lst:isIssuedIn ;
      owl:someValuesFrom fibo-sec-sec-lst:SecuritiesExchange ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-sec-lst:isTradedOn ;
      owl:someValuesFrom fibo-sec-sec-lst:SecuritiesExchange ;
    ] ;
  owl:equivalentClass fibo-sec-sec-sec:ExchangeTradedSecurity ;
  skos:definition "a listed security that may be traded on a particular exchange" ;
.
fibo-sec-sec-lst:FinancialMarket
  rdf:type owl:Class ;
  rdfs:label "financial market" ;
  rdfs:subClassOf fibo-fnd-bus-srv:Market ;
.
fibo-sec-sec-lst:ListedSecurity
  rdf:type owl:Class ;
  rdfs:label "listed security" ;
  rdfs:subClassOf fibo-sec-sec-lst:RegisteredSecurity ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-dt-fd:Date ;
      owl:onProperty fibo-sec-sec-lst:hasListingDate ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-acc-cur:hasCurrency ;
      owl:someValuesFrom fibo-fnd-acc-cur:Currency ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-sec-lst:isListedVia ;
      owl:someValuesFrom fibo-sec-sec-lst:SecurityListing ;
    ] ;
  skos:definition "a registered security listed on a specific exchange" ;
.
fibo-sec-sec-lst:MultilateralTradingFacility
  rdf:type owl:Class ;
  rdfs:label "multilateral trading facility" ;
  rdfs:subClassOf fibo-sec-sec-lst:SecuritiesTradingFacility ;
.
fibo-sec-sec-lst:NotionalFinancialMarketplace
  rdf:type owl:Class ;
  rdfs:label "notional financial marketplace" ;
  rdfs:subClassOf fibo-fbc-fct-mkt:Market ;
  rdfs:subClassOf fibo-fnd-plc-vrt:NotionalPlace ;
  rdfs:subClassOf fibo-fnd-plcx-vrtx:NotionalPlace ;
  skos:definition "a place which is identified as a financial market but which is not a physical place such as a country or region. This may be a conceptual place such as the Eurobond market or the Global market." ;
  skos:editorialNote "These may still have properties such as holiday calendars, e.g. the London Interbank Market in USD has New York dates fo negotiation but pricing can changes whenever London is open." ;
.
fibo-sec-sec-lst:RegisteredSecurity
  rdf:type owl:Class ;
  rdfs:label "registered security" ;
  rdfs:subClassOf fibo-fbc-fi-fi:Security ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fbc-fct-ra:RegistrationAuthority ;
      owl:onProperty fibo-fbc-fct-ra:isRegisteredBy ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-dt-fd:Date ;
      owl:onProperty fibo-fbc-fct-ra:hasRegistrationDate ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fbc-fi-fi:mayBeTradedIn ;
      owl:someValuesFrom fibo-fbc-fct-mkt:Market ;
    ] ;
  skos:definition "a security that is registered with some registration authority and that may be traded in some trading venue (market)" ;
.
fibo-sec-sec-lst:SecuritiesExchange
  rdf:type owl:Class ;
  rdfs:label "securities exchange" ;
  rdfs:subClassOf fibo-fbc-fct-mkt:Exchange ;
  rdfs:subClassOf fibo-fbc-pas-fpas:FinancialServiceProvider ;
  rdfs:subClassOf fibo-fnd-plc-fac:Facility ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-sec-sec-lst:ExchangeIdentifier ;
      owl:onProperty fibo-fnd-aap-agt:isIdentifiedBy ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pas-pas:provisions ;
      owl:someValuesFrom fibo-sec-sec-lst:ExchangeAndListingService ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pas-pas:provisions ;
      owl:someValuesFrom fibo-sec-sec-lst:SecuritiesExchange ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:provides ;
      owl:someValuesFrom fibo-sec-sec-lst:ExchangeAndListingService ;
    ] ;
  owl:equivalentClass fibo-sec-sec-lisx:SecuritiesExchange ;
  skos:definition "an organized, national exchange where securities, options, and futures contracts may be traded by members for their own accounts and for the accounts of their customers" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Finance and Investment Terms, Ninth Edition, 2014." ;
  fibo-fnd-utl-av:explanatoryNote """A securities exchange is a corporation or mutual organization that provides securities trading services, where securities may be bought and sold by third parties. As a facility, a securities exchange is also a place of trade associated with a particular site, i.e., stock exchange, regulated market such as an Electronic Trading Platform (ECN), or unregulated market, such as an Automated Trading System (ATS) (MIC - ISO 10383). Stock exchanges also provide facilities for the issue and redemption of securities as well as other financial instruments and capital events including the payment of income and dividends.

The securities traded on a stock exchange include: shares issued by companies, unit trusts, derivatives, pooled investment products and bonds. To be able to trade a security on a certain stock exchange, it has to be listed there. Usually there is a central location at least for recordkeeping, but trade is less and less linked to such a physical place, as modern markets are electronic networks, which gives them advantages of speed and cost of transactions. Trade on an exchange is by members only.""" ;
.
fibo-sec-sec-lst:SecuritiesTradingFacility
  rdf:type owl:Class ;
  rdfs:label "securities trading facility" ;
  rdfs:subClassOf fibo-fnd-bus-srv:MarketplaceProvider ;
.
fibo-sec-sec-lst:SecurityListing
  rdf:type owl:Class ;
  rdfs:label "security listing" ;
  rdfs:subClassOf fibo-fbc-fct-ra:RegistrationService ;
  rdfs:subClassOf fibo-fbc-pas-fpas:FinancialService ;
  rdfs:subClassOf fibo-fnd-bus-srv:MarketplaceService ;
  rdfs:subClassOf fibo-fnd-plc-fac:Capability ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-sec-sec-rst:SecuritiesRestriction ;
      owl:onProperty fibo-sec-sec-rst:hasRestriction ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-sec-sec-lst:ListedSecurity ;
      owl:onProperty fibo-sec-sec-lst:lists ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:isProvidedBy ;
      owl:someValuesFrom fibo-sec-sec-lst:SecuritiesExchange ;
    ] ;
  skos:definition "a capability for listing a security provided by a securities trading facility" ;
  fibo-fnd-utl-av:explanatoryNote "extends the definition of a security listing to identify any relevant restrictions" ;
.
fibo-sec-sec-lst:hasListing
  rdf:type owl:ObjectProperty ;
  rdfs:label "has listing" ;
.
fibo-sec-sec-lst:hasListingDate
  rdf:type owl:ObjectProperty ;
  rdfs:label "has listing date" ;
  rdfs:range fibo-fnd-dt-fd:Date ;
  rdfs:subPropertyOf fibo-fnd-dt-fd:hasDate ;
.
fibo-sec-sec-lst:hasPlaceOfListing
  rdf:type owl:ObjectProperty ;
  rdfs:label "has place of listing" ;
.
fibo-sec-sec-lst:hasSettlementDate
  rdf:type owl:ObjectProperty ;
  rdfs:label "has settlement date" ;
  rdfs:range fibo-fnd-dt-fd:Date ;
  rdfs:subPropertyOf fibo-fnd-dt-fd:hasDate ;
  skos:definition "the date by which an executed order must be settled, either by a buyer paying for the securities with cash or by a seller delivering the securities and receiving the proceeds of the sale for them" ;
  fibo-fnd-utl-av:adaptedFrom "In a 'regular-way' delivery of stocks and bonds, the settlement date is three days after the trade was executed.  For listed options and government securities, settlement is required by the next business day." ;
  fibo-fnd-utl-av:explanatoryNote "In a 'regular-way' delivery of stocks and bonds, the settlement date is three days after the trade was executed.  For listed options and government securities, settlement is required by the next business day." ;
.
fibo-sec-sec-lst:isAt
  rdf:type owl:ObjectProperty ;
  rdfs:label "is at" ;
.
fibo-sec-sec-lst:isIssuedIn
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-sec-lst:ExchangeTradedSecurity ;
  rdfs:label "is issued in" ;
  rdfs:range fibo-sec-sec-lst:SecuritiesExchange ;
  rdfs:subPropertyOf fibo-sec-sec-lst:isTradedOn ;
  skos:definition "identifies the exchange in which a security is issued" ;
.
fibo-sec-sec-lst:isListedVia
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-sec-lst:ListedSecurity ;
  rdfs:label "is listed via" ;
  rdfs:range fibo-sec-sec-lst:SecurityListing ;
  skos:definition "identifies the service responsible for listing the security" ;
.
fibo-sec-sec-lst:isTradedOn
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-sec-lst:ListedSecurity ;
  rdfs:label "is traded on" ;
  rdfs:range fibo-sec-sec-lst:SecuritiesExchange ;
  skos:definition "identifies the trading facility on which the security is traded" ;
.
fibo-sec-sec-lst:issuedIn
  rdf:type owl:ObjectProperty ;
  rdfs:label "issued in" ;
.
fibo-sec-sec-lst:lists
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-sec-lst:SecurityListing ;
  rdfs:label "lists" ;
  rdfs:range fibo-sec-sec-lst:ListedSecurity ;
  skos:definition "identifies a security that is listed by the listing service" ;
.
fibo-sec-sec-lst:tradedOn
  rdf:type owl:ObjectProperty ;
  rdfs:label "traded on" ;
.
fibo-sec-sec-lisx:
  rdfs:label "SecuritiesListingsExt"@en ;
.
fibo-sec-sec-lisx:BulldogMarket
  rdf:type owl:Class ;
  rdfs:label "bulldog market"@en ;
  rdfs:subClassOf fibo-sec-sec-lst:NotionalFinancialMarketplace ;
  skos:definition "As for Yankee bond market but in UK instead of the US."@en ;
.
fibo-sec-sec-lisx:DomesticMarket
  rdf:type owl:Class ;
  rdfs:label "domestic market"@en ;
  rdfs:subClassOf fibo-sec-sec-lst:NotionalFinancialMarketplace ;
  skos:definition "Bonds issued and traded in the country to which the debt and currency apply."@en ;
.
fibo-sec-sec-lisx:EurobondMarket
  rdf:type owl:Class ;
  rdfs:label "eurobond market"@en ;
  rdfs:subClassOf fibo-sec-sec-lst:NotionalFinancialMarketplace ;
  skos:definition "The non domestic market (note this does not relate to the Euro as a currency)."@en ;
.
fibo-sec-sec-lisx:GlobalMarket
  rdf:type owl:Class ;
  rdfs:label "global market"@en ;
  rdfs:subClassOf fibo-sec-sec-lst:NotionalFinancialMarketplace ;
  skos:definition "Definition needed"@en ;
.
fibo-sec-sec-lisx:PrimaryGlobal
  rdf:type owl:Class ;
  rdfs:label "primary global"@en ;
  rdfs:subClassOf fibo-sec-sec-lisx:TradingLocationPreference ;
.
fibo-sec-sec-lisx:PrimaryLocal
  rdf:type owl:Class ;
  rdfs:label "primary local"@en ;
  rdfs:subClassOf fibo-sec-sec-lisx:TradingLocationPreference ;
.
fibo-sec-sec-lisx:SamuraiMarket
  rdf:type owl:Class ;
  rdfs:label "samurai market"@en ;
  rdfs:subClassOf fibo-sec-sec-lst:NotionalFinancialMarketplace ;
  skos:definition "As for Yankee bond market but in Japan instead of the US."@en ;
.
fibo-sec-sec-lisx:SecuritiesExchange
  rdf:type owl:Class ;
  rdfs:label "securities exchange"@en ;
  rdfs:subClassOf fibo-sec-sec-lst:SecuritiesTradingFacility ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-sec-lisx:hasOfficialClosingPriceType ;
      owl:someValuesFrom fibo-md-tem-ins:ClosingPriceDeterminationMethod ;
    ] ;
  skos:definition "A stock exchange, (formerly a securities exchange) is a corporation or mutual organization which provides \"trading\" facilities for stock brokers and traders, to trade stocks and other securities. Stock exchanges also provide facilities for the issue and redemption of securities as well as other financial instruments and capital events including the payment of income and dividends."@en ;
  fibo-fnd-utl-av:explanatoryNote "The securities traded on a stock exchange include: shares issued by companies, unit trusts, derivatives, pooled investment products and bonds. To be able to trade a security on a certain stock exchange, it has to be listed there. Usually there is a central location at least for recordkeeping, but trade is less and less linked to such a physical place, as modern markets are electronic networks, which gives them advantages of speed and cost of transactions. Trade on an exchange is by members only. The initial offering of stocks and bonds to investors is by definition done in the primary market and subsequent trading is do This is distinguished by providing a formal listing for the securities that are traded on that venue. Many exchanges also play a defined role in the issuance of new securities."@en ;
.
fibo-sec-sec-lisx:TradingLocationPreference
  rdf:type owl:Class ;
  rdfs:label "trading location preference"@en ;
  rdfs:subClassOf fibo-fnd-arrx-meth:Preference ;
.
fibo-sec-sec-lisx:YankeeMarket
  rdf:type owl:Class ;
  rdfs:label "yankee market"@en ;
  rdfs:subClassOf fibo-sec-sec-lst:NotionalFinancialMarketplace ;
  skos:definition "A Yankee bond is a bond denominated in U.S. dollars and is publicly issued in the U.S. by foreign banks and corporations."@en ;
.
fibo-sec-sec-lisx:hasOfficialClosingPriceType
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-sec-sec-lisx:SecuritiesExchange ;
  rdfs:label "has official closing price type"@en ;
  rdfs:range fibo-md-tem-ins:ClosingPriceDeterminationMethod ;
  skos:definition "What the exchange determines as the official close price."@en ;
  skos:editorialNote "This may have a number of complex rules such as disregarding certain trades. Europe: KASSA Price, Listino Price in Italy etc. special types based on different exchanges. Not calculation methods as such. Action: look for a list of these"@en ;
.
