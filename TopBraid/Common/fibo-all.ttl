  sm:fileAbstract "This ontology defines abstract concepts for representation of identifiers, identification schemes, indices and indexing schemes for use in other FIBO ontology elements." ;
  sm:filename "IdentifiersAndIndices.rdf" ;
  rdfs:label "Identifiers and Indices Ontology" ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/AboutFND/"^^xsd:anyURI ;
