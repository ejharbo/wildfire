fibo-sec-dbt-ab-cdo:ArbitrageCDO
  rdf:type owl:Class ;
  rdfs:label "arbitrage c d o"@en ;
  rdfs:subClassOf fibo-sec-dbt-ab-cdo:CDODeal ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
