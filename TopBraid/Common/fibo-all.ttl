  sm:fileAbbreviation "fibo-fbc-dae-gty" ;
  sm:fileAbstract "This ontology defines concepts that ..." ;
  sm:filename "Guaranty.rdf" ;
  rdfs:label "Guaranty Ontology" ;
  rdfs:seeAlso <https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/> ;
  rdfs:seeAlso <https://spec.edmcouncil.org/fibo/FBC/AboutFBC/> ;
  rdfs:seeAlso <https://spec.edmcouncil.org/fibo/FBC/DebtAndEquities/AboutDebtAndEquities/> ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/FBC/20170201/DebtAndEquities/Guaranty/> ;
.
fibo-fbc-dae-gty:Guarantor
  rdf:type owl:Class ;
  rdfs:label "guarantor" ;
  rdfs:subClassOf fibo-fnd-agr-ctr:ContractThirdParty ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-be-le-lp:LegalPerson ;
      owl:onProperty fibo-fnd-rel-rel:hasIdentity ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
      owl:someValuesFrom [
          rdf:type owl:Restriction ;
          owl:onProperty fibo-fnd-pty-pty:isAPartyTo ;
          owl:someValuesFrom fibo-fnd-agr-ctr:Contract ;
        ] ;
    ] ;
  owl:disjointWith fibo-fnd-agr-ctr:ContractParty ;
  skos:definition "a party that guarantees, endorses, or provides indemnity for some obligation on behalf of some other party" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fbc-dae-gty:Guaranty
  rdf:type owl:Class ;
  rdfs:label "guaranty" ;
  rdfs:subClassOf fibo-fnd-agr-agr:Commitment ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fbc-dae-gty:Guarantor ;
      owl:onProperty fibo-fbc-dae-gty:isGuaranteedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fbc-dae-gty:PriorityLevel ;
      owl:onProperty fibo-fbc-dae-gty:hasPriorityLevel ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-acc-cur:MonetaryAmount ;
      owl:onProperty fibo-fbc-dae-gty:hasGuaranteedAmount ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-dt-fd:Date ;
      owl:onProperty fibo-fnd-arr-doc:hasExpirationDate ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-dt-fd:DatePeriod ;
      owl:onProperty fibo-fnd-dt-bd:holdsDuring ;
    ] ;
  owl:equivalentClass fibo-sec-dbt-dbt-gtyx:Guaranty ;
  skos:definition "a commitment whereby something, such as a debt, cash flows on a debt instrument (e.g., interest payments), or performance of some obligation, is guaranteed if the person or organization with primary liability fails to perform" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fbc-dae-gty:PriorityLevel
  rdf:type owl:Class ;
  rdfs:label "priority level" ;
  rdfs:subClassOf fibo-fnd-agr-ctr:ContractualElement ;
  skos:definition "the level of priority that guaranty has in the context of the contract, for example for a credit enhancement priority" ;
.
fibo-fbc-dae-gty:hasGuaranteedAmount
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fbc-dae-gty:Guaranty ;
  rdfs:label "has guaranteed amount" ;
  rdfs:range fibo-fnd-acc-cur:MonetaryAmount ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "relates the guaranty to the monetary amount guaranteed" ;
.
fibo-fbc-dae-gty:hasGuarantor
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-agr-ctr:Contract ;
  rdfs:label "has guarantor" ;
  rdfs:range fibo-fbc-dae-gty:Guarantor ;
  rdfs:subPropertyOf fibo-fnd-agr-ctr:hasThirdParty ;
  skos:definition "relates the guarantor to the contract for which they are providing a guaranty" ;
.
fibo-fbc-dae-gty:hasPriorityLevel
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fbc-dae-gty:Guaranty ;
  rdfs:label "has priority level" ;
  rdfs:range fibo-fbc-dae-gty:PriorityLevel ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "relates a guaranty to some level of priority that guaranty has in the context of the contract, for example for a credit enhancement priority" ;
.
fibo-fbc-dae-gty:isGuaranteedBy
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fbc-dae-gty:Guaranty ;
  rdfs:label "is guaranteed by" ;
  rdfs:range fibo-fbc-dae-gty:Guarantor ;
  skos:definition "relates guaranty to the contract guarantor, i.e., to the legal person providing the guaranty" ;
.
fibo-fbc-fi-fi:
  terms:license sm:MITLicense ;
  sm:contentLanguage <http://www.omg.org/spec/ODM/> ;
  sm:contentLanguage <http://www.w3.org/standards/techs/owl#w3c_all> ;
  sm:copyright """Copyright (c) 2015-2017 EDM Council, Inc.
Copyright (c) 2015-2017 Object Management Group, Inc.""" ;
  sm:dependsOn <https://spec.edmcouncil.org/fibo/BE/> ;
  sm:dependsOn fibo-fbc-fct-breg: ;
  sm:dependsOn fibo-fbc-fct-mkt: ;
  sm:dependsOn <https://spec.edmcouncil.org/fibo/FND/> ;
  sm:fileAbbreviation "fibo-fbc-fi-fi" ;
  sm:fileAbstract "This ontology defines the fundamental concepts relevant to financial instruments in general, with the intent of providing the high-level hooks for build-out in more detail in the relevant domain areas.  These include, but are not limited to, equities, options, debt instruments, and so forth, some of which may be negotiable." ;
  sm:filename "FinancialInstruments.rdf" ;
  rdfs:label "Financial Instruments Ontology" ;
  rdfs:seeAlso <https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/> ;
  rdfs:seeAlso <https://spec.edmcouncil.org/fibo/FBC/AboutFBC/> ;
  rdfs:seeAlso <https://spec.edmcouncil.org/fibo/FBC/FinancialInstruments/AboutFinancialInstruments/> ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/FBC/20170201/FinancialInstruments/FinancialInstruments/> ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/FBC/20150801/FinancialInstruments/FinancialInstruments/ version of this ontology was modified to reflect issue resolutions detailed in the FIBO FBC 1.0 FTF report." ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/FBC/20160801/FinancialInstruments/FinancialInstruments/ version of this ontology was modified to reflect issue resolutions detailed in the FIBO FBC 1.1 RTF report." ;
.
fibo-fbc-fi-fi:CashInstrument
  rdf:type owl:Class ;
  rdfs:label "cash instrument" ;
  rdfs:subClassOf fibo-fbc-fi-fi:FinancialInstrument ;
  skos:definition "a financial instrument whose value is ascertained directly by markets and that can be converted into cash quickly with minimal impact on the price received" ;
  skos:editorialNote "From a modeling perspective in FIBO, these are independent entities; the corresponding relative entity would be liquid asset. Cash instruments are therefore disjoint with derivative instruments, per IASB international accounting guidance." ;
  skos:scopeNote "Cash instruments can be classified into two subgroups, as securities and other cash instruments such as loans and deposits." ;
  fibo-fnd-utl-av:adaptedFrom "http://definitions.uslegal.com/"^^xsd:anyURI ;
  fibo-fnd-utl-av:adaptedFrom "http://www.investopedia.com/terms/l/liquidasset.asp"^^xsd:anyURI ;
.
fibo-fbc-fi-fi:CommodityInstrument
  rdf:type owl:Class ;
  rdfs:label "commodity instrument" ;
  rdfs:subClassOf fibo-fbc-fi-fi:FinancialInstrument ;
  skos:definition "a financial instrument representing an ownership interest in a commodity such as coal, oil, precious metal, livestock, an agricultural product, etc." ;
  fibo-fnd-utl-av:definitionOrigin "Citigroup Inc." ;
.
fibo-fbc-fi-fi:CurrencyInstrument
  rdf:type owl:Class ;
  rdfs:label "currency instrument" ;
  rdfs:subClassOf fibo-fbc-fi-fi:FinancialInstrument ;
  skos:definition "a financial instrument used for the purposes of currency trading" ;
  skos:example "Example currencies include UK pounds, US dollars, Euro.  An example currency instrument is spot currency instrument." ;
  skos:scopeNote "Each instance of a currency instrument has a one to one relationship with its associated currency." ;
  fibo-fnd-utl-av:adaptedFrom "Parameswaran, Sunil. Fundamentals of Financial Instruments: An Introduction to Stocks, Bonds, Foreign Exchange, and Derivatives. John Wiley and Sons (Asia) Pte. Lte., Singapore, 2011." ;
.
fibo-fbc-fi-fi:DebtInstrument
  rdf:type owl:Class ;
  rdfs:label "debt instrument" ;
  rdfs:subClassOf fibo-fbc-dae-dbt:CreditAgreement ;
  rdfs:subClassOf fibo-fbc-fi-fi:FinancialInstrument ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-sec-dbt-dbti:DebtOffering ;
      owl:onProperty fibo-sec-sec-iss:hasOffering ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-agr-ctr:hasContractParty ;
      owl:someValuesFrom fibo-fbc-dae-dbt:Borrower ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-agr-ctr:hasContractParty ;
      owl:someValuesFrom fibo-fbc-dae-dbt:Lender ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-agr-ctr:hasContractualElement ;
      owl:someValuesFrom fibo-fbc-dae-dbt:DebtTerms ;
    ] ;
  owl:equivalentClass fibo-sec-dbt-dbt-dbt:DebtInstrument ;
  skos:definition "a financial instrument evidencing a contractual obligation by the issuer to the holder to pay cash or deliver some other asset on terms as specified" ;
  fibo-fnd-utl-av:adaptedFrom "Bloomberg, LP" ;
  fibo-fnd-utl-av:adaptedFrom "ISO 10962, Securities and related financial instruments - Classification of Financial Instruments (CFI code), Second edition, 2001-05-01." ;
.
fibo-fbc-fi-fi:DerivativeInstrument
  rdf:type owl:Class ;
  rdfs:label "derivative instrument" ;
  rdfs:subClassOf fibo-fbc-fi-fi:FinancialInstrument ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-drc-bsc:hasUnderlier ;
      owl:someValuesFrom fibo-der-drc-bsc:Underlier ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-agr-ctr:hasContractualElement ;
      owl:someValuesFrom fibo-der-drc-bsc:DerivativeTransactionTerms ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-agr-ctr:hasContractualElement ;
      owl:someValuesFrom fibo-fbc-pas-fpas:SettlementTerms ;
    ] ;
  owl:disjointWith fibo-fbc-fi-fi:CashInstrument ;
  owl:equivalentClass fibo-der-der-dbx:DerivativeContract ;
  owl:equivalentClass fibo-sec-sec-finx:DerivativeInstrument ;
  skos:definition "a financial instrument that confers on its holders certain rights or obligations, whose value is derived from one or more underlying assets" ;
  skos:example "The three major categories of derivatives are (1) forward and future contracts, (2) options contracts, and (3) swaps. The most common underlying assets include stocks, bonds, commodities, currencies, interest rates and market indexes." ;
  skos:scopeNote "Derivatives can be characterized by whether they are exchange-traded or traded over-the-counter (OTC)." ;
  fibo-fnd-utl-av:adaptedFrom "Parameswaran, Sunil. Fundamentals of Financial Instruments: An Introduction to Stocks, Bonds, Foreign Exchange, and Derivatives. John Wiley and Sons (Asia) Pte. Lte., Singapore, 2011." ;
  fibo-fnd-utl-av:adaptedFrom "http://www.investopedia.com/"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "Derivative contracts owe their availability to the existence of markets for an underlying asset or a portfolio of assets on which such agreements are written. The derivative itself is merely a contract between two or more parties. Its value is determined by fluctuations in the underlying asset. Most derivatives are characterized by high leverage." ;
  fibo-fnd-utl-av:synonym "derivative contract" ;
.
fibo-fbc-fi-fi:Entitlement
  rdf:type owl:Class ;
  rdfs:label "entitlement" ;
  rdfs:subClassOf fibo-fbc-fi-fi:DerivativeInstrument ;
  skos:definition "a financial instrument that provides the holder the privilege to subscribe to or to receive specific assets on terms specified" ;
  fibo-fnd-utl-av:definitionOrigin "ISO 10962, Securities and related financial instruments - Classification of Financial Instruments (CFI code), Second edition, 2001-05-01." ;
  fibo-fnd-utl-av:synonym "right" ;
.
fibo-fbc-fi-fi:EquityInstrument
  rdf:type owl:Class ;
  rdfs:label "equity instrument" ;
  rdfs:subClassOf fibo-fbc-fi-fi:Security ;
  skos:definition "a financial instrument representing an ownership interest in an entity or pool of assets" ;
  fibo-fnd-utl-av:definitionOrigin "ISO 10962, Securities and related financial instruments - Classification of Financial Instruments (CFI code), Second edition, 2001-05-01." ;
.
fibo-fbc-fi-fi:FinancialInstrument
  rdf:type owl:Class ;
  rdfs:label "financial instrument" ;
  rdfs:subClassOf fibo-fnd-agr-ctr:WrittenContract ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fbc-fi-fi:FinancialInstrumentIdentifier ;
      owl:onProperty fibo-fnd-aap-agt:isIdentifiedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fbc-fi-fi:Issuer ;
      owl:onProperty fibo-fbc-fi-fi:hasIssuer ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-arr-cls:IndustrySectorClassifier ;
      owl:onProperty fibo-fnd-rel-rel:isClassifiedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-law-jur:Jurisdiction ;
      owl:onProperty fibo-fbc-fi-fi:isLegallyRecordedIn ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-sec-sec-cls:FinancialInstrumentClassifier ;
      owl:onProperty fibo-fnd-rel-rel:isClassifiedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-sec-sec-rst:ContractualRestriction ;
      owl:onProperty fibo-fnd-rel-rel:defines ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-sec-sec-rst:RegistrationRequirement ;
      owl:onProperty fibo-sec-sec-rst:hasRegistrationRequirement ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-sec-sec-rst:SecuritiesRestriction ;
      owl:onProperty fibo-sec-sec-rst:hasRestriction ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onDataRange xsd:boolean ;
      owl:onProperty fibo-fbc-fi-fi:isTransferable ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  owl:equivalentClass fibo-der-der-dbx:FinancialInstrument ;
  owl:equivalentClass fibo-md-tem-ins:FinancialInstrument ;
  owl:equivalentClass fibo-sec-sec-resx:FinancialInstrument ;
  owl:equivalentClass fibo-sec-sec-tax:FinancialInstrument ;
  skos:definition "a written contract that gives rise to both a financial asset of one entity and a financial liability or equity instrument of another entity" ;
  skos:example "Examples of financial instruments include: cash, evidence of an ownership interest in an entity, or a contractual right to receive (or deliver) cash, or another financial instrument." ;
  fibo-fnd-utl-av:adaptedFrom "http://ec.europa.eu/internal_market/accounting/docs/consolidated/ias32_en.pdf"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "A financial instrument can be thought of as a template that defines an arrangement structure that remains to be fleshed out with terms and parameters in order to establish a specific instance of the contract." ;
  fibo-fnd-utl-av:explanatoryNote "extends the definition of a financial instrument to identify any relevant restrictions or registration requirements" ;
.
fibo-fbc-fi-fi:FinancialInstrumentIdentifier
  rdf:type owl:Class ;
  rdfs:label "financial instrument identifier" ;
  rdfs:subClassOf fibo-fnd-arr-id:Identifier ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-aap-agt:identifies ;
      owl:someValuesFrom fibo-fbc-fi-fi:FinancialInstrument ;
    ] ;
  skos:definition "an identifier for a financial instrument" ;
  skos:scopeNote "Identifiers for financial instruments may include an ISIN, Sedol, CUSIP, BBGID, FIGI, or other identifier issued approximately when the instrument itself is issued, and based on the kind of instrument and jurisdiction in which it is issued." ;
  fibo-fnd-utl-av:adaptedFrom "OMG Financial Instrument Global Identifier (FIGI) Specification, available at http://www.omg.org/spec/FIGI/." ;
.
fibo-fbc-fi-fi:Future
  rdf:type owl:Class ;
  rdfs:label "future" ;
  rdfs:subClassOf fibo-fbc-fi-fi:DerivativeInstrument ;
  skos:definition "a contract that obligates the buyer to receive and the seller to deliver, in the future, the assets specified at an agreed price" ;
  fibo-fnd-utl-av:definitionOrigin "ISO 10962, Securities and related financial instruments - Classification of Financial Instruments (CFI code), Second edition, 2001-05-01." ;
.
fibo-fbc-fi-fi:Issuer
  rdf:type owl:Class ;
  rdfs:label "issuer" ;
  rdfs:subClassOf fibo-fnd-agr-ctr:ContractOriginator ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-be-le-fbo:FormallyConstitutedOrganization ;
      owl:onProperty fibo-fnd-rel-rel:hasIdentity ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
      owl:someValuesFrom [
          rdf:type owl:Restriction ;
          owl:onProperty fibo-fnd-pty-pty:isAPartyTo ;
          owl:someValuesFrom fibo-fbc-fi-fi:FinancialInstrument ;
        ] ;
    ] ;
  skos:definition "any party who issues (or proposes to issue, in a formal filing) any financial instrument, where a party can be a natural person, company, government, or political subdivision, agency, or instrumentality of a government" ;
  fibo-fnd-utl-av:adaptedFrom "Securities Exchange Act of 1934, as amended 12 August 2012" ;
  fibo-fnd-utl-av:explanatoryNote "With respect to certificates of deposit for securities, voting-trust certificates, or collateral- trust certificates, or with respect to certificates of interest or shares in an unincorporated investment trust not having a board of directors or of the fixed, restricted management, or unit type, the term issuer means the person or persons performing the acts and assuming the duties of depositor or manager pursuant to the provisions of the trust or other agreement or instrument under which such securities are issued; and except that with respect to equipment-trust certificates or like securities, the term issuer means the person by whom the equipment or property is, or is to be, used." ;
.
fibo-fbc-fi-fi:Option
  rdf:type owl:Class ;
  rdfs:label "option" ;
  rdfs:subClassOf fibo-fbc-fi-fi:DerivativeInstrument ;
  skos:definition "a contract that grants to the holder either the privilege to purchase or the privilege to sell the assets specified at a predetermined price or formula at or within a time in the future" ;
  fibo-fnd-utl-av:definitionOrigin "ISO 10962, Securities and related financial instruments - Classification of Financial Instruments (CFI code), Second edition, 2001-05-01." ;
.
fibo-fbc-fi-fi:SecuritiesTransaction
  rdf:type owl:Class ;
  rdfs:label "securities transaction" ;
  rdfs:subClassOf fibo-fnd-pas-pas:TransactionEvent ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:appliesTo ;
      owl:someValuesFrom fibo-fbc-fi-fi:FinancialInstrument ;
    ] ;
  skos:definition "a transaction event that represents the execution of an order to buy or sell a security" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Finance and Investment Terms, Ninth Edition, 2014" ;
.
fibo-fbc-fi-fi:Security
  rdf:type owl:Class ;
  rdfs:label "security" ;
  rdfs:subClassOf fibo-fbc-fi-fi:FinancialInstrument ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:hasValue "true"^^xsd:boolean ;
      owl:onProperty fibo-fbc-fi-fi:isTransferable ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-law-jur:Jurisdiction ;
      owl:onProperty fibo-fbc-fi-fi:isLegallyRecordedIn ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  owl:equivalentClass fibo-cae-ce-cae:Security ;
  owl:equivalentClass fibo-der-ass-ass:Security ;
  owl:equivalentClass fibo-md-tem-crr:Security ;
  owl:equivalentClass fibo-md-tem-trs:Security ;
  owl:equivalentClass fibo-sec-sec-finx:Security ;
  owl:equivalentClass fibo-sec-sec-sec:Security ;
  owl:equivalentClass fibo-sec-sec-clax:Security ;
  owl:equivalentClass fibo-sec-sec-idx:Security ;
  owl:equivalentClass fibo-sec-sec-resx:Security ;
  owl:equivalentClass fibo-sec-sec-risk:Security ;
  owl:equivalentClass fibo-sec-sec-assx:Security ;
  skos:definition "a transferable financial instrument that can be bought or sold" ;
  skos:editorialNote "This is a publicly traded security (corresponding to the scope of Security in ISO 20022) and therefore does not include privately held equity or equity in companies that are not publicly traded." ;
  fibo-fnd-utl-av:adaptedFrom "Securities Exchange Act of 1934, as amended 12 August 2012" ;
  fibo-fnd-utl-av:explanatoryNote "A security can be any note, stock, treasury stock, security future, security-based swap, bond, debenture,certificate of interest or participation in any profit-sharing agreement or in any oil, gas, or other mineral royalty or lease, any collateral-trust certificate, preorganization certificate or subscription, transferable share, investment contract, voting-trust certificate, certificate of deposit for a security, any put, call, straddle, option, or privilege on any security, certificate of deposit, or group or index of securities (including any interest therein or based on the value thereof), or any put, call, straddle, option, or privilege entered into on a national securities exchange relating to foreign currency, or in general, any instrument commonly known as a security, or any certificate of interest or participation in, temporary or interim certificate for, receipt for, or warrant or right to subscribe to or purchase, any of the foregoing; but shall not include currency or any note, draft, bill of exchange, or bankers' acceptance which has a maturity at the time of issuance of not exceeding nine months, exclusive of days of grace, or any renewal thereof the maturity of which is likewise limited." ;
  fibo-fnd-utl-av:explanatoryNote "A security may be traded over the counter, or through an exchange, or via some other trading venue such as an electronic trading platform." ;
  fibo-fnd-utl-av:synonym "negotiable security" ;
.
fibo-fbc-fi-fi:StandardizedTermsSet
  rdf:type owl:Class ;
  rdfs:label "standardized terms set" ;
  rdfs:subClassOf fibo-fnd-agr-ctr:ContractualCommitment ;
  skos:definition "a set of standardized terms, published by some body, and capable of being incorporated into a contract by agreement" ;
  skos:editorialNote "The things that are standardized: The strike price Contract size Expiry Date These three are standardized regardless of what exchange they are on. Each exchange has a standard for these specific terms, for contracts that are traded on that exchange. Expiry: there is a standard date that the exchange will have, for example in the US it is the Saturday following the third Friday of every month. So there is a set expiration date for all exchange traded optons Strike: there are set increments for strike for exchange traded options. Contract sizes are also stipulated, for example in the US these are standardized by the OPRA Convention (Options Pricing Reporting Authority). Ajay: Exchange traded is tri party contract with central counterparty for the setelemtn fo the options. Market place (usually electronic), hence standard to process data. Not necessarily a legal difference? So for instance you would not be able to trade different lot sizes. So they enter in the contract which is guaranteed by the exchange. Members of the exchange trade with each other. The Exchange then being a third party. OTC: Bilateral Exchange traded: Still bilateral, using standardized terms. Lookalikes: for examples, where there are usually ones with monthly margin calls (exchange traded), there are 'lookalike' ones which look like an ET but are traded outside the market. Common in commodities market. What else can be standardised? What must be standardidsed? Date Convention: Standardized by juridction US: All single-Equity Options are American; all European exercise for Indices. This is not a convention for say London, where various conventions may exist on different contracts." ;
.
fibo-fbc-fi-fi:hasIssuer
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fbc-fi-fi:FinancialInstrument ;
  rdfs:label "has issuer" ;
  rdfs:range fibo-fbc-fi-fi:Issuer ;
  rdfs:subPropertyOf fibo-fnd-pty-pty:hasPartyInRole ;
  skos:definition "relates an instrument to the party that issued it" ;
.
fibo-fbc-fi-fi:hasPrincipalExecutiveOfficeAddress
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-be-le-fbo:FormallyConstitutedOrganization ;
  rdfs:label "has principal executive office address" ;
  rdfs:range fibo-fbc-fct-breg:RegistrationAddress ;
  rdfs:subPropertyOf fibo-fnd-plc-adr:hasAddress ;
  skos:definition "relates an organization, specifically the issuer of a financial instrument, to its principal executive address, as required for issuance of that instrument" ;
.
fibo-fbc-fi-fi:hasValueExpressedIn
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fbc-fi-fi:FinancialInstrument ;
  rdfs:label "has value expressed in" ;
  rdfs:range fibo-fnd-acc-cur:Currency ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "relates an instrument to the currency its value is typically expressed in" ;
  fibo-fnd-utl-av:explanatoryNote "This should be the same currency that was declared at the time of issuance." ;
.
fibo-fbc-fi-fi:isLegallyRecordedIn
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fbc-fi-fi:FinancialInstrument ;
  rdfs:label "is legally recorded in" ;
  rdfs:range fibo-fnd-law-jur:Jurisdiction ;
  rdfs:subPropertyOf fibo-fnd-agr-ctr:hasGoverningJurisdiction ;
  skos:definition "jurisdiction (country, county, state, province, city) in which the financial instrument is legally recorded for regulatory and/or tax purposes" ;
.
fibo-fbc-fi-fi:isTransferable
  rdf:type owl:DatatypeProperty ;
  rdfs:domain fibo-fbc-fi-fi:FinancialInstrument ;
  rdfs:label "is transferable" ;
  rdfs:range xsd:boolean ;
  skos:definition "specifies whether a particular financial instrument is or is not transferable" ;
.
fibo-fbc-fi-fi:mayBeTradedIn
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fbc-fi-fi:Security ;
  rdfs:label "may be traded in" ;
  rdfs:range fibo-fbc-fct-mkt:Market ;
  skos:definition "optionally identifies one or more markets in which the security may be traded" ;
.
