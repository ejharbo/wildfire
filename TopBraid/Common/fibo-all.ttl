    ] ;
  owl:equivalentClass [
      rdf:type owl:Class ;
      owl:intersectionOf (
          fibo-der-rtd-irswp:InterestRateSwapLeg
          [
            rdf:type owl:Restriction ;
            owl:onProperty fibo-fbc-dae-dbt:hasInterestRate ;
            owl:someValuesFrom fibo-fbc-dae-dbt:FloatingInterestRate ;
          ]
        ) ;
    ] ;
  skos:definition "a swapstream in which variable interest is paid on some notional amount, linked to some underlying interest reference rate" ;
  skos:editorialNote "Instead of an absolute rate you have an underlier and an offset called a spread (same as margin in floating rate notes)." ;
  fibo-fnd-utl-av:synonym "floating interest rate swap stream terms" ;
.
fibo-der-rtd-irswp:FloatingRateFixingValue
  rdf:type owl:Class ;
  rdfs:label "floating rate fixing value" ;
  rdfs:subClassOf fibo-fnd-utl-alx:Variable ;
  skos:definition "rdf:datatype=\"http://www.w3.org/2001/XMLSchema#string\">value of floating interest rate amount during a given calculation period, along with the relevant parameters in its calculation" ;
  skos:editorialNote "Forms an explicit set of terms at a point in time, and is part of the explicit schedule information." ;
