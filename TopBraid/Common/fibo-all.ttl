  skos:changeNote "The https://spec.edmcouncil.org/fibo/FBC/20160801/ProductsAndServices/FinancialProductsAndServices/ version of this ontology was modified to reflect issue resolutions detailed in the FIBO FBC 1.1 RTF report, including, but not limited to, the addition of trade settlement concepts." ;
.
fibo-fbc-pas-fpas:Basket
  rdf:type owl:Class ;
  rdfs:label "basket" ;
  rdfs:subClassOf fibo-fnd-arr-arr:Collection ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fbc-pas-fpas:hasConstituent ;
      owl:someValuesFrom fibo-fbc-pas-fpas:BasketConstituent ;
    ] ;
  owl:equivalentClass fibo-der-der-dbx:Basket ;
  skos:definition "a collection of goods, services, or other things (e.g., financial contracts) that can be purchased and sold in some marketplace" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.investopedia.com/terms/m/market_basket.asp"^^xsd:anyURI ;
  fibo-fnd-utl-av:adaptedFrom "http://www.merriam-webster.com/dictionary/market%20basket"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "A basket may be associated with a specific market sector, and may be delineated for the purposes of statistical analysis, such as for calculating CPI. According to the US Bureau of Labor Statistics (BLS), with respect to the CPI, a market basket is a package of goods and services that consumers purchase for day-to-day living. The weight of each item is based on the amount of expenditure reported by a sample of households." ;
  fibo-fnd-utl-av:explanatoryNote "From a securities perspective, a basket is a collection of products or securities that are designated to mimic the performance of a market. For investors, the market basket is the principal idea behind index funds, which are essentially a broad sample of stocks, bonds or other securities in the market; this provides investors with a benchmark against which to compare their investment returns." ;
.
fibo-fbc-pas-fpas:BasketConstituent
  rdf:type owl:Class ;
  rdfs:label "basket constituent" ;
  rdfs:subClassOf fibo-fnd-pty-rl:ThingInRole ;
  owl:equivalentClass fibo-sec-sec-basx:BasketConstituent ;
  owl:equivalentClass [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
      owl:someValuesFrom [
          rdf:type owl:Restriction ;
          owl:onProperty fibo-fbc-pas-fpas:isConstituentOf ;
          owl:someValuesFrom fibo-fbc-pas-fpas:Basket ;
        ] ;
    ] ;
  skos:definition "a component of a basket" ;
.
fibo-fbc-pas-fpas:Catalog
  rdf:type owl:Class ;
  rdfs:label "catalog" ;
  rdfs:subClassOf fibo-be-fct-pub:Publication ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-arr-id:Identifier ;
      owl:onProperty fibo-fnd-aap-agt:isIdentifiedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:characterizes ;
      owl:someValuesFrom fibo-fnd-pas-pas:Product ;
    ] ;
  skos:definition "a list of goods and/or services available for sale with their description and possibly prices, published as a printed or  electronic document (e-catalog)" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.businessdictionary.com/definition/catalog.html"^^xsd:anyURI ;
.
fibo-fbc-pas-fpas:Exposure
  rdf:type owl:Class ;
  rdfs:label "exposure" ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "the extent to which an individual or organization is unprotected and open to damage, danger, risk of suffering a loss, or uncertainty" ;
  skos:example "Examples include financial exposure, credit exposure, legal exposure, credit rating exposure, reputational exposure, and so forth." ;
  fibo-fnd-utl-av:adaptedFrom "http://www.businessdictionary.com/definition/exposure.html"^^xsd:anyURI ;
.
fibo-fbc-pas-fpas:FinancialExposure
  rdf:type owl:Class ;
  rdfs:label "financial exposure" ;
  rdfs:subClassOf fibo-fbc-pas-fpas:Exposure ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:appliesTo ;
      owl:someValuesFrom fibo-fnd-pty-pty:PartyInRole ;
    ] ;
  skos:definition "the extent to which an individual or organization is open to risk of suffering a loss in a transaction, or with respect to some investment or set of investments, e.g., some holding; the amount one stands to lose in that transaction or investment" ;
  skos:example "Examples in banking include the total amount of unsecured loans, the amount of loans advanced to a single borrower, group, industry, or country, and the probability of loss from devaluation, revaluation, or foreign exchange fluctuations." ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.businessdictionary.com/definition/exposure.html"^^xsd:anyURI ;
.
fibo-fbc-pas-fpas:FinancialIntermediationService
  rdf:type owl:Class ;
  rdfs:label "financial intermediation service" ;
  rdfs:subClassOf fibo-fbc-pas-fpas:FinancialService ;
  skos:definition "any financial service in which a third party (the intermediary) matches lenders and investors with entrepreneurs and other borrowers in need of capital" ;
  fibo-fnd-utl-av:definitionOrigin "Office of Financial Research (OFR) Annual Report, 2012, Glossary" ;
  fibo-fnd-utl-av:explanatoryNote "Often investors and borrowers do not have precisely matching needs, and the intermediary's capital is put at risk to transform the credit risk and maturity of the liabilities to meet the needs of investors." ;
.
fibo-fbc-pas-fpas:FinancialProduct
  rdf:type owl:Class ;
  rdfs:label "financial product" ;
  rdfs:subClassOf fibo-fnd-pas-pas:Product ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:isProvidedBy ;
      owl:someValuesFrom fibo-fbc-pas-fpas:FinancialServiceProvider ;
    ] ;
  skos:definition "a product provided to consumers and businesses by financial institutions such as banks, insurance companies, brokerage firms, consumer finance companies, and investment companies all of which comprise the financial services industry" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.investorwords.com/19080/financial_services.html"^^xsd:anyURI ;
.
fibo-fbc-pas-fpas:FinancialService
  rdf:type owl:Class ;
  rdfs:label "financial service" ;
  rdfs:subClassOf fibo-fnd-pas-pas:Service ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:isProvidedBy ;
      owl:someValuesFrom fibo-fbc-pas-fpas:FinancialServiceProvider ;
    ] ;
  skos:definition "a service provided to consumers and businesses by financial institutions such as banks, insurance companies, brokerage firms, consumer finance companies, and investment companies all of which comprise the financial services industry" ;
  fibo-fnd-utl-av:definitionOrigin "http://www.investorwords.com/19080/financial_services.html"^^xsd:anyURI ;
.
fibo-fbc-pas-fpas:FinancialServiceProvider
  rdf:type owl:Class ;
  rdfs:label "financial service provider" ;
  rdfs:subClassOf fibo-be-fct-fct:FunctionalEntity ;
  rdfs:subClassOf fibo-fnd-pas-pas:ServiceProvider ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass [
          rdf:type owl:Class ;
          owl:intersectionOf (
              fibo-be-le-fbo:FormallyConstitutedOrganization
              fibo-be-le-lp:LegalEntity
            ) ;
        ] ;
      owl:onProperty fibo-fnd-rel-rel:hasIdentity ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:provides ;
      owl:someValuesFrom fibo-fbc-pas-fpas:FinancialService ;
    ] ;
  skos:definition "a functional entity either licensed to provide financial services to consumers and/or businesses or established by law to provide financial services, such as a central bank" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.investorwords.com/19080/financial_services.html"^^xsd:anyURI ;
.
fibo-fbc-pas-fpas:Holding
  rdf:type owl:Class ;
  rdfs:label "holding" ;
  rdfs:subClassOf fibo-fnd-oac-own:Ownership ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-oac-own:hasOwnedThing ;
      owl:someValuesFrom fibo-fnd-oac-own:Asset ;
    ] ;
  skos:definition "real or personal property (assets), including but not limited to financial assets, to which one holds title and of which one has possession" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
  fibo-fnd-utl-av:adaptedFrom "http://legal-dictionary.thefreedictionary.com/Holding"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "Note that a holding may refer to a single asset, such as a piece of real estate, a portfolio of assets, multiple portfolios, and so forth, and is frequently aggregated over multiple assets." ;
.
fibo-fbc-pas-fpas:LegalAgent
  rdf:type owl:Class ;
  rdfs:label "legal agent" ;
  rdfs:subClassOf fibo-fnd-pty-pty:PartyInRole ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
      owl:someValuesFrom [
          rdf:type owl:Restriction ;
          owl:onProperty fibo-fnd-law-lcap:hasCapacity ;
          owl:someValuesFrom fibo-fnd-law-lcap:DelegatedLegalAuthority ;
        ] ;
    ] ;
  skos:definition "any party that has been legally empowered to act on behalf of another party" ;
  fibo-fnd-utl-av:adaptedFrom "17 CFR 45.1, Definitions - see the definition of agent" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fbc-pas-fpas:LicensedAgent
  rdf:type owl:Class ;
  rdfs:label "licensed agent" ;
  rdfs:subClassOf fibo-be-oac-exec:ResponsibleParty ;
  rdfs:subClassOf fibo-fbc-pas-fpas:LegalAgent ;
  rdfs:subClassOf fibo-fnd-law-lcap:Licensee ;
  skos:definition "any individual who is licensed to perform a legally binding function, and who has been legally empowered to act on behalf of another party" ;
  skos:example "Insurance agents, realtors, financial advisors, certain attorneys, and brokers are examples of legal agents." ;
  fibo-fnd-utl-av:adaptedFrom "17 CFR 45.1, Definitions - see the definition of agent" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fbc-pas-fpas:Lifecycle
  rdf:type owl:Class ;
  rdfs:label "lifecycle" ;
  rdfs:subClassOf fibo-fnd-arr-cls:ClassificationScheme ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fbc-pas-fpas:hasStage ;
      owl:someValuesFrom fibo-fbc-pas-fpas:LifecycleStage ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-dt-bd:holdsDuring ;
      owl:someValuesFrom fibo-fnd-dt-fd:DatePeriod ;
    ] ;
  skos:definition "an arrangement that compares the cyclical nature of families, organizations, processes, products, marketing, and order management, portfolio management or other systems with the cradle to grave life stages (birth, growth, maturity, decay, and death) of living organisms" ;
  skos:example "The product life cycle describes the period of time over which an item is developed, brought to market and eventually removed from the market. The cycle is broken into four stages: introduction, growth, maturity and decline. The idea of the product life cycle is used in marketing to decide when it is appropriate to advertise, reduce prices, explore new markets or create new packaging." ;
  fibo-fnd-utl-av:adaptedFrom "http://www.businessdictionary.com/definition/life-cycle.html"^^xsd:anyURI ;
  fibo-fnd-utl-av:adaptedFrom "http://www.investopedia.com/terms/p/product-life-cycle.asp"^^xsd:anyURI ;
.
fibo-fbc-pas-fpas:LifecycleEvent
  rdf:type owl:Class ;
  rdfs:label "lifecycle event" ;
  rdfs:subClassOf fibo-fnd-dt-oc:OccurrenceKind ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:appliesTo ;
      owl:someValuesFrom fibo-fbc-pas-fpas:Lifecycle ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:appliesTo ;
      owl:someValuesFrom fibo-fbc-pas-fpas:LifecycleStage ;
    ] ;
  skos:definition "an event that occurs during a given stage of some lifecycle" ;
  skos:example "a call notification or coupon payment as a part of a bond lifecycle" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.investopedia.com/terms/p/product-life-cycle.asp"^^xsd:anyURI ;
.
fibo-fbc-pas-fpas:LifecycleStage
  rdf:type owl:Class ;
  rdfs:label "lifecycle stage" ;
  rdfs:subClassOf fibo-fnd-arr-cls:Classifier ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fbc-pas-fpas:isStageOf ;
      owl:someValuesFrom fibo-fbc-pas-fpas:Lifecycle ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-dt-bd:holdsDuring ;
      owl:someValuesFrom fibo-fnd-dt-fd:DatePeriod ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:comprises ;
      owl:someValuesFrom fibo-fbc-pas-fpas:LifecycleEvent ;
    ] ;
  skos:definition "a phase in a lifecycle" ;
  skos:example "a research and development phase of a product lifecycle, the introduction phase in a marketing lifecycle, a growth stage in an economic lifecycle, or the origination phase in the lifecycle of a loan" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.businessdictionary.com/definition/life-cycle.html"^^xsd:anyURI ;
  fibo-fnd-utl-av:adaptedFrom "http://www.investopedia.com/terms/p/product-life-cycle.asp"^^xsd:anyURI ;
.
fibo-fbc-pas-fpas:Position
  rdf:type owl:Class ;
  rdfs:label "position" ;
  rdfs:subClassOf fibo-fbc-pas-fpas:Holding ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-oac-own:Asset ;
      owl:onProperty fibo-fnd-oac-own:hasOwnedThing ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "an investor's stake, i.e., a holding, in a particular asset (such as an individual security)" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.investorwords.com/3748/position.html"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "A position can be long or short, and it can be in any asset class, such as stocks, bonds, futures, or options. A position can be open (current) or closed (past), but in general use, unless a position is specifically referred to as closed, the assumption is that it references an open position." ;
.
fibo-fbc-pas-fpas:RegulatedCommodity
  rdf:type owl:Class ;
  rdfs:label "regulated commodity" ;
  rdfs:subClassOf fibo-fnd-pas-pas:Commodity ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fbc-fct-rga:RegulatoryAgency ;
      owl:onProperty fibo-fbc-fct-rga:isRegulatedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:isGovernedBy ;
      owl:someValuesFrom fibo-fnd-law-jur:Jurisdiction ;
    ] ;
  skos:definition "a commodity under the jurisdiction of the regulatory agency, such as the Commodities Futures Trading Commission (CFTF), which includes any commodity traded in an organized contracts market" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
  fibo-fnd-utl-av:explanatoryNote "The CFTC polices matters of information and disclosure, fair trading practices, registration of firms and individuals, protection of customer funds, record keeping, and maintenance of orderly options and futures markets in the United States." ;
.
fibo-fbc-pas-fpas:SettlementTerms
  rdf:type owl:Class ;
  rdfs:label "settlement terms" ;
  rdfs:subClassOf fibo-fnd-agr-ctr:ContractualCommitment ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-dt-fd:Date ;
      owl:onProperty fibo-fbc-pas-fpas:hasSettlementDate ;
    ] ;
  owl:equivalentClass fibo-der-der-dbx:SettlementTerms ;
  skos:definition "contract terms that define the commitment to and mechanism for settling one or more sides of a transaction" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
  fibo-fnd-utl-av:explanatoryNote "In general, settlement involves arrangement of disposition of property, typically for legal reasons.  With respect to financial transactions, it involves completion of a trade, either between brokers or agents, or beween a broker and client. This may include settlement in cash, either for the entire transaction or for the cash leg of a transaction, either now or at some specified time in the future." ;
.
fibo-fbc-pas-fpas:ThirdPartyAgent
  rdf:type owl:Class ;
  rdfs:label "third-party agent" ;
  rdfs:subClassOf fibo-fbc-pas-fpas:LegalAgent ;
  rdfs:subClassOf fibo-fnd-law-lcap:Licensee ;
  rdfs:subClassOf fibo-fnd-pas-pas:ServiceProvider ;
  skos:definition "any service provider that is licensed to perform a legally binding function and has been legally empowered to act on behalf of another party" ;
  fibo-fnd-utl-av:adaptedFrom "17 CFR 45.1, Definitions - see the definition of agent" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
  fibo-fnd-utl-av:adaptedFrom "http://thelawdictionary.org/agent/"^^xsd:anyURI ;
  fibo-fnd-utl-av:usageNote "Note that third-party agent is defined as a service provider (organization) acting in an agency capacity, such as a law firm, accountancy, or investment bank.  This is distinct from the concept of an individual (licensed agent), for example one who works for a broker-dealer, that is a registered agent licensed to sell securities." ;
.
fibo-fbc-pas-fpas:Trade
  rdf:type owl:Class ;
  rdfs:label "trade" ;
  rdfs:subClassOf fibo-fnd-pas-pas:TransactionEvent ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fbc-pas-fpas:TradeIdentifier ;
      owl:onProperty fibo-fnd-aap-agt:isIdentifiedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fbc-pas-fpas:Trader ;
      owl:onProperty fibo-fbc-pas-fpas:isFacilitatedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fbc-pas-fpas:isEmbodiedIn ;
      owl:someValuesFrom fibo-fnd-agr-ctr:Contract ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:involves ;
      owl:someValuesFrom [
          rdf:type owl:Class ;
          owl:unionOf (
              fibo-fnd-pas-pas:Good
              fibo-fnd-pas-pas:Service
              fibo-fnd-agr-ctr:Contract
            ) ;
        ] ;
    ] ;
  skos:definition "an agreement between parties participating in a voluntary negotiation and the exchange of goods and/or services for some consideration (e.g., desired goods and services that someone else possesses)" ;
  fibo-fnd-utl-av:adaptedFrom "Deutsche Bank Presentation on the Lifecycle of a Trade, available at http://www.slideshare.net/ahaline/23512555-tradelifecycle" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.investopedia.com/terms/trade.asp"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "The advent of money as a medium of exchange has allowed trade to be conducted in a manner that is much simpler and effective compared to earlier forms of trade, such as bartering. In financial markets, trading also can mean performing a transaction that involves the selling and purchasing of a security." ;
  fibo-fnd-utl-av:explanatoryNote "The seller must deliver the commodity sold to the buyer; the buyer must pay the agreed purchase price, which could be in the form of other goods or services, on the agreed date." ;
  fibo-fnd-utl-av:explanatoryNote "Trading activities typically include (a) regularly underwriting or dealing in securities; interest rate, foreign exchange rate, commodity, equity, and credit derivative contracts; other financial instruments; and other assets for resale, (b) acquiring or taking positions in such items principally for the purpose of selling in the near term or otherwise with the intent to resell in order to profit from short-term price movements, and (c) acquiring or taking positions in such items as an accommodation to customers or for other trading purposes. (Source: Instructions for Preparation of Consolidated Reports of Condition and Income (FFIEC 031 and 041), Schedule RC-D - Trading Assets and Liabilities, 2013." ;
.
fibo-fbc-pas-fpas:TradeIdentifier
  rdf:type owl:Class ;
  rdfs:label "trade identifier" ;
  rdfs:subClassOf fibo-fnd-arr-id:Identifier ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fbc-pas-fpas:Trade ;
      owl:onProperty fibo-fnd-aap-agt:identifies ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "an identifier for a trade" ;
.
fibo-fbc-pas-fpas:Trader
  rdf:type owl:Class ;
  rdfs:label "trader" ;
  rdfs:subClassOf fibo-fnd-pas-pas:ServiceProvider ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fbc-pas-fpas:facilitates ;
      owl:someValuesFrom fibo-fbc-pas-fpas:Trade ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
      owl:someValuesFrom [
          rdf:type owl:Restriction ;
          owl:onProperty fibo-fnd-law-lcap:hasCapacity ;
          owl:someValuesFrom fibo-fnd-law-lcap:ContractualCapability ;
        ] ;
    ] ;
  skos:definition "an individual or organization or representative thereof that engages in the transfer of financial assets in any financial market on behalf of a client or the financial services provider" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.investopedia.com/terms/t/trader.asp"^^xsd:anyURI ;
.
fibo-fbc-pas-fpas:UniqueTradeIdentifier
  rdf:type owl:Class ;
  rdfs:label "unique trade identifier" ;
  rdfs:subClassOf fibo-fbc-pas-fpas:TradeIdentifier ;
  skos:definition "a globally unique identifier for a trade, unique to the transaction known to and used by all relevant parties, as required by various regulatory agencies around the world" ;
  fibo-fnd-utl-av:abbreviation "UTI" ;
  fibo-fnd-utl-av:adaptedFrom "Unique Trade Identifier (UTI): Generation, Communication and Matching, as of 20 July 2015, described in http://www2.isda.org/attachment/NzczMg==/2015%20July%2020%20UTI%20Best%20Practice%20v11.6_final.pdf" ;
.
fibo-fbc-pas-fpas:facilitates
  rdf:type owl:ObjectProperty ;
  rdfs:label "facilitates" ;
  skos:definition "provides the context in which an event, a task, a conversation or something else can occur" ;
