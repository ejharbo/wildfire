fibo-fnd-acc-crf:trancheLoanPurpose
  rdf:type owl:DatatypeProperty ;
  rdfs:label "tranche loan purpose"@en ;
.
fibo-fnd-acc-cur:
  terms:license "http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense"^^xsd:anyURI ;
  sm:contentLanguage "http://www.omg.org/spec/ODM/"^^xsd:anyURI ;
  sm:contentLanguage "http://www.w3.org/standards/techs/owl#w3c_all"^^xsd:anyURI ;
  sm:copyright """Copyright (c) 2013-2016 EDM Council, Inc.
Copyright (c) 2013-2016 Object Management Group, Inc.""" ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Arrangements/Codes/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Arrangements/IdentifiersAndIndices/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Law/Jurisdiction/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Places/Countries/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Quantities/QuantitiesAndUnits/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Utilities/Analytics/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Utilities/BusinessFacingTypes/"^^xsd:anyURI ;
  sm:directSource "ISO 4217 Codes for the representation of currencies and funds, Seventh edition, 2008-07-15" ;
  sm:directSource "ISO 4217 Currency and funds code list, 2014-03-28" ;
  sm:fileAbbreviation "fibo-fnd-acc-cur" ;
  sm:fileAbstract """This ontology defines currency and monetary amount related concepts for use in defining other FIBO ontology elements. There are two distinct kinds of concepts that correspond to money and amounts: a concrete, actual amount of money, and the monetary measure of something denominated in some currency. These are dimensionally the same but whereas 'money amount' is defined as an amount of money, 'monetary amount' is an abstract monetary measure.

The definition of currency provided herein is compliant with the definitions given in ISO 4217.  ISO 4217 provides universally applicable coded representations of names of currencies and funds, used internationally for financial transaction support.  The ontology has been partitioned into 2 parts: (1) the essential concept system describing the standard (this module), and (2) ISO4217-1-CurrencyCodes, which contains all of the individuals specified in ISO 4217.""" ;
  sm:filename "CurrencyAmount.rdf" ;
  rdfs:label "Currency Amount Ontology" ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/AboutFND/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/Accounting/AboutAccounting/"^^xsd:anyURI ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/FND/20160201/Accounting/CurrencyAmount/> ;
  skos:changeNote "The FIBO FND 1.0 (https://spec.edmcouncil.org/fibo/FND/20141101/Accounting/CurrencyAmount.rdf) version of this ontology was modified per the additions introduced in the FIBO FBC RFC and related issue resolutions identified in the FIBO FND 1.1 RTF report and http://spec.edmcouncil.org/fibo/FND/1.0/AboutFND-1.1/, including adding support for ISO 4217 currency codes." ;
  skos:editorialNote "(1) The present version of the ontology covers the English sections of the ISO 4217 standard only, and (2) UTF-8 character encodings are employed in names in the currency codes ontology to support the broadest number of tools." ;
.
fibo-fnd-acc-cur:AmountOfMoney
  rdf:type owl:Class ;
  rdfs:label "amount of money" ;
  rdfs:subClassOf fibo-fnd-qt-qtu:QuantityValue ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-acc-cur:Currency ;
      owl:onProperty fibo-fnd-acc-cur:hasBaseMoneyUnit ;
    ] ;
  skos:definition "a sum of money" ;
  skos:editorialNote "This is an actual sum of money, not the measure of a sum of money in monetary units, although it has the same basic properties (decimal number with a currenct unit)." ;
  fibo-fnd-utl-av:synonym "cash" ;
.
fibo-fnd-acc-cur:CalculatedPrice
  rdf:type owl:Class ;
  rdfs:label "calculated price" ;
  rdfs:subClassOf fibo-fnd-acc-cur:MonetaryPrice ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-utl-alx:CalculationFormula ;
      owl:onProperty fibo-fnd-utl-alx:hasFormula ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "a monetary price determined by a formula" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.businessdictionary.com/definition/price.html"^^xsd:anyURI ;
.
fibo-fnd-acc-cur:Currency
  rdf:type owl:Class ;
  rdfs:label "currency" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-plc-cty:GeopoliticalEntity ;
      owl:onProperty fibo-fnd-rel-rel:isUsedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange xsd:string ;
      owl:onProperty fibo-fnd-acc-cur:hasMinorUnit ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onDataRange xsd:string ;
      owl:onProperty fibo-fnd-acc-cur:hasCurrencyName ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onDataRange xsd:string ;
      owl:onProperty fibo-fnd-acc-cur:hasNumericCode ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "medium of exchange value, defined by reference to the geographical location of the authorities responsible for it" ;
  fibo-fnd-utl-av:definitionOrigin "Codes for the representation of currencies and funds, ISO 4217, Seventh edition, 2008-07-15, section 3.1." ;
.
fibo-fnd-acc-cur:CurrencyIdentifier
  rdf:type owl:Class ;
  rdfs:label "currency identifier" ;
  rdfs:subClassOf fibo-fnd-arr-cd:CodeElement ;
  rdfs:subClassOf fibo-fnd-arr-id:Identifier ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onDataRange xsd:string ;
      owl:onProperty fibo-fnd-acc-cur:hasCurrencyTag ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-aap-agt:identifies ;
      owl:someValuesFrom fibo-fnd-acc-cur:Currency ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:denotes ;
      owl:someValuesFrom fibo-fnd-acc-cur:Currency ;
    ] ;
  skos:definition "the trigraph representing the currency" ;
  fibo-fnd-utl-av:explanatoryNote """The first (left-most) two characters of the currency identifier provide a code unique to the currency authority to which it is assigned. Wherever practicable, it is derived from the geographical location of the currency authority, as described in ISO 3166.

The third (right-most) character of the identifier (alphabetic code) is an indicator, preferably mnemonic, derived from the name of the major currency unit or fund.

Where the currency is not associated with a single geographical entity as described in ISO 3166-1, a specially allocated identifier (alpha-2 code) will be used to describe the currency authority. This code will be allocated by the Maintenance Agency from within the user-assigned range of codes XA to XZ specified in 8.1.3 of ISO 3166-1:1997. The character following X will be a mnemonic, where possible, derived from the name of the geographical area concerned.""" ;
.
fibo-fnd-acc-cur:ExchangeRate
  rdf:type owl:Class ;
  rdfs:label "exchange rate" ;
  rdfs:subClassOf fibo-fnd-acc-cur:MonetaryMeasure ;
  rdfs:subClassOf fibo-fnd-qt-qtu:Rate ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom xsd:decimal ;
      owl:onProperty fibo-fnd-acc-cur:hasRateValue ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-acc-cur:MonetaryMeasure ;
      owl:onProperty fibo-fnd-qt-qtu:hasQuantityKind ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-acc-cur:Currency ;
      owl:onProperty fibo-fnd-acc-cur:hasBaseCurrency ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-acc-cur:Currency ;
      owl:onProperty fibo-fnd-acc-cur:hasDealtCurrency ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "a rate at which one currency can be exchanged for another" ;
  skos:example "The exchange rate between the U.S. dollar and British pound is distinct from the exchange rate between the U.S. dollar and the euro." ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Finance and Investment Terms, Ninth Edition, 2014." ;
.
fibo-fnd-acc-cur:Funds
  rdf:type owl:Class ;
  rdfs:label "funds" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-plc-cty:GeopoliticalEntity ;
      owl:onProperty fibo-fnd-rel-rel:isUsedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange xsd:string ;
      owl:onProperty fibo-fnd-acc-cur:hasMinorUnit ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-acc-cur:Currency ;
      owl:onProperty fibo-fnd-rel-rel:refersTo ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onDataRange xsd:string ;
      owl:onProperty fibo-fnd-acc-cur:hasNumericCode ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onDataRange xsd:string ;
      owl:onProperty fibo-fnd-aap-agt:hasName ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "monetary resources associated with a currency" ;
  fibo-fnd-utl-av:definitionOrigin "Codes for the representation of currencies and funds, ISO 4217, Seventh edition, 2008-07-15, section 3.2." ;
.
fibo-fnd-acc-cur:FundsIdentifier
  rdf:type owl:Class ;
  rdfs:label "funds identifier" ;
  rdfs:subClassOf fibo-fnd-arr-cd:CodeElement ;
  rdfs:subClassOf fibo-fnd-arr-id:Identifier ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onDataRange xsd:string ;
      owl:onProperty fibo-fnd-acc-cur:hasFundsTag ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-aap-agt:identifies ;
      owl:someValuesFrom fibo-fnd-acc-cur:Funds ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:denotes ;
      owl:someValuesFrom fibo-fnd-acc-cur:Funds ;
    ] ;
  skos:definition "the trigraph representing the funds" ;
.
fibo-fnd-acc-cur:InterestRate
  rdf:type owl:Class ;
  rdfs:label "interest rate" ;
  rdfs:subClassOf fibo-fnd-acc-cur:MonetaryMeasure ;
  rdfs:subClassOf fibo-fnd-qt-qtu:Rate ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom xsd:decimal ;
      owl:onProperty fibo-fnd-acc-cur:hasRateValue ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-acc-cur:Currency ;
      owl:onProperty fibo-fnd-acc-cur:hasCurrency ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-acc-cur:MonetaryMeasure ;
      owl:onProperty fibo-fnd-qt-qtu:hasQuantityKind ;
    ] ;
  skos:definition "an amount charged, expressed as a percentage of principal, by a lender to a borrower for the use of assets" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Finance and Investment Terms, Ninth Edition, 2014." ;
  fibo-fnd-utl-av:explanatoryNote "Interest rates are typically noted on an annual basis, known as the annual percentage rate (APR). The assets borrowed could include cash, consumer goods, and large assets such as a vehicle or building. The rate is derived by dividing the amount of interest by the amount of principal borrowed. Interest rates are quoted on bills, notes, bonds, credit cards, and many kinds of consumer and business loans." ;
.
fibo-fnd-acc-cur:MonetaryAmount
  rdf:type owl:Class ;
  rdfs:label "monetary amount" ;
  rdfs:subClassOf fibo-fnd-acc-cur:MonetaryMeasure ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom xsd:decimal ;
      owl:onProperty fibo-fnd-acc-cur:hasAmount ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-acc-cur:Currency ;
      owl:onProperty fibo-fnd-acc-cur:hasCurrency ;
    ] ;
  skos:definition "the measure which is an amount of money specified in monetary units" ;
  skos:editorialNote "This is an abstract concept, not to be confused with a sum of money (Money Amount)." ;
.
fibo-fnd-acc-cur:MonetaryMeasure
  rdf:type owl:Class ;
  rdfs:label "monetary measure" ;
  rdfs:subClassOf fibo-fnd-utl-alx:Measure ;
  skos:definition "some measure of some sum of money" ;
  skos:editorialNote "This may be a measure expressed in terms of decimal plus currency, or it may be a measure expressed in terms of a percentage amount with reference to some other monetary amount or to some Money Amount (actual amount of money)." ;
.
fibo-fnd-acc-cur:MonetaryPrice
  rdf:type owl:Class ;
  rdfs:label "monetary price" ;
  rdfs:subClassOf fibo-fnd-acc-cur:MonetaryAmount ;
  rdfs:subClassOf fibo-fnd-acc-cur:Price ;
  skos:definition "a price that that is expressed as a monetary amount" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.businessdictionary.com/definition/price.html"^^xsd:anyURI ;
  fibo-fnd-utl-av:adaptedFrom "http://www.oxforddictionaries.com/definition/english/price"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "As the consideration given in exchange for transfer of ownership, price forms the essential basis of commercial transactions. It may be fixed by a contract, left to be determined by an agreed upon formula at a future date, or discovered or negotiated during the course of dealings between the parties involved. In commerce, price is determined by what (1) a buyer is willing to pay, (2) a seller is willing to accept, and (3) the competition is allowing to be charged." ;
.
fibo-fnd-acc-cur:MoneyAmount
  rdf:type owl:Class ;
  rdfs:label "money amount" ;
  rdfs:subClassOf fibo-fnd-qt-qtu:QuantityValue ;
  owl:deprecated "true"^^xsd:boolean ;
  owl:equivalentClass fibo-fnd-acc-cur:AmountOfMoney ;
  skos:definition "a sum of money" ;
.
fibo-fnd-acc-cur:PercentageMonetaryAmount
  rdf:type owl:Class ;
  rdfs:label "percentage monetary amount" ;
  rdfs:subClassOf fibo-fnd-acc-cur:MonetaryMeasure ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-acc-cur:MonetaryAmount ;
      owl:onProperty fibo-fnd-utl-bt:isPercentageOf ;
    ] ;
  skos:definition "a measure of some amount of money expressed as a percentage of some other amount, some notional amount or some concrete money amount" ;
  skos:editorialNote "This will have a relationship to what it is a percentage of. Alternatively and for some applications of this term, there may be an enumerated list of possible things it is a percentage of." ;
.
fibo-fnd-acc-cur:Price
  rdf:type owl:Class ;
  rdfs:label "price" ;
  rdfs:subClassOf fibo-fnd-qt-qtu:QuantityValue ;
  skos:definition "an amount of money, goods, or services requested, expected, required, or given in exchange for something else" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.businessdictionary.com/definition/price.html"^^xsd:anyURI ;
  fibo-fnd-utl-av:adaptedFrom "http://www.oxforddictionaries.com/definition/english/price"^^xsd:anyURI ;
.
fibo-fnd-acc-cur:hasAmount
  rdf:type owl:DatatypeProperty ;
  rdfs:label "has amount" ;
  rdfs:range xsd:decimal ;
  rdfs:subPropertyOf fibo-fnd-qt-qtu:hasNumericValue ;
  skos:definition "a total number or quantity" ;
.
fibo-fnd-acc-cur:hasBaseCurrency
  rdf:type owl:InverseFunctionalProperty ;
  rdf:type owl:ObjectProperty ;
  rdfs:label "has base currency" ;
  rdfs:range fibo-fnd-acc-cur:Currency ;
  rdfs:subPropertyOf fibo-fnd-acc-cur:hasCurrency ;
  skos:definition "a predicate indicating the base currency in an exchange rate; one unit of this currency represents R units of the dealt currency, where R is the exchange rate value" ;
.
fibo-fnd-acc-cur:hasBaseMoneyUnit
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-acc-cur:AmountOfMoney ;
  rdfs:label "has base money unit" ;
  rdfs:range fibo-fnd-acc-cur:Currency ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "the currency in which the money amount is denominated" ;
.
fibo-fnd-acc-cur:hasCurrency
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-acc-cur:MonetaryAmount ;
  rdfs:label "has currency" ;
  rdfs:range fibo-fnd-acc-cur:Currency ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "the currency in which the monetary amount is defined" ;
.
fibo-fnd-acc-cur:hasCurrencyName
  rdf:type owl:DatatypeProperty ;
  rdfs:domain fibo-fnd-acc-cur:Currency ;
  rdfs:label "has currency name" ;
  rdfs:range xsd:string ;
  rdfs:subPropertyOf fibo-fnd-aap-agt:hasName ;
  skos:definition "provides the full currency name, including diacritical marks" ;
.
fibo-fnd-acc-cur:hasCurrencyTag
  rdf:type owl:DatatypeProperty ;
  rdfs:domain fibo-fnd-acc-cur:CurrencyIdentifier ;
  rdfs:label "has currency tag" ;
  rdfs:range xsd:string ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:hasUniqueIdentifier ;
  skos:definition "relates a unique three-character string to the identifier for a currency" ;
.
fibo-fnd-acc-cur:hasDealtCurrency
  rdf:type owl:ObjectProperty ;
  rdfs:label "has dealt currency" ;
  rdfs:range fibo-fnd-acc-cur:Currency ;
  rdfs:subPropertyOf fibo-fnd-acc-cur:hasCurrency ;
  skos:definition "a predicate indicating the dealt currency in an exchange rate; R units of this currency represent one unit of the base currency" ;
.
fibo-fnd-acc-cur:hasFundsTag
  rdf:type owl:DatatypeProperty ;
  rdfs:domain fibo-fnd-acc-cur:FundsIdentifier ;
  rdfs:label "has funds tag" ;
  rdfs:range xsd:string ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:hasUniqueIdentifier ;
  skos:definition "relates a unique three-character string to the identifier for funds" ;
.
fibo-fnd-acc-cur:hasFundsType
  rdf:type owl:DatatypeProperty ;
  rdfs:domain fibo-fnd-acc-cur:Funds ;
  rdfs:label "has funds type" ;
  rdfs:range xsd:string ;
  skos:definition "indicates the type of funds, such as next day for US funds" ;
.
fibo-fnd-acc-cur:hasMinorUnit
  rdf:type owl:DatatypeProperty ;
  rdfs:label "has minor unit" ;
  rdfs:range xsd:string ;
  skos:definition "relates a code for the minor unit of currency to the currency or fund" ;
  skos:scopeNote """Requirements sometimes arise for values to be expressed in terms of minor units of currency. When this occurs, it is necessary to know the decimal relationship that exists between the currency concerned and its minor unit. 
- 0 means that there is no minor unit for the currency; 
- 1, 2, and 3 signify a ratio of 10 to 1, 100 to 1 and 1000 to 1 respectively.""" ;
.
fibo-fnd-acc-cur:hasNotionalAmount
  rdf:type owl:ObjectProperty ;
  rdfs:label "has notional amount" ;
  rdfs:range fibo-fnd-acc-cur:MonetaryAmount ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  rdfs:subPropertyOf fibo-ln-ln-up:cur-hasAmount ;
  skos:definition "has a notional value expressed as some monetary amount, that is a number and a currency in which that number is denominated" ;
  skos:editorialNote "The domain for this property should be interpreted as being an abstraction which covers various forms of commitment, which may set out the existence of some notional amount of money, specified via this property.  This is left unspecified for now, so that the property can also be defined directly as being a property of some contractual term which describes that commitment." ;
.
fibo-fnd-acc-cur:hasNumericCode
  rdf:type owl:DatatypeProperty ;
  rdfs:label "has numeric code" ;
  rdfs:range xsd:string ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:hasUniqueIdentifier ;
  skos:definition "relates a numeric code to the currency or fund" ;
  skos:scopeNote "The numeric currency code is derived, where possible, from the United Nations Standard Country or Area Code. Additional codes to meet special requirements (as described in 5.1.3) and in respect of funds will be allocated as necessary from within the user-assigned range of codes 950 to 998. Funds codes are allocated in descending order commencing at 998." ;
.
fibo-fnd-acc-cur:hasPrice
  rdf:type owl:ObjectProperty ;
  rdfs:label "has price" ;
  rdfs:range fibo-fnd-acc-cur:Price ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "indicates the value of something expressed as an amount of money or goods" ;
.
fibo-fnd-acc-cur:hasRateValue
  rdf:type owl:DatatypeProperty ;
  rdfs:label "has rate value" ;
  rdfs:range xsd:decimal ;
  skos:definition "has a value for a rate expressed as a number (may be a percentage or raw number)" ;
.
fibo-fnd-acc-cur:isTenderIn
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-acc-cur:Currency ;
  rdfs:label "is tender in" ;
  rdfs:range fibo-fnd-law-jur:Jurisdiction ;
  skos:definition "a jurisdiction in which the currency is exchangeable for goods and services" ;
  fibo-fnd-utl-av:explanatoryNote "Commonly referred to also as legal tender, however this definition does not hold literally in some countries e.g. Scotland." ;
.
fibo-fnd-acc-dbpx:
  rdfs:label "DebtPerspectives"@en ;
.
fibo-fnd-acc-dbpx:CreditSide
  rdf:type owl:Class ;
  rdfs:label "credit side"@en ;
  rdfs:subClassOf fibo-fnd-acc-dbpx:OwingSide ;
  rdfs:subClassOf fibo-fnd-soc-asp:Claim ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-acc-dbpx:hasOtherViewpoint ;
      owl:someValuesFrom fibo-fnd-acc-dbpx:DebtSide ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-acc-dbpx:isPositionInRespectOf ;
      owl:someValuesFrom fibo-fbc-dae-dbt:Debt ;
    ] ;
  owl:disjointWith fibo-fnd-acc-dbpx:DebtSide ;
.
fibo-fnd-acc-dbpx:DebtSide
  rdf:type owl:Class ;
  rdfs:label "debt side"@en ;
  rdfs:subClassOf fibo-fnd-acc-acc:Liability ;
  rdfs:subClassOf fibo-fnd-acc-dbpx:OwingSide ;
  rdfs:subClassOf fibo-fnd-soc-asp:Claim ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-acc-dbpx:isDebtTo ;
      owl:someValuesFrom fibo-fbc-dae-dbt:Creditor ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-acc-dbpx:isPositionInRespectOf ;
      owl:someValuesFrom fibo-fbc-dae-dbt:Debt ;
    ] ;
  skos:definition "Money which is owed by one party (the Debtor) to another party (the Creditor) at some point in time, as seen from the perspective of the Debtor."@en ;
  fibo-fnd-utl-av:explanatoryNote "This is an amount that varies over time, in the form of a debt balance. This is distinct from \"Debt\" such as a bond or other debt instrument, which is a specific debt amount advanced to the borrower at a given point of time; the balance of the latter at any point in time is the debt owed by the borrower (as Debtor) to the Lender (as Creditor)."@en ;
.
fibo-fnd-acc-dbpx:OwingSide
  rdf:type owl:Class ;
  rdfs:label "owing side"@en ;
  rdfs:subClassOf owl:Thing ;
.
fibo-fnd-acc-dbpx:hasOtherViewpoint
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-acc-dbpx:CreditSide ;
  rdfs:label "has other viewpoint"@en ;
  rdfs:range fibo-fnd-acc-dbpx:DebtSide ;
.
fibo-fnd-acc-dbpx:isDebtTo
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-acc-dbpx:DebtSide ;
  rdfs:label "is debt to"@en ;
  rdfs:range fibo-fbc-dae-dbt:Creditor ;
.
fibo-fnd-acc-dbpx:isPositionInRespectOf
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-acc-dbpx:OwingSide ;
  rdfs:label "is position in respect of"@en ;
  rdfs:range fibo-fbc-dae-dbt:Debt ;
.
fibo-fnd-acc-dtt:
  rdfs:label "fibo-fnd-acc-dtt"@en ;
.
fibo-fnd-acc-dtt:Accreting
  rdf:type owl:Class ;
  rdfs:label "accreting"@en ;
  rdfs:subClassOf fibo-fnd-acc-dtt:AmortizationType ;
.
fibo-fnd-acc-dtt:Accrual
  rdf:type owl:Class ;
  rdfs:label "accrual"@en ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-acc-dtt:appliesTo ;
      owl:someValuesFrom fibo-fnd-acc-dtt:Interest ;
    ] ;
  skos:definition "The build-up of interest over a period of time."@en ;
  fibo-fnd-utl-av:explanatoryNote "There are legal contractual terms for the accrual of interest, as distinct from the payment of interest. Action: Add this."@en ;
.
fibo-fnd-acc-dtt:Amortization
  rdf:type owl:Class ;
  rdfs:label "amortization"@en ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-acc-dtt:isAmortizationOf ;
      owl:someValuesFrom fibo-fnd-acc-dtt:DebtFinance ;
    ] ;
.
fibo-fnd-acc-dtt:AmortizationType
  rdf:type owl:Class ;
  rdfs:label "amortization type"@en ;
  rdfs:subClassOf fibo-fnd-arrx-meth:Method ;
  skos:definition "The different types of principal payments applicable to a debt security."@en ;
.
fibo-fnd-acc-dtt:Borrower
  rdf:type owl:Class ;
  rdfs:label "borrower"@en ;
  rdfs:subClassOf fibo-fnd-acc-dtt:Debtor ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty <https://spec.edmcouncil.org/fibo/FND/Accounting/DebtTerms/partyTo.1> ;
      owl:someValuesFrom fibo-fnd-acc-dtt:DebtContractTermsSet ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pty-pty:hasPartyIdentity ;
      owl:someValuesFrom fibo-be-le-lp:LegalPerson ;
    ] ;
.
fibo-fnd-acc-dtt:Collateral
  rdf:type owl:Class ;
  rdfs:label "collateral"@en ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-acc-acc:takesForm ;
      owl:someValuesFrom fibo-fnd-oac-own:Asset ;
    ] ;
  skos:definition "Assets pledged in the provision of debt or loan products or securities."@en ;
  fibo-fnd-utl-av:explanatoryNote "See also \"Loan collateral\" which is defined specifically in the context of an individual loan or mortage. These terms may be duplicates, depending on whether there is some more abstract meaning for collateralization of securitized debt."@en ;
.
fibo-fnd-acc-dtt:Creditor
  rdf:type owl:Class ;
  rdfs:label "creditor"@en ;
  rdfs:subClassOf fibo-fnd-pty-pty:PartyInRole ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-acc-dtt:isOwed ;
      owl:someValuesFrom fibo-fnd-acc-dtt:MoniesOwing ;
    ] ;
.
fibo-fnd-acc-dtt:DebtContractTermsSet
  rdf:type owl:Class ;
  rdfs:label "debt terms"@en ;
  rdfs:subClassOf fibo-fnd-agr-ctr:ContractTermsSet ;
  skos:definition "Terms setting out the formal rights and obligations of borrower and lender under a contract in which funds are lent from the one party to the other."@en ;
  fibo-fnd-utl-av:explanatoryNote "These may be terms in a Loan Contract (incliding for example Mportage ContracT) or they may be the contractual terms of a Debt security."@en ;
.
fibo-fnd-acc-dtt:DebtFinance
  rdf:type owl:Class ;
  rdfs:label "debt finance"@en ;
  rdfs:subClassOf fibo-fbc-dae-dbt:Debt ;
  rdfs:subClassOf fibo-fnd-acc-dtt:MoniesOwing ;
  rdfs:subClassOf fibo-fnd-ptyx-prc:RelationshipContext ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-acc-dtt:Collateral ;
      owl:onProperty fibo-fnd-acc-dtt:collateralizedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-acc-dtt:hasBorrowingParty ;
      owl:someValuesFrom fibo-fnd-acc-dtt:Borrower ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-acc-dtt:hasLendingParty ;
      owl:someValuesFrom fibo-fnd-acc-dtt:Lender ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-acc-dtt:hasfibo-fnd-acc-dtt ;
      owl:someValuesFrom fibo-fnd-acc-dtt:DebtContractTermsSet ;
    ] ;
  skos:definition "Funds loaned or extended from one party to another for some purpose. Further notes: This is a loan or a debt security principal amount. This term is distinguished from \"Debt\" or \"Credit\" in that the latter are balances at a point in time; however it is also referred to as debt for example in debt securities, since a loaned or securitized amount constitutes a debt when it is advanced (and ceases to constitute a debt when it is paid off). As such, this usually has interest payable and may have collateral. In Islamic products there is no interest. This may be a loan (realized as a draw-down on a credit facillity) or it may be a debt set up as part of the issuance of a debt security."@en ;
.
fibo-fnd-acc-dtt:DebtInterest
  rdf:type owl:Class ;
  rdfs:label "debt interest"@en ;
  rdfs:subClassOf fibo-fnd-acc-dtt:Interest ;
  skos:definition "An amount payable by a debtor to a creditor in consideration of a current debt amount at a given point in time."@en ;
.
fibo-fnd-acc-dtt:DebtInterestPaymentTermsSet
  rdf:type owl:Class ;
  rdfs:label "debt interest payment terms set"@en ;
  rdfs:subClassOf fibo-fnd-acc-dtt:DebtContractTermsSet ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-acc-dtt:governsPaymentOf ;
      owl:someValuesFrom fibo-fnd-acc-dtt:Interest ;
    ] ;
  skos:definition "Terms for payment of Interest on any Debt."@en ;
  fibo-fnd-utl-av:explanatoryNote "Interest is usually payable on any outstanding principal amount, therefore Interest itself relates to the amount of debt at any given point of time, not to the principal amount advanced at the time that the loan was advanced or the debt security issued."@en ;
.
fibo-fnd-acc-dtt:DebtPrincipalRepaymentTermsSet
  rdf:type owl:Class ;
  rdfs:label "debt principal repayment terms set"@en ;
  rdfs:subClassOf fibo-fnd-acc-dtt:DebtContractTermsSet ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-sec-dbt-dbt-dbt:DayCountBasis ;
      owl:onProperty fibo-fnd-acc-dtt:amortizationCalculationsDayCountConvention ;
    ] ;
  skos:definition "Terms for repayment on the principal of any debt."@en ;
.
fibo-fnd-acc-dtt:Debtor
  rdf:type owl:Class ;
  rdfs:label "debtor"@en ;
  rdfs:subClassOf fibo-fnd-pty-pty:PartyInRole ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-acc-dtt:owes ;
      owl:someValuesFrom fibo-fnd-acc-dtt:MoniesOwing ;
    ] ;
  skos:definition "The party which owes the debt."@en ;
.
fibo-fnd-acc-dtt:Interest
  rdf:type owl:Class ;
  rdfs:label "interest"@en ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-acc-dtt:isInterestOn ;
      owl:someValuesFrom fibo-fnd-acc-dtt:MoniesOwing ;
    ] ;
  skos:definition "An amount payable by a debtor to a creditor in consideration of the credit extended to the debtor."@en ;
.
fibo-fnd-acc-dtt:InterestOnly
  rdf:type owl:Class ;
  rdfs:label "interest only"@en ;
  rdfs:subClassOf fibo-fnd-acc-dtt:AmortizationType ;
.
fibo-fnd-acc-dtt:Lender
  rdf:type owl:Class ;
  rdfs:label "lender"@en ;
  rdfs:subClassOf fibo-fnd-acc-dtt:Creditor ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-acc-dtt:partyTo ;
      owl:someValuesFrom fibo-fnd-acc-dtt:DebtContractTermsSet ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pty-pty:hasPartyIdentity ;
      owl:someValuesFrom fibo-be-le-lp:LegalPerson ;
    ] ;
  skos:editorialNote "lender: The lender of a loan is some form of Legal Entity."@en ;
  fibo-fnd-utl-av:explanatoryNote "For formal loans, the entity will be an Artificial Person. For loans more generally, it may be any legal entity."@en ;
.
fibo-fnd-acc-dtt:MoniesOwing
  rdf:type owl:Class ;
  rdfs:label "debt"@en ;
  rdfs:subClassOf fibo-fnd-agr-agr:Commitment ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-acc-acc:isMeasuredAs ;
      owl:someValuesFrom fibo-fnd-acc-acc:DatedBalance ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-acc-dtt:owedBy ;
      owl:someValuesFrom fibo-fnd-acc-dtt:Debtor ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-acc-dtt:owedTo ;
      owl:someValuesFrom fibo-fnd-acc-dtt:Creditor ;
    ] ;
  skos:definition "A sum of money owed by one party to another party."@en ;
  fibo-fnd-utl-av:explanatoryNote "This is an amount of money regarded as a debt by one party, or as credit by another party. Note that the sub terms Credit and Debt are also balance sheet entries, that is these can be anything in which monies are owed by one party to another. Monies Owing may therefore also be goods payable/receivable. This term is therefore independent of the existence of any formal credit facility provided by a credit provider. Change note Reviewed 18 Oct 2012 and determined that Monies Owing is a mediating thing (context0 in which Credit and Debt are defined, and not a common parent of those two things. Credit and Debt changed to be sub classes of Relative Thing and not cub classes of Monies Owing as before. New relationship fact 'viewpoint' added to Credit and similarly to Debt, with Monies Owing as the target of that fact, in place of the previous superclass relationship."@en ;
.
fibo-fnd-acc-dtt:PrincipalOnly
  rdf:type owl:Class ;
  rdfs:label "principal only"@en ;
  rdfs:subClassOf fibo-fnd-acc-dtt:AmortizationType ;
.
fibo-fnd-acc-dtt:PrincipalPlusInterest
  rdf:type owl:Class ;
  rdfs:label "principal plus interest"@en ;
  rdfs:subClassOf fibo-fnd-acc-dtt:AmortizationType ;
.
fibo-fnd-acc-dtt:accrualBasis
  rdf:type owl:DatatypeProperty ;
  rdfs:label "accrual basis"@en ;
.
fibo-fnd-acc-dtt:amortizationCalculationsDayCountConvention
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-acc-dtt:DebtPrincipalRepaymentTermsSet ;
  rdfs:label "amortization calculations day count convention"@en ;
  rdfs:range fibo-sec-dbt-dbt-dbt:DayCountBasis ;
.
fibo-fnd-acc-dtt:appliesTo
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-acc-dtt:Accrual ;
  rdfs:label "applies to1"@en ;
  rdfs:range fibo-fnd-acc-dtt:Interest ;
.
fibo-fnd-acc-dtt:collateralizedBy
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-acc-dtt:DebtFinance ;
  rdfs:label "collateralized by"@en ;
  rdfs:range fibo-fnd-acc-dtt:Collateral ;
.
fibo-fnd-acc-dtt:debtAmount
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-acc-dtt:DebtFinance ;
  rdfs:label "debt amount"@en ;
  rdfs:range fibo-fnd-acc-cur:MonetaryAmount ;
  skos:definition "The amount of the finance extended as debt from one party to the other. Further notes: This is not the balance of the debt at a point in time, but the amount drawn down at a specific point in time, after which (for interest bearing debts) interest becomes payable. Semantic Modeling note This term is over-ridden for specific kinds of debt (securities, loans) and is therefore not needed in any data model. It is included here to define part of the meaning of the Debt Finance term. It is a Relationship Fact and strictly speaking should be regarded as being specialized and over-ridden by the terms in the Loan and Security classes, however it is modeled here as a \"Referenceable Archetype\" meaning that it appears in diagrams as a textual entry not a relationship line, and so it is not possible to formally show this specialization."@en ;
.
fibo-fnd-acc-dtt:governsPaymentOf
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-acc-dtt:DebtInterestPaymentTermsSet ;
  rdfs:label "governs payment of"@en ;
  rdfs:range fibo-fnd-acc-dtt:Interest ;
.
fibo-fnd-acc-dtt:hasAccrualBasis
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-acc-dtt:DebtInterestPaymentTermsSet ;
  rdfs:label "accrual basis"@en ;
  rdfs:range fibo-sec-dbt-dbt-dbt:DayCountBasis ;
  skos:definition "The basis on which interest accrues on the debt, that is the number of days in a month and days in a year that are counted when performing interest accrual calculations."@en ;
.
fibo-fnd-acc-dtt:hasBorrowingParty
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-acc-dtt:DebtFinance ;
  rdfs:label "has borrowing party"@en ;
  rdfs:range fibo-fnd-acc-dtt:Borrower ;
  rdfs:subPropertyOf fibo-fnd-ptyx-prc:hasPartyInRole ;
.
fibo-fnd-acc-dtt:hasDebtTerms
  rdf:type owl:ObjectProperty ;
  rdfs:label "has debt terms" ;
.
fibo-fnd-acc-dtt:hasInterestRate
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-acc-dtt:DebtInterestPaymentTermsSet ;
  rdfs:label "has interest rate"@en ;
  rdfs:range fibo-fnd-utl-bt:percentage ;
  skos:definition "The rate of annual interest that is to be paid by the debtor to the lender on the debt."@en ;
.
fibo-fnd-acc-dtt:hasLendingParty
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-acc-dtt:DebtFinance ;
  rdfs:label "has lending party"@en ;
  rdfs:range fibo-fnd-acc-dtt:Lender ;
  rdfs:subPropertyOf fibo-fnd-ptyx-prc:hasPartyInRole ;
.
fibo-fnd-acc-dtt:hasPrincipalAmortizationType
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-acc-dtt:DebtPrincipalRepaymentTermsSet ;
  rdfs:label "has principal amortization type"@en ;
  rdfs:range fibo-fnd-acc-dtt:AmortizationType ;
  skos:definition "The type of principal payments applicable to this security."@en ;
.
fibo-fnd-acc-dtt:hasfibo-fnd-acc-dtt
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-acc-dtt:DebtFinance ;
  rdfs:label "has debt terms"@en ;
  rdfs:range fibo-fnd-acc-dtt:DebtContractTermsSet ;
  rdfs:subPropertyOf fibo-fnd-agr-ctr:contractHasTerms ;
.
fibo-fnd-acc-dtt:interestRate
  rdf:type owl:DatatypeProperty ;
  rdfs:label "interest rate"@en ;
.
fibo-fnd-acc-dtt:isAmortizationOf
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-acc-dtt:Amortization ;
  rdfs:label "is amortization of"@en ;
  rdfs:range fibo-fnd-acc-dtt:DebtFinance ;
.
fibo-fnd-acc-dtt:isInterestOn
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-acc-dtt:Interest ;
  rdfs:label "is interest on"@en ;
  rdfs:range owl:Thing ;
.
fibo-fnd-acc-dtt:isOwed
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-acc-dtt:Creditor ;
  rdfs:label "is owed"@en ;
  rdfs:range fibo-fnd-acc-dtt:MoniesOwing ;
  rdfs:subPropertyOf fibo-fnd-pty-pty:isPartyInRelationTo ;
.
fibo-fnd-acc-dtt:isSumOf
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-acc-dtt:MoniesOwing ;
  rdfs:label "is sum of"@en ;
  rdfs:range fibo-md-tem-tem:DatedMonetaryAmount ;
  skos:definition "The monetary amount of the credit or loan."@en ;
.
fibo-fnd-acc-dtt:owedBy
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-acc-dtt:MoniesOwing ;
  rdfs:label "owed by"@en ;
  rdfs:range fibo-fnd-acc-dtt:Debtor ;
  rdfs:subPropertyOf fibo-fnd-agr-agr:hasCommitmentParty ;
.
fibo-fnd-acc-dtt:owedTo
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-acc-dtt:MoniesOwing ;
  rdfs:label "owed to"@en ;
  rdfs:range fibo-fnd-acc-dtt:Creditor ;
  rdfs:subPropertyOf fibo-fnd-agr-agr:hasCommitmentParty ;
.
fibo-fnd-acc-dtt:owes
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-acc-dtt:Debtor ;
  rdfs:label "owes"@en ;
  rdfs:range fibo-fnd-acc-dtt:MoniesOwing ;
  rdfs:subPropertyOf fibo-fnd-pty-pty:isPartyInRelationTo ;
  owl:inverseOf fibo-fnd-acc-dtt:owedBy ;
.
fibo-fnd-acc-dtt:partyTo
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-acc-dtt:Lender ;
  rdfs:label "party to"@en ;
  rdfs:range fibo-fnd-acc-dtt:DebtContractTermsSet ;
.
<https://spec.edmcouncil.org/fibo/FND/Accounting/DebtTerms/partyTo.1>
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-acc-dtt:Borrower ;
  rdfs:label "party to"@en ;
  rdfs:range fibo-fnd-acc-dtt:DebtContractTermsSet ;
.
fibo-fnd-acc-dtt:principalRepaymentDate
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-acc-dtt:DebtPrincipalRepaymentTermsSet ;
  rdfs:label "principal repayment date"@en ;
  rdfs:range fibo-fnd-dt-fd:Date ;
  skos:definition "The date when the principal of the debt or loan is to have been repaid in full. Depending on the terms of the debt, or the terms of a debt security in the case of secufritized debt, this may be the date of a single payment of the debt principal or o fthe completion of scheduled partial redemption payments."@en ;
.
