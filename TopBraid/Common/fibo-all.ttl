      rdf:type owl:Restriction ;
      owl:onProperty loan:hasClosingDate ;
      owl:someValuesFrom fibo-fnd-dt-fd:Date ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-ln-ln-up:isEvidencedBy_g ;
      owl:someValuesFrom fibo-fnd-arr-doc:Document ;
    ] ;
  owl:equivalentClass [
      rdf:type owl:Class ;
      owl:intersectionOf (
          loan:LoanContract
          [
            rdf:type owl:Restriction ;
            owl:onProperty fibo-fnd-rel-rel:hasPart ;
            owl:someValuesFrom [
                rdf:type owl:Class ;
