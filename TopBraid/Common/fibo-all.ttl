fibo-sec-sec-id:NationalSecuritiesIdentifyingNumberRegistry
  rdf:type owl:Class ;
  rdfs:label "National Securities Identifying Number registry" ;
  rdfs:subClassOf fibo-fbc-fct-ra:Registry ;
