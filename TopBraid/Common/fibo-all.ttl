  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Parties/Roles/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"^^xsd:anyURI ;
  sm:fileAbbreviation "fibo-fnd-oac-own" ;
  sm:fileAbstract "This ontology defines high-level, ownership-related concepts for use in other FIBO ontology elements. These include the concept of owner, asset and ownership along with relationships between them whereby an asset is some thing owned by some owner." ;
  sm:filename "Ownership.rdf" ;
  rdfs:label "Ownership Ontology" ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/AboutFND/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/OwnershipAndControl/AboutOwnershipAndControl/"^^xsd:anyURI ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/FND/20141101/OwnershipAndControl/Ownership/> ;
  skos:changeNote "The http://www.omg.org/spec/EDMC-FIBO/FND/20130801/OwnershipAndControl/Ownership.rdf version of the ontology was was modified per the issue resolutions identified in the FIBO FND 1.0 FTF report and in http://www.omg.org/spec/EDMC-FIBO/FND/1.0/AboutFND-1.0/." ;
  skos:changeNote """The http://www.omg.org/spec/FIBO/Foundations/20130601/OwnershipAndControl/Ownership.owl version of the ontology was revised in advance of the September 2013 New Brunswick, NJ meeting, as follows:
   (1) to use slash style URI/IRIss (also called 303 URIs, vs. hash style) as required to support server side processing 
   (2) to use version-independent IRIs for all definitions internally as opposed to version-specific IRIs
   (3) to change the file suffix from .owl to .rdf to increase usability in RDF tools
   (4) to use 4-level abbreviations and corresponding namespace prefixes for all FIBO ontologies, reflecting a family/specification/module/ontology structure
   (5) to incorporate changes to the specification metadata to support documentation at the family, specification, module, and ontology level, similar to the abbreviations.""" ;
.
fibo-fnd-oac-own:Asset
  rdf:type owl:Class ;
  rdfs:label "asset" ;
  rdfs:subClassOf fibo-fnd-pty-rl:ThingInRole ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-oac-own:Owner ;
      owl:onProperty fibo-fnd-oac-own:isOwnedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onDataRange xsd:dateTime ;
      owl:onProperty fibo-fnd-rel-rel:hasAcquisitionDate ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "A thing held by some party and having some value." ;
.
fibo-fnd-oac-own:Owner
  rdf:type owl:Class ;
  rdfs:label "owner" ;
  rdfs:subClassOf fibo-fnd-pty-pty:PartyInRole ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
      owl:someValuesFrom [
          rdf:type owl:Restriction ;
          owl:minCardinality "1"^^xsd:nonNegativeInteger ;
          owl:onProperty fibo-fnd-oac-own:owns ;
        ] ;
    ] ;
  skos:definition "A party in the ownership role; one that owns something. The thing owned is an Asset to that Party." ;
.
fibo-fnd-oac-own:Ownership
  rdf:type owl:Class ;
  rdfs:label "ownership" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-dt-fd:DatePeriod ;
      owl:onProperty fibo-fnd-dt-bd:holdsDuring ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-pty-pty:PartyInRole ;
      owl:onProperty fibo-fnd-oac-own:hasOwningParty ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-pty-rl:ThingInRole ;
      owl:onProperty fibo-fnd-oac-own:hasOwnedThing ;
    ] ;
  owl:equivalentClass fibo-fnd-ptyx-prc:Ownership ;
  skos:definition "Ownership is the context in which some Party is said to own some Independent Thing. The Party is defined as such due to its being the owning party to that Thing." ;
.
fibo-fnd-oac-own:hasOwnedThing
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-oac-own:Ownership ;
  rdfs:label "has owned thing" ;
  rdfs:range fibo-fnd-oac-own:Asset ;
  owl:inverseOf fibo-fnd-oac-own:isOwnedThingInRole ;
  skos:definition "indicates the party in an ownership relationship where a party owns a thing" ;
.
fibo-fnd-oac-own:hasOwningParty
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-oac-own:Ownership ;
  rdfs:label "has owning party" ;
  rdfs:range fibo-fnd-oac-own:Owner ;
  owl:inverseOf fibo-fnd-oac-own:isOwningPartyInRole ;
  skos:definition "indicates the thing in an ownership relationship where a party owns a thing" ;
.
fibo-fnd-oac-own:isOwnedBy
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-oac-own:Asset ;
  rdfs:label "is an asset of" ;
  rdfs:range fibo-fnd-oac-own:Owner ;
  skos:definition "identifies the party that owns the asset" ;
.
fibo-fnd-oac-own:isOwnedThingInRole
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-oac-own:Asset ;
  rdfs:label "is an owned thing in role" ;
  rdfs:range fibo-fnd-oac-own:Ownership ;
  skos:definition "indicates the context of ownership in which the thing plays the role of an asset" ;
.
fibo-fnd-oac-own:isOwningPartyInRole
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-oac-own:Owner ;
  rdfs:label "is an owning party in role" ;
  rdfs:range fibo-fnd-oac-own:Ownership ;
  skos:definition "indicates the context of ownership in which the party plays the role of owner" ;
.
fibo-fnd-oac-own:owns
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-pty-pty:IndependentParty ;
  rdfs:label "owns" ;
  skos:definition "to have (something) as one's own, possess" ;
.
