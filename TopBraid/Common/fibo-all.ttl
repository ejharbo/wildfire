  rdfs:subClassOf loan:LoanContract ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty loan:hasCreditLimit ;
      owl:someValuesFrom fibo-fnd-acc-cur:MonetaryAmount ;
    ] ;
