      rdf:type owl:Restriction ;
      owl:onProperty fibo-ln-ln-up:rel-from ;
      owl:someValuesFrom fibo-fnd-aap-agt:AutonomousAgent ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-ln-ln-up:rel-to ;
      owl:someValuesFrom fibo-fnd-aap-agt:AutonomousAgent ;
    ] ;
