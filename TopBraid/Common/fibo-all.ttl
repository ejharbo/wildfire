  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/IND/AboutIND/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/IND/EconomicIndicators/AboutEconomicIndicators/"^^xsd:anyURI ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/IND/20160801/EconomicIndicators/EconomicIndicators/> ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/IND/20140601/EconomicIndicators/EconomicIndicators.rdf version of this ontology was modified per the issue resolutions identified in the FIBO IND 1.0 FTF 1 report." ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/IND/20150501/EconomicIndicators/EconomicIndicators.rdf version of this ontology was modified per the issue resolutions identified in the FIBO IND 1.0 FTF 3 report." ;
.
fibo-ind-ei-ei:Civilian
  rdf:type owl:Class ;
  rdfs:label "civilian" ;
  rdfs:subClassOf fibo-fnd-aap-ppl:LegallyCapablePerson ;
  skos:definition "a person that is not a member of the military (i.e., that is not on active duty)" ;
  fibo-fnd-utl-av:adaptedFrom "U.S. Bureau of Labor Statistics and Statistics Canada reference definitions - https://wiki.edmcouncil.org/pages/viewpage.action?pageId=6358041" ;
.
fibo-ind-ei-ei:CivilianLaborForce
  rdf:type owl:Class ;
  rdfs:label "civilian labor force" ;
  rdfs:subClassOf fibo-ind-ei-ei:CivilianNonInstitutionalPopulation ;
  owl:disjointWith fibo-ind-ei-ei:PopulationNotInLaborForce ;
  skos:definition "a subset of the civilian, non-institutional population considered to be part of the labor force during a given reporting period" ;
  fibo-fnd-utl-av:adaptedFrom "U.S. Bureau of Labor Statistics and Statistics Canada reference definitions - https://wiki.edmcouncil.org/pages/viewpage.action?pageId=6358041" ;
.
fibo-ind-ei-ei:CivilianLaborForceParticipationRate
  rdf:type owl:Class ;
  rdfs:label "civilian labor force participation rate" ;
  rdfs:subClassOf fibo-fnd-utl-alx:Expression ;
  rdfs:subClassOf fibo-ind-ei-ei:EconomicIndicator ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-ind-ei-ei:CivilianLaborForce ;
      owl:onProperty fibo-fnd-utl-alx:hasArgument ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-ind-ei-ei:CivilianNonInstitutionalPopulation ;
      owl:onProperty fibo-fnd-utl-alx:hasArgument ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:appliesTo ;
      owl:someValuesFrom fibo-ind-ei-ei:CivilianLaborForce ;
    ] ;
  skos:definition "an economic indicator representing the rate of participation the labor force of a given economy for some specified period" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.bls.gov/bls/cps_fact_sheets/lfp_mock.htm"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "The labor force participation rate is the percentage of the population that is either employed or unemployed (that is, either working or actively seeking work)." ;
  fibo-ind-ei-ei:actualExpression "civilian labor force ÷ civilian non-institutional population" ;
.
fibo-ind-ei-ei:CivilianNonInstitutionalPopulation
  rdf:type owl:Class ;
  rdfs:label "civilian non-institutional population" ;
  rdfs:subClassOf fibo-ind-ei-ei:StatisticalUniverse ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-ind-ei-ei:InstitutionalPerson ;
      owl:onProperty fibo-fnd-rel-rel:hasMember ;
      owl:qualifiedCardinality "0"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:hasMember ;
      owl:someValuesFrom [
          rdf:type owl:Class ;
          owl:intersectionOf (
              fibo-ind-ei-ei:Civilian
              [
                rdf:type owl:Restriction ;
                owl:allValuesFrom xsd:integer ;
                owl:onProperty fibo-ind-ei-ei:hasMinimumLegalWorkingAge ;
              ]
            ) ;
        ] ;
    ] ;
  skos:definition "a statistical universe consisting of people of a certain age who reside in a given region, do not live in institutions (for example, correctional facilities, long-term care hospitals, and nursing homes), and are not on active military duty" ;
  skos:scopeNote "The civilian non-institutional population is typically reported in thousands." ;
  fibo-fnd-utl-av:adaptedFrom "U.S. Bureau of Labor Statistics and Statistics Canada reference definitions - https://wiki.edmcouncil.org/pages/viewpage.action?pageId=6358041" ;
.
fibo-ind-ei-ei:CombinedStatisticalArea
  rdf:type owl:Class ;
  rdfs:label "combined statistical area" ;
  rdfs:subClassOf fibo-ind-ei-ei:StatisticalArea ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-ind-ei-ei:StatisticalArea ;
      owl:onProperty fibo-fnd-rel-rel:comprises ;
    ] ;
  skos:definition "a combination of adjacent metropolitan and micropolitan areas with economic ties measured by commuting patterns" ;
  fibo-fnd-utl-av:abbreviation "CSA" ;
  fibo-fnd-utl-av:adaptedFrom <https://en.wikipedia.org/wiki/Combined_statistical_area> ;
  fibo-fnd-utl-av:adaptedFrom <https://www.census.gov/population/metro/> ;
  fibo-fnd-utl-av:adaptedFrom <https://www.whitehouse.gov/sites/default/files/omb/assets/fedreg_2010/06282010_metro_standards-Complete.pdf> ;
  fibo-fnd-utl-av:explanatoryNote "These areas that combine retain their own designations as metropolitan or micropolitan statistical areas within the larger combined statistical area." ;
.
fibo-ind-ei-ei:Consumer
  rdf:type owl:Class ;
  rdfs:label "consumer" ;
  rdfs:subClassOf fibo-fnd-aap-ppl:Person ;
  skos:definition "a person that is the ultimate user of a product or service" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
  fibo-fnd-utl-av:explanatoryNote "For the purposes of the CPI, the definition of consumer is limited to humans.  In general, a consumer could include a pet, as the consumer of pet food, for example, although the pet owner would likely be the purchaser and target of advertising." ;
  fibo-fnd-utl-av:explanatoryNote "The consumer is not always the purchaser of the product.  Consumers are considered to be the users of the final product.  For example, purchasers of building products are interim users of these products while constructing the finished product, which then may be purchased by the consumer." ;
.
fibo-ind-ei-ei:ConsumerPriceIndex
  rdf:type owl:Class ;
  rdfs:label "consumer price index" ;
  rdfs:subClassOf fibo-fnd-utl-alx:Expression ;
  rdfs:subClassOf fibo-ind-ei-ei:EconomicIndicator ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-ind-ei-ei:CivilianNonInstitutionalPopulation ;
      owl:onProperty fibo-fnd-utl-alx:hasArgument ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:appliesTo ;
      owl:someValuesFrom fibo-ind-ei-ei:CivilianNonInstitutionalPopulation ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-utl-alx:hasArgument ;
      owl:someValuesFrom fibo-fbc-pas-fpas:Basket ;
    ] ;
  owl:disjointWith fibo-ind-ei-ei:CivilianLaborForceParticipationRate ;
  owl:disjointWith fibo-ind-ei-ei:EmploymentPopulationRatio ;
  owl:disjointWith fibo-ind-ei-ei:GrossDomesticProduct ;
  owl:disjointWith fibo-ind-ei-ei:InflationRate ;
  owl:disjointWith fibo-ind-ei-ei:UnemploymentRate ;
  skos:definition "an economic indicator representing a measure of the change over time in the prices of consumer goods and services that households consume" ;
  fibo-fnd-utl-av:abbreviation "CPI" ;
  fibo-fnd-utl-av:adaptedFrom "http://unstats.un.org/unsd/nationalaccount/docs/SNA2008.pdf"^^xsd:anyURI ;
  fibo-fnd-utl-av:adaptedFrom "http://www.ilo.org/public/english/bureau/stat/guides/cpi/"^^xsd:anyURI ;
.
fibo-ind-ei-ei:EconomicIndicator
  rdf:type owl:Class ;
  rdfs:label "economic indicator" ;
  rdfs:subClassOf fibo-fnd-utl-alx:StatisticalMeasure ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom xsd:boolean ;
      owl:onProperty fibo-ind-ei-ei:isSeasonallyAdjusted ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-dt-fd:Date ;
      owl:onProperty fibo-ind-ei-ei:hasReleaseDate ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-dt-fd:Date ;
      owl:onProperty fibo-ind-ei-ei:hasSeriesOrigin ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-dt-fd:RecurrenceInterval ;
      owl:onProperty fibo-ind-ei-ei:hasPeriodicity ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-dt-fd:ExplicitDatePeriod ;
      owl:onProperty fibo-ind-ei-ei:hasReportingPeriod ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-qt-qtu:QuantityValue ;
      owl:onProperty fibo-ind-ei-ei:hasIndicatorValue ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:appliesTo ;
      owl:someValuesFrom fibo-ind-ei-ei:StatisticalArea ;
    ] ;
  skos:definition "a statistical measure of some economic activity in the context of a statistical area (region), used for analysis of economic performance and predictions of future performance" ;
  skos:example "Example indicators include the average work week, weekly claims for unemployment insurance, new orders, vendor performance, stock prices, and changes in the money supply." ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economic Terms, Fifth Edition, 2012" ;
.
fibo-ind-ei-ei:EmployedPopulation
  rdf:type owl:Class ;
  rdfs:label "employed population" ;
  rdfs:subClassOf fibo-ind-ei-ei:CivilianLaborForce ;
  skos:definition "a subset of the civilian labor force considered to be employed during the reporting period" ;
  fibo-fnd-utl-av:adaptedFrom "U.S. Bureau of Labor Statistics and Statistics Canada reference definitions - https://wiki.edmcouncil.org/pages/viewpage.action?pageId=6358041" ;
  fibo-fnd-utl-av:explanatoryNote "There are a number of distinctions with respect to how individuals are counted from country to country, including whether or not they are considered employed if they are on unpaid leave for some reason, and whether or not they are counted multiple times if they have more than one paying job." ;
.
fibo-ind-ei-ei:EmploymentPopulationRatio
  rdf:type owl:Class ;
  rdfs:label "employment-population ratio" ;
  rdfs:seeAlso <http://www.bls.gov/news.release/pdf/empsit.pdf> ;
  rdfs:subClassOf fibo-fnd-utl-alx:Expression ;
  rdfs:subClassOf fibo-ind-ei-ei:EconomicIndicator ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-ind-ei-ei:CivilianNonInstitutionalPopulation ;
      owl:onProperty fibo-fnd-utl-alx:hasArgument ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-ind-ei-ei:EmployedPopulation ;
      owl:onProperty fibo-fnd-utl-alx:hasArgument ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:appliesTo ;
      owl:someValuesFrom fibo-ind-ei-ei:EmployedPopulation ;
    ] ;
  skos:definition "an economic indicator representing the ratio of the employed population with respect to the overall civilian non-institutional population of a given economy for some specified period" ;
  fibo-ind-ei-ei:actualExpression "employed population ÷ civilian non-institutional population" ;
.
fibo-ind-ei-ei:Enterprise
  rdf:type owl:Class ;
  rdfs:label "enterprise" ;
  rdfs:subClassOf fibo-be-fct-fct:FunctionalBusinessEntity ;
  rdfs:subClassOf [
      rdf:type owl:Class ;
      owl:unionOf (
          fibo-fnd-pas-pas:Producer
          fibo-fnd-pas-pas:ServiceProvider
        ) ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:appliesTo ;
      owl:someValuesFrom [
          rdf:type owl:Class ;
          owl:unionOf (
              fibo-fnd-pas-pas:Good
              fibo-fnd-pas-pas:Service
            ) ;
        ] ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:isClassifiedBy ;
      owl:someValuesFrom fibo-fnd-arr-cls:IndustrySectorClassifier ;
    ] ;
  skos:definition "a functional business entity that produces and/or sells goods or services" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.bls.gov/opub/hom/glossary.htm#E"^^xsd:anyURI ;
  fibo-fnd-utl-av:adaptedFrom "http://www.businessdictionary.com/definition/establishment.html"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "An enterprise (a private firm, government, or nonprofit organization) can consist of a single establishment or multiple establishments. All establishments in an enterprise may be classified in one industry (e.g., a chain), or they may be classified in different industries (e.g., a conglomerate)." ;
.
fibo-ind-ei-ei:EnterprisePopulation
  rdf:type owl:Class ;
  rdfs:label "enterprise population" ;
  rdfs:subClassOf fibo-ind-ei-ei:StatisticalUniverse ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:hasMember ;
      owl:someValuesFrom fibo-ind-ei-ei:Enterprise ;
    ] ;
  skos:definition "a statistical universe consisting of enterprises designed for the purposes of supporting surveys such as those used as the basis for employment and producer price indices" ;
.
fibo-ind-ei-ei:Establishment
  rdf:type owl:Class ;
  rdfs:label "establishment" ;
  rdfs:subClassOf fibo-ind-ei-ei:Enterprise ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-plc-adr:PhysicalAddress ;
      owl:onProperty fibo-fnd-plc-adr:hasAddress ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "an enterprise (or part of an enterprise) that operates from a single physical location" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.bls.gov/opub/hom/glossary.htm#E"^^xsd:anyURI ;
  fibo-fnd-utl-av:adaptedFrom "http://www.businessdictionary.com/definition/establishment.html"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "The physical location of a certain economic activity - for example, a factory, mine, store, or office. A single establishment generally produces a single good or provides a single service." ;
.
fibo-ind-ei-ei:EstablishmentPopulation
  rdf:type owl:Class ;
  rdfs:label "establishment population" ;
  rdfs:subClassOf fibo-ind-ei-ei:EnterprisePopulation ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:hasMember ;
      owl:someValuesFrom fibo-ind-ei-ei:Establishment ;
    ] ;
  skos:definition "a subset of the enterprise population focused on establishments" ;
.
fibo-ind-ei-ei:FixedBasket
  rdf:type owl:Class ;
  rdfs:label "fixed basket" ;
  rdfs:subClassOf fibo-fbc-pas-fpas:Basket ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fbc-pas-fpas:hasConstituent ;
      owl:someValuesFrom fibo-ind-ei-ei:FixedBasketConstituent ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-dt-fd:hasDatePeriod ;
      owl:someValuesFrom fibo-fnd-dt-fd:DatePeriod ;
    ] ;
  skos:definition "a basket of goods and services whose quantity and quality are held fixed for some period of time" ;
  fibo-fnd-utl-av:adaptedFrom "https://www.imf.org/external/pubs/ft/ppi/2010/manual/ppi.pdf"^^xsd:anyURI ;
.
fibo-ind-ei-ei:FixedBasketConstituent
  rdf:type owl:Class ;
  rdfs:label "fixed basket constituent" ;
  rdfs:subClassOf fibo-fbc-pas-fpas:BasketConstituent ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom [
          rdf:type owl:Class ;
          owl:unionOf (
              fibo-fnd-pas-pas:Good
              fibo-fnd-pas-pas:Service
            ) ;
        ] ;
      owl:onProperty fibo-fnd-rel-rel:hasIdentity ;
    ] ;
  owl:equivalentClass [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
      owl:someValuesFrom [
          rdf:type owl:Restriction ;
          owl:onProperty fibo-fbc-pas-fpas:isConstituentOf ;
          owl:someValuesFrom fibo-ind-ei-ei:FixedBasket ;
        ] ;
    ] ;
  skos:definition "a component of a fixed basket" ;
.
fibo-ind-ei-ei:GoodsOrServicesPopulation
  rdf:type owl:Class ;
  rdfs:label "goods or services population" ;
  rdfs:subClassOf fibo-ind-ei-ei:StatisticalUniverse ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fbc-pas-fpas:hasConstituent ;
      owl:someValuesFrom [
          rdf:type owl:Class ;
          owl:unionOf (
              fibo-fnd-pas-pas:Good
              fibo-fnd-pas-pas:Service
            ) ;
        ] ;
    ] ;
  skos:definition "a statistical universe consisting of specific goods and/or services designed for the purposes of supporting surveys such as those used as the basis for price indices" ;
  fibo-fnd-utl-av:adaptedFrom "https://www.imf.org/external/pubs/ft/ppi/2010/manual/ppi.pdf"^^xsd:anyURI ;
.
fibo-ind-ei-ei:GrossDomesticProduct
  rdf:type owl:Class ;
  rdfs:label "gross domestic product" ;
  rdfs:seeAlso <http://unstats.un.org/unsd/nationalaccount/docs/SNA2008.pdf> ;
  rdfs:subClassOf fibo-ind-ei-ei:EconomicIndicator ;
  owl:disjointWith fibo-ind-ei-ei:UnemploymentRate ;
  skos:definition "an economic indicator representing the broadest measure of aggregate economic activity, measuring the total unduplicated market value of all final goods and services produced within a statistical area in a period" ;
  fibo-fnd-utl-av:abbreviation "GDP" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.treasury.gov/initiatives/ofr/about/Documents/AR2013_Back_Matter_Glossary+Bib_Refs+Endnotes.pdf"^^xsd:anyURI ;
  fibo-fnd-utl-av:adaptedFrom "https://en.wikipedia.org/wiki/Gross_domestic_product"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "GDP represents a valuation expressed in terms of the prices actually paid by the purchaser after all applicable taxes and subsidies." ;
.
fibo-ind-ei-ei:Household
  rdf:type owl:Class ;
  rdfs:label "household" ;
  rdfs:subClassOf fibo-be-fct-fct:FunctionalEntity ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-ind-ei-ei:HousingUnit ;
      owl:onProperty fibo-fnd-plc-loc:isLocatedAt ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:hasMember ;
      owl:someValuesFrom fibo-fnd-aap-ppl:Person ;
    ] ;
  skos:definition "an individual or small group of persons who occupy a housing unit (such as a house or apartment) as their usual place of residence, who pool some, or all, of their income and wealth and who consume certain types of goods and services collectively, mainly housing and food" ;
  fibo-fnd-utl-av:adaptedFrom <http://stats.oecd.org/glossary/detail.asp?ID=1255> ;
  fibo-fnd-utl-av:adaptedFrom <http://www.census.gov/glossary/#term_Household> ;
  fibo-fnd-utl-av:explanatoryNote """A household may be either (a) a one-person household, that is to say, a person who makes provision for his or her own food or other essentials for living without combining with any other person to form part of a multi-person household or (b) a multi-person household, that is to say, a group of two or more persons living together who make common provision for food or other essentials for living. The persons in the group may pool their incomes and may, to a greater or lesser extent, have a common budget; they may be related or unrelated persons or constitute a combination of persons both related and unrelated. 

A household may be located in a housing unit or in a set of collective living quarters such as a boarding house, a hotel or a camp, or may comprise the administrative personnel in an institution. The household may also be homeless.""" ;
  fibo-fnd-utl-av:explanatoryNote "From the perspective of the U.S Census Bureau, a household includes the related family members and all the unrelated people, if any, such as lodgers, foster children, wards, or employees who share the housing unit. A person living alone in a housing unit, or a group of unrelated people sharing a housing unit such as partners or roomers, is also counted as a household. The count of households excludes group quarters [such as institutional facilities]. There are two major categories of households, 'family' and 'nonfamily'." ;
.
fibo-ind-ei-ei:HousingUnit
  rdf:type owl:Class ;
  rdfs:label "housing unit" ;
  rdfs:subClassOf fibo-fnd-plc-loc:PhysicalLocation ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-plc-adr:PhysicalAddress ;
      owl:onProperty fibo-fnd-plc-adr:hasAddress ;
    ] ;
  skos:definition "a house, an apartment, a mobile home or trailer, a group of rooms, or a single room occupied as separate living quarters, or if vacant, intended for occupancy as separate living quarters" ;
  fibo-fnd-utl-av:adaptedFrom <http://www.census.gov/glossary/#term_Housingunit> ;
  fibo-fnd-utl-av:explanatoryNote "Separate living quarters are those in which the occupants live separately from any other individuals in the building and which have direct access from outside the building or through a common hall. For vacant units, the criteria of separateness and direct access are applied to the intended occupants whenever possible." ;
.
fibo-ind-ei-ei:InflationRate
  rdf:type owl:Class ;
  rdfs:label "inflation rate" ;
  rdfs:subClassOf fibo-ind-ei-ei:EconomicIndicator ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-ind-ei-ei:StatisticalArea ;
      owl:onProperty fibo-fnd-rel-rel:appliesTo ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "an economic indicator representing a change in prices of goods and services for a specified period, for a given statistical area" ;
  skos:editorialNote "Always either includes or excludes: Energy prices; Food prices. ALL inflation rates cite whether or not they exclude energy and food prices. If nothing stated it is assumed they include them." ;
  fibo-fnd-utl-av:explanatoryNote "Inflation rate can be used to define changes, from period-to-period, in wage (wage inflation), house prices or producer inputs/outputs. It can be calculated month-over-month and quarter-over-quarter, as well as year-over-year, or on any periodic basis required by the publisher and its community of interest." ;
.
fibo-ind-ei-ei:InputProducerPriceIndex
  rdf:type owl:Class ;
  rdfs:label "input producer price index" ;
  rdfs:subClassOf fibo-ind-ei-ei:ProducerPriceIndex ;
  skos:definition "an economic indicator representing a measure of the rate of change over time in the prices of inputs of goods and services purchased by the producer" ;
  fibo-fnd-utl-av:abbreviation "input PPI" ;
  fibo-fnd-utl-av:adaptedFrom "https://www.imf.org/external/pubs/ft/ppi/2010/manual/ppi.pdf"^^xsd:anyURI ;
.
fibo-ind-ei-ei:InstitutionalPerson
  rdf:type owl:Class ;
  rdfs:label "institutional person" ;
  rdfs:subClassOf fibo-fnd-aap-ppl:Person ;
  skos:definition "a person that resides in an institution for some reason, due, for example, to hospitalization, rehabilitation, or incarceration" ;
  fibo-fnd-utl-av:adaptedFrom "U.S. Bureau of Labor Statistics and Statistics Canada reference definitions - https://wiki.edmcouncil.org/pages/viewpage.action?pageId=6358041" ;
.
fibo-ind-ei-ei:MetropolitanStatisticalArea
  rdf:type owl:Class ;
  rdfs:label "metropolitan statistical area" ;
  rdfs:subClassOf fibo-ind-ei-ei:StatisticalArea ;
  skos:definition "one or more adjacent counties or county equivalents that have at least one urban core area of at least 50,000 population, plus adjacent territory that has a high degree of social and economic integration with the core as measured by commuting ties" ;
  fibo-fnd-utl-av:abbreviation "MSA" ;
  fibo-fnd-utl-av:adaptedFrom <https://en.wikipedia.org/wiki/List_of_Metropolitan_Statistical_Areas> ;
  fibo-fnd-utl-av:adaptedFrom <https://www.census.gov/population/metro/> ;
  fibo-fnd-utl-av:adaptedFrom <https://www.whitehouse.gov/sites/default/files/omb/assets/fedreg_2010/06282010_metro_standards-Complete.pdf> ;
.
fibo-ind-ei-ei:MicropolitanStatisticalArea
  rdf:type owl:Class ;
  rdfs:label "micropolitan statistical area" ;
  rdfs:subClassOf fibo-ind-ei-ei:StatisticalArea ;
  skos:definition "one or more adjacent counties or county equivalents that have at least one urban core area of at least 10,000 population but less than 50,000, plus adjacent territory that has a high degree of social and economic integration with the core as measured by commuting ties" ;
  fibo-fnd-utl-av:abbreviation "μSA" ;
  fibo-fnd-utl-av:adaptedFrom <https://en.wikipedia.org/wiki/List_of_micropolitan_statistical_areas> ;
  fibo-fnd-utl-av:adaptedFrom <https://www.census.gov/population/metro/> ;
  fibo-fnd-utl-av:adaptedFrom <https://www.whitehouse.gov/sites/default/files/omb/assets/fedreg_2010/06282010_metro_standards-Complete.pdf> ;
.
fibo-ind-ei-ei:MilitaryPerson
  rdf:type owl:Class ;
  rdfs:label "military person" ;
  rdfs:subClassOf fibo-fnd-aap-ppl:LegallyCapablePerson ;
  owl:disjointWith fibo-ind-ei-ei:Civilian ;
  skos:definition "a person that is a member of the active duty military" ;
  fibo-fnd-utl-av:adaptedFrom "U.S. Bureau of Labor Statistics and Statistics Canada reference definitions - https://wiki.edmcouncil.org/pages/viewpage.action?pageId=6358041" ;
.
fibo-ind-ei-ei:OutputProducerPriceIndex
  rdf:type owl:Class ;
  rdfs:label "output producer price index" ;
  rdfs:subClassOf fibo-ind-ei-ei:ProducerPriceIndex ;
  skos:definition "an economic indicator representing a measure of the rate of change over time in the prices of products sold as they leave the producer" ;
  fibo-fnd-utl-av:abbreviation "output PPI" ;
  fibo-fnd-utl-av:adaptedFrom "https://www.imf.org/external/pubs/ft/ppi/2010/manual/ppi.pdf"^^xsd:anyURI ;
.
fibo-ind-ei-ei:PopulationNotInLaborForce
  rdf:type owl:Class ;
  rdfs:label "population not in the labor force" ;
  rdfs:subClassOf fibo-ind-ei-ei:CivilianNonInstitutionalPopulation ;
  owl:disjointWith fibo-ind-ei-ei:CivilianLaborForce ;
  skos:definition "a subset of the civilian, noninstitutional population, that is considered neither employed nor unemployed by the reporting agency during the reporting period" ;
  fibo-fnd-utl-av:adaptedFrom "U.S. Bureau of Labor Statistics and Statistics Canada reference definitions - https://wiki.edmcouncil.org/pages/viewpage.action?pageId=6358041" ;
  fibo-fnd-utl-av:explanatoryNote "There are a number of distinctions with respect to how individuals are counted from country to country, including whether or not they are considered employed if they are on unpaid leave for some reason, and whether or not they are counted multiple times if they have more than one paying job." ;
.
fibo-ind-ei-ei:ProducerPriceIndex
  rdf:type owl:Class ;
  rdfs:label "producer price index" ;
  rdfs:subClassOf fibo-fnd-utl-alx:Expression ;
  rdfs:subClassOf fibo-ind-ei-ei:EconomicIndicator ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass [
          rdf:type owl:Class ;
          owl:unionOf (
              fibo-ind-ei-ei:EnterprisePopulation
              fibo-ind-ei-ei:EstablishmentPopulation
              fibo-ind-ei-ei:GoodsOrServicesPopulation
            ) ;
        ] ;
      owl:onProperty fibo-fnd-utl-alx:hasArgument ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:appliesTo ;
      owl:someValuesFrom [
          rdf:type owl:Class ;
          owl:unionOf (
              fibo-ind-ei-ei:EnterprisePopulation
              fibo-ind-ei-ei:EstablishmentPopulation
              fibo-ind-ei-ei:GoodsOrServicesPopulation
            ) ;
        ] ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-utl-alx:hasArgument ;
      owl:someValuesFrom fibo-ind-ei-ei:FixedBasket ;
    ] ;
  owl:disjointWith fibo-ind-ei-ei:ConsumerPriceIndex ;
  skos:definition "an economic indicator representing a measure of the rate of change over time in the prices of goods and services bought and sold by producers" ;
  fibo-fnd-utl-av:abbreviation "PPI" ;
  fibo-fnd-utl-av:adaptedFrom "https://www.imf.org/external/pubs/ft/ppi/2010/manual/ppi.pdf"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "Statistical agencies implement the Laspeyres index by putting it into price-relative (price change from the base period) and revenue-share (from the base period) format. In this form, the Laspeyres index can be written as the sum of base-period revenue shares of the items in the index times their corresponding price relatives. Statistical agency practice has introduced some approximations to the theoretical Laspeyres target due to a number of practical problems with producing the Laspeyres index exactly. For these and other pragmatic reasons, some agencies use alternatives depending on circumstances. See the IMF publication cited for a full explanation of the most commonly used approaches and trade-offs made for determining PPI." ;
  fibo-fnd-utl-av:explanatoryNote "The standard methodology for a typical PPI is based on a Laspeyres price index with fixed quantities from an earlier base period. The construction of this index can be thought of in terms of selecting a basket of goods and services representative of base-period revenues, valuing this at base-period prices, and then repricing the same basket at current-period prices. The target PPI in this case is defined to be the ratio of these two revenues. Practicing statisticians use this methodology because it has at least three practical advantages. It is easily explained to the public, it can use often expensive and untimely weighting information from the date of the last (or an even earlier) survey or administrative source (rather than requiring sources of data for the current month), and it need not be revised if users accept the Laspeyres premise." ;
.
fibo-ind-ei-ei:StatisticalArea
  rdf:type owl:Class ;
  rdfs:label "statistical area" ;
  rdfs:subClassOf fibo-fnd-plc-loc:PhysicalLocation ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-aap-agt:isIdentifiedBy ;
      owl:someValuesFrom fibo-ind-ei-ei:StatisticalAreaIdentifier ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:isGovernedBy ;
      owl:someValuesFrom fibo-be-ge-ge:GovernmentBody ;
    ] ;
  skos:definition "a physical location that is defined per a nationally consistent program for designating geographic regions for the purposes of tabulating and presenting statistical data" ;
  fibo-fnd-utl-av:adaptedFrom <http://www.census.gov/prod/cen2010/doc/gqsf.pdf> ;
  fibo-fnd-utl-av:adaptedFrom "U.S. Bureau of Labor Statistics and Statistics Canada reference definitions - https://wiki.edmcouncil.org/display/IND/Statistics+Canada+Census+Information" ;
  fibo-fnd-utl-av:adaptedFrom "U.S. Bureau of Labor Statistics and Statistics Canada reference definitions - https://wiki.edmcouncil.org/pages/viewpage.action?pageId=6358041" ;
.
fibo-ind-ei-ei:StatisticalAreaIdentifier
  rdf:type owl:Class ;
  rdfs:label "statistical area identifier" ;
  rdfs:subClassOf fibo-fnd-arr-id:Identifier ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-ind-ei-ei:StatisticalArea ;
      owl:onProperty fibo-fnd-aap-agt:identifies ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "an identifier for a physical location that is defined per a nationally consistent program for designating geographic regions for the purposes of tabulating and presenting statistical data" ;
  fibo-fnd-utl-av:adaptedFrom <http://www.census.gov/prod/cen2010/doc/gqsf.pdf> ;
  fibo-fnd-utl-av:adaptedFrom "U.S. Bureau of Labor Statistics and Statistics Canada reference definitions - https://wiki.edmcouncil.org/display/IND/Statistics+Canada+Census+Information" ;
  fibo-fnd-utl-av:adaptedFrom "U.S. Bureau of Labor Statistics and Statistics Canada reference definitions - https://wiki.edmcouncil.org/pages/viewpage.action?pageId=6358041" ;
.
fibo-ind-ei-ei:StatisticalPopulation
  rdf:type owl:Class ;
  rdfs:label "statistical population" ;
  rdfs:subClassOf fibo-ind-ei-ei:StatisticalUniverse ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom xsd:decimal ;
      owl:onProperty fibo-ind-ei-ei:hasPopulationSize ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-dt-fd:ExplicitDatePeriod ;
      owl:onProperty fibo-ind-ei-ei:hasTimeContext ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-ind-ei-ei:StatisticalArea ;
      owl:onProperty fibo-ind-ei-ei:hasRegionalContext ;
    ] ;
  skos:definition "a statistical universe filtered by time and region" ;
  fibo-fnd-utl-av:adaptedFrom <http://stats.oecd.org/glossary/detail.asp?ID=2079> ;
  fibo-fnd-utl-av:explanatoryNote "A common aim of statistical analysis is to produce information about some chosen population. In statistical inference, a subset of the population (a statistical sample) is chosen to represent the population in a statistical analysis. If a sample is chosen properly, characteristics of the entire population that the sample is drawn from can be estimated from corresponding characteristics of the sample." ;
.
fibo-ind-ei-ei:StatisticalProgram
  rdf:type owl:Class ;
  rdfs:label "statistical program" ;
  rdfs:subClassOf fibo-be-fct-pub:Publication ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-dt-bd:BusinessRecurrenceInterval ;
      owl:onProperty fibo-ind-ei-ei:hasReferencePeriod ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-dt-fd:Date ;
      owl:onProperty fibo-ind-ei-ei:hasReleaseDate ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-dt-fd:ExplicitDatePeriod ;
      owl:onProperty fibo-ind-ei-ei:hasReportingPeriod ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fbc-fct-ra:specifies ;
      owl:someValuesFrom fibo-ind-ei-ei:StatisticalUniverse ;
    ] ;
  skos:definition "a publication program that presents a detailed investigation and analysis of a subject or situation involving one or more studies or surveys" ;
  fibo-fnd-utl-av:adaptedFrom <http://www.oxforddictionaries.com/definition/english/study> ;
.
fibo-ind-ei-ei:StatisticalUniverse
  rdf:type owl:Class ;
  rdfs:label "statistical universe" ;
  rdfs:subClassOf fibo-fnd-arr-arr:Collection ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom xsd:decimal ;
      owl:onProperty fibo-ind-ei-ei:hasUniverseSize ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:hasContext ;
      owl:someValuesFrom fibo-ind-ei-ei:StatisticalProgram ;
    ] ;
  skos:definition "a collection representing the total membership, or 'universe', of people, resources, products, services, events, or entities of interest for some question, experiment, survey or statistical program" ;
  skos:example "A statistical universe can be a group of actually existing objects (e.g. the set of all stars within the Milky Way galaxy) or a hypothetical and potentially infinite group of objects conceived as a generalization from experience (e.g. the set of all possible hands in a game of poker)." ;
  fibo-fnd-utl-av:adaptedFrom <http://stats.oecd.org/glossary/detail.asp?ID=2087> ;
.
fibo-ind-ei-ei:UnemployedPopulation
  rdf:type owl:Class ;
  rdfs:label "unemployed population" ;
  rdfs:subClassOf fibo-ind-ei-ei:CivilianLaborForce ;
  owl:disjointWith fibo-ind-ei-ei:EmployedPopulation ;
  skos:definition "a subset of the civilian labor force that is considered to have had no employment but was available for work, except for temporary illness, and had made specific efforts to find employment sometime during a specified period, during the reporting period" ;
  fibo-fnd-utl-av:adaptedFrom "U.S. Bureau of Labor Statistics and Statistics Canada reference definitions - https://wiki.edmcouncil.org/pages/viewpage.action?pageId=6358041" ;
  fibo-fnd-utl-av:explanatoryNote "Persons who were waiting to be recalled to a job from which they had been laid off need not have been looking for work to be classified as unemployed." ;
.
fibo-ind-ei-ei:UnemploymentRate
  rdf:type owl:Class ;
  rdfs:label "unemployment rate" ;
  rdfs:seeAlso <http://www.bls.gov/news.release/pdf/empsit.pdf> ;
  rdfs:subClassOf fibo-fnd-utl-alx:Expression ;
  rdfs:subClassOf fibo-ind-ei-ei:EconomicIndicator ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-ind-ei-ei:CivilianLaborForce ;
      owl:onProperty fibo-fnd-utl-alx:hasArgument ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-ind-ei-ei:UnemployedPopulation ;
      owl:onProperty fibo-fnd-utl-alx:hasArgument ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:appliesTo ;
      owl:someValuesFrom fibo-ind-ei-ei:UnemployedPopulation ;
    ] ;
  skos:definition "an economic indicator representing the ratio of the unemployed population with respect to the civilian labor force of a given economy for some specified period" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.bls.gov/cps/faq.htm#Ques3"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "Persons are classified as unemployed if they do not have a job, have actively looked for work in the prior 4 weeks, and are currently available for work. Workers expecting to be recalled from layoff are counted as unemployed, whether or not they have engaged in a specific jobseeking activity. In all other cases, the individual must have been engaged in at least one active job search activity in the 4 weeks preceding the interview and be available for work (except for temporary illness)." ;
  fibo-ind-ei-ei:actualExpression "unemployed population ÷ civilian labor force" ;
.
fibo-ind-ei-ei:ValueAddedProducerPriceIndex
  rdf:type owl:Class ;
  rdfs:label "value-added producer price index" ;
  rdfs:subClassOf fibo-ind-ei-ei:ProducerPriceIndex ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-utl-alx:hasArgument ;
      owl:someValuesFrom fibo-ind-ei-ei:InputProducerPriceIndex ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-utl-alx:hasArgument ;
      owl:someValuesFrom fibo-ind-ei-ei:OutputProducerPriceIndex ;
    ] ;
  skos:definition "an economic indicator representing a weighted average of the input and output producer price indices" ;
  fibo-fnd-utl-av:abbreviation "value-added PPI" ;
  fibo-fnd-utl-av:adaptedFrom "https://www.imf.org/external/pubs/ft/ppi/2010/manual/ppi.pdf"^^xsd:anyURI ;
.
fibo-ind-ei-ei:actualExpression
  rdf:type owl:AnnotationProperty ;
  rdfs:label "actual expression" ;
  skos:definition "the calculation or expression used to determine the value of the indicator" ;
.
fibo-ind-ei-ei:excludesEnergyAndFood
  rdf:type owl:DatatypeProperty ;
  rdfs:domain fibo-ind-ei-ei:ConsumerPriceIndex ;
  rdfs:label "excludes energy and food" ;
  rdfs:range fibo-fnd-utl-bt:yesOrNo ;
  skos:definition "a predicate indicating whether the index excludes energy and food prices" ;
.
fibo-ind-ei-ei:hasIndicatorValue
  rdf:type owl:ObjectProperty ;
  rdfs:label "has indicator value" ;
  rdfs:range fibo-fnd-qt-qtu:QuantityValue ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "specifies quantity value for a given indicator" ;
.
fibo-ind-ei-ei:hasMinimumLegalWorkingAge
  rdf:type owl:DatatypeProperty ;
  rdfs:label "has minimum legal working age" ;
  rdfs:range xsd:integer ;
  skos:definition "a predicate indicating the legal working age (minimum), in years, of people that are counted as members of the working population" ;
  fibo-fnd-utl-av:adaptedFrom <http://www.investopedia.com/terms/w/working-age-population.asp> ;
  fibo-fnd-utl-av:explanatoryNote "The working-age population is the total population in a region, within a set range of ages, that is considered to be able and likely to work. The working-age population measure is used to give an estimate of the total number of potential workers within an economy. For example, in the U.S., it is 16, whereas in Canada it is 15." ;
.
fibo-ind-ei-ei:hasPeriodicity
  rdf:type owl:ObjectProperty ;
  rdfs:label "has periodicity" ;
  rdfs:range fibo-fnd-dt-fd:RecurrenceInterval ;
  rdfs:subPropertyOf fibo-fnd-dt-fd:hasRecurrenceInterval ;
  skos:definition "specifies a recurrence interval (monthly, quarterly, annual) that an indicator reflects" ;
.
fibo-ind-ei-ei:hasPopulationSize
  rdf:type owl:DatatypeProperty ;
  rdfs:label "has population size" ;
  rdfs:range xsd:decimal ;
  rdfs:subPropertyOf fibo-fnd-acc-cur:hasAmount ;
  skos:definition "a predicate indicating the number of elements in a given population" ;
.
fibo-ind-ei-ei:hasReferencePeriod
  rdf:type owl:ObjectProperty ;
  rdfs:label "has reference period" ;
  rdfs:range fibo-fnd-dt-fd:RecurrenceInterval ;
  rdfs:subPropertyOf fibo-fnd-dt-fd:hasRecurrenceInterval ;
  skos:definition "specifies a reference (baseline) recurrence interval for which a given economic indicator applies" ;
.
fibo-ind-ei-ei:hasRegionalContext
  rdf:type owl:ObjectProperty ;
  rdfs:label "has regional context" ;
  rdfs:range fibo-ind-ei-ei:StatisticalArea ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:hasContext ;
  skos:definition "specifies the regional (geopolitical) context for a given measurement, population, or economic indicator" ;
.
fibo-ind-ei-ei:hasReleaseDate
  rdf:type owl:ObjectProperty ;
  rdfs:label "has release date" ;
  rdfs:range fibo-fnd-dt-fd:Date ;
  rdfs:subPropertyOf fibo-fnd-dt-fd:hasDate ;
  skos:definition "specifies the release date for a given economic indicator" ;
.
fibo-ind-ei-ei:hasReportingPeriod
  rdf:type owl:ObjectProperty ;
  rdfs:label "has reporting period" ;
  rdfs:range fibo-fnd-dt-fd:ExplicitDatePeriod ;
  rdfs:subPropertyOf fibo-fnd-dt-fd:hasDatePeriod ;
  skos:definition "specifies the reporting period for which a given economic indicator applies" ;
.
fibo-ind-ei-ei:hasSeriesOrigin
  rdf:type owl:ObjectProperty ;
  rdfs:label "has series origin" ;
  rdfs:range fibo-fnd-dt-fd:Date ;
  rdfs:subPropertyOf fibo-fnd-dt-fd:hasStartDate ;
  skos:definition "specifies the original starting date for the time series for a given economic indicator" ;
.
fibo-ind-ei-ei:hasTimeContext
  rdf:type owl:ObjectProperty ;
  rdfs:label "has time context" ;
  rdfs:range fibo-fnd-dt-fd:ExplicitDatePeriod ;
  rdfs:subPropertyOf fibo-fnd-dt-fd:hasDatePeriod ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:hasContext ;
  skos:definition "specifies the time context for a given measurement, population, or economic indicator" ;
.
fibo-ind-ei-ei:hasUniverseSize
  rdf:type owl:DatatypeProperty ;
  rdfs:label "has universe size" ;
  rdfs:range xsd:decimal ;
  rdfs:subPropertyOf fibo-fnd-acc-cur:hasAmount ;
  skos:definition "a predicate indicating the number of elements in a given universe" ;
.
fibo-ind-ei-ei:isSeasonallyAdjusted
  rdf:type owl:DatatypeProperty ;
  rdfs:label "is seasonally adjusted" ;
  rdfs:range xsd:boolean ;
  skos:definition "a predicate indicating whether some published formal method is applied that compensates for seasonal variations in the population or index value" ;
  fibo-fnd-utl-av:explanatoryNote "Example explanation from the US Bureau of Labor Statistics: Because price data are used for different purposes by different groups, the Bureau of Labor Statistics publishes seasonally adjusted as well as unadjusted changes each month. ...  Seasonal factors used in computing the seasonally adjusted indexes are derived by the X-13ARIMA-SEATS Seasonal Adjustment Method.  Seasonally adjusted indexes and seasonal factors are computed annually.  Each year, the last five years of seasonally adjusted data are revised." ;
.
