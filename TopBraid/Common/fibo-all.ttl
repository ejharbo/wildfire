  rdfs:subClassOf fibo-fnd-txn-ecr:EconomicResource ;
  skos:definition "Some benefit from one party to the other party, defined in contractual terms."@en ;
  fibo-fnd-utl-av:explanatoryNote "This could for example be a contractual promise of future cash flows such as dividends on a specific instrument."@en ;
.
fibo-fnd-txn-txnx:CoveredTransaction
  rdf:type owl:Class ;
  rdfs:label "covered transaction"@en ;
  rdfs:subClassOf fibo-fnd-txn-rea:ContractualTransaction ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-txn-txnx:governedBy ;
      owl:someValuesFrom fibo-fnd-txn-txnx:MasterAgreement ;
    ] ;
  skos:definition "A transaction covered by some Master Agreement."@en ;
  fibo-fnd-utl-av:explanatoryNote "The Master Agreement sets out the terms and conditions under which these transactions are to take place between the parties. These are Over the Counter transactions, including OTC Derivatives."@en ;
.
fibo-fnd-txn-txnx:MasterAgreement
  rdf:type owl:Class ;
  rdfs:label "master agreement"@en ;
  rdfs:subClassOf fibo-fnd-agr-ctr:WrittenContract ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-agrx-con:mandates ;
      owl:someValuesFrom fibo-der-der-ma:MasterAgreementObligation ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-txn-txnx:governs ;
      owl:someValuesFrom fibo-fnd-agrx-ctrx:ContractualRelationship ;
    ] ;
  skos:definition "A legal contract which is deemed to apply to a number of future activities between the parties thereto and having force over future agreements or contracts to be brought into effect between those parties."@en ;
.
fibo-fnd-txn-txnx:governedBy
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-txn-txnx:CoveredTransaction ;
  rdfs:label "governed by"@en ;
  rdfs:range fibo-fnd-txn-txnx:MasterAgreement ;
.
fibo-fnd-txn-txnx:governs
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-txn-txnx:MasterAgreement ;
  rdfs:label "governs"@en ;
  rdfs:range fibo-fnd-agrx-ctrx:ContractualRelationship ;
  skos:definition "The contractual relationship defined by the master agreement."@en ;
.
fibo-fnd-utl-alx:
  terms:license "http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense"^^xsd:anyURI ;
  sm:contentLanguage "http://www.omg.org/spec/ODM/"^^xsd:anyURI ;
  sm:contentLanguage "http://www.w3.org/standards/techs/owl#w3c_all"^^xsd:anyURI ;
  sm:copyright """Copyright (c) 2013-2016 EDM Council, Inc.
Copyright (c) 2013-2016 Object Management Group, Inc.""" ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"^^xsd:anyURI ;
  sm:fileAbbreviation "fibo-fnd-utl-alx" ;
  sm:fileAbstract "This ontology provides mathematical abstractions for use in other ontologies, including for example the basic components of formulae, parameters and values." ;
  sm:filename "Analytics.rdf" ;
  rdfs:label "Analytics Ontology" ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/AboutFND/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/Utilities/AboutUtilities/"^^xsd:anyURI ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/FND/20160201/Utilities/Analytics/> ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/FND/20140501/Utilities/Analytics.rdf version of this ontology was modified per the issue resolutions identified in the FIBO FND 1.0 FTF report and in http://spec.edmcouncil.org/fibo/FND/1.0/AboutFND-1.0/." ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/FND/20141101/Utilities/Analytics.rdf version of this ontology was modified to address issue FIBOFND11-20, which added the definition of Calculation and corrected a reasoning issue related to the use of a custom datatype." ;
  skos:changeNote "This ontology was added to Foundations in advance of the June 2014 Boston meeting in support of the IND RFC." ;
.
fibo-fnd-utl-alx:CalculationFormula
  rdf:type owl:Class ;
  rdfs:label "calculation formula" ;
  rdfs:subClassOf fibo-fnd-utl-alx:Formula ;
  skos:definition "a mathematical formula that transforms one or more inputs into an amount or number of something" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.oxforddictionaries.com/definition/english/calculation"^^xsd:anyURI ;
  fibo-fnd-utl-av:adaptedFrom "https://en.wikipedia.org/wiki/Calculation"^^xsd:anyURI ;
.
fibo-fnd-utl-alx:Constant
  rdf:type owl:Class ;
  rdfs:label "constant" ;
  rdfs:subClassOf fibo-fnd-rel-rel:Reference ;
  skos:definition "a symbol that represents a constant in a formula or expression" ;
.
fibo-fnd-utl-alx:Expression
  rdf:type owl:Class ;
  rdfs:label "expression" ;
  rdfs:subClassOf fibo-fnd-rel-rel:Reference ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-utl-alx:Constant ;
      owl:onProperty fibo-fnd-utl-alx:hasArgument ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-utl-alx:Variable ;
      owl:onProperty fibo-fnd-utl-alx:hasArgument ;
    ] ;
  skos:definition "a finite combination of symbols that are well-formed according to applicable rules" ;
.
fibo-fnd-utl-alx:Formula
  rdf:type owl:Class ;
  rdfs:label "formula" ;
  rdfs:subClassOf fibo-fnd-rel-rel:Reference ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-utl-alx:hasExpression ;
      owl:someValuesFrom fibo-fnd-utl-alx:Expression ;
    ] ;
  owl:equivalentClass fibo-fnd-inf-tem:Formula ;
  skos:definition "a general fact or rule expressed in letters and symbols; may consist of one or more expressions" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.merriam-webster.com/dictionary/formula"^^xsd:anyURI ;
.
fibo-fnd-utl-alx:Measure
  rdf:type owl:Class ;
  rdfs:label "measure" ;
  rdfs:subClassOf fibo-fnd-rel-rel:Reference ;
  skos:definition "an amount or degree of something; the dimensions, capacity, or amount of something ascertained by measuring" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.merriam-webster.com/dictionary/measure"^^xsd:anyURI ;
.
fibo-fnd-utl-alx:StatisticalMeasure
  rdf:type owl:Class ;
  rdfs:label "statistical measure" ;
  rdfs:subClassOf fibo-fnd-utl-alx:Measure ;
  skos:definition "A summary (means, mode, total, index, etc.) of the individual quantitative variable values for the statistical units in a specific group (study domains)." ;
  fibo-fnd-utl-av:definitionOrigin "http://stats.oecd.org/glossary/detail.asp?ID=5068"^^xsd:anyURI ;
.
fibo-fnd-utl-alx:Variable
  rdf:type owl:Class ;
  rdfs:label "variable" ;
  rdfs:subClassOf fibo-fnd-rel-rel:Reference ;
  skos:definition "a symbol that represents a parameter in a formula or expression" ;
.
fibo-fnd-utl-alx:forYear
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-utl-alx:StatisticalMeasure ;
  rdfs:label "for calendar year" ;
  rdfs:range fibo-fnd-dt-fd:ExplicitDatePeriod ;
  skos:definition "a predicate indicating the calendar year for which the statistical measure is applicable" ;
.
fibo-fnd-utl-alx:hasApplicableDatePeriod
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-utl-alx:StatisticalMeasure ;
  rdfs:label "has applicable date period" ;
  rdfs:range fibo-fnd-dt-fd:DatePeriod ;
  rdfs:subPropertyOf fibo-fnd-dt-fd:hasDatePeriod ;
  skos:definition "a predicate indicating the date period for which the statistical measure is applicable" ;
.
fibo-fnd-utl-alx:hasApplicablePeriodEnd
  rdf:type owl:DatatypeProperty ;
  rdfs:domain fibo-fnd-utl-alx:StatisticalMeasure ;
  rdfs:label "has applicable period end" ;
  rdfs:range xsd:dateTime ;
  rdfs:subPropertyOf fibo-fnd-dt-fd:hasDateTimeValue ;
  skos:definition "a predicate indicating the end of the period for which the statistical measure is applicable" ;
.
fibo-fnd-utl-alx:hasApplicablePeriodStart
  rdf:type owl:DatatypeProperty ;
  rdfs:domain fibo-fnd-utl-alx:StatisticalMeasure ;
  rdfs:label "has applicable period start" ;
  rdfs:range xsd:dateTime ;
  rdfs:subPropertyOf fibo-fnd-dt-fd:hasDateTimeValue ;
  skos:definition "a predicate indicating the start of the period for which the statistical measure is applicable" ;
.
fibo-fnd-utl-alx:hasArgument
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-rel-rel:Reference ;
  rdfs:label "has argument" ;
  rdfs:range fibo-fnd-rel-rel:Reference ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "indicates a specific input to a function, formula or expression, also known as an independent variable" ;
.
fibo-fnd-utl-alx:hasExpression
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-rel-rel:Reference ;
  rdfs:label "has expression" ;
  rdfs:range fibo-fnd-utl-alx:Expression ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "indicates a mathematical or other formal expression, which may be part of a formula" ;
.
fibo-fnd-utl-alx:hasFormula
  rdf:type owl:ObjectProperty ;
  rdfs:label "has formula" ;
  rdfs:range fibo-fnd-utl-alx:Formula ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "indicates a concise way of expressing information symbolically, as in a mathematical or chemical formula" ;
.
fibo-fnd-utl-alx:hasMeasurementDate
  rdf:type owl:DatatypeProperty ;
  rdfs:domain fibo-fnd-utl-alx:Measure ;
  rdfs:label "has measurement date" ;
  rdfs:range fibo-fnd-dt-fd:dateValue ;
  rdfs:subPropertyOf fibo-fnd-dt-fd:hasDateValue ;
  skos:definition "a predicate indicating the date on which the measure was taken" ;
.
fibo-fnd-utl-alx:hasMeasurementPeriodInMonths
  rdf:type owl:DatatypeProperty ;
  rdfs:domain fibo-fnd-utl-alx:Measure ;
  rdfs:label "has measurement period in months" ;
  rdfs:range xsd:integer ;
  skos:definition "a predicate indicating the coverage period for which the measure is applicable expressed in months" ;
.
fibo-fnd-utl-alx:hasOperand
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-rel-rel:Reference ;
  rdfs:label "has operand" ;
  rdfs:range fibo-fnd-rel-rel:Referent ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "indicates the operand of a mathematical argument, parameter or other similar concept" ;
.
fibo-fnd-utl-alx:isMeasureOf
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-utl-alx:Measure ;
  rdfs:label "is a measure of" ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:appliesTo ;
  skos:definition "a predicate indicating the concept being measured" ;
.
fibo-fnd-utl-av:
  terms:license "http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense"^^xsd:anyURI ;
  sm:contentLanguage "http://www.omg.org/spec/ODM/"^^xsd:anyURI ;
  sm:contentLanguage "http://www.w3.org/standards/techs/owl#w3c_all"^^xsd:anyURI ;
  sm:copyright """Copyright (c) 2013-2014 EDM Council, Inc.
Copyright (c) 2013-2014 Object Management Group, Inc.""" ;
  sm:fileAbbreviation "fibo-fnd-utl-av" ;
  sm:fileAbstract """This vocabulary provides a set of metadata annotations for use in describing FIBO ontology elements.  The annotations extend properties defined in the OMG's Specification Metadata Recommendation, in the Dublin Core Metadata Terms Vocabulary and in the W3C Simple Knowledge Organization System (SKOS) Vocabulary, and have been customized to suit the FIBO specification development process.  

Note that any of the original properties provided in Dublin Core and SKOS can be used in addition to the terms provided herein.  However, any Dublin Core terms that are not explicitly defined as OWL annotation properties in this ontology or in any of its imports must be so declared in the ontologies that use them.""" ;
  sm:filename "AnnotationVocabulary.rdf" ;
  rdfs:label "Annotation Vocabulary" ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/AboutFND/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/Utilities/AboutUtilities/"^^xsd:anyURI ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/FND/20141101/Utilities/AnnotationVocabulary/> ;
  skos:changeNote "The http://www.omg.org/spec/EDMC-FIBO/FND/20130801/Utilities/AnnotationVocabulary.rdf version of this ontology was modified per the issue resolutions identified in the FIBO FND 1.0 FTF report and in http://www.omg.org/spec/EDMC-FIBO/FND/1.0/AboutFND-1.0/." ;
  skos:changeNote """The http://www.omg.org/spec/FIBO/Foundations/20130601/Utilities/AnnotationVocabulary.owl version of the ontology was revised in advance of the September 2013 New Brunswick, NJ meeting, as follows:
   (1) to use slash style URI/IRIss (also called 303 URIs, vs. hash style) as required to support server side processing 
   (2) to use version-independent IRIs for all definitions internally as opposed to version-specific IRIs
   (3) to change the file suffix from .owl to .rdf to increase usability in RDF tools
   (4) to use 4-level abbreviations and corresponding namespace prefixes for all FIBO ontologies, reflecting a family/specification/module/ontology structure
   (5) to incorporate changes to the specification metadata to support documentation at the family, specification, module, and ontology level, similar to the abbreviations""" ;
.
fibo-fnd-utl-av:abbreviation
  rdf:type owl:AnnotationProperty ;
  rdfs:label "abbreviation" ;
  rdfs:subPropertyOf skos:altLabel ;
  skos:definition "An abbreviation is short form for a particular designation that can be substituted for the primary representation." ;
  fibo-fnd-utl-av:adaptedFrom "ISO 1087-1 Terminology work - Vocabulary" ;
.
fibo-fnd-utl-av:adaptedFrom
  rdf:type owl:AnnotationProperty ;
  rdfs:label "adapted from" ;
  rdfs:subPropertyOf sm:directSource ;
  skos:definition "the document from which a given term (or its definition) was adapted; the range for this annotation can be a string (either xsd:string or fibo-fnd-utl-bt:text), URI (either xsd:anyURI or fibo-fnd-utl-bt:uri), or BibliographicCitation" ;
.
fibo-fnd-utl-av:definitionOrigin
  rdf:type owl:AnnotationProperty ;
  rdfs:label "definition origin" ;
  rdfs:subPropertyOf sm:directSource ;
  skos:definition "Document from which a given definition was taken directly; the range for this annotation can be a string (either xsd:string or fibo-fnd-utl-bt:text), URI (either xsd:anyURI or fibo-fnd-utl-bt:uri), or BibliographicCitation" ;
.
fibo-fnd-utl-av:explanatoryNote
  rdf:type owl:AnnotationProperty ;
  rdfs:label "explanatory note" ;
  rdfs:subPropertyOf skos:note ;
  skos:definition "a note that provides additional explanatory information about a given concept" ;
.
fibo-fnd-utl-av:modifiedBy
  rdf:type owl:AnnotationProperty ;
  rdfs:label "modified by" ;
  rdfs:subPropertyOf sm:contributor ;
  skos:definition "identifies the organization or person responsible for making a change to a model element in the body of an ontology" ;
.
fibo-fnd-utl-av:modifiedOn
  rdf:type owl:AnnotationProperty ;
  rdfs:label "modified on" ;
  rdfs:subPropertyOf terms:modified ;
  skos:definition "identifies the date a model element in the body of an ontology was changed" ;
.
fibo-fnd-utl-av:nameOrigin
  rdf:type owl:AnnotationProperty ;
  rdfs:label "name origin" ;
  rdfs:subPropertyOf sm:directSource ;
  skos:definition "provides the means to document the name of the original term in the source referenced via termOrigin; the range for this annotation can be a string (either xsd:string or fibo-fnd-utl-bt:text), URI (either xsd:anyURI or fibo-fnd-utl-bt:uri), or BibliographicCitation" ;
.
fibo-fnd-utl-av:synonym
  rdf:type owl:AnnotationProperty ;
  rdfs:label "synonym" ;
  rdfs:subPropertyOf skos:altLabel ;
  skos:definition "A synonym is another designation that can be substituted for the primary representation. It is a designation for the same concept." ;
  fibo-fnd-utl-av:adaptedFrom "ISO 1087-1 Terminology work - Vocabulary" ;
.
fibo-fnd-utl-av:termOrigin
  rdf:type owl:AnnotationProperty ;
  rdfs:label "term origin" ;
  rdfs:subPropertyOf sm:directSource ;
  skos:definition "Document from which a given term was taken directly; the range for this annotation can be a string (either xsd:string or fibo-fnd-utl-bt:text), URI (either xsd:anyURI or fibo-fnd-utl-bt:uri), or BibliographicCitation" ;
.
fibo-fnd-utl-av:usageNote
  rdf:type owl:AnnotationProperty ;
  rdfs:label "usage note" ;
  rdfs:subPropertyOf skos:note ;
  skos:definition "a note that provides information about how a given concept is used in the FIBO context" ;
.
fibo-fnd-utl-bt:
  terms:license "http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense"^^xsd:anyURI ;
  sm:contentLanguage "http://www.omg.org/spec/ODM/"^^xsd:anyURI ;
  sm:contentLanguage "http://www.w3.org/standards/techs/owl#w3c_all"^^xsd:anyURI ;
  sm:copyright """Copyright (c) 2013-2014 EDM Council, Inc.
Copyright (c) 2013-2014 Object Management Group, Inc.""" ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"^^xsd:anyURI ;
  sm:fileAbbreviation "fibo-fnd-utl-bt" ;
  sm:fileAbstract "This ontology provides high level definitions for business facing datatypes, including a few related properties, for use in other FIBO ontology elements. These types are primarily aliases of existing RDF datatypes, and are provided in order to be able to present datatype properties to a business audience with non technical names, for example yes or no in place of boolean and text in place of string." ;
  sm:filename "BusinessFacingTypes.rdf" ;
  rdfs:label "Business Facing Types Ontology" ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/AboutFND/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/Utilities/AboutUtilities/"^^xsd:anyURI ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/FND/20141101/Utilities/BusinessFacingTypes/> ;
  skos:changeNote "The http://www.omg.org/spec/EDMC-FIBO/FND/20130801/Utilities/BusinessFacingTypes.rdf version of this ontology was revised to add the datatype properties hasPercentageValue and hasNumericValue in support of the IND RFC." ;
  skos:changeNote "The http://www.omg.org/spec/EDMC-FIBO/FND/20140501/Utilities/BusinessFacingTypes.rdf version of this ontology was modified per the issue resolutions identified in the FIBO FND 1.0 FTF report and in http://www.omg.org/spec/EDMC-FIBO/FND/1.0/AboutFND-1.0/." ;
  skos:changeNote """The http://www.omg.org/spec/FIBO/Foundations/20130601/Utilities/BusinessFacingTypes.owl version of the ontology was revised in advance of the September 2013 New Brunswick, NJ meeting, as follows:
   (1) to use slash style URI/IRIss (also called 303 URIs, vs. hash style) as required to support server side processing 
   (2) to use version-independent IRIs for all definitions internally as opposed to version-specific IRIs
   (3) to change the file suffix from .owl to .rdf to increase usability in RDF tools
   (4) to use 4-level abbreviations and corresponding namespace prefixes for all FIBO ontologies, reflecting a family/specification/module/ontology structure
   (5) to incorporate changes to the specification metadata to support documentation at the family, specification, module, and ontology level, similar to the abbreviations
   (6) to change the ontology name from BusinessTypes to BusinessFacingTypes
   (7) to add a number of missing definitions, as well as the term, basis points.""" ;
.
fibo-fnd-utl-bt:Percentage
  rdf:type owl:Class ;
  rdfs:label "percentage" ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "a proportion of something expressed as a percentage amount" ;
.
fibo-fnd-utl-bt:RestrictedPercentage
  rdf:type owl:Class ;
  rdfs:label "restricted percentage" ;
  rdfs:subClassOf fibo-fnd-utl-bt:Percentage ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-utl-bt:restrictedPercentageValue ;
      owl:onProperty fibo-fnd-utl-bt:hasPercentageValue ;
    ] ;
  skos:definition "a proportion of something expressed as a percentage amount and not exceeding 100% of that of which it is the percentage" ;
.
fibo-fnd-utl-bt:URI
  rdf:type rdfs:Datatype ;
  rdfs:label "URI" ;
  rdfs:subClassOf rdfs:Resource ;
  owl:equivalentClass xsd:anyURI ;
  skos:definition "A uniform resource identifier (URI) is a string of characters used to identify a name or a web resource." ;
  skos:editorialNote "Such identification enables interaction with representations of the web resource over a network (typically the World Wide Web) using specific protocols. Schemes specifying a concrete syntax and associated protocols define each URI." ;
  skos:editorialNote "This is used in some cases to identify publicly defined schemes (namespaces) for sets of identifiers or other publicly defined codes." ;
  fibo-fnd-utl-av:definitionOrigin "https://en.wikipedia.org/wiki/Uniform_resource_identifier"^^xsd:anyURI ;
.
fibo-fnd-utl-bt:basisPoints
  rdf:type rdfs:Datatype ;
  rdfs:label "basis points" ;
  rdfs:subClassOf rdfs:Resource ;
  owl:equivalentClass [
      rdf:type rdfs:Datatype ;
      owl:onDatatype xsd:integer ;
      owl:withRestrictions (
          [
            xsd:minInclusive 0 ;
          ]
        ) ;
    ] ;
  skos:altLabel "bp" ;
  skos:definition "A basis point is a unit equal to one hundredth of a percentage point, or one part per ten thousand, 1/10000." ;
  skos:editorialNote "Basis points are used as a convenient unit of measurement in contexts where percentage differences of less than 1% are discussed. The most common example is interest rates, where differences in interest rates of less than 1% per year are usually meaningful to talk about. For example, a difference of 0.10 percentage points is equivalent to a change of 10 basis points (e.g., a 4.67% rate increases by 10 basis points to 4.77%). In other words, an increase of 100 basis points means a rise of 1%." ;
  fibo-fnd-utl-av:definitionOrigin "https://en.wikipedia.org/wiki/Basis_points"^^xsd:anyURI ;
.
fibo-fnd-utl-bt:hasNumericValue
  rdf:type owl:DatatypeProperty ;
  rdfs:label "has numeric value" ;
  rdfs:range fibo-fnd-utl-bt:number ;
  skos:definition "indicates a value expressed as a number" ;
.
fibo-fnd-utl-bt:hasPercentageValue
  rdf:type owl:DatatypeProperty ;
  rdfs:domain fibo-fnd-utl-bt:Percentage ;
  rdfs:label "has percentage value" ;
  rdfs:range fibo-fnd-utl-bt:percentageValue ;
  skos:definition "indicates the value of a percentage expressed as a fraction of 100" ;
.
fibo-fnd-utl-bt:isPercentageOf
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-utl-bt:Percentage ;
  rdfs:label "is percentage of" ;
  skos:definition "that of which the percentage is a ratio, expressed as a fraction of 100 where 100 represents the whole of that thing or quantity" ;
.
fibo-fnd-utl-bt:negativeWholeNumber
  rdf:type rdfs:Datatype ;
  rdfs:label "negative whole number" ;
  rdfs:subClassOf rdfs:Resource ;
  owl:equivalentClass xsd:negativeInteger ;
.
fibo-fnd-utl-bt:nonNegativeNumber
  rdf:type rdfs:Datatype ;
  rdfs:label "non-negative number" ;
  rdfs:subClassOf rdfs:Resource ;
  owl:equivalentClass [
      rdf:type rdfs:Datatype ;
