fibo-fnd-acc-aeq:
  terms:license "http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense"^^xsd:anyURI ;
  sm:contentLanguage "http://www.omg.org/spec/ODM/"^^xsd:anyURI ;
  sm:contentLanguage "http://www.w3.org/standards/techs/owl#w3c_all"^^xsd:anyURI ;
  sm:copyright """Copyright (c) 2013-2016 EDM Council, Inc.
Copyright (c) 2013-2016 Object Management Group, Inc.""" ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Organizations/FormalOrganizations/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/OwnershipAndControl/Ownership/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"^^xsd:anyURI ;
  sm:fileAbbreviation "fibo-fnd-acc-aeq" ;
  sm:fileAbstract "This ontology defines equity-related concepts for use in defining other FIBO ontology elements. These are based on basic accounting principles as they relate to equity, debt, assets and liabilities of a firm. Equity forms the basis for ownership of certain forms of corporate body." ;
  sm:filename "AccountingEquity.rdf" ;
  rdfs:label "Accounting Equity Ontology" ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/AboutFND/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/Accounting/AboutAccounting/"^^xsd:anyURI ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/FND/20160201/Accounting/AccountingEquity/> ;
  skos:changeNote """The http://www.omg.org/spec/FIBO/Foundations/20130601/Accounting/AccountingEquity.owl version of the ontology was revised in advance of the September 2013 New Brunswick, NJ meeting, as follows:
   (1) to use slash style URI/IRIss (also called 303 URIs, vs. hash style) as required to support server side processing 
   (2) to use version-independent IRIs for all definitions internally as opposed to version-specific IRIs
   (3) to change the file suffix from .owl to .rdf to increase usability in RDF tools
   (4) to use 4-level abbreviations and corresponding namespace prefixes for all FIBO ontologies, reflecting a family/specification/module/ontology structure
   (5) to incorporate changes to the specification metadata to support documentation at the family, specification, module, and ontology level, similar to the abbreviations.""" ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/FND/20130801/Accounting/AccountingEquity.rdf version of this ontology was modified per the issue resolutions identified in the FIBO FND 1.0 FTF report and in http://spec.edmcouncil.org/fibo/FND/1.0/AboutFND-1.0/." ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/FND/20141101/Accounting/AccountingEquity.rdf version of this ontology was modified per the issue resolutions identified in the FIBO FND 1.1 RTF report, namely, to rename MoneyAmount to AmountOfMoney and replace it herein with MonetaryAmount, as appropriate." ;
.
fibo-fnd-acc-aeq:Capital
  rdf:type owl:Class ;
  rdfs:label "capital" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-acc-cur:MonetaryAmount ;
      owl:onProperty fibo-fnd-acc-aeq:takesForm ;
    ] ;
  skos:definition "financial capital, which represents obligations, and is liquidated as money for trade, and owned by legal entities" ;
  fibo-fnd-utl-av:definitionOrigin "http://en.wikipedia.org/wiki/Capital_(economics)"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "Financial capital is in the form of capital assets, traded in financial markets. Its market value is not based on the historical accumulation of money invested but on the perception by the market of its expected revenues and of the risk entailed." ;
.
fibo-fnd-acc-aeq:CapitalSurplus
  rdf:type owl:Class ;
  rdfs:label "capital surplus" ;
  rdfs:subClassOf fibo-fnd-acc-aeq:Equity ;
  skos:altLabel "additional paid in capital" ;
  skos:definition "Capital surplus is that amount which a firm raises in excess of the par value (nominal value) of the shares (common stock)." ;
  skos:prefLabel "capital surplus" ;
  fibo-fnd-utl-av:definitionOrigin "http://en.wikipedia.org/wiki/Additional_paid_in_capital"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "Capital surplus is a term that frequently appears as a balance sheet item as a component of shareholders equity." ;
.
fibo-fnd-acc-aeq:Equity
  rdf:type owl:Class ;
  rdfs:label "equity" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-acc-cur:MonetaryAmount ;
      owl:onProperty fibo-fnd-acc-aeq:takesForm ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-org-fm:FormalOrganization ;
      owl:onProperty fibo-fnd-acc-aeq:representsAnInterestIn ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "the value of an ownership interest in property, including shareholders equity in a business" ;
  fibo-fnd-utl-av:definitionOrigin "http://en.wikipedia.org/wiki/Equity"^^xsd:anyURI ;
.
fibo-fnd-acc-aeq:FinancialAsset
  rdf:type owl:Class ;
  rdfs:label "financial asset" ;
  rdfs:subClassOf fibo-fnd-oac-own:Asset ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-acc-cur:Price ;
      owl:onProperty fibo-sec-sec-ast:hasAcquisitionPrice ;
    ] ;
  skos:definition "An asset consisting of one or more financial instruments, treated as an asset" ;
.
fibo-fnd-acc-aeq:IssuedEquity
  rdf:type owl:Class ;
  rdfs:label "issued equity" ;
  rdfs:subClassOf fibo-fnd-acc-aeq:StockholdersEquity ;
  skos:definition "externally-held stockholders equity that may be transferred from one party to another" ;
.
fibo-fnd-acc-aeq:OwnersEquity
  rdf:type owl:Class ;
  rdfs:label "owners' equity" ;
  rdfs:subClassOf fibo-fnd-acc-aeq:Equity ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:hasPart ;
      owl:someValuesFrom fibo-fnd-acc-aeq:CapitalSurplus ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:hasPart ;
      owl:someValuesFrom fibo-fnd-acc-aeq:StockholdersEquity ;
    ] ;
  owl:equivalentClass fibo-fnd-acc-bal:OwnersEquity ;
  skos:definition "equity owned in some concern as recorded on the books of that concern" ;
.
fibo-fnd-acc-aeq:RetainedEarnings
  rdf:type owl:Class ;
  rdfs:label "retained earnings" ;
  rdfs:subClassOf fibo-fnd-acc-aeq:Equity ;
  skos:definition "the portion of net income which is retained by the corporation rather than distributed to its owners as dividends" ;
  fibo-fnd-utl-av:definitionOrigin "http://en.wikipedia.org/wiki/Retained_earnings"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "If the corporation takes a loss, then that loss is retained and called variously retained losses, accumulated losses or accumulated deficit. Retained earnings and losses are cumulative from year to year with losses offsetting earnings." ;
.
fibo-fnd-acc-aeq:StockholdersEquity
  rdf:type owl:Class ;
  rdfs:label "stockholders' equity" ;
  rdfs:subClassOf fibo-fnd-acc-aeq:Equity ;
  skos:definition "equity held in a concern by stockholders" ;
  skos:editorialNote "paid in capital, donated capital, and retained earnings less the liabilities of a corporation (Barron's)" ;
  fibo-fnd-utl-av:adaptedFrom "http://en.wikipedia.org/wiki/Retained_earnings"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "When total assets are greater than total liabilities, stockholders have a positive equity (positive book value). Conversely, when total liabilities are greater than total assets, stockholders have a negative stockholders equity (negative book value, also sometimes called stockholders' deficit." ;
.
fibo-fnd-acc-aeq:representsAnInterestIn
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-acc-aeq:Equity ;
  rdfs:label "represents an interest in" ;
  skos:definition "Equity always represents an interest in some business organization. This is the organization, company or venture in which the holder of the equity has a stake in by virtue of holding that equity" ;
.
fibo-fnd-acc-aeq:takesForm
  rdf:type owl:ObjectProperty ;
  rdfs:label "takes form" ;
  skos:definition "the form taken by some amount of money defined according to its purpose, such as capital or equity" ;
.
