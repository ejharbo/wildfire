      owl:onProperty fibo-loan-loan-loan:hasPaymentSchedule ;
      owl:someValuesFrom fibo-loan-loan-loan:LoanPaymentSchedule ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-loan-loan-loan:hasPrepaymentTerms ;
      owl:someValuesFrom fibo-loan-loan-loan:LoanPrepaymentTermsSet ;
    ] ;
