      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:has ;
      owl:someValuesFrom fibo-ln-ln-up:PaymentHistory ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:has ;
      owl:someValuesFrom fibo-ln-ln-up:PaymentSchedule ;
    ] ;
