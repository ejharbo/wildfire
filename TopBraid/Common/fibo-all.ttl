  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fbc-dae-gty:isGuaranteedBy ;
      owl:someValuesFrom fibo-sec-dbt-dbt-gty:StateGuarantor ;
    ] ;
  skos:definition "Guaranty is provided by a government, e.g. a security is guaranteed by a Government."@en ;
.
