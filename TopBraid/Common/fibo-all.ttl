fibo-ind-ir-pub:BaseRateAuthority
  rdf:type owl:Class ;
  rdfs:label "base rate authority" ;
  rdfs:subClassOf fibo-ind-ir-pub:InterestRateAuthority ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-ind-ir-pub:sets ;
      owl:someValuesFrom fibo-ind-ir-ir:BaseRate ;
    ] ;
  skos:definition "an interest rate authority responsible for setting the base rate for a given currency, typically a central bank" ;
.
fibo-ind-ir-pub:InterbankRateAuthority
  rdf:type owl:Class ;
  rdfs:label "interbank rate authority" ;
  rdfs:subClassOf fibo-ind-ir-pub:InterestRateAuthority ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-ind-ir-ir:InterbankRate ;
      owl:onProperty fibo-ind-ir-pub:sets ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-ind-ir-pub:convenes ;
      owl:someValuesFrom fibo-fbc-fct-fse:MonetaryAuthority ;
    ] ;
  skos:definition "an interest rate authority responsible for setting the interbank rate" ;
  fibo-fnd-utl-av:explanatoryNote "This is announced by the relevant central bank or other authority at intervals following a meeting of the relevant policy group or committee." ;
.
fibo-ind-ir-pub:InterestRateAuthority
  rdf:type owl:Class ;
  rdfs:label "interest rate authority" ;
  rdfs:subClassOf fibo-fbc-pas-fpas:FinancialServiceProvider ;
  rdfs:subClassOf fibo-ind-ind-ind:FinancialInformationPublisher ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-ind-ir-pub:sets ;
      owl:someValuesFrom fibo-ind-ir-ir:ReferenceInterestRate ;
    ] ;
  skos:definition "an authority responsible for the publication of some interest rate" ;
  skos:example "This is typically a bank, central bank in the case of the publication of bank interest rates, or the committee responsible for publishing interbank rates, such as LIBOR." ;
  fibo-fnd-utl-av:explanatoryNote "Interest rates which are referred to as market rates, for example as used in interest rate derivatives, are published by some authority responsible for the rate as a kind of market data." ;
.
fibo-ind-ir-pub:MarketDataProvider
