      rdf:type owl:Class ;
      owl:intersectionOf (
          fibo-fnd-agr-ctr:WrittenContract
          [
            rdf:type owl:Restriction ;
            owl:onProperty loan:hasCollateral ;
            owl:someValuesFrom [
                rdf:type owl:Class ;
                owl:intersectionOf (
                    [
                      rdf:type owl:Restriction ;
                      owl:onProperty fibo-ln-ln-up:cur-hasEstimatedValue ;
                      owl:someValuesFrom fibo-fnd-acc-cur:MonetaryAmount ;
