      owl:someValuesFrom loan:CreditScoreModel ;
    ] ;
  owl:equivalentClass [
      rdf:type owl:Class ;
      owl:unionOf (
          loan:CreditScoreNumber
          loan:CreditScoreCategory
        ) ;
    ] ;
