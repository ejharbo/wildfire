  fibo-fnd-utl-av:adaptedFrom "https://en.wikipedia.org/wiki/Separation_of_powers"^^xsd:anyURI ;
  fibo-fnd-utl-av:adaptedFrom "https://www.usa.gov/branches-of-government"^^xsd:anyURI ;
