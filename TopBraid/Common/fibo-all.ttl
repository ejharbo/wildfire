fibo-sec-sec-id:NationalSecuritiesIdentifyingNumberRegistryEntry
  rdf:type owl:Class ;
  rdfs:label "national numbering agency registry entry" ;
  rdfs:subClassOf fibo-fbc-fct-ra:RegistryEntry ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fbc-fi-fi:Security ;
