  sm:fileAbstract "This ontology defines basic concepts such as portfolio, security holding and holder, and extends the notion of a financial asset to include an acquisition price." ;
  sm:filename "SecurityAssets.rdf" ;
  rdfs:label "Security Assets Ontology" ;
