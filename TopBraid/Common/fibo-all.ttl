  sm:fileAbbreviation "fibo-sec-sec-bsk" ;
  sm:fileAbstract "This ontology defines the concept of a tradable container of securities, indices, and/or market rates, and identifies the elements that can be constituents of a such a basket." ;
  sm:filename "Baskets.rdf" ;
  rdfs:label "Baskets Ontology" ;
