fibo-der-cr-cds:EscrowAgent
  rdf:type owl:Class ;
  rdfs:label "escrow agent"@en ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-der-cr-cds:holds ;
