  rdfs:label "distribution by reinvestment"@en ;
  rdfs:subClassOf fibo-sec-eq-eq:DividendsDistributionMethod ;
  skos:definition "Distribution by reinvestment in the same equity or fund."@en ;
.
fibo-sec-eq-eq:DistributionBySecurity
  rdf:type owl:Class ;
  rdfs:label "distribution by security"@en ;
  rdfs:subClassOf fibo-sec-eq-eq:DividendsDistributionMethod ;
  skos:definition "Pay by Security"@en ;
.
fibo-sec-eq-eq:DividendsDistributionMethod
  rdf:type owl:Class ;
