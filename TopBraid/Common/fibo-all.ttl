fibo-sec-dbt-ab-spv:DebtSPV
  rdf:type owl:Class ;
  rdfs:label "debt s p v"@en ;
  rdfs:subClassOf fibo-be-fctx-spv:SpecialPurposeVehicle ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-be-fctx-spv:isSetUpFor ;
      owl:someValuesFrom fibo-sec-dbt-ab-spv:DebtSecuritiesIssuance ;
    ] ;
