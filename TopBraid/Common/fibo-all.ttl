      owl:someValuesFrom fibo-fnd-inf-idx:IdentificationScheme ;
    ] ;
  skos:definition "Identification of the scheme under which an Identifier is defined."@en ;
.
fibo-fnd-inf-idn:PartyAndEntityIdentification
