  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"^^xsd:anyURI ;
  sm:fileAbbreviation "fibo-fnd-dt-oc" ;
  sm:fileAbstract "This ontology extends definitions of date and schedule concepts from the FinancialDates ontology with concepts defining occurrences (i.e., event-related concepts) for use in other FIBO ontologies." ;
  sm:filename "Occurrences.rdf" ;
  rdfs:label "Occurrences Ontology" ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/AboutFND/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/DatesAndTimes/AboutDatesAndTimes/"^^xsd:anyURI ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/FND/20141101/DatesAndTimes/Occurrences/> ;
  skos:changeNote """This ontology was added to Foundations in advance of the December 2014 Long Beach meeting in support of the SEC specification.  It is also needed to provide temporal relationships for Ownership and Control.

These three ontologies are designed for use together:

   * FinancialDates  -- financial Dates and Schedules
   * BusinessDates -- business day adjustments
   * Occurrences    -- occurrences (events) and kinds of occurrences

They are modularized this way to minimize the ontological committments that are imposed upon ontologies that rely upon them.  Ontologies can import FinancialDates alone, or FinancialDates + BusinessDates, or FinancialDates + Occurrences, or all three together.""" ;
.
fibo-fnd-dt-oc:Occurrence
  rdf:type owl:Class ;
  rdfs:label "occurrence" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-dt-oc:OccurrenceKind ;
      owl:onProperty fibo-fnd-dt-oc:exemplifies ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-plc-loc:Location ;
      owl:onProperty fibo-fnd-plc-loc:isLocatedAt ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-dt-fd:hasStartDate ;
      owl:someValuesFrom fibo-fnd-dt-fd:Date ;
    ] ;
  skos:definition "An Occurrence is a happening of an OccurrenceKind. Each Occurrence has a DateTimeStamp, which identifies when the Occurrence happened, and a Location (possibly virtual), that identifies where the Occurrence happened." ;
  skos:editorialNote "In order for other ontologies to accept FinancialDates without committing to the particular notions of 'Occurrence' and 'OccurrenceKind' that is modeled here, all aspects of Occurrences are captured in this ontology." ;
  skos:editorialNote "This should be reviewed and put in FND somewhere as appropriate." ;
.
fibo-fnd-dt-oc:OccurrenceBasedDate
  rdf:type owl:Class ;
  rdfs:label "occurrence based date" ;
  rdfs:subClassOf fibo-fnd-dt-fd:CalculatedDate ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-dt-oc:OccurrenceKind ;
      owl:onProperty fibo-fnd-dt-oc:isTriggeredBy ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "An OccurrenceBaseDate is a CalculatedDate that is defined with respect to the Occurrence of some OccurrenceKind. The 'hasDateValue' property of an OccurrenceBasedDate is not set until the Occurrence happens.  The 'triggeredBy' property relates an OccurrenceBasedDate to the OccurrenceKind that gives the meaning of the OccurrenceBasedDate." ;
.
fibo-fnd-dt-oc:OccurrenceKind
  rdf:type owl:Class ;
  rdfs:label "occurrence kind" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onDataRange xsd:string ;
      owl:onProperty fibo-fnd-dt-oc:hasDescription ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "An OccurrenceKind is a type of event, which has a description. An OccurrenceKind may or may not ever happen, and thus does not have a Date. An OccurrenceKind happens as an Occurrence, which does have a Date." ;
  skos:editorialNote "In order for other ontologies to accept FinancialDates without committing to the particular notions of 'Occurrence' and 'OccurrenceKind' that is modeled here, all aspects of Occurrences are captured in this ontolog" ;
  skos:example "Loan origination" ;
  skos:example "Trade settlement" ;
.
fibo-fnd-dt-oc:exemplifies
  rdf:type owl:FunctionalProperty ;
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-dt-oc:Occurrence ;
  rdfs:label "exemplifies" ;
  rdfs:range fibo-fnd-dt-oc:OccurrenceKind ;
  owl:inverseOf fibo-fnd-dt-oc:hasOccurrence ;
  skos:definition "illustrates by example" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.merriam-webster.com/dictionary/exemplify"^^xsd:anyURI ;
.
fibo-fnd-dt-oc:hasDescription
  rdf:type owl:DatatypeProperty ;
  rdfs:label "has description" ;
  rdfs:range xsd:string ;
  skos:definition "a textual description of something" ;
.
fibo-fnd-dt-oc:hasOccurrence
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-dt-oc:OccurrenceKind ;
  rdfs:label "has occurrence" ;
  rdfs:range fibo-fnd-dt-oc:Occurrence ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  owl:equivalentProperty fibo-fnd-dt-oc:isExemplifiedBy ;
  skos:definition "identifies occurrences of a given occurrence kind" ;
.
fibo-fnd-dt-oc:isExemplifiedBy
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-dt-oc:OccurrenceKind ;
  rdfs:label "is exemplified by" ;
  rdfs:range fibo-fnd-dt-oc:Occurrence ;
  owl:inverseOf fibo-fnd-dt-oc:exemplifies ;
  skos:definition "identifies examples of a given concept" ;
.
fibo-fnd-dt-oc:isTriggeredBy
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-dt-oc:OccurrenceBasedDate ;
  rdfs:label "is triggered by" ;
  rdfs:range fibo-fnd-dt-oc:OccurrenceKind ;
  skos:definition "An OccurrenceBasedDate is triggered by an Occurrence that exemplifies the OccurrenceKind." ;
.
fibo-fnd-gao-gl:
