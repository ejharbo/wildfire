.
fibo-sec-sec-rst:BlueSkyLaw
  rdf:type owl:Class ;
  rdfs:label "Blue-Sky law" ;
  rdfs:subClassOf fibo-sec-sec-rst:SecuritiesRegulation ;
  skos:definition "a kind of securities regulation passed by various states, designed to protect investors against securities fraud by requiring sellers of new issues to register their offerings and provide financial details" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Finance and Investment Terms, Ninth Edition, 2014" ;
  fibo-fnd-utl-av:explanatoryNote "This allows investors to base their judgments on trustworthy data." ;
.
fibo-sec-sec-rst:ContractualRestriction
  rdf:type owl:Class ;
  rdfs:label "contractual restriction" ;
  rdfs:subClassOf fibo-fnd-agr-ctr:ContractualCommitment ;
  rdfs:subClassOf fibo-sec-sec-rst:SecuritiesRestriction ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-agr-ctr:Contract ;
      owl:onProperty fibo-fnd-rel-rel:isMandatedBy ;
    ] ;
  skos:definition "contract terms setting out restrictions on either the holder or the issuer of the security, as specified in the terms of the instrument itself" ;
.
fibo-sec-sec-rst:IndividualInvestor
  rdf:type owl:Class ;
  rdfs:label "individual investor" ;
  rdfs:subClassOf fibo-be-oac-opty:Investor ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-aap-ppl:Person ;
      owl:onProperty fibo-fnd-rel-rel:hasIdentity ;
    ] ;
  skos:definition "an investor that is a person, rather than an organization, who may or may not be considered a qualified investor from a legal perspective" ;
.
fibo-sec-sec-rst:InstitutionalInvestor
  rdf:type owl:Class ;
  rdfs:label "institutional investor" ;
  rdfs:subClassOf fibo-be-oac-opty:Investor ;
  skos:definition "an organization that trades large volumes of securities" ;
  skos:example "Example institutional investors include banks, insurance companies, mutual funds, pension funds, and other similar large funds." ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Finance and Investment Terms, Ninth Edition, 2014" ;
  fibo-fnd-utl-av:explanatoryNote "Typically more than 70 percent of the daily trading on the New York Stock Exchange is conducted on behalf of institutional investors." ;
.
fibo-sec-sec-rst:InvestorsDomicileRestriction
  rdf:type owl:Class ;
  rdfs:label "investors' domicile restriction" ;
  rdfs:subClassOf fibo-sec-sec-rst:LegalHoldingRestriction ;
  skos:definition "a legal holding restriction that specifies where holders of a security may be domiciled" ;
  skos:editorialNote "Needs review and provenance" ;
.
fibo-sec-sec-rst:LegalHoldingRestriction
  rdf:type owl:Class ;
  rdfs:label "legal holding restriction" ;
  rdfs:subClassOf fibo-sec-sec-rst:SecuritiesRestriction ;
  skos:definition "a security restriction applicable to holding the security" ;
.
fibo-sec-sec-rst:LegalRestriction
  rdf:type owl:Class ;
  rdfs:label "legal restriction" ;
  rdfs:subClassOf fibo-sec-sec-rst:SecuritiesRestriction ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom [
          rdf:type owl:Class ;
          owl:unionOf (
              fibo-fnd-law-cor:Law
              fibo-sec-sec-rst:SecuritiesRegulation
            ) ;
        ] ;
      owl:onProperty fibo-fnd-rel-rel:isDefinedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom [
          rdf:type owl:Class ;
          owl:unionOf (
              fibo-fnd-law-cor:Law
              fibo-sec-sec-rst:SecuritiesRegulation
            ) ;
        ] ;
      owl:onProperty fibo-fnd-rel-rel:isMandatedBy ;
    ] ;
  skos:definition "a securities restriction that is mandated by law or regulation" ;
.
fibo-sec-sec-rst:QualifiedInvestorRestriction
  rdf:type owl:Class ;
  rdfs:label "qualified investor restriction" ;
  rdfs:subClassOf fibo-sec-sec-rst:LegalHoldingRestriction ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-dt-fd:DatePeriod ;
      owl:onProperty fibo-sec-sec-rst:hasHoldingPeriod ;
    ] ;
  skos:definition "a legal holding restriction specifying that only qualified investors may hold the security, optionally for a defined holding period (if this is not defined then the period is indefinite)" ;
  skos:editorialNote "Needs review and provenance" ;
  fibo-fnd-utl-av:explanatoryNote "If a holding period is not defined, then the period for which the restriction applies is indefinite." ;
.
fibo-sec-sec-rst:RegistrationRequirement
  rdf:type owl:Class ;
  rdfs:label "registration requirement" ;
  rdfs:subClassOf fibo-sec-sec-rst:SecuritiesRegulation ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fbc-fi-fi:Security ;
      owl:onProperty fibo-fnd-rel-rel:appliesTo ;
    ] ;
  skos:definition "a requirement for registraton of a security within some country, jurisdiction or federal province  ... applies to a registered security, is a function of registration regulations" ;
  skos:editorialNote "Needs review and provenance" ;
.
fibo-sec-sec-rst:RegulationS
  rdf:type owl:Class ;
  rdfs:label "Regulation S" ;
  rdfs:subClassOf fibo-sec-sec-rst:InvestorsDomicileRestriction ;
  rdfs:subClassOf fibo-sec-sec-rst:QualifiedInvestorRestriction ;
  rdfs:subClassOf fibo-sec-sec-rst:SecuritiesRegulation ;
  skos:definition "a securities regulation defining an exemption through which corporations can issue unregistered securities to qualified foreign investors and foreign institutions" ;
  skos:editorialNote "Needs review and provenance" ;
.
fibo-sec-sec-rst:Restriction144A
  rdf:type owl:Class ;
  rdfs:label "Restriction 144A" ;
  rdfs:subClassOf fibo-sec-sec-rst:LegalRestriction ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-dt-fd:DatePeriod ;
      owl:onProperty fibo-sec-sec-rst:hasHoldingPeriod ;
    ] ;
  skos:definition "a Securities & Exchange Commission rule modifying a two-year holding period requirement on privately placed securities to permit qualified institutional buyers to trade these positions among themselves" ;
  skos:editorialNote "Needs review and provenance" ;
.
fibo-sec-sec-rst:SecuritiesRegulation
  rdf:type owl:Class ;
  rdfs:label "securities regulation" ;
  rdfs:subClassOf fibo-fbc-fct-rga:Regulation ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-law-jur:Jurisdiction ;
      owl:onProperty fibo-fnd-agr-ctr:hasGoverningJurisdiction ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-law-jur:Jurisdiction ;
      owl:onProperty fibo-fnd-law-cor:isInForceIn ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-sec-sec-rst:SecuritiesRestriction ;
      owl:onProperty fibo-fnd-rel-rel:defines ;
    ] ;
  skos:definition "a regulation codified in law specific to securities and investments" ;
.
fibo-sec-sec-rst:SecuritiesRestriction
  rdf:type owl:Class ;
  rdfs:label "securities restriction" ;
  rdfs:subClassOf fibo-fnd-law-lcap:LegalConstruct ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-law-jur:Jurisdiction ;
      owl:onProperty fibo-fnd-agr-ctr:hasGoverningJurisdiction ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-law-jur:Jurisdiction ;
      owl:onProperty fibo-fnd-law-cor:isInForceIn ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom [
          rdf:type owl:Class ;
          owl:unionOf (
              fibo-fbc-fi-fi:FinancialInstrument
              fibo-sec-sec-lst:SecurityListing
            ) ;
        ] ;
      owl:onProperty fibo-fnd-rel-rel:appliesTo ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom [
