  rdf:type owl:Class ;
  rdfs:label "loan participation note facility"@en ;
  rdfs:subClassOf fibo-fnd-acc-crf:CreditFacilityTranche ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-dbt-mm-lpn:hasLeadManager ;
      owl:someValuesFrom fibo-sec-dbt-mm-lpn:LeadManager ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-sec-dbt-mm-lpn:hasSyndicateMember ;
      owl:someValuesFrom fibo-sec-dbt-mm-lpn:LoanParticipationNoteSyndicateMember ;
    ] ;
  skos:definition "A facility formed as part of a LPN which can be drawn down by the borrower."@en ;
.
fibo-sec-dbt-mm-lpn:LoanParticipationNoteSyndicateMember
  rdf:type owl:Class ;
