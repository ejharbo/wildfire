  owl:versionIRI <https://spec.edmcouncil.org/fibo/IND/20160801/EconomicIndicators/EconomicIndicatorPublishers/> ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/IND/20140601/EconomicIndicators/EconomicIndicatorPublishers.rdf version of this ontology was modified per the issue resolutions identified in the FIBO IND 1.0 FTF report." ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/IND/20150501/EconomicIndicators/EconomicIndicators.rdf version of this ontology was modified per the issue resolutions identified in the FIBO IND 1.0 FTF 3 report." ;
.
fibo-ind-ei-pub:EconomicIndicatorPublisher
  rdf:type owl:Class ;
  rdfs:label "economic indicator publisher" ;
  rdfs:subClassOf fibo-ind-ei-pub:StatisticalInformationPublisher ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-be-fct-pub:publishes ;
      owl:someValuesFrom fibo-ind-ei-pub:EconomicNewsRelease ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:hasIdentity ;
      owl:someValuesFrom fibo-be-ge-ge:GovernmentBody ;
    ] ;
  skos:definition "a statistical information publisher that publishes economic news, including economic indicators" ;
.
fibo-ind-ei-pub:EconomicNewsRelease
  rdf:type owl:Class ;
  rdfs:label "economic news release" ;
  rdfs:subClassOf fibo-be-fct-pub:Publication ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:involves ;
      owl:someValuesFrom fibo-ind-ei-ei:EconomicIndicator ;
    ] ;
  skos:definition "published statistical information that is about economic indicators and possibly other economic news" ;
.
fibo-ind-ei-pub:StatisticalInformationPublisher
  rdf:type owl:Class ;
  rdfs:label "statistical information publisher" ;
  rdfs:subClassOf fibo-be-fct-pub:Publisher ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:hasIdentity ;
      owl:someValuesFrom fibo-fnd-org-fm:FormalOrganization ;
    ] ;
  skos:definition "a publisher that publishes statistical information" ;
.
