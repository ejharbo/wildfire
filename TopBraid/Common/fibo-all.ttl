fibo-sec-dbt-dbt-gty:LetterOfCreditGuaranty
  rdf:type owl:Class ;
  rdfs:label "letter of credit guaranty"@en ;
  rdfs:subClassOf fibo-fbc-dae-gty:Guaranty ;
  rdfs:subClassOf [
