  rdfs:subPropertyOf fibo-fnd-inf-inf:derivedFrom ;
.
fibo-fnd-inf-tem:ratingHasTense
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-inf-tem:Rating ;
  rdfs:label "rating has tense"@en ;
  rdfs:range fibo-fnd-tim-tim:Tense ;
  rdfs:subPropertyOf fibo-fnd-inf-tem:hasTense ;
.
fibo-fnd-inf-tem:statusHasTense
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-inf-tem:Status ;
  rdfs:label "status has tense"@en ;
  rdfs:range fibo-fnd-tim-tim:Tense ;
  rdfs:subPropertyOf fibo-fnd-inf-tem:hasTense ;
.
fibo-fnd-law-jur:
  terms:license "http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense"^^xsd:anyURI ;
  sm:contentLanguage "http://www.omg.org/spec/ODM/"^^xsd:anyURI ;
  sm:contentLanguage "http://www.w3.org/standards/techs/owl#w3c_all"^^xsd:anyURI ;
  sm:copyright """Copyright (c) 2013-2014 EDM Council, Inc.
Copyright (c) 2013-2014 Object Management Group, Inc.""" ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Law/LegalCore/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Places/Countries/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Places/Locations/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"^^xsd:anyURI ;
  sm:fileAbbreviation "fibo-fnd-law-jur" ;
  sm:fileAbstract "This ontology defines high level concepts relating to jurisdictions for use in other FIBO ontology elements. This includes a general definition of jurisdiction along with some basic types of jurisdiction, along with the factors which distinguish one type of jurisdiction from another. This ontology also defines basic types of legal system, and extends the basic concept of law which is in the LegalCore ontology.." ;
  sm:filename "Jurisdiction.rdf" ;
  rdfs:label "Jurisdiction Ontology" ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/AboutFND/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/Law/AboutLaw/"^^xsd:anyURI ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/FND/20141101/Law/Jurisdiction/> ;
  skos:changeNote "The http://www.omg.org/spec/EDMC-FIBO/FND/20130801/Law/Jurisdiction.rdf version of the ontology was was modified per the issue resolutions identified in the FIBO FND 1.0 FTF report and in http://www.omg.org/spec/EDMC-FIBO/FND/1.0/AboutFND-1.0/." ;
  skos:changeNote """The http://www.omg.org/spec/FIBO/Foundations/20130601/Law/Jurisdiction.owl version of the ontology was revised in advance of the September 2013 New Brunswick, NJ meeting, as follows:
   (1) to use slash style URI/IRIss (also called 303 URIs, vs. hash style) as required to support server side processing 
   (2) to use version-independent IRIs for all definitions internally as opposed to version-specific IRIs
   (3) to change the file suffix from .owl to .rdf to increase usability in RDF tools
   (4) to use 4-level abbreviations and corresponding namespace prefixes for all FIBO ontologies, reflecting a family/specification/module/ontology structure
   (5) to incorporate changes to the specification metadata to support documentation at the family, specification, module, and ontology level, similar to the abbreviations
   (6) to revise definitions using more formal sources.""" ;
.
fibo-fnd-law-jur:CivilLawJurisdiction
  rdf:type owl:Class ;
  rdfs:label "civil law jurisdiction" ;
  rdfs:subClassOf fibo-fnd-law-jur:Jurisdiction ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-law-jur:CivilLawSystem ;
      owl:onProperty fibo-fnd-rel-rel:isGovernedBy ;
    ] ;
  owl:equivalentClass fibo-fnd-lawx-jr:CivilLawJurisdiction ;
  skos:definition "a civil law jurisdiction" ;
.
fibo-fnd-law-jur:CivilLawSystem
  rdf:type owl:Class ;
  rdfs:label "civil law system" ;
  rdfs:subClassOf fibo-fnd-law-jur:LegalSystem ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-law-jur:appliesIn ;
      owl:someValuesFrom fibo-fnd-law-jur:CivilLawJurisdiction ;
    ] ;
  skos:definition "a legal system originating in Europe, intellectualized within the framework of late Roman law, and whose most prevalent feature is that its core principles are codified into a referable system which serves as the primary source of law" ;
  fibo-fnd-utl-av:definitionOrigin "http://en.wikipedia.org/wiki/Civil_law_(legal_system)"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "This can be contrasted with common law systems whose intellectual framework comes from judge-made decisional law which gives precedential authority to prior court decisions on the principle that it is unfair to treat similar facts differently on different occasions (doctrine of judicial precedent)." ;
.
fibo-fnd-law-jur:CommonLawJurisdiction
  rdf:type owl:Class ;
  rdfs:label "common law jurisdiction" ;
  rdfs:subClassOf fibo-fnd-law-jur:Jurisdiction ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-law-jur:CommonLawSystem ;
      owl:onProperty fibo-fnd-rel-rel:isGovernedBy ;
    ] ;
  skos:definition "a jurisdiction based on common law" ;
.
fibo-fnd-law-jur:CommonLawSystem
  rdf:type owl:Class ;
  rdfs:label "common law system" ;
  rdfs:subClassOf fibo-fnd-law-jur:LegalSystem ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-law-jur:appliesIn ;
      owl:someValuesFrom fibo-fnd-law-jur:CommonLawJurisdiction ;
    ] ;
  skos:definition "common law, also known as case law or precedent, is law developed by judges through decisions of courts and similar tribunals" ;
  fibo-fnd-utl-av:definitionOrigin "http://en.wikipedia.org/wiki/Common_law"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "A jurisdiction which is based in Common Law will also have alongside a legislature that passes statutes." ;
  fibo-fnd-utl-av:explanatoryNote """By contrast, civil law (codified/continental law) is set on statutes adopted through the legislative/parliamentary process and/or regulations issued by the executive branch on base of the parliamentary statutes.

A common law system is a legal system that gives great potential precedential weight to common law, on the principle that it is unfair to treat similar facts differently on different occasions. The body of precedent is called common law and it binds future decisions. In cases where the parties disagree on what the law is, a common law court looks to past precedential decisions of relevant courts. If a similar dispute has been resolved in the past, the court is bound to follow the reasoning used in the prior decision (this principle is known as stare decisis). If, however, the court finds that the current dispute is fundamentally distinct from all previous cases (called a matter of first impression), judges have the authority and duty to make law by creating precedent. Thereafter, the new decision becomes precedent, and will bind future courts.""" ;
.
fibo-fnd-law-jur:Jurisdiction
  rdf:type owl:Class ;
  rdfs:label "jurisdiction" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-law-jur:LegalSystem ;
      owl:onProperty fibo-fnd-rel-rel:isGovernedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-law-jur:hasReach ;
      owl:someValuesFrom fibo-fnd-plc-loc:Location ;
    ] ;
  owl:equivalentClass fibo-fnd-lawx-jr:Jurisdiction ;
  skos:definition "the limits or territory within which authority may be exercised; the power, right, or authority to interpret and apply the law" ;
  fibo-fnd-utl-av:definitionOrigin "Merriam-Webster Online Dictionary" ;
.
fibo-fnd-law-jur:LegalSystem
  rdf:type owl:Class ;
  rdfs:label "legal system" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-law-jur:Jurisdiction ;
      owl:onProperty fibo-fnd-law-jur:appliesIn ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-law-cor:GovernmentalConstitution ;
      owl:onProperty fibo-fnd-rel-rel:isGovernedBy ;
    ] ;
  owl:equivalentClass fibo-fnd-lawx-jurx:LegalSystem ;
  skos:definition "Legal regimen of a country consisting of (1) a written or oral constitution, (2) primary legislation (statutes) enacted by the legislative body established by the constitution, (3) subsidiary legislation (bylaws) made by person or bodies authorized by the primary legislation to do so, (4) customs applied by the courts on the basis of traditional practices, and (5) principles or practices of civil, common, Roman, or other code of law." ;
  skos:editorialNote "This is a Mediating Thing, that is some context in which things have their meaning and existence - in this case, laws and the interpretation thereof by courts." ;
  fibo-fnd-utl-av:definitionOrigin "http://www.businessdictionary.com/definition/legal-system.html"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "The contemporary legal systems of the world are generally based on one of three basic systems: civil law, common law, and religious law, or combinations of these. However, the legal system of each country is shaped by its unique history and so incorporates individual variations." ;
.
fibo-fnd-law-jur:StatuteLaw
  rdf:type owl:Class ;
  rdfs:label "statute law" ;
  rdfs:subClassOf fibo-fnd-law-cor:Law ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-law-jur:Jurisdiction ;
      owl:onProperty fibo-fnd-law-cor:isInForceIn ;
    ] ;
  owl:equivalentClass fibo-bp-reg-req:StatuteLaw ;
  owl:equivalentClass fibo-fnd-lawx-jr:StatuteLaw ;
  owl:equivalentClass fibo-fnd-lawx-rst:StatuteLaw ;
  skos:altLabel "statutory law" ;
  skos:definition "written law (as opposed to oral or customary law) set down by a legislature or by a legislator (in the case of an absolute monarchy)" ;
  fibo-fnd-utl-av:definitionOrigin "http://en.wikipedia.org/wiki/Statute_law"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "Statutes may originate with national, state legislatures or local municipalities. Statutory laws are subordinate to the higher constitutional laws of the land." ;
.
fibo-fnd-law-jur:appliesIn
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-law-jur:LegalSystem ;
  rdfs:label "applies in" ;
  rdfs:range fibo-fnd-law-jur:Jurisdiction ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:governs ;
  skos:definition "indicates the jurisdiction in which a particular legal system applies" ;
.
fibo-fnd-law-jur:hasReach
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-law-jur:Jurisdiction ;
  rdfs:label "has reach" ;
  rdfs:range fibo-fnd-plc-loc:Location ;
  skos:definition "indicates the geopolitical unit (country, federal province or municipality) or geophysical extent in which the jurisdiction has effect" ;
.
fibo-fnd-law-lcap:
  terms:license "http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense"^^xsd:anyURI ;
  sm:contentLanguage "http://www.omg.org/spec/ODM/"^^xsd:anyURI ;
  sm:contentLanguage "http://www.w3.org/standards/techs/owl#w3c_all"^^xsd:anyURI ;
  sm:copyright """Copyright (c) 2013-2016 EDM Council, Inc.
Copyright (c) 2013-2016 Object Management Group, Inc.""" ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Agreements/Agreements/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Agreements/Contracts/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Arrangements/IdentifiersAndIndices/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Law/Jurisdiction/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Law/LegalCore/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Parties/Parties/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Parties/Roles/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"^^xsd:anyURI ;
  sm:fileAbbreviation "fibo-fnd-law-lcap" ;
  sm:fileAbstract "This ontology defines high-level legal concepts, especially those related to legal responsibilities, for use in other FIBO ontology elements. The ontology defines things which are conferred upon some entity by some legal instrument, and elaborates this into a number of specific capacities, responsibilities and powers, each of which forms the basis for many of the concepts used elsewhere in FIBO in defining legal personhood, executive powers and the like." ;
  sm:filename "LegalCapacity.rdf" ;
  rdfs:label "Legal Capacity Ontology" ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/AboutFND/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/Law/AboutLaw/"^^xsd:anyURI ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/FND/20160201/Law/LegalCapacity/> ;
  skos:changeNote """The http://www.omg.org/spec/FIBO/Foundations/20130601/Law/LegalCapacity.owl version of the ontology was revised in advance of the September 2013 New Brunswick, NJ meeting, as follows:
   (1) to use slash style URI/IRIss (also called 303 URIs, vs. hash style) as required to support server side processing 
   (2) to use version-independent IRIs for all definitions internally as opposed to version-specific IRIs
   (3) to change the file suffix from .owl to .rdf to increase usability in RDF tools
   (4) to use 4-level abbreviations and corresponding namespace prefixes for all FIBO ontologies, reflecting a family/specification/module/ontology structure
   (5) to incorporate changes to the specification metadata to support documentation at the family, specification, module, and ontology level, similar to the abbreviations.""" ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/FND/20130801/Law/LegalCapacity.rdf version of the ontology was modified per the issue resolutions identified in the FIBO FND 1.0 FTF report." ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/FND/20141101/Law/LegalCapacity.rdf version of the ontology was modified per the FIBO FBC RFC, namely to add concepts to support license, licensee, and licensor to the ontology." ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/FND/20150801/Law/LegalCapacity.rdf version of the ontology was modified per the FIBO 1.1 RTF to add the concept of litigation capacity to the ontology." ;
.
fibo-fnd-law-lcap:ContractualCapability
  rdf:type owl:Class ;
  rdfs:label "contractual capability" ;
  rdfs:subClassOf fibo-fnd-law-lcap:LegalCapacity ;
  owl:equivalentClass fibo-be-lex-lec:ContractualCapability ;
  skos:definition "the capacity to enter into legally binding contracts" ;
  skos:editorialNote "This is the capacity which defines Contractually Capable Entity (sometimes labeled as 'Legal Entity') as distinct from 'Legal Person'. In the latter case the liabilities incurred in the contract accrue also to the Legal Person. In the case of contractual capability, the entity has the authority to enter into contracts, whether or not the liabilities accrue to that same entity (which they do if it is also a Legal Person). For Legal Entities which are not Legal Persons, the liability unwinds to some legal person within the structure of the entity, for example a General Partner or a Trustee." ;
.
fibo-fnd-law-lcap:DelegatedLegalAuthority
  rdf:type owl:Class ;
  rdfs:label "delegated legal authority" ;
  rdfs:subClassOf fibo-fnd-law-lcap:LegalCapacity ;
  skos:definition "institutionalized and legal power inherent in a particular job, function, or position that is meant to enable its holder to successfully carry out his or her responsibilities, where such power has been delegated through some formal means" ;
  skos:scopeNote "This specifically means the authority to make legally binding commitments." ;
  fibo-fnd-utl-av:explanatoryNote "This is always accompanied by an equal responsibility for one's actions or a failure to act." ;
.
fibo-fnd-law-lcap:Duty
  rdf:type owl:Class ;
  rdfs:label "duty" ;
  rdfs:subClassOf fibo-fnd-law-lcap:LegalConstruct ;
  skos:definition "some obligation which exists and is imposed on some individual" ;
  skos:editorialNote "This can also be thought of as an obligation - not in the sense in which an obligation and a right are the converse aspects of one another, but in and of itself, independent of the perspective from which it is considered. Examples include statutory obligations, reporting obligations and so on." ;
.
fibo-fnd-law-lcap:LegalCapacity
  rdf:type owl:Class ;
  rdfs:label "legal capacity" ;
  rdfs:subClassOf fibo-fnd-law-lcap:LegalConstruct ;
  skos:definition "the capacity to carry out certain actions or to have certain rights" ;
  skos:editorialNote "suggested definition only" ;
.
fibo-fnd-law-lcap:LegalConstruct
  rdf:type owl:Class ;
  rdfs:label "legal construct" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-aap-agt:AutonomousAgent ;
      owl:onProperty fibo-fnd-rel-rel:isConferredOn ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass [
          rdf:type owl:Class ;
          owl:unionOf (
              fibo-fnd-agr-ctr:Contract
              fibo-fnd-law-cor:Constitution
              fibo-fnd-law-cor:Law
            ) ;
        ] ;
      owl:onProperty fibo-fnd-rel-rel:isConferredBy ;
    ] ;
  skos:definition "something which is conferred by way of law or contract, such as a right" ;
  skos:editorialNote "Obligations are an aspect of this category of thing, as are rights." ;
.
fibo-fnd-law-lcap:LiabilityCapacity
  rdf:type owl:Class ;
  rdfs:label "liability capacity" ;
  rdfs:subClassOf fibo-fnd-law-lcap:LegalCapacity ;
  owl:equivalentClass fibo-be-lex-lpx:LiabilityCapacity ;
  skos:definition "the ability to be sued at law" ;
  skos:editorialNote "Note that for the purposes of this model, this is distinct from culpability (the ability to commit criminal acts). That would be a separate and analogous term but with grounding in criminal rather than civil law." ;
.
fibo-fnd-law-lcap:License
  rdf:type owl:Class ;
  rdfs:label "license" ;
  rdfs:subClassOf fibo-fnd-agr-agr:Agreement ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:confers ;
      owl:someValuesFrom fibo-fnd-law-lcap:LegalCapacity ;
    ] ;
  skos:definition "grant of permission needed to do something" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fnd-law-lcap:LicenseIdentifier
  rdf:type owl:Class ;
  rdfs:label "license identifier" ;
  rdfs:subClassOf fibo-fnd-arr-id:Identifier ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-law-lcap:License ;
      owl:onProperty fibo-fnd-aap-agt:identifies ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "an identifier associated with a license" ;
.
fibo-fnd-law-lcap:Licensee
  rdf:type owl:Class ;
  rdfs:label "licensee" ;
  rdfs:subClassOf fibo-fnd-pty-pty:PartyInRole ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
      owl:someValuesFrom [
          rdf:type owl:Restriction ;
          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
          owl:onClass fibo-fnd-law-lcap:License ;
          owl:onProperty fibo-fnd-pty-pty:isAPartyTo ;
        ] ;
    ] ;
  skos:definition "a party to whom a license has been granted" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fnd-law-lcap:Licensor
  rdf:type owl:Class ;
  rdfs:label "licensor" ;
  rdfs:subClassOf fibo-fnd-pty-pty:PartyInRole ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty fibo-fnd-law-lcap:licenses ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
      owl:someValuesFrom [
          rdf:type owl:Restriction ;
          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
          owl:onClass fibo-fnd-law-lcap:License ;
          owl:onProperty fibo-fnd-pty-pty:isAPartyTo ;
        ] ;
    ] ;
  skos:definition "a party who grants a license" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fnd-law-lcap:LitigationCapacity
  rdf:type owl:Class ;
  rdfs:label "litigation capacity" ;
  rdfs:subClassOf fibo-fnd-law-lcap:LegalCapacity ;
  skos:definition "the legal capacity to pursue a litigation action in law" ;
.
fibo-fnd-law-lcap:SignatoryCapacity
  rdf:type owl:Class ;
  rdfs:label "signatory capacity" ;
  rdfs:subClassOf fibo-fnd-law-lcap:LegalCapacity ;
  skos:definition "the capacity of some natural person to sign agreements on the part of some organization or legal person" ;
.
fibo-fnd-law-lcap:StatutoryResponsibility
  rdf:type owl:Class ;
  rdfs:label "statutory responsibility" ;
  rdfs:subClassOf fibo-fnd-law-lcap:Duty ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-law-jur:StatuteLaw ;
      owl:onProperty fibo-fnd-rel-rel:isMandatedBy ;
    ] ;
  skos:definition "an obligation which is defined under some body of law (statute)" ;
.
fibo-fnd-law-lcap:hasCapacity
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-aap-agt:AutonomousAgent ;
  rdfs:label "has capacity" ;
  rdfs:range fibo-fnd-law-lcap:LegalCapacity ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  owl:inverseOf fibo-fnd-law-lcap:isCapacityOf ;
  skos:definition "identifies an individual or organization that has some capability to carry out certain actions, or has certain rights or obligations" ;
.
fibo-fnd-law-lcap:isCapacityOf
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-law-lcap:LegalCapacity ;
  rdfs:label "is capacity of" ;
  rdfs:range fibo-fnd-aap-agt:AutonomousAgent ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:isConferredOn ;
  skos:definition "identifies an individual or organization on which a given legal capacity has been conferred" ;
  skos:scopeNote "This includes capacities specific to duties at law (such as those for corporate officers) as well as the ability or capacity to incur liability." ;
.
fibo-fnd-law-lcap:isLicensedBy
  rdf:type owl:ObjectProperty ;
  rdfs:label "is licensed by" ;
  rdfs:range fibo-fnd-pty-pty:PartyInRole ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:isGovernedBy ;
  owl:inverseOf fibo-fnd-law-lcap:licenses ;
  skos:definition "indicates the party that has issued a particular license to some other party" ;
.
fibo-fnd-law-lcap:licenses
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-pty-pty:PartyInRole ;
  rdfs:label "licenses" ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:governs ;
  skos:definition "issues a license required in order to perform some task, provide some service, exercise some privilege, or pursue some line of business or occupation to some party" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.merriam-webster.com/dictionary/regulate"^^xsd:anyURI ;
.
fibo-fnd-law-cor:
  terms:license "http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense"^^xsd:anyURI ;
  sm:contentLanguage "http://www.omg.org/spec/ODM/"^^xsd:anyURI ;
  sm:contentLanguage "http://www.w3.org/standards/techs/owl#w3c_all"^^xsd:anyURI ;
  sm:copyright """Copyright (c) 2013-2014 EDM Council, Inc.
Copyright (c) 2013-2014 Object Management Group, Inc.""" ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Organizations/FormalOrganizations/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"^^xsd:anyURI ;
  sm:fileAbbreviation "fibo-fnd-law-cor" ;
  sm:fileAbstract "This ontology defines high-level legal concepts for use in other FIBO ontology elements. These concepts include law and constitution, both of which are framed at a more abstract level than national or state laws and constitutions, so that law forms the basis both for statutes and for company by-laws, and constitution forms the basis both for national or state constitutions and for instruments which are constitutive of incorporated legal entities. This ontology also defines some of the variants of these such as governmental constitutions and ordinances. Other types of law are provided in the Jurisdictions ontology as extensions of concepts in this ontology. Court of Law is also defined here." ;
  sm:filename "LegalCore.rdf" ;
  rdfs:label "Legal Core Ontology" ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/AboutFND/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/Law/AboutLaw/"^^xsd:anyURI ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/FND/20141101/Law/LegalCore/> ;
  skos:changeNote "The http://www.omg.org/spec/EDMC-FIBO/FND/20130801/Law/LegalCore.rdf version of the ontology was was modified per the issue resolutions identified in the FIBO FND 1.0 FTF report and in http://www.omg.org/spec/EDMC-FIBO/FND/1.0/AboutFND-1.0/." ;
  skos:changeNote """The http://www.omg.org/spec/FIBO/Foundations/20130601/Law/LegalCore.owl version of the ontology was revised in advance of the September 2013 New Brunswick, NJ meeting, as follows:
   (1) to use slash style URI/IRIss (also called 303 URIs, vs. hash style) as required to support server side processing 
   (2) to use version-independent IRIs for all definitions internally as opposed to version-specific IRIs
   (3) to change the file suffix from .owl to .rdf to increase usability in RDF tools
   (4) to use 4-level abbreviations and corresponding namespace prefixes for all FIBO ontologies, reflecting a family/specification/module/ontology structure
   (5) to incorporate changes to the specification metadata to support documentation at the family, specification, module, and ontology level, similar to the abbreviations.
   (6) to revise some of the text definitions using more formal sources.""" ;
.
fibo-fnd-law-cor:Constitution
  rdf:type owl:Class ;
  rdfs:label "constitution" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-law-cor:Law ;
      owl:onProperty fibo-fnd-rel-rel:governs ;
    ] ;
  skos:definition "A constitution defines the basic principles and laws of a nation, state, or social group that determine the powers and duties of the government and guarantee certain rights to the people in it." ;
  skos:editorialNote "This defines the framework in which laws (for a country constitution), rules and regulations (for a party or organization constitution) or contractual commitments are made and in which they have force." ;
  fibo-fnd-utl-av:definitionOrigin "Merriam-Webster Online Dictionary" ;
.
fibo-fnd-law-cor:CourtOfLaw
  rdf:type owl:Class ;
  rdfs:label "court of law" ;
  rdfs:subClassOf fibo-fnd-org-fm:FormalOrganization ;
  skos:definition "A court of law is a court that hears cases and decides them on the basis of statutes or the common law." ;
  fibo-fnd-utl-av:definitionOrigin "Merriam-Webster Online Dictionary" ;
.
fibo-fnd-law-cor:GovernmentalConstitution
  rdf:type owl:Class ;
  rdfs:label "governmental constitution" ;
  rdfs:subClassOf fibo-fnd-law-cor:Constitution ;
  skos:definition "a set of rules and principles that define the nature and extent of government" ;
  skos:editorialNote "This defines the framework in which laws are made and in which they have force." ;
  fibo-fnd-utl-av:definitionOrigin "http://en.wikipedia.org/wiki/Constitution#Governmental_constitutions"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "Most constitutions seek to regulate the relationship between institutions of the state, in a basic sense the relationship between the executive, legislature and the judiciary, but also the relationship of institutions within those branches.  For example, executive branches can be divided into a head of government, government departments/ministries, executive agencies and a civil service/administration.  Most constitutions also attempt to define the relationship between individuals and the state, and to establish the broad rights of individual citizens. It is thus the most basic law of a territory from which all the other laws and rules are hierarchically derived; in some territories it is in fact called Basic Law." ;
.
fibo-fnd-law-cor:Law
  rdf:type owl:Class ;
  rdfs:label "Law" ;
  rdfs:subClassOf owl:Thing ;
  owl:equivalentClass fibo-fnd-lawx-rst:Law ;
  skos:definition "a system of rules and guidelines which are enforced through social institutions to govern behavior" ;
  skos:editorialNote "Any law or body of law, which may have force in some context, including national laws, company bylaws and the like." ;
  fibo-fnd-utl-av:definitionOrigin "http://en.wikipedia.org/wiki/Law"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "Law is a term which does not have a universally accepted definition. Certain laws are made by governments, specifically by their legislatures although the sense intended here is broader. The formation of laws themselves may be influenced by a constitution (written or unwritten) and the rights encoded therein.  The law shapes politics, economics and society in countless ways and serves as a social mediator of relations between people." ;
.
fibo-fnd-law-cor:Ordinance
  rdf:type owl:Class ;
  rdfs:label "ordinance" ;
  rdfs:subClassOf fibo-fnd-law-cor:Law ;
  skos:definition "An authoritative rule or law; a decree or command; a public injunction or regulation, such as a city ordinance against excessive horn blowing. (Source: Dictionary.com)" ;
.
fibo-fnd-law-cor:constrains
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-law-cor:Law ;
  rdfs:label "constrains" ;
  rdfs:range fibo-fnd-aap-agt:AutonomousAgent ;
  skos:definition "forces, compels, or obliges" ;
  fibo-fnd-utl-av:definitionOrigin "http://dictionary.reference.com/browse/constrains"^^xsd:anyURI ;
.
fibo-fnd-law-cor:hasInforce
  rdf:type owl:ObjectProperty ;
  rdfs:label "has in force" ;
  rdfs:range fibo-fnd-law-cor:Law ;
  owl:inverseOf fibo-fnd-law-cor:isInForceIn ;
  skos:definition "relates a jurisdiction or situation to a rule, regulation or law (collectively \"law\") that is currently in force in that situation or jurisdiction" ;
.
fibo-fnd-law-cor:isConstrainedBy
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-aap-agt:AutonomousAgent ;
  rdfs:label "is constrained by" ;
  rdfs:range fibo-fnd-law-cor:Law ;
  owl:inverseOf fibo-fnd-law-cor:constrains ;
  skos:definition "identifies the policy, rule, regulation, contract, or other thing that compels or obliges someone to act in some way" ;
.
fibo-fnd-law-cor:isInForceIn
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-law-cor:Law ;
  rdfs:label "is in force in" ;
  skos:definition "identifies a jurisdiction or similar context in which some law (including by-law, company by-law and state law) has effect" ;
.
fibo-fnd-lawx-jr:
  rdfs:label "JuridicialReach"@en ;
.
fibo-fnd-lawx-jr:CivilLawJurisdiction
  rdf:type owl:Class ;
  rdfs:label "civil law jurisdiction"@en ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-lawx-jr:civilJurisdictionHasGeographicalReach ;
      owl:someValuesFrom fibo-fnd-plc-cty:GeopoliticalEntity ;
    ] ;
.
fibo-fnd-lawx-jr:Jurisdiction
  rdf:type owl:Class ;
  rdfs:label "jurisdiction"@en ;
  rdfs:subClassOf owl:Thing ;
.
fibo-fnd-lawx-jr:StatuteLaw
  rdf:type owl:Class ;
  rdfs:label "statute law"@en ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-lawx-jr:statuteLawHasGeographicalReach ;
      owl:someValuesFrom fibo-fnd-plc-cty:GeopoliticalEntity ;
    ] ;
.
fibo-fnd-lawx-jr:civilJurisdictionHasGeographicalReach
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-lawx-jr:Jurisdiction ;
  rdfs:label "civil jurisdiction has geographical reach"@en ;
  rdfs:range fibo-fnd-plc-cty:GeopoliticalEntity ;
  rdfs:subPropertyOf fibo-fnd-law-jur:hasReach ;
  skos:definition "The geopolitical entity (usually country) over which the Civil Jurisdiction has its reach."@en ;
  fibo-fnd-utl-av:explanatoryNote "This is typically one country, since civil law applies to the country which makes the laws."@en ;
.
fibo-fnd-lawx-jr:statuteLawHasGeographicalReach
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-lawx-jr:Jurisdiction ;
  rdfs:label "statute law has geographical reach"@en ;
  rdfs:range fibo-fnd-plc-cty:GeopoliticalEntity ;
  rdfs:subPropertyOf fibo-fnd-law-jur:hasReach ;
  skos:definition "The geographical extent of the jurisdiction in which the Statute Law has effect."@en ;
  fibo-fnd-utl-av:explanatoryNote "This is usually but not always a Country. It may also be a part of a country (England-and-Wales) or a State or Province within a Federal country, or it may be the federal country itself if the statute is a federal law."@en ;
.
fibo-fnd-lawx-jurx:
  rdfs:label "JurisdictionExtended"@en ;
.
fibo-fnd-lawx-jurx:BijuridicialJurisdiction
  rdf:type owl:Class ;
  rdfs:label "bijuridicial jurisdiction"@en ;
  rdfs:subClassOf fibo-fnd-law-jur:Jurisdiction ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-lawx-jurx:isPartiallyGovernedUnder ;
      owl:someValuesFrom fibo-fnd-law-jur:CivilLawSystem ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-lawx-jurx:isPartiallyGovernedUnder ;
      owl:someValuesFrom fibo-fnd-law-jur:CommonLawSystem ;
    ] ;
  skos:definition "A Jurisdiction in which two or more systems of law are in force."@en ;
  skos:definition "One system of law under which a Bijuridicial Jurisdiction is governed is the system of Civil Law."@en ;
  skos:definition "One system of law under which a Bijuridicial Jurisdiction is governed is the system of Common Law."@en ;
  fibo-fnd-utl-av:explanatoryNote "A Bijuridicial Jurisdiction is governed under two systems of law, of which this is one."@en ;
  fibo-fnd-utl-av:explanatoryNote "Examples include South Africa, Quebec, Louisiana and a few others, where or example Civil Law and Common Law arrangements are in force."@en ;
.
fibo-fnd-lawx-jurx:BodyOfLaw
  rdf:type owl:Class ;
  rdfs:label "body of law"@en ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-lawx-jurx:incorporatesSome ;
      owl:someValuesFrom fibo-fnd-law-cor:Law ;
    ] ;
  skos:definition "Some body of law, that is some set of laws, statutes, ordinances and the like."@en ;
.
fibo-fnd-lawx-jurx:CanonLawSystem
  rdf:type owl:Class ;
  rdfs:label "canon law system"@en ;
  rdfs:subClassOf fibo-fnd-lawx-jurx:ReligiousLawSystem ;
  skos:definition "The body of laws & regulations made or adopted by ecclesiastical authority, for the government of the Christian organization and its members."@en ;
  fibo-fnd-utl-av:explanatoryNote "In force in both the Western (Roman) and Eastern churches, and in some form in the Anglican Church. There appear to be no current instances national jurisdictions which take this as a legal framework, with the possible exception of the Vatican. In historical contexts there would be."@en ;
.
fibo-fnd-lawx-jurx:CivilLaw
  rdf:type owl:Class ;
  rdfs:label "civil law"@en ;
  rdfs:subClassOf fibo-fnd-lawx-jurx:CommonLaw ;
  skos:definition "The branch of the law dealing with disputes between individuals or organizations, in which compensation may be awarded to the victim."@en ;
.
fibo-fnd-lawx-jurx:CommonLaw
  rdf:type owl:Class ;
  rdfs:label "common law"@en ;
  rdfs:subClassOf fibo-fnd-lawx-jurx:BodyOfLaw ;
  skos:definition "Law developed by judges through decisions of courts and similar tribunals rather than through legislative statutes or executive branch action."@en ;
.
fibo-fnd-lawx-jurx:ConstitutionalLaw
  rdf:type owl:Class ;
  rdfs:label "constitutional law"@en ;
  rdfs:subClassOf fibo-fnd-lawx-jurx:BodyOfLaw ;
  skos:definition "The body of law which defines the relationship of different entities within a state, namely the executive, legislature and the judiciary. Further notes: (from Wikipedia): Constitutional laws may be considered second order rulemaking or rules about making rules to exercise power."@en ;
.
fibo-fnd-lawx-jurx:CriminalLaw
  rdf:type owl:Class ;
  rdfs:label "criminal law"@en ;
  rdfs:subClassOf fibo-fnd-lawx-jurx:CommonLaw ;
  skos:definition "The body of law that defines conduct that is not allowed because it is held to threaten, harm or endanger the safety and welfare of people, and that sets out the punishments to be imposed on people who do not obey these laws."@en ;
.
fibo-fnd-lawx-jurx:InternationalLaw
  rdf:type owl:Class ;
  rdfs:label "international law"@en ;
  rdfs:subClassOf fibo-fnd-lawx-jurx:BodyOfLaw ;
  skos:definition "The set of rules generally regarded and accepted as binding in relations between states and nations."@en ;
  fibo-fnd-utl-av:explanatoryNote "This can refer to three distinct legal disciplines: 1. Public international law governing the relations between provinces and national entities 2. Private international lse, which addresses questions of which jurisdiction may hear a case and which jurisdiction applies to issues in a case 3. Supranational law which concerns regional agreements where the laws of nation states may be held inapplicable when conflicting with a supranational legal system."@en ;
.
fibo-fnd-lawx-jurx:LegalSystem
  rdf:type owl:Class ;
  rdfs:label "legal system"@en ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-lawx-jurx:definesTheApplicationOf ;
      owl:someValuesFrom fibo-fnd-lawx-jurx:BodyOfLaw ;
    ] ;
.
fibo-fnd-lawx-jurx:ReligiousLaw
  rdf:type owl:Class ;
  rdfs:label "religious law"@en ;
  rdfs:subClassOf fibo-fnd-lawx-jurx:BodyOfLaw ;
  skos:definition "Some set of laws derived from and practised in the name of some religion."@en ;
.
fibo-fnd-lawx-jurx:ReligiousLawJurisdiction
  rdf:type owl:Class ;
  rdfs:label "religious law jurisdiction"@en ;
  rdfs:subClassOf fibo-fnd-law-jur:Jurisdiction ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-lawx-jurx:ReligiousLawSystem ;
      owl:onProperty fibo-fnd-rel-rel:isGovernedBy ;
    ] ;
  skos:definition "A Jurisdiction in which some system of religious law is in force."@en ;
  fibo-fnd-utl-av:explanatoryNote "This includes Sharia Law jurisdictions."@en ;
.
fibo-fnd-lawx-jurx:ReligiousLawSystem
  rdf:type owl:Class ;
  rdfs:label "religious law system"@en ;
  rdfs:subClassOf fibo-fnd-law-jur:LegalSystem ;
  skos:definition "Some system of law based on the ordering principle of reality as being knowledge as revealed by God and governing all human affairs."@en ;
  fibo-fnd-utl-av:explanatoryNote "There are several mutually incompatible systems of Religious Law - the term referred to here is ancestral to all of those and defines simply that the jurisdiction in question is some kind of religious law jurisdiction - specific types are to be defined per system of religious law (two are included in the model for reference and possible use; there may be others)."@en ;
.
fibo-fnd-lawx-jurx:ShariaLawJurisdiction
  rdf:type owl:Class ;
  rdfs:label "sharia law jurisdiction"@en ;
  rdfs:subClassOf fibo-fnd-lawx-jurx:ReligiousLawJurisdiction ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:isGovernedBy ;
      owl:someValuesFrom fibo-fnd-lawx-jurx:ShariaLawSystem ;
    ] ;
  skos:definition "A Jurisdiction in which a Shari'a system of law is in force."@en ;
.
fibo-fnd-lawx-jurx:ShariaLawSystem
  rdf:type owl:Class ;
  rdfs:label "sharia law system"@en ;
  rdfs:subClassOf fibo-fnd-lawx-jurx:ReligiousLawSystem ;
  skos:definition "A system of Shari'a Law."@en ;
  fibo-fnd-utl-av:explanatoryNote "There are several schools of law in Islamic jurisprudence; these are not articulated here at present. The bodies of law and other textual corpora referred to in the implementation of this system of law are the Qur'an and the Hadith, with variations in interpretation and precedence according to the system (school) in question. Also known as the Qanun 'Islami (Islamic Canon)"@en ;
.
fibo-fnd-lawx-jurx:StatutoryLaw
  rdf:type owl:Class ;
  rdfs:label "statutory law"@en ;
  rdfs:subClassOf fibo-fnd-lawx-jurx:BodyOfLaw ;
  skos:definition "Written law set down by a legislature or by a legislator."@en ;
  fibo-fnd-utl-av:explanatoryNote "Statutory Law may originate with national, state legislatures or local municipalities. Statutes of lower jurisdictions are subordinate to the law of higher. Definition adapted from: Wikipedia"@en ;
.
fibo-fnd-lawx-jurx:definesTheApplicationOf
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-lawx-jurx:LegalSystem ;
  rdfs:label "defines the application of"@en ;
  rdfs:range fibo-fnd-lawx-jurx:BodyOfLaw ;
  skos:definition "The Body of Law, the application of which is defined in the Legal System."@en ;
.
fibo-fnd-lawx-jurx:incorporatesSome
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-lawx-jurx:BodyOfLaw ;
  rdfs:label "incorporates some"@en ;
  rdfs:range fibo-fnd-law-cor:Law ;
  skos:definition "The laws included in this Body of Law."@en ;
.
fibo-fnd-lawx-jurx:isPartiallyGovernedUnder
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-lawx-jurx:BijuridicialJurisdiction ;
  rdfs:label "is partially governed under"@en ;
  rdfs:range fibo-fnd-law-jur:LegalSystem ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:isGovernedBy ;
  fibo-fnd-utl-av:explanatoryNote "A Bijuridicial Jurisdiction is governed under two systems of law, of which this is one."@en ;
.
fibo-fnd-lawx-rst:
  rdfs:label "RegulatoryRestrictions" ;
.
fibo-fnd-lawx-rst:Contract
  rdf:type owl:Class ;
  rdfs:label "contract" ;
  rdfs:subClassOf owl:Thing ;
.
fibo-fnd-lawx-rst:ContractualRestriction
  rdf:type owl:Class ;
  rdfs:label "contractual restriction" ;
  rdfs:subClassOf fibo-fnd-lawx-rst:Restriction ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-lawx-rst:contractuallyImposedBy ;
      owl:someValuesFrom fibo-fnd-agr-ctr:Contract ;
    ] ;
  skos:definition "A restriction defined in a Contract as opposed to being defined in a body of Law." ;
.
fibo-fnd-lawx-rst:Instrumentality
  rdf:type owl:Class ;
  rdfs:label "instrumentality" ;
  rdfs:subClassOf owl:Thing ;
  owl:equivalentClass [
      rdf:type owl:Class ;
      owl:unionOf (
          fibo-fnd-lawx-rst:Contract
          fibo-fnd-lawx-rst:Law
        ) ;
    ] ;
  skos:definition "some law or contract which is instrumental in the existence of some recognized construct such as a restriction" ;
.
fibo-fnd-lawx-rst:Law
  rdf:type owl:Class ;
  rdfs:label "law" ;
  rdfs:subClassOf owl:Thing ;
.
fibo-fnd-lawx-rst:LegalRestriction
  rdf:type owl:Class ;
  rdfs:label "legal restriction" ;
  rdfs:subClassOf fibo-fnd-lawx-rst:Restriction ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-lawx-rst:legallyImposedBy ;
      owl:someValuesFrom fibo-fnd-lawx-rst:StatuteLaw ;
    ] ;
  skos:definition "A Restriction defined in a body of Law." ;
.
fibo-fnd-lawx-rst:RegulatoryRequirement
  rdf:type owl:Class ;
  rdfs:label "regulatory requirement" ;
  rdfs:subClassOf fibo-bp-reg-req:LegalRequirement ;
  skos:definition "A requirement of some sort by which something is to be regulated." ;
  skos:editorialNote "There is not presently a common \"Financial\" sub-type of this, since regulations like investment, consumer protection, systemic risk and so on are quite different and quite far reaching. However, readers of this model should keep in mind that this term does not apply only to financial types of regulation. For instance, insurance regulation would also be a sub-type of this item." ;
.
fibo-fnd-lawx-rst:Restriction
  rdf:type owl:Class ;
  rdfs:label "restriction" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-lawx-rst:restricts ;
      owl:someValuesFrom owl:Thing ;
    ] ;
  skos:definition "Anything which prevents something being done which might otherwise be done. The Restriction is defined relative to the thing which might otherwise be done, which may be an activity of some sort. It is therefore a Relative Thing, with a property identifying that act or event." ;
.
fibo-fnd-lawx-rst:StatuteLaw
  rdf:type owl:Class ;
  rdfs:label "statute law" ;
  rdfs:subClassOf owl:Thing ;
.
fibo-fnd-lawx-rst:contractuallyImposedBy
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-lawx-rst:ContractualRestriction ;
  rdfs:label "contractually imposed by" ;
  rdfs:range fibo-fnd-agr-ctr:Contract ;
  rdfs:subPropertyOf fibo-fnd-lawx-rst:imposedBy ;
.
fibo-fnd-lawx-rst:contractuallyImposes
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-lawx-rst:Contract ;
  rdfs:label "contractually imposes" ;
  rdfs:range fibo-fnd-lawx-rst:ContractualRestriction ;
  rdfs:subPropertyOf fibo-fnd-lawx-rst:imposes ;
.
fibo-fnd-lawx-rst:imposedBy
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-lawx-rst:Restriction ;
  rdfs:label "imposed by" ;
  rdfs:range fibo-fnd-lawx-rst:Instrumentality ;
.
fibo-fnd-lawx-rst:imposes
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-lawx-rst:Instrumentality ;
  rdfs:label "imposes" ;
  rdfs:range fibo-fnd-lawx-rst:Restriction ;
  owl:inverseOf fibo-fnd-lawx-rst:imposedBy ;
  skos:definition "some instrumentality such as a contract or law imposes a restriction on some entity in regard to some subject matter" ;
.
fibo-fnd-lawx-rst:legallyImposedBy
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-lawx-rst:LegalRestriction ;
  rdfs:label "legally imposed by" ;
  rdfs:range fibo-fnd-lawx-rst:StatuteLaw ;
  rdfs:subPropertyOf fibo-fnd-lawx-rst:imposedBy ;
.
fibo-fnd-lawx-rst:legallyImposes
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-lawx-rst:StatuteLaw ;
  rdfs:label "legally imposes" ;
  rdfs:range fibo-fnd-lawx-rst:LegalRestriction ;
  rdfs:subPropertyOf fibo-fnd-lawx-rst:imposes ;
.
fibo-fnd-lawx-rst:restrictionAppliesIn
  rdf:type owl:ObjectProperty ;
  rdfs:label "restriction applies in" ;
.
fibo-fnd-lawx-rst:restricts
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-lawx-rst:Restriction ;
  rdfs:label "restricts" ;
  rdfs:range owl:Thing ;
.
fibo-fnd-lawx-rst:restrictsActivity
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-lawx-rst:Restriction ;
  rdfs:label "restricts activity" ;
  rdfs:subPropertyOf fibo-fnd-lawx-rst:restricts ;
.
fibo-fnd-math-amt:
  rdfs:label "Amounts"@en ;
.
fibo-fnd-math-amt:MaterialAmount
  rdf:type owl:Class ;
  rdfs:label "material amount"@en ;
  rdfs:subClassOf fibo-fnd-math-amt:NumericAmount ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-math-amt:hasBaseUnit ;
      owl:someValuesFrom fibo-fnd-arrx-qua:Unit ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-math-amt:isAmountOf ;
      owl:someValuesFrom fibo-fnd-phy-phy:ContinuousPhysicalStuff ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-math-amt:mayBeExpressedAs ;
      owl:someValuesFrom fibo-fnd-math-amt:Measure ;
    ] ;
  skos:definition "A qualified amount of some continuous substance or some thing measured in units. Material Amount is always qualified by some unit in which the substance is measured."@en ;
.
fibo-fnd-math-amt:MathematicalThing
  rdf:type owl:Class ;
  rdfs:label "mathematical thing"@en ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "A Mathematical Thing is something in the universe of mathematics. That is, something that is a parameter (mathematical quantity) or something that is used to express that parameter, such as a value or components of a formula."@en ;
.
fibo-fnd-math-amt:Measure
  rdf:type owl:Class ;
  rdfs:label "measure"@en ;
  rdfs:subClassOf fibo-fnd-math-amt:MathematicalThing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-math-amt:isMeasuredIn ;
      owl:someValuesFrom fibo-fnd-arrx-qua:Unit ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-math-amt:reciprocal ;
      owl:someValuesFrom fibo-fnd-math-amt:Measure ;
    ] ;
.
fibo-fnd-math-amt:NumericAmount
  rdf:type owl:Class ;
  rdfs:label "numeric amount"@en ;
  rdfs:subClassOf fibo-fnd-math-amt:QuantitativeAmount ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-math-amt:isQuantityOf ;
      owl:someValuesFrom fibo-fnd-phy-phy:DiscretePhysicalThing ;
    ] ;
  skos:definition "An amount of some discrete thing Note: does not require units as the individual things are counted. Tech note: in other (data) standards this stands alongside (material) Amount rather than above it see e.g. CCTS, ISO 20022 v1.5"@en ;
.
fibo-fnd-math-amt:QuantitativeAmount
  rdf:type owl:Class ;
  rdfs:label "quantitative amount"@en ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "A measured amount of anything (material or time). This is an actual quantity of something rather than the abstract concept of the amount that something is of. Hence it is concrete not abstract."@en ;
  skos:editorialNote "Later internal review: This term is currently ancestral to terms with both meanings - an actual amount of some kind of thing, such as an amount of money; and the measure which is the amount of some quantity of things, e.g. an amount specified in a contract as \"this much\" of something. We should probably clean this up. Until we do, this term is ancestral to both types of amount term. So for instance it is not incorrect to talk about some amount of something, and have a datatype for a term within that, which is a Monetary Amount (for example, Monetary Amount as part of some specification of a Retained Amount in a loan contract). . This will probably flag up as inconsistent and need fixing later."@en ;
.
fibo-fnd-math-amt:Unit
  rdf:type owl:Class ;
  rdfs:label "unit"@en ;
  rdfs:subClassOf fibo-fnd-math-math:ParametricThing ;
  skos:definition "A unit of measure ofa quantity, time or anything else which is measurable. NB don't confuse this with units in a Trust Fund!!"@en ;
.
fibo-fnd-math-amt:amount
  rdf:type owl:DatatypeProperty ;
  rdfs:label "amount"@en ;
.
fibo-fnd-math-amt:hasBaseUnit
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-math-amt:MaterialAmount ;
  rdfs:label "has base unit"@en ;
  rdfs:range fibo-fnd-arrx-qua:Unit ;
  skos:definition "That in which the material amount is measured."@en ;
.
fibo-fnd-math-amt:hasValue
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-math-amt:NumericAmount ;
  rdfs:label "has value"@en ;
  rdfs:range fibo-fnd-math-math:Value ;
  skos:definition "The amount of whatever this is a quantity of."@en ;
.
fibo-fnd-math-amt:isAmountOf
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-math-amt:MaterialAmount ;
  rdfs:label "is amount of"@en ;
  rdfs:range fibo-fnd-phy-phy:ContinuousPhysicalStuff ;
  rdfs:subPropertyOf fibo-fnd-math-amt:isQuantityOf ;
  skos:definition "That which the Material Amount is an amount of."@en ;
.
fibo-fnd-math-amt:isMeasuredIn
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-math-amt:Measure ;
  rdfs:label "is measured in"@en ;
  rdfs:range fibo-fnd-arrx-qua:Unit ;
  skos:definition "The unit of measure in which the Measure is formally expressed."@en ;
.
fibo-fnd-math-amt:isQuantityOf
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-math-amt:NumericAmount ;
  rdfs:label "is quantity of"@en ;
  rdfs:range fibo-fnd-phy-phy:DiscretePhysicalThing ;
  skos:definition "That which the amount is an amount of."@en ;
.
fibo-fnd-math-amt:mayBeExpressedAs
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-math-amt:MaterialAmount ;
  rdfs:label "may be expressed as"@en ;
  rdfs:range fibo-fnd-math-amt:Measure ;
  skos:definition "A measure in which the material amount may be expressed."@en ;
.
fibo-fnd-math-amt:reciprocal
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-math-amt:Measure ;
  rdfs:label "reciprocal"@en ;
  rdfs:range fibo-fnd-math-amt:Measure ;
  skos:definition "A measure stated in terms of the recpirocal of another measure."@en ;
.
fibo-fnd-math-math:
  rdfs:label "Math"@en ;
.
fibo-fnd-math-math:Comparison
  rdf:type owl:Class ;
  rdfs:label "comparison"@en ;
  rdfs:subClassOf fibo-fnd-math-math:MathematicalQuantity ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-math-math:NumericalParameter ;
      owl:onProperty fibo-fnd-math-math:isBetween ;
    ] ;
  skos:definition "A comparison among numerical parameters."@en ;
.
fibo-fnd-math-math:Constant
  rdf:type owl:Class ;
  rdfs:label "constant"@en ;
  rdfs:subClassOf fibo-fnd-math-math:NumericalParameter ;
.
fibo-fnd-math-math:ContractuallyDefinedNumericalParameter
  rdf:type owl:Class ;
  rdfs:label "contractually defined numerical parameter"@en ;
  rdfs:subClassOf fibo-fnd-math-math:NumericalParameter ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-math-math:hasTerms ;
      owl:someValuesFrom fibo-fnd-agr-ctr:ContractualDefinition ;
    ] ;
.
fibo-fnd-math-math:CurrentValue
  rdf:type owl:Class ;
  rdfs:label "current value"@en ;
  rdfs:subClassOf fibo-fnd-math-math:TimedValue ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-math-math:tense ;
      owl:someValuesFrom fibo-fnd-tim-tim:Present ;
    ] ;
  owl:disjointWith fibo-fnd-math-math:PastValue ;
  owl:disjointWith fibo-fnd-math-math:ProjectedValue ;
  skos:definition "The value at the current point in time."@en ;
  fibo-fnd-utl-av:explanatoryNote "This immediately becomes a past value."@en ;
.
fibo-fnd-math-math:Difference
  rdf:type owl:Class ;
  rdfs:label "difference"@en ;
  rdfs:subClassOf fibo-fnd-math-math:Comparison ;
  skos:definition "The difference between two numerical parameters."@en ;
.
fibo-fnd-math-math:Direction
  rdf:type owl:Class ;
  rdfs:label "Direction"@en ;
  rdfs:subClassOf owl:Thing ;
.
fibo-fnd-math-math:Down
  rdf:type owl:Class ;
  rdfs:label "down"@en ;
  rdfs:subClassOf fibo-fnd-math-math:Direction ;
.
fibo-fnd-math-math:Formula
  rdf:type owl:Class ;
  rdfs:label "formula"@en ;
  rdfs:subClassOf fibo-fnd-math-math:MathematicalExpression ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-math-math:hasExpression ;
      owl:someValuesFrom fibo-fnd-math-math:FormulaExpression ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-math-math:hasSubject ;
      owl:someValuesFrom fibo-fnd-math-math:FormulaSubject ;
    ] ;
.
fibo-fnd-math-math:FormulaArgument
  rdf:type owl:Class ;
  rdfs:label "formula argument"@en ;
  rdfs:subClassOf fibo-fnd-math-amt:MathematicalThing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-utl-val:DecimalValue ;
      owl:onProperty <https://spec.edmcouncil.org/fibo/FND/Math/Math/constant.1> ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-math-math:hasOperand ;
      owl:someValuesFrom fibo-fnd-math-math:NumericalParameter ;
    ] ;
  skos:definition "The Right Hand Side of the Formula."@en ;
  fibo-fnd-utl-av:usageNote "This is modeled as one or more numerical parameters, and one or more constants. The constants are the simple fact \"Constant\" while the parameters are the target of the relationship fact \"Operand\". Note that it is not possible to model the argument mathematically. This model simply identifies what are the operands and constants that make up the argument, or right hand side, of the formula. Note also that any formula can be expressed with different parameters on the left and right hand sides; this model identifies and lists the elements on the right hand side of the formula when a given \"Formulka Subject\" makes up the left hand side of the formula, that is, semantically the formula relates a particular variable to a number of other variables and constants in an expression, and is not set up to be able to identify equivalent formule in which a different subject is the left hand side of that formula. If useful, an equivalence relationship could be defined between two such formula semantics. That is, the same formula may not have the same meaning and vice versa."@en ;
.
fibo-fnd-math-math:FormulaExpression
  rdf:type owl:Class ;
  rdfs:label "formula expression"@en ;
  rdfs:subClassOf fibo-fnd-math-math:FormulaPart ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-math-math:hasArgument ;
      owl:someValuesFrom fibo-fnd-math-math:FormulaArgument ;
    ] ;
  skos:definition "How the Formula is expressed i.e. the Right Hand Side of the Formula."@en ;
  fibo-fnd-utl-av:usageNote "This is alternative to and in addition to the textual expression of the Formula in the \"Expression\" Simple Fact for Formula. This is so that if appropriate and relevant, the formula can be described semantically in terms of its component parts. Where this would not add to the model, use the textual simple fact \"Expression\" instead."@en ;
.
fibo-fnd-math-math:FormulaPart
  rdf:type owl:Class ;
  rdfs:label "formula part"@en ;
  rdfs:subClassOf fibo-fnd-math-amt:MathematicalThing ;
.
fibo-fnd-math-math:FormulaSubject
  rdf:type owl:Class ;
  rdfs:label "formula subject"@en ;
  rdfs:subClassOf fibo-fnd-math-math:FormulaPart ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-math-math:subjectTakesForm ;
      owl:someValuesFrom fibo-fnd-math-math:FormulaArgument ;
    ] ;
  skos:definition "The Left Hand Side of the Formula, giving what is being expressed in terms of the Right Hand Side."@en ;
.
fibo-fnd-math-math:InternalValueDetermination
  rdf:type owl:Class ;
  rdfs:label "internal value determination"@en ;
  rdfs:subClassOf owl:Thing ;
.
fibo-fnd-math-math:MathematicalCurve
  rdf:type owl:Class ;
  rdfs:label "mathematical curve"@en ;
  rdfs:subClassOf fibo-fnd-math-math:MathematicalSurface ;
.
fibo-fnd-math-math:MathematicalExpression
  rdf:type owl:Class ;
  rdfs:label "mathematical expression"@en ;
  rdfs:subClassOf fibo-fnd-math-amt:MathematicalThing ;
  skos:definition "Some expression of some mathematical quantity or quantities."@en ;
.
fibo-fnd-math-math:MathematicalModel
  rdf:type owl:Class ;
  rdfs:label "mathematical model"@en ;
  rdfs:subClassOf fibo-fnd-math-math:MathematicalExpression ;
  skos:definition "A model for determining one or more parameters or the behaviour of parameters with respect to one another."@en ;
.
fibo-fnd-math-math:MathematicalQuantity
  rdf:type owl:Class ;
  rdfs:label "mathematical quantity"@en ;
  rdfs:subClassOf fibo-fnd-math-math:ParametricThing ;
  skos:definition "Some mathematical quantity which is to be expressed in some way. This is not itself an expression."@en ;
.
fibo-fnd-math-math:MathematicalSurface
  rdf:type owl:Class ;
  rdfs:label "mathematical surface"@en ;
  rdfs:subClassOf fibo-fnd-math-math:MathematicalModel ;
.
fibo-fnd-math-math:MeasuredThing
  rdf:type owl:Class ;
  rdfs:label "measured thing"@en ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-math-math:hasNumericallyMeasurableProperty ;
      owl:someValuesFrom fibo-fnd-math-math:TimedParameter ;
    ] ;
  skos:definition "Any Independent Thing. This is included as a separate term only so that we can refer to the trivial fact that all Independent Things have any number of properties, without cluttering the Independent Thing itself. This is for the modeling of reference entities. Note that the measurable quantity should be replaced with the actual parent property in all cases. DO NOT USE THIS TERM ELSEWHERE"@en ;
.
fibo-fnd-math-math:NumericalParameter
  rdf:type owl:Class ;
  rdfs:label "numerical parameter"@en ;
  rdfs:subClassOf fibo-fnd-math-math:MathematicalQuantity ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-math-math:takesValue ;
      owl:someValuesFrom fibo-fnd-math-math:Value ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:appliesTo ;
      owl:someValuesFrom owl:Thing ;
    ] ;
  skos:definition "Anything which is defined as a parameter of something, and takes a numerical value."@en ;
.
fibo-fnd-math-math:NumericalVariable
  rdf:type owl:Class ;
  rdfs:label "numerical variable"@en ;
  rdfs:subClassOf fibo-fnd-math-math:NumericalParameter ;
  skos:definition "Some numerical variable."@en ;
  skos:editorialNote "Not used to date. Use Dated Parameter instead in most cases. Use this when it is necessary to identify a variable but not that it's the variable value of something."@en ;
.
fibo-fnd-math-math:Operator
  rdf:type owl:Class ;
  rdfs:label "operator"@en ;
  rdfs:subClassOf owl:Thing ;
.
fibo-fnd-math-math:ParametricReferenceEntity
  rdf:type owl:Class ;
  rdfs:label "parametric reference entity"@en ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-math-math:takesReferenceParameter ;
      owl:someValuesFrom fibo-fnd-math-math:TimedParameter ;
    ] ;
  skos:definition "An entity, the value of some property of which is used in calculations for some purpose."@en ;
  fibo-fnd-utl-av:explanatoryNote "The identity of this entity can be any Independent Thing, provided that that thing has some property which is numerical in nature and can be identified as the property used for reference in the context of this Reference Entity. For example, a Treasury Bill, the interest rate of which is the reference rate. This model does not have the means to formally identify which property of the Reference Entity is used for reference. Instead, the modeler should as a matter of convention provide a reference relationship which inherits its meaning both from the property relationship of the Independent Thing and the identify relationship of the Reference Entity. Future: On OWL2 this arrangement should be reviewed, and a transitive relationship used as appropriate."@en ;
.
fibo-fnd-math-math:ParametricThing
  rdf:type owl:Class ;
  rdfs:label "parametric thing"@en ;
  rdfs:subClassOf fibo-fnd-math-amt:MathematicalThing ;
.
fibo-fnd-math-math:PastValue
  rdf:type owl:Class ;
  rdfs:label "past value"@en ;
  rdfs:subClassOf fibo-fnd-math-math:TimedValue ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-math-math:tense ;
      owl:someValuesFrom fibo-fnd-tim-tim:Past ;
    ] ;
  owl:disjointWith fibo-fnd-math-math:ProjectedValue ;
  skos:definition "The value is determined at some point in the past."@en ;
.
fibo-fnd-math-math:Probability
  rdf:type owl:Class ;
  rdfs:label "probability"@en ;
  rdfs:subClassOf fibo-fnd-math-math:NumericalParameter ;
.
fibo-fnd-math-math:ProbabilityDistribution
  rdf:type owl:Class ;
  rdfs:label "probability distribution"@en ;
  rdfs:subClassOf fibo-fnd-math-math:MathematicalModel ;
.
fibo-fnd-math-math:ProjectedValue
  rdf:type owl:Class ;
  rdfs:label "projected value"@en ;
  rdfs:subClassOf fibo-fnd-math-math:Value ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-math-math:projectedValueDeterminedBy ;
      owl:someValuesFrom [
          rdf:type owl:Class ;
          owl:unionOf (
              fibo-fnd-math-math:InternalValueDetermination
              fibo-fnd-math-math:PublishedFutureValue
              fibo-fnd-pub-pub:Publisher
            ) ;
        ] ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-math-math:tense ;
      owl:someValuesFrom fibo-fnd-tim-tim:Future ;
    ] ;
  skos:definition "The value is some projected future value."@en ;
  fibo-fnd-utl-av:explanatoryNote "Note that the future is not known, so in fact future tense refers to some project value. The value should therefore be accompanied by terms defining how and by what means it is being predicted."@en ;
.
fibo-fnd-math-math:PublishedFutureValue
  rdf:type owl:Class ;
  rdfs:label "published future value"@en ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "A value defined in advance in a schedule, as part of the bond terms in the prospectus. an example of this is a stepped coupon in a bond."@en ;
.
fibo-fnd-math-math:Rate
  rdf:type owl:Class ;
  rdfs:label "rate"@en ;
