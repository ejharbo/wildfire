  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-agrx-con:setsOutRight ;
      owl:someValuesFrom fibo-fnd-agrx-con:ContractualRight ;
    ] ;
.
fibo-fnd-agrx-con:ContractualObligation
  rdf:type owl:Class ;
  rdfs:label "contractual obligation"@en ;
  rdfs:subClassOf fibo-fnd-soc-asp:ContingentObligation ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-agrx-con:ContractualRight ;
      owl:onProperty fibo-fnd-soc-asp:impliedBy ;
    ] ;
  owl:disjointWith fibo-fnd-soc-asp:LegalObligation ;
.
fibo-fnd-agrx-con:ContractualObligor
  rdf:type owl:Class ;
  rdfs:label "contractual obligor"@en ;
  rdfs:subClassOf fibo-fnd-agr-ctr:ContractParty ;
  rdfs:subClassOf fibo-fnd-soc-pty:Obligor ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-soc-pty:obligedTo ;
      owl:someValuesFrom fibo-fnd-agrx-con:ContractualBeneficiary ;
    ] ;
.
fibo-fnd-agrx-con:ContractualOption
  rdf:type owl:Class ;
  rdfs:label "contractual option"@en ;
  rdfs:subClassOf fibo-fnd-agrx-con:ContractualRight ;
  skos:definition "The option to take up some right at some future time, at the discretion of the party defined as the holder of this option in the contract."@en ;
.
fibo-fnd-agrx-con:ContractualRight
  rdf:type owl:Class ;
  rdfs:label "contractual right"@en ;
  rdfs:subClassOf fibo-fnd-soc-asp:ContingentRight ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-agr-ctr:Contract ;
      owl:onProperty fibo-fnd-agrx-con:hasContractualBasis ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-soc-asp:impliesSome ;
      owl:someValuesFrom fibo-fnd-agrx-con:ContractualObligation ;
    ] ;
  skos:definition "Some right conferred by some contract."@en ;
  fibo-fnd-utl-av:explanatoryNote "Whereas Rights may be conferred by various instruments, this is the kind of Right specifically conferred by some Contract."@en ;
.
fibo-fnd-agrx-con:agreementFormalizedBy
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-agrx-con:ContractualAgreement ;
  rdfs:label "agreement formalized by"@en ;
  rdfs:range fibo-fnd-agrx-con:Contract ;
.
fibo-fnd-agrx-con:confers
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-agrx-con:Contract ;
  rdfs:label "confers"@en ;
  rdfs:range fibo-fnd-agrx-con:ContractualRight ;
  skos:definition "Some right conferred by the contract, by one party upon the other, for example the right to occupy some space unmolested or the right to receive some cash flows."@en ;
.
fibo-fnd-agrx-con:confersContractualCommitment
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-agrx-con:ContractualAgreement ;
  rdfs:label "confers contractual commitment"@en ;
  rdfs:range fibo-fnd-agrx-ctrx:ContractuallyConferredCommitment ;
  owl:inverseOf fibo-fnd-agrx-ctrx:contractualCommitmentConferredBy ;
  skos:definition "Some Contractual Commitment which arises from the Contractual Agreement and is documented in the Contract which evidences the agreement between the parties."@en ;
.
fibo-fnd-agrx-con:contractEmbodies
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-agrx-con:Contract ;
  rdfs:label "contract embodies"@en ;
  rdfs:range fibo-fnd-agrx-con:ContractualAgreement ;
.
fibo-fnd-agrx-con:hasContractualBasis
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-agrx-con:ContractualRight ;
  rdfs:label "has contractual basis"@en ;
  rdfs:range fibo-fnd-agr-ctr:Contract ;
.
fibo-fnd-agrx-con:mandates
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-agrx-con:Contract ;
  rdfs:label "mandates"@en ;
  rdfs:range fibo-fnd-agrx-ctrx:ContractuallyConferredCommitment ;
  owl:inverseOf fibo-fnd-agrx-ctrx:mandatedBy ;
  skos:definition "Some obligation set out by the contract, by one party upon the other."@en ;
.
fibo-fnd-agrx-con:setsOutRight
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-agrx-con:ContractualCommitment ;
  rdfs:label "sets out right"@en ;
  rdfs:range fibo-fnd-agrx-con:ContractualRight ;
  skos:definition "The rights set out in the set of terms."@en ;
.
fibo-fnd-agrx-con:varies
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-agrx-con:Contract ;
  rdfs:label "varies"@en ;
  rdfs:range fibo-fnd-agrx-ctrx:ContractuallyConferredCommitment ;
  skos:definition "Some Obligation defined in some other contract or instrument between the parties, which is varied in some way by this contract."@en ;
  fibo-fnd-utl-av:explanatoryNote "Typical usage is when a Confirmation for an OTC Derivative specifies some variation in an Obligation defined in the covering Master Agreement under which the transaction is conducted."@en ;
.
fibo-fnd-arr-arr:
  terms:license sm:MITLicense ;
  sm:contentLanguage <http://www.omg.org/spec/ODM/> ;
  sm:contentLanguage <http://www.w3.org/standards/techs/owl#w3c_all> ;
  sm:copyright """Copyright (c) 2014-2017 EDM Council, Inc.
Copyright (c) 2014-2017 Object Management Group, Inc.""" ;
  sm:dependsOn fibo-fnd-utl-av: ;
  sm:fileAbbreviation "fibo-fnd-arr-arr" ;
  sm:fileAbstract "This ontology defines abstract structural concepts, including arrangement and collection, for use in other FIBO ontology elements. These abstract concepts are further refined to support definition of identifiers, codes, quantities, and schemata that organize and classify such identifiers and codes." ;
  sm:filename "Arrangements.rdf" ;
  rdfs:label "Arrangements Ontology" ;
  rdfs:seeAlso <https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/> ;
  rdfs:seeAlso <https://spec.edmcouncil.org/fibo/FND/AboutFND/> ;
  rdfs:seeAlso <https://spec.edmcouncil.org/fibo/FND/Arrangements/AboutArrangements/> ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/FND/20170201/Arrangements/Arrangements/> ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/FND/20140801/Arrangements/Arrangements.rdf version of this ontology was introduced as a part of the issue resolutions identified in the FIBO FND 1.0 FTF report and in http://spec.edmcouncil.org/fibo/FND/1.0/AboutFND-1.0/.  It was further revised in the FTF in advance of the Long Beach meeting to promote Collection to the top level in the hierarchy, resulting in http://spec.edmcouncil.org/fibo/FND/20141101/Arrangements/Arrangements/." ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/FND/20141101/Arrangements/Arrangements.rdf version of this ontology was revised as a part of the issue resolutions identified in the FIBO FND 1.2 RTF report to add a definition for a structured collection." ;
.
fibo-fnd-arr-arr:Arrangement
  rdf:type owl:Class ;
  rdfs:label "arrangement" ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "a structure or means of organizing information such as a schema, numbering system, organization scheme, measurement system, taxonomy, or language for organizing information" ;
.
fibo-fnd-arr-arr:Collection
  rdf:type owl:Class ;
  rdfs:label "collection" ;
  rdfs:subClassOf owl:Thing ;
  owl:equivalentClass fibo-fnd-arrx-col:Collection ;
  skos:definition "a grouping of some variable number of things (may be zero) that have some shared significance" ;
.
fibo-fnd-arr-arr:StructuredCollection
  rdf:type owl:Class ;
  rdfs:label "structured collection" ;
  rdfs:subClassOf fibo-fnd-arr-arr:Collection ;
  skos:definition "a collection that has a clearly defined structure or organization" ;
.
fibo-fnd-arr-cls:
  terms:license "http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense"^^xsd:anyURI ;
  sm:contentLanguage "http://www.omg.org/spec/ODM/"^^xsd:anyURI ;
  sm:contentLanguage "http://www.w3.org/standards/techs/owl#w3c_all"^^xsd:anyURI ;
  sm:copyright """Copyright (c) 2015 EDM Council, Inc.
Copyright (c) 2015 Object Management Group, Inc.""" ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Arrangements/Arrangements/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"^^xsd:anyURI ;
  sm:fileAbbreviation "fibo-fnd-arr-cls" ;
  sm:fileAbstract "This ontology defines abstract concepts for representation of classification schemes that themselves are intended to permit the classification of arbitrary concepts into hierarchies (or partial orders) for use in other FIBO ontology elements." ;
  sm:filename "ClassificationSchemes.rdf" ;
  rdfs:label "Classification Schemes Ontology" ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/AboutFND/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/Arrangements/AboutArrangements/"^^xsd:anyURI ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/FND/20150501/Arrangements/ClassificationSchemes/> ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/FND/20150501/Arrangements/ClassificationSchemes.rdf version of this ontology was introduced as a part of the initial FIBO FBC RFC." ;
.
fibo-fnd-arr-cls:ClassificationScheme
  rdf:type owl:Class ;
  rdfs:label "classification scheme" ;
  rdfs:subClassOf fibo-fnd-arr-arr:Arrangement ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-arr-cls:Classifier ;
      owl:onProperty fibo-fnd-rel-rel:defines ;
    ] ;
  skos:definition "a system for allocating classifiers (elements in a classification scheme) to objects, similar to identifiers in some cases; such classification schemes are intended to permit the classification of arbitrary objects into hierarchies (or partial orders)" ;
  fibo-fnd-utl-av:adaptedFrom "ISO/IEC 11179-3 Information technology - Metadata registries (MDR) - Part 3: Registry metamodel and basic attributes, Third edition, 2013-02-15" ;
  fibo-fnd-utl-av:explanatoryNote "A classification scheme may be a taxonomy, a network, an ontology, or any other terminological system. The classification may also be just a list of controlled vocabulary of property words (or terms). The list might be taken from the 'leaf level' of a taxonomy." ;
.
fibo-fnd-arr-cls:Classifier
  rdf:type owl:Class ;
  rdfs:label "classifier" ;
  rdfs:subClassOf fibo-fnd-rel-rel:Reference ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty fibo-fnd-rel-rel:classifies ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-arr-cls:ClassificationScheme ;
      owl:onProperty fibo-fnd-rel-rel:hasDefinition ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-arr-cls:ClassificationScheme ;
      owl:onProperty fibo-fnd-rel-rel:isMemberOf ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "a standardized classification or delineation for something, per some scheme for such delineation, within a specified context" ;
  fibo-fnd-utl-av:adaptedFrom "ISO/IEC 11179-3 Information technology - Metadata registries (MDR) - Part 3: Registry metamodel and basic attributes, Third edition, 2013-02-15" ;
.
fibo-fnd-arr-cls:IndustrySectorClassificationScheme
  rdf:type owl:Class ;
  rdfs:label "industry sector classification scheme" ;
  rdfs:seeAlso "http://www.investopedia.com/terms/g/gics.asp"^^xsd:anyURI ;
  rdfs:subClassOf fibo-fnd-arr-cls:ClassificationScheme ;
  skos:definition "a system for allocating classifiers to organizations by industry sector" ;
  skos:example "Examples include the North American Industry Classification System (NAICS), and older Standardized Industry Classification (SIC), which is still used today by the US Securities and Exchange Commission (SEC), developed by government to classify industries, and commercial classification schemes, such as the Global Industry Standard Classification (GICS) developed jointly by Morgan Stanley Capital International (MSCI) and Standard and Poor's, or competing schemes including the Industry Classification Benchmark (ICB) system, maintained by Dow Jones and London's FTSE Group, among others." ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, 2012" ;
.
fibo-fnd-arr-cls:IndustrySectorClassifier
  rdf:type owl:Class ;
  rdfs:label "industry sector classifier" ;
  rdfs:subClassOf fibo-fnd-arr-cls:Classifier ;
  owl:equivalentClass fibo-sec-sec-clax:IndustrySectorClassifier ;
  skos:definition "a standardized classification or delineation for an organization, or possibly for a security representing an interest in a given organization, per some scheme for such delineation, by industry" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, 2012" ;
.
fibo-fnd-arr-cd:
  terms:license "http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense"^^xsd:anyURI ;
  sm:contentLanguage "http://www.omg.org/spec/ODM/"^^xsd:anyURI ;
  sm:contentLanguage "http://www.w3.org/standards/techs/owl#w3c_all"^^xsd:anyURI ;
  sm:copyright """Copyright (c) 2014 EDM Council, Inc.
Copyright (c) 2014 Object Management Group, Inc.""" ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Arrangements/Arrangements/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"^^xsd:anyURI ;
  sm:fileAbbreviation "fibo-fnd-arr-cd" ;
  sm:fileAbstract "This ontology defines abstract concepts for representation of codes and coding schemes for use in other FIBO ontology elements." ;
  sm:filename "Codes.rdf" ;
  rdfs:label "Codes and Code Sets Ontology" ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/AboutFND/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/Arrangements/AboutArrangements/"^^xsd:anyURI ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/FND/20141101/Arrangements/Codes/> ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/FND/20141101/Arrangements/Codes.rdf version of this ontology was introduced as a part of the issue resolutions identified in the FIBO FND 1.0 FTF report and in http://spec.edmcouncil.org/fibo/FND/1.0/AboutFND-1.0/ in advance of the Long Beach meeting in December 2014." ;
.
fibo-fnd-arr-cd:CodeElement
  rdf:type owl:Class ;
  rdfs:label "code element" ;
  rdfs:subClassOf fibo-fnd-rel-rel:Reference ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty fibo-fnd-rel-rel:denotes ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-arr-cd:CodeSet ;
      owl:onProperty fibo-fnd-rel-rel:isMemberOf ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "a sequence of characters, capable of identifying that with which it is associated for some purpose, within a specified context, i.e., a code set, according to a pre-established set of rules" ;
  fibo-fnd-utl-av:adaptedFrom "ISO/IEC 11179-3 Information technology - Metadata registries (MDR) - Part 3: Registry metamodel and basic attributes, Third edition, 2013-02-15" ;
.
fibo-fnd-arr-cd:CodeSet
  rdf:type owl:Class ;
  rdfs:label "code set" ;
  rdfs:subClassOf fibo-fnd-arr-arr:Arrangement ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-arr-cd:CodeElement ;
      owl:onProperty fibo-fnd-rel-rel:comprises ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-arr-cd:CodeElement ;
      owl:onProperty fibo-fnd-rel-rel:defines ;
    ] ;
  skos:altLabel "coding scheme" ;
  skos:definition "A system of valid symbols that substitute for specified values, e.g., alpha, numeric, symbols and/or combinations" ;
  fibo-fnd-utl-av:adaptedFrom "ISO/IEC 11179-3 Information technology - Metadata registries (MDR) - Part 3: Registry metamodel and basic attributes, Third edition, 2013-02-15" ;
.
fibo-fnd-arrx-col:
  rdfs:label "Collections"@en ;
.
fibo-fnd-arrx-col:Collection
  rdf:type owl:Class ;
  rdfs:label "collection"@en ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-arrx-col:hasCollectionConstituent ;
      owl:someValuesFrom fibo-fnd-arrx-col:Constituent ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:hasMember ;
      owl:someValuesFrom owl:Thing ;
    ] ;
  skos:definition "A set of things collected together for some purpose."@en ;
.
fibo-fnd-arrx-col:Constituent
  rdf:type owl:Class ;
  rdfs:label "constituent"@en ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "Something which makes up part of a collection, formalized in terms of its existence as a member of that collection."@en ;
  fibo-fnd-utl-av:explanatoryNote "The collection may be of almost anything, brought together in some specific context in which the Constituent is defined as such. It differs from a less formal collection in that there are facts defined about the constituents and the proportions of these in the collection, and specific facts about that item as a member of that collection. Constituents are defined for Pools and for Baskets."@en ;
.
fibo-fnd-arrx-col:hasCollectionConstituent
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-arrx-col:Collection ;
  rdfs:label "has collection constituent"@en ;
  rdfs:range fibo-fnd-arrx-col:Constituent ;
  rdfs:subPropertyOf fibo-fbc-pas-fpas:hasConstituent ;
  skos:definition "A constituent of the Collection, that is one of the things that is collected up in bring the collection into being. Further notes: Some kinds of collection are defined in terms of collections of relative things (e.g. basket constituents) with qualifying facts, while others may be defined simply as sets of independent things. It is also possible to have a collection of collections. This is covered by the identity criterion for the Constituent being any Independent Thing, including Collection."@en ;
.
fibo-fnd-arr-doc:
  terms:license sm:MITLicense ;
  sm:contentLanguage <http://www.omg.org/spec/ODM/> ;
  sm:contentLanguage <http://www.w3.org/standards/techs/owl#w3c_all> ;
  sm:copyright """Copyright (c) 2014-2017 EDM Council, Inc.
Copyright (c) 2014-2017 Object Management Group, Inc.""" ;
  sm:dependsOn fibo-fnd-arr-arr: ;
  sm:dependsOn fibo-fnd-dt-fd: ;
  sm:dependsOn fibo-fnd-utl-av: ;
  sm:fileAbbreviation "fibo-fnd-arr-doc" ;
  sm:fileAbstract "This ontology defines abstract concepts for representation documents for use in other FIBO ontology elements." ;
  sm:filename "Documents.rdf" ;
  rdfs:label "Documents Ontology" ;
  rdfs:seeAlso <https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/> ;
  rdfs:seeAlso <https://spec.edmcouncil.org/fibo/FND/AboutFND/> ;
  rdfs:seeAlso <https://spec.edmcouncil.org/fibo/FND/Arrangements/AboutArrangements/> ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/FND/20170201/Arrangements/Documents/> ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/FND/20141101/Arrangements/Documents.rdf version of this ontology was introduced as a part of the issue resolutions identified in the FIBO FND 1.0 FTF report and in http://spec.edmcouncil.org/fibo/FND/1.0/AboutFND-1.0/ in advance of the Long Beach meeting in December 2014." ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/FND/20141101/Arrangements/Documents.rdf version of this ontology was revised as a part of the issue resolutions identified in the FIBO FND 1.1 RTF report to add a parent of hasDate to date properties." ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/FND/20160201/Arrangements/Documents.rdf version of this ontology was revised as a part of the issue resolutions identified in the FIBO FND 1.2 RTF report to add a definition for a record." ;
.
fibo-fnd-arr-doc:Document
  rdf:type owl:Class ;
  rdfs:label "document" ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "something tangible that records something, such as a recording or a photograph, or a writing that can be used to furnish evidence or information" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.businessdictionary.com/definition/document.html"^^xsd:anyURI ;
  fibo-fnd-utl-av:adaptedFrom "https://en.oxforddictionaries.com/definition/document"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "A document serves to establish one or several facts, and can be relied upon as a proof thereof." ;
.
fibo-fnd-arr-doc:LegalDocument
  rdf:type owl:Class ;
  rdfs:label "legal document" ;
  rdfs:subClassOf fibo-fnd-arr-doc:Document ;
  skos:definition "a written or printed paper that bears the original, official, or legal form of something and can be used to furnish decisive evidence or information" ;
.
fibo-fnd-arr-doc:Record
  rdf:type owl:Class ;
  rdfs:label "record" ;
  rdfs:subClassOf fibo-fnd-arr-arr:Collection ;
  skos:definition "a memorialization and objective evidence of activities performed, events occurred, results achieved, or statements made, regardless of its characteristics, media, physical form, or the manner in which it is recorded or stored" ;
  skos:example "Records include accounts, agreements, books, drawings, letters, magnetic/optical disks, memos, micrographics, etc." ;
  fibo-fnd-utl-av:adaptedFrom "http://www.businessdictionary.com/definition/record.html"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "Records are created or received by an organization in routine transaction of its business or in pursuance of its legal obligations." ;
.
fibo-fnd-arr-doc:ReferenceDocument
  rdf:type owl:Class ;
  rdfs:label "reference document" ;
  rdfs:subClassOf fibo-fnd-arr-doc:Document ;
  skos:definition "a document that provides pertinent details for consultation about a subject" ;
  fibo-fnd-utl-av:adaptedFrom "ISO/IEC 11179-3 Information technology - Metadata registries (MDR) - Part 3: Registry metamodel and basic attributes, Third edition, 2013-02-15" ;
.
fibo-fnd-arr-doc:hasDateOfIssuance
  rdf:type owl:FunctionalProperty ;
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-arr-doc:Document ;
  rdfs:label "has date of issuance" ;
  rdfs:range fibo-fnd-dt-fd:Date ;
  rdfs:subPropertyOf fibo-fnd-dt-fd:hasDate ;
  skos:definition "links something, typically an agreement, contract, or document, with the date it was issued" ;
.
fibo-fnd-arr-doc:hasExpirationDate
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-arr-doc:Document ;
  rdfs:label "has expiration date" ;
  rdfs:range fibo-fnd-dt-fd:Date ;
  rdfs:subPropertyOf fibo-fnd-dt-fd:hasDate ;
  skos:definition "links something, typically an agreement, contract, document, or perishable item, with an expiration date" ;
.
