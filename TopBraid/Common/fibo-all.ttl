      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-ln-ln-up:PropertyInspection ;
      owl:onProperty fibo-ln-ln-up:rel-isInformedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-dt-fd:hasDate ;
      owl:someValuesFrom fibo-fnd-dt-fd:Date ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty loan:hasAppraisalManagementCompany ;
      owl:someValuesFrom fibo-fnd-pty-pty:IndependentParty ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty loan:hasAppraisedValue ;
      owl:someValuesFrom fibo-fnd-acc-cur:MonetaryAmount ;
    ] ;
