    ] ;
  skos:definition "an entry in a registry that specifies something about the thing that is registered" ;
  fibo-fnd-utl-av:adaptedFrom "ISO/IEC 11179-3 Information technology - Metadata registries (MDR) - Part 3: Registry metamodel and basic attributes, Third edition, 2013-02-15" ;
.
fibo-fbc-fct-ra:RegistryIdentifier
  rdf:type owl:Class ;
  rdfs:label "registry identifier" ;
  rdfs:subClassOf fibo-fnd-arr-id:Identifier ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-arr-id:isIndexTo ;
      owl:someValuesFrom fibo-fbc-fct-ra:Registry ;
    ] ;
  skos:definition "an identifier associated with an entry in a registry, i.e., one that provides an index to the registry for the identified item" ;
  fibo-fnd-utl-av:adaptedFrom "ISO/IEC 11179-3 Information technology - Metadata registries (MDR) - Part 3: Registry metamodel and basic attributes, Third edition, 2013-02-15" ;
.
fibo-fbc-fct-ra:hasRegistrationDate
  rdf:type owl:ObjectProperty ;
  rdfs:label "has registration date" ;
  rdfs:range fibo-fnd-dt-fd:Date ;
  rdfs:subPropertyOf fibo-fnd-dt-fd:hasDate ;
.
fibo-fbc-fct-ra:hasRegistryEntry
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fbc-fct-ra:Registry ;
  rdfs:label "has registry entry" ;
  rdfs:range fibo-fbc-fct-ra:RegistryEntry ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:hasPart ;
.
fibo-fbc-fct-ra:isRegisteredBy
  rdf:type owl:ObjectProperty ;
  rdfs:label "is registered by" ;
  rdfs:range fibo-fbc-fct-ra:RegistrationAuthority ;
  skos:definition "indicates the registration authority that registers something" ;
.
fibo-fbc-fct-ra:isRegisteredIn
  rdf:type owl:ObjectProperty ;
  rdfs:label "is registered in" ;
  rdfs:range fibo-fbc-fct-ra:Registry ;
  skos:definition "indicates the registry that something is registered in" ;
.
fibo-fbc-fct-ra:registers
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fbc-fct-ra:RegistrationAuthority ;
  rdfs:label "registers" ;
  skos:definition "links a registration authority to something it registers" ;
.
fibo-fbc-fct-ra:specifies
  rdf:type owl:ObjectProperty ;
  rdfs:label "specifies" ;
  skos:definition "states a fact about something" ;
.
fibo-fbc-fct-rga:
  terms:license sm:MITLicense ;
  sm:contentLanguage <http://www.omg.org/spec/ODM/> ;
  sm:contentLanguage <http://www.w3.org/standards/techs/owl#w3c_all> ;
  sm:copyright """Copyright (c) 2015-2017 EDM Council, Inc.
Copyright (c) 2015-2017 Object Management Group, Inc.""" ;
  sm:dependsOn <https://spec.edmcouncil.org/fibo/BE/> ;
  sm:dependsOn <https://spec.edmcouncil.org/fibo/FND/> ;
  sm:fileAbbreviation "fibo-fbc-fct-rga" ;
  sm:fileAbstract "This ontology defines general purpose concepts for representation of regulatory agencies, also known as regulatory authorities or regulators.  Examples of financial industry regulatory agencies in the US include the Securities Exchange Commission, FINRA, and the FDIC, among others.  The SEC and FINRA are both registration authorities and regulatory agencies.  The FDIC is a regulatory agency and an insurer, and may be a registration authority for certain state-chartered banks in the US without bank holding companies." ;
  sm:filename "RegulatoryAgencies.rdf" ;
  rdfs:label "Regulatory Agencies Ontology" ;
  rdfs:seeAlso <https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/> ;
  rdfs:seeAlso <https://spec.edmcouncil.org/fibo/FBC/AboutFBC/> ;
  rdfs:seeAlso <https://spec.edmcouncil.org/fibo/FBC/FunctionalEntities/AboutFBCFunctionalEntities/> ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/FBC/20170201/FunctionalEntities/RegulatoryAgencies/> ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/FBC/20150801/FunctionalEntities/RegulatoryAgencies.rdf version of this ontology was modified per the issue resolutions identified in the FIBO FBC 1.1 RTF report, including deprecation of the hasJurisdiction property that was duplicated in BE via the BE 1.1 RTF." ;
.
fibo-fbc-fct-rga:Examiner
  rdf:type owl:Class ;
  rdfs:label "examiner" ;
  rdfs:subClassOf fibo-be-oac-exec:ResponsibleParty ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-aap-ppl:LegallyCapablePerson ;
      owl:onProperty fibo-fnd-rel-rel:hasIdentity ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
      owl:someValuesFrom [
          rdf:type owl:Class ;
          owl:intersectionOf (
              [
                rdf:type owl:Restriction ;
                owl:onProperty fibo-fnd-law-lcap:hasCapacity ;
                owl:someValuesFrom fibo-fbc-fct-rga:RegulatoryCapacity ;
              ]
              [
                rdf:type owl:Restriction ;
                owl:onClass fibo-fbc-fct-rga:RegulatoryAgency ;
                owl:onProperty fibo-fnd-rel-rel:represents ;
                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
              ]
            ) ;
        ] ;
    ] ;
  skos:definition "a party appointed by, and who has the capacity to act as an official representative of, a regulatory agency to provide examination, review, and other regulatory services" ;
  fibo-fnd-utl-av:adaptedFrom "Black's Law Dictionary, see http://thelawdictionary.org/examiner/" ;
.
fibo-fbc-fct-rga:GovernmentIssuedLicense
  rdf:type owl:Class ;
  rdfs:label "government-issued license" ;
  rdfs:subClassOf fibo-fnd-arr-doc:LegalDocument ;
  rdfs:subClassOf fibo-fnd-law-lcap:License ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-law-lcap:LicenseIdentifier ;
      owl:onProperty fibo-fnd-aap-agt:isIdentifiedBy ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fbc-fct-rga:isIssuedBy ;
      owl:someValuesFrom fibo-fbc-fct-rga:RegulatoryAgency ;
    ] ;
  skos:definition "grant of permission needed to legally perform some task, provide some service, exercise a certain privilege, or pursue some business or occupation" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fbc-fct-rga:Regulation
  rdf:type owl:Class ;
  rdfs:label "regulation" ;
  rdfs:subClassOf fibo-fnd-law-lcap:LegalConstruct ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty fibo-fnd-rel-rel:governs ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fbc-fct-rga:RegulationIdentifier ;
      owl:onProperty fibo-fnd-aap-agt:isIdentifiedBy ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fbc-fct-rga:RegulatoryAgency ;
      owl:onProperty fibo-fbc-fct-rga:isIssuedBy ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:hasDefinition ;
      owl:someValuesFrom fibo-fnd-law-cor:Law ;
    ] ;
  skos:definition "a rule used to carry out a law" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
  fibo-fnd-utl-av:explanatoryNote "Many government agencices issue regulations to administer laws." ;
.
fibo-fbc-fct-rga:RegulationIdentifier
  rdf:type owl:Class ;
  rdfs:label "regulation identifier" ;
  rdfs:subClassOf fibo-fnd-arr-id:Identifier ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fbc-fct-rga:Regulation ;
      owl:onProperty fibo-fnd-aap-agt:identifies ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "an identifier associated with a regulation" ;
.
fibo-fbc-fct-rga:RegulatoryAgency
  rdf:type owl:Class ;
  rdfs:label "regulatory agency" ;
  rdfs:subClassOf fibo-fnd-pas-pas:ServiceProvider ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fbc-fct-rga:GovernmentIssuedLicense ;
      owl:onProperty fibo-fbc-fct-rga:issues ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass [
          rdf:type owl:Class ;
          owl:intersectionOf (
              fibo-be-le-fbo:FormallyConstitutedOrganization
              fibo-be-le-lp:StatutoryBody
            ) ;
        ] ;
      owl:onProperty fibo-fnd-rel-rel:hasIdentity ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-be-ge-ge:hasJurisdiction ;
      owl:someValuesFrom fibo-fnd-law-jur:Jurisdiction ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:provides ;
      owl:someValuesFrom fibo-fbc-fct-rga:RegulatoryService ;
    ] ;
  skos:definition "a public authority or government agency responsible for exercising autonomous authority over some area of human activity in a regulatory or supervisory capacity" ;
  skos:example "See http://www.finra.org/AboutFINRA/ for an example describing a regulatory agency." ;
  fibo-fnd-utl-av:adaptedFrom "http://en.wikipedia.org/wiki/Regulatory_agency" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.thefreedictionary.com/regulatory+agency" ;
  fibo-fnd-utl-av:explanatoryNote "Typically, a regulatory agency is chartered to protect some constituancy, (e.g., investors in the financial industry), to ensure the fairness and integrity of some market (e.g., the securities market), and fair and safe business practices among the service providers in that market." ;
  fibo-fnd-utl-av:synonym "regulator" ;
  fibo-fnd-utl-av:synonym "regulatory authority" ;
  fibo-fnd-utl-av:synonym "regulatory body" ;
.
fibo-fbc-fct-rga:RegulatoryCapacity
  rdf:type owl:Class ;
  rdfs:label "regulatory capacity" ;
  rdfs:subClassOf fibo-fnd-law-lcap:LegalCapacity ;
  skos:definition "the capacity of some natural person to regulate some industry, organization, or product by virtue of some certification program on behalf of some regulatory agency" ;
.
fibo-fbc-fct-rga:RegulatoryService
  rdf:type owl:Class ;
  rdfs:label "regulatory service" ;
  rdfs:subClassOf fibo-fnd-pas-pas:Service ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:isProvidedBy ;
      owl:someValuesFrom fibo-fbc-fct-rga:RegulatoryAgency ;
    ] ;
  skos:definition "a service provided by a regulatory agency, which may include, but not be limited to, examination, monitoring, supervision, testing, or other capabilities required to ensure the integrity, fairness, safety, or other capacity of a given industry, organization, or product" ;
.
fibo-fbc-fct-rga:hasJurisdiction
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fbc-fct-rga:RegulatoryAgency ;
  rdfs:label "has jurisdiction" ;
  rdfs:range fibo-fnd-law-jur:Jurisdiction ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  owl:deprecated "true"^^xsd:boolean ;
  skos:definition "indicates the jurisdiction in which the regulatory agency has authority" ;
.
fibo-fbc-fct-rga:isIssuedBy
  rdf:type owl:ObjectProperty ;
  rdfs:label "is issued by" ;
  rdfs:range fibo-fnd-pty-pty:PartyInRole ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:isProvidedBy ;
  owl:inverseOf fibo-fbc-fct-rga:issues ;
  skos:definition "indicates the party that issues something" ;
.
fibo-fbc-fct-rga:isRegulatedBy
  rdf:type owl:ObjectProperty ;
  rdfs:label "is regulated by" ;
  rdfs:range fibo-fbc-fct-rga:RegulatoryAgency ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:isGovernedBy ;
  skos:definition "indicates the regulatory agency that regulates something" ;
.
fibo-fbc-fct-rga:issues
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fnd-pty-pty:PartyInRole ;
  rdfs:label "issues" ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:provides ;
  skos:definition "officially makes something available or gives something to people to be used" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.merriam-webster.com/dictionary/issue"^^xsd:anyURI ;
.
fibo-fbc-fct-rga:regulates
  rdf:type owl:ObjectProperty ;
  rdfs:domain fibo-fbc-fct-rga:RegulatoryAgency ;
  rdfs:label "regulates" ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:governs ;
  skos:definition "governs or directs according to rule or law, typically an industry, organization, or product" ;
  fibo-fnd-utl-av:adaptedFrom "http://www.merriam-webster.com/dictionary/regulate" ;
.
fibo-fbc-pas-caa:
  terms:license sm:MITLicense ;
  sm:contentLanguage <http://www.omg.org/spec/ODM/> ;
  sm:contentLanguage <http://www.w3.org/standards/techs/owl#w3c_all> ;
  sm:copyright """Copyright (c) 2015-2017 EDM Council, Inc.
Copyright (c) 2015-2017 Object Management Group, Inc.""" ;
  sm:dependsOn <https://spec.edmcouncil.org/fibo/BE/> ;
  sm:dependsOn fibo-fbc-pas-fpas: ;
  sm:dependsOn <https://spec.edmcouncil.org/fibo/FND/> ;
  sm:fileAbbreviation "fibo-fbc-pas-caa" ;
  sm:fileAbstract "This ontology provides basic concepts such as account, account holder, account provider, relationship manager that are commonly used by financial services providers to describe customers and to determine counterparty identities." ;
  sm:filename "ClientsAndAccounts.rdf" ;
  rdfs:label "Clients and Accounts Ontology" ;
  rdfs:seeAlso <https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/> ;
  rdfs:seeAlso <https://spec.edmcouncil.org/fibo/FBC/AboutFBC/> ;
  rdfs:seeAlso <https://spec.edmcouncil.org/fibo/FBC/ProductsAndServices/AboutProductsAndServices/> ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/FBC/20170201/ProductsAndServices/ClientsAndAccounts/> ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/FND/20150801/ProductsAndServices/ClientsAndAccounts.rdf version of this ontology was revised as a part of the issue resolutions identified in the FIBO FBC 1.1 RTF report to revise the definition for an account and add the definition of a bank account." ;
.
fibo-fbc-pas-caa:Account
  rdf:type owl:Class ;
  rdfs:label "account" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fbc-pas-caa:AccountIdentifier ;
      owl:onProperty fibo-fnd-aap-agt:isIdentifiedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-arr-doc:Record ;
      owl:onProperty fibo-fnd-rel-rel:comprises ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pty-pty:hasPartyInRole ;
      owl:someValuesFrom fibo-fbc-pas-caa:AccountProvider ;
    ] ;
  skos:definition "a container for records associated with a business arrangement for regular dealings or services (such as personal or professional services, banking)" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
  fibo-fnd-utl-av:adaptedFrom "https://www.merriam-webster.com/dictionary/account"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "In general, an account is associated with a contractual relationship between a buyer and seller under which payment may be made at a later time." ;
.
fibo-fbc-pas-caa:AccountHolder
  rdf:type owl:Class ;
  rdfs:label "account holder" ;
  rdfs:subClassOf fibo-fnd-oac-own:Owner ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
      owl:someValuesFrom [
          rdf:type owl:Restriction ;
          owl:onProperty fibo-fnd-oac-own:owns ;
          owl:someValuesFrom fibo-fbc-pas-caa:Account ;
        ] ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
      owl:someValuesFrom [
          rdf:type owl:Restriction ;
          owl:onProperty fibo-fnd-pty-pty:isAPartyTo ;
          owl:someValuesFrom fibo-fbc-pas-caa:Account ;
        ] ;
    ] ;
  skos:definition "a party holding an account" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fbc-pas-caa:AccountIdentifier
  rdf:type owl:Class ;
  rdfs:label "account number" ;
  rdfs:subClassOf fibo-fnd-arr-id:Identifier ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fbc-pas-caa:Account ;
      owl:onProperty fibo-fnd-aap-agt:identifies ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "an identifier assigned to each client account for ease in referring to that party's activity" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fbc-pas-caa:AccountNumber
  rdf:type owl:Class ;
  rdfs:label "account number" ;
  rdfs:subClassOf fibo-fnd-arr-id:Identifier ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fbc-pas-caa:Account ;
      owl:onProperty fibo-fnd-aap-agt:identifies ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  owl:deprecated "true"^^xsd:boolean ;
  skos:definition "an identifier assigned to each client account for ease in referring to that party's activity" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fbc-pas-caa:AccountProvider
  rdf:type owl:Class ;
  rdfs:label "account provider" ;
  rdfs:subClassOf fibo-fnd-pas-pas:ServiceProvider ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
      owl:someValuesFrom [
          rdf:type owl:Restriction ;
          owl:onProperty fibo-fnd-pty-pty:isAPartyTo ;
          owl:someValuesFrom fibo-fbc-pas-caa:Account ;
        ] ;
    ] ;
  skos:definition "a party that provides and services the account" ;
.
fibo-fbc-pas-caa:AccountSpecificServiceAgreement
  rdf:type owl:Class ;
  rdfs:label "account-specific service agreement" ;
  rdfs:subClassOf fibo-fnd-pas-pas:ServiceAgreement ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-agr-ctr:hasContractParty ;
      owl:someValuesFrom fibo-fbc-pas-caa:AccountHolder ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-agr-ctr:hasContractParty ;
      owl:someValuesFrom fibo-fbc-pas-caa:AccountProvider ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:appliesTo ;
      owl:someValuesFrom fibo-fbc-pas-caa:Account ;
    ] ;
  skos:definition "a service-agreement that is account-specific, applicable in cases where a client holds multiple accounts with differing terms and conditions" ;
  fibo-fnd-utl-av:explanatoryNote "Customers of financial service providers frequently hold multiple accounts - brokerage accounts, checking and savings accounts, trust accounts, and so forth - which may have specific terms and conditions associated with them." ;
.
fibo-fbc-pas-caa:AccountingTransaction
  rdf:type owl:Class ;
  rdfs:label "accounting transaction" ;
  rdfs:subClassOf fibo-fnd-pas-pas:TransactionEvent ;
  skos:definition "event or condition recognized by an entry in the records of an account" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Finance and Investment Terms, Ninth Edition, 2014" ;
.
fibo-fbc-pas-caa:AccountingTransactionEvent
  rdf:type owl:Class ;
  rdfs:label "accounting transaction" ;
  rdfs:subClassOf fibo-fnd-pas-pas:TransactionEvent ;
  owl:deprecated "true"^^xsd:boolean ;
  skos:definition "event or condition recognized by an entry in the books of an account" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Finance and Investment Terms, Ninth Edition, 2014" ;
.
fibo-fbc-pas-caa:FinancialServiceAccount
  rdf:type owl:Class ;
  rdfs:label "financial service account" ;
  rdfs:subClassOf fibo-fbc-pas-caa:Account ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-aap-agt:isIdentifiedBy ;
      owl:someValuesFrom fibo-fbc-pas-caa:AccountIdentifier ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pty-pty:hasPartyInRole ;
      owl:someValuesFrom [
          rdf:type owl:Class ;
          owl:intersectionOf (
              fibo-fbc-pas-caa:AccountHolder
              [
                rdf:type owl:Class ;
                owl:unionOf (
                    fibo-fnd-pas-pas:Client
                    fibo-fnd-pas-pas:Customer
                  ) ;
              ]
            ) ;
        ] ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pty-pty:hasPartyInRole ;
      owl:someValuesFrom [
          rdf:type owl:Class ;
          owl:intersectionOf (
              fibo-fbc-pas-caa:AccountProvider
              fibo-fbc-pas-fpas:FinancialServiceProvider
            ) ;
        ] ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:appliesTo ;
      owl:someValuesFrom [
          rdf:type owl:Class ;
          owl:unionOf (
              fibo-fbc-pas-fpas:FinancialProduct
              fibo-fbc-pas-fpas:FinancialService
            ) ;
        ] ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:hasDefinition ;
      owl:someValuesFrom fibo-fnd-pas-pas:ServiceAgreement ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:isProvidedBy ;
      owl:someValuesFrom fibo-fbc-pas-fpas:FinancialServiceProvider ;
    ] ;
  skos:definition "an account provided by a financial service provider associated with a specific financial product or service" ;
  fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
.
fibo-fbc-pas-caa:RelationshipManager
  rdf:type owl:Class ;
  rdfs:label "relationship manager" ;
  rdfs:subClassOf fibo-be-oac-exec:ResponsibleParty ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:manages ;
      owl:someValuesFrom fibo-fbc-pas-caa:Account ;
    ] ;
  skos:definition "a responsible party who manages the client's account (or portfolio of accounts) and oversees their relationship with the service provider" ;
  fibo-fnd-utl-av:synonym "account manager" ;
.
fibo-fbc-pas-fpas:
