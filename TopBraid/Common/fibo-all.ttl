  owl:versionIRI <https://spec.edmcouncil.org/fibo/FND/20160201/Places/Addresses/> ;
  skos:changeNote "The http://www.omg.org/spec/EDMC-FIBO/FND/20141101/Places/Addresses.rdf version of this ontology was modified per the issue resolutions identified in the FIBO FND 1.1 RTF report. Differences from the 1.0 version include the addition of a hasAddress property and PhysicalAddress class as a parent of PostalAddress." ;
  skos:changeNote "The https://spec.edmcouncil.org/fibo/FND/20130801/Places/Addresses.rdf version of this ontology was modified per the issue resolutions identified in the FIBO FND 1.0 FTF report and in http://spec.edmcouncil.org/fibo/FND/1.0/AboutFND-1.0/. Primary differences include elimination of data properties in favor of a simple class model,the addition of virtual address, and the addition of addressing scheme." ;
  skos:changeNote """The https://spec.edmcouncil.org/fibo/Foundations/20130601/Organizations/Addresses.owl version of the ontology was revised in advance of the September 2013 New Brunswick, NJ meeting, as follows:
   (1) to use slash style URI/IRIss (also called 303 URIs, vs. hash style) as required to support server side processing 
   (2) to use version-independent IRIs for all definitions internally as opposed to version-specific IRIs
   (3) to change the file suffix from .owl to .rdf to increase usability in RDF tools
   (4) to use 4-level abbreviations and corresponding namespace prefixes for all FIBO ontologies, reflecting a family/specification/module/ontology structure
   (5) to incorporate changes to the specification metadata to support documentation at the family, specification, module, and ontology level, similar to the abbreviations
   (6) to move this ontology from Organizations to Places and eliminate unnecessary properties and related imports dependencies.""" ;
.
fibo-fnd-plc-adr:Address
  rdf:type owl:Class ;
  rdfs:label "address" ;
  rdfs:subClassOf fibo-fnd-arr-id:Index ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-plc-adr:AddressingScheme ;
      owl:onProperty fibo-fnd-rel-rel:hasDefinition ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-plc-loc:Location ;
      owl:onProperty fibo-fnd-rel-rel:appliesTo ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  owl:equivalentClass fibo-fnd-inf-adrx:Address ;
  skos:definition "an index to a location to which communications may be delivered" ;
  skos:editorialNote "This came from FDTF Address Reviews Aug/Sept 2011. It represents a place holder for mapping to other standards, such as those for email, network, and other electronic addresses as well as physical and mailing addresses." ;
.
fibo-fnd-plc-adr:AddressingScheme
  rdf:type owl:Class ;
  rdfs:label "addressing scheme" ;
  rdfs:subClassOf fibo-fnd-arr-id:IndexingScheme ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-plc-adr:Address ;
      owl:onProperty fibo-fnd-rel-rel:defines ;
    ] ;
  skos:definition "a system for allocating addresses to objects" ;
.
fibo-fnd-plc-adr:PhysicalAddress
  rdf:type owl:Class ;
  rdfs:label "physical address" ;
  rdfs:subClassOf fibo-fnd-plc-adr:Address ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-plc-loc:PhysicalLocation ;
      owl:onProperty fibo-fnd-rel-rel:appliesTo ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  owl:equivalentClass fibo-fnd-plcx-re:PhysicalAddress ;
  skos:definition "a physical address where communications can be addressed, papers served or representatives located for any kind of organization or person" ;
  skos:editorialNote "There are existing international and regional standards for defining postal addresses.  This is a place holder for mapping to regional standards for postal address representation" ;
  skos:scopeNote "An address is a collection of information, presented in a mostly fixed format, used for describing the location of a building, apartment, or other structure or a plot of land, generally using political boundaries and street names as references, along with other identifiers such as house or apartment numbers.  Some addresses also contain special codes to aid routing of mail and packages, such as a ZIP code or post code. (Wikipedia)" ;
.
fibo-fnd-plc-adr:PostCodeArea
  rdf:type owl:Class ;
  rdfs:label "post code area" ;
  rdfs:subClassOf fibo-fnd-plc-loc:PhysicalLocation ;
  owl:equivalentClass fibo-fnd-inf-padr:PostCodeArea ;
  skos:definition "a physical area uniquely identified by some postal code" ;
.
fibo-fnd-plc-adr:PostalAddress
  rdf:type owl:Class ;
  rdfs:label "postal address" ;
  rdfs:subClassOf fibo-fnd-plc-adr:PhysicalAddress ;
  skos:definition "a physical address where postal communications can be addressed, for any kind of organization or person" ;
.
fibo-fnd-plc-adr:VirtualAddress
  rdf:type owl:Class ;
  rdfs:label "virtual address" ;
  rdfs:subClassOf fibo-fnd-plc-adr:Address ;
  owl:disjointWith fibo-fnd-plc-adr:PhysicalAddress ;
  skos:definition "an address identifying a virtual, i.e. non-physical location" ;
.
fibo-fnd-plc-adr:hasAddress
  rdf:type owl:ObjectProperty ;
  rdfs:label "has address" ;
  rdfs:range fibo-fnd-plc-adr:Address ;
  rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
  skos:definition "has a means by which the entity may be located or contacted or may receive correspondence" ;
.
fibo-fnd-plc-cty:
  terms:license "http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense"^^xsd:anyURI ;
  sm:contentLanguage "http://www.omg.org/spec/ODM/"^^xsd:anyURI ;
  sm:contentLanguage "http://www.w3.org/standards/techs/owl#w3c_all"^^xsd:anyURI ;
  sm:copyright """Copyright (c) 2013-2014 EDM Council, Inc.
Copyright (c) 2013-2014 Object Management Group, Inc.""" ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Places/Locations/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"^^xsd:anyURI ;
  sm:fileAbbreviation "fibo-fnd-plc-cty" ;
  sm:fileAbstract "This ontology provides a very high level definition of country related concepts, essentially a placeholder for use in mapping countries and intra-country concepts to the appropriate regional standards or to some as yet undefined global address ontology, for use in other FIBO ontology elements. A minimal set of geopolitical and geophysical terms are included as required for financial risk management and other application use cases, and these are all to be considered as placeholders for suitable standard ontologies for these concepts as these become available. These terms may also be mapped to controlled vocabulary standards such as ISO 3166." ;
  sm:filename "Countries.rdf" ;
  rdfs:label "Countries Ontology" ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/AboutFND/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/Places/AboutPlaces/"^^xsd:anyURI ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/FND/20141101/Places/Countries/> ;
  skos:changeNote "The http://www.omg.org/spec/EDMC-FIBO/FND/20130801/Places/Countries.rdf version of this ontology was modified per the issue resolutions identified in the FIBO FND 1.0 FTF report and in http://www.omg.org/spec/EDMC-FIBO/FND/1.0/AboutFND-1.0/." ;
.
fibo-fnd-plc-cty:BusinessCenter
  rdf:type owl:Class ;
  rdfs:label "business center" ;
  rdfs:subClassOf fibo-fnd-plc-cty:Municipality ;
  skos:definition "a municipality where business is conducted, especially one that is considered a financial center" ;
  fibo-fnd-utl-av:adaptedFrom "FpML Business Center and related codes, see http://www.fpml.org/coding-scheme/business-center-7-14.xml" ;
.
fibo-fnd-plc-cty:Country
  rdf:type owl:Class ;
  rdfs:label "country" ;
  rdfs:subClassOf fibo-fnd-plc-cty:GeopoliticalEntity ;
  owl:equivalentClass fibo-fnd-inf-iid:Country ;
  owl:equivalentClass fibo-fnd-inf-padr:Country ;
  owl:equivalentClass fibo-fnd-inf-sch:Country ;
  owl:equivalentClass fibo-fnd-plcx-gpo:Country ;
  skos:definition "a self-governing geopolitical unit that is recognized as a country by the United Nations" ;
.
fibo-fnd-plc-cty:FederalCapitalArea
  rdf:type owl:Class ;
  rdfs:label "federal capital administrative area" ;
  rdfs:subClassOf fibo-fnd-plc-cty:GeopoliticalEntity ;
  skos:definition "The capital administrative region of a country which is a federation, if the physical area of this region does not form a part of any of the states or pronvinces which make up the federal country." ;
  skos:scopeNote "This model includes representation of the capital administrative region of those federations for which the capital is situated other than within one of the states or provinces which make up the country as a whole. This is so that addresses and other references which may need to refer to any location within the country may do so. Examples include the District of Columbia in the US, and the Canberra area in Australia. Not all federations have such an area." ;
.
fibo-fnd-plc-cty:FederalState
  rdf:type owl:Class ;
  rdfs:label "federal state" ;
  rdfs:subClassOf fibo-fnd-plc-cty:GeopoliticalEntity ;
  skos:definition "a self-governing geopolitical unit which forms part of a wider geopolitical unit that is recognized as a country" ;
  fibo-fnd-utl-av:explanatoryNote "This type of entity, variously referred to as a state, province or canton, has a level of self government including its own legal system and court jurisdiction, but cedes a level of autonomy to the federation of which it forms a part." ;
.
fibo-fnd-plc-cty:GeopoliticalEntity
  rdf:type owl:Class ;
  rdfs:label "geopolitical entity" ;
  rdfs:subClassOf fibo-fnd-plc-loc:PhysicalLocation ;
  skos:definition "any country, federal province, city or other administrative unit which is both geographical and political in its identity" ;
.
fibo-fnd-plc-cty:Municipality
  rdf:type owl:Class ;
  rdfs:label "municipality" ;
  rdfs:subClassOf fibo-fnd-plc-cty:GeopoliticalEntity ;
  skos:definition "an urban administrative division having corporate status and usually powers of self-government or jurisdiction" ;
  skos:example "A municipality can be any political jurisdiction from a sovereign state, such as the Principality of Monaco, or a small village, such as West Hampton Dunes, New York." ;
  skos:scopeNote """The territory over which a municipality has jurisdiction may encompass:
- only one populated place such as a city, town, or village
- several of such places (e.g., early jurisdictions in the state of New Jersey (1798-1899) as townships governing several villages, Municipalities of Mexico)
- only parts of such places, sometimes boroughs of a city such as the 34 municipalities of Santiago, Chile.""" ;
  fibo-fnd-utl-av:adaptedFrom "http://en.wikipedia.org/wiki/Municipality"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "A municipality is a general-purpose administrative subdivision, as opposed to a special-purpose district." ;
.
fibo-fnd-plc-cty:PopulatedPlace
  rdf:type owl:Class ;
  rdfs:label "populated place" ;
  rdfs:subClassOf fibo-fnd-plc-loc:PhysicalLocation ;
  skos:definition "a community in which people live or have lived, without being specific as to size, population or importance" ;
.
fibo-fnd-plc-cr:isSubdivisionOf
  rdf:type owl:ObjectProperty ;
.
fibo-fnd-plc-fac:
  terms:license "http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense"^^xsd:anyURI ;
  sm:contentLanguage "http://www.omg.org/spec/ODM/"^^xsd:anyURI ;
  sm:contentLanguage "http://www.w3.org/standards/techs/owl#w3c_all"^^xsd:anyURI ;
  sm:copyright """Copyright (c) 2014 EDM Council, Inc.
Copyright (c) 2014 Object Management Group, Inc.""" ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Parties/Roles/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Places/Addresses/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Places/Locations/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Relations/Relations/"^^xsd:anyURI ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"^^xsd:anyURI ;
  sm:fileAbbreviation "fibo-fnd-plc-fac" ;
  sm:fileAbstract "This ontology provides scaffolding for use in describing concepts related to facilities, both virtual and physical, including physical sites that provide various facilities." ;
  sm:filename "Facilities.rdf" ;
  rdfs:label "Facilities Ontology" ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/AboutFND/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/Places/AboutPlaces/"^^xsd:anyURI ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/FND/20141101/Places/Facilities/> ;
  skos:changeNote "This ontology was added to Foundations in advance of the December 2014 Long Beach meeting in support of the SEC specification." ;
.
fibo-fnd-plc-fac:Capability
  rdf:type owl:Class ;
  rdfs:label "capability" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:involves ;
      owl:someValuesFrom fibo-fnd-plc-fac:Facility ;
    ] ;
  skos:definition "A capability represents the ability to perform a particular type of work and may involve people with particular skills and knowledge, intellectual property, defined practices, operating facilities, tools and equipment." ;
  fibo-fnd-utl-av:adaptedFrom "Value Delivery Modeling Language Specification, http://www.omg.org/spec/VDML/" ;
.
fibo-fnd-plc-fac:Facility
  rdf:type owl:Class ;
  rdfs:label "facility" ;
  rdfs:subClassOf fibo-fnd-pty-rl:ThingInRole ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom fibo-fnd-plc-fac:Site ;
      owl:onProperty fibo-fnd-plc-fac:isSituatedAt ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:provides ;
      owl:someValuesFrom fibo-fnd-plc-fac:Capability ;
    ] ;
  skos:definition "something that is built, contrived, established, or installed to serve a particular purpose, or make some course of action or operation easier, or provide some capability or service" ;
  fibo-fnd-utl-av:explanatoryNote "A facility may be concrete (as in a manufacturing facility) or abstract. Concrete facilities may be permanent, semi-permanent, or temporary structures, providing one or more capabilities at a given site.  A single site may include multiple facilities and a given facility may span multiple sites." ;
.
fibo-fnd-plc-fac:PhysicalSite
  rdf:type owl:Class ;
  rdfs:label "physical site" ;
  rdfs:subClassOf fibo-fnd-plc-fac:Site ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-plc-adr:Address ;
      owl:onProperty fibo-fnd-rel-rel:hasIdentity ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-plc-loc:PhysicalLocation ;
      owl:onProperty fibo-fnd-plc-loc:isLocatedAt ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  skos:definition "A physical site is a an actual location that situates something, typically a structure or building, archeological dig, landing location for an aircraft or spacecraft, etc.  From biology, this could also be the site of a wound, and active site, and so forth. A physical site has certain characteristics that contribute to the context it provides, including area, shape, accessibility, and in the case of a geographic site, landforms, soil and ground conditions, climate, and so forth." ;
.
fibo-fnd-plc-fac:Site
  rdf:type owl:Class ;
  rdfs:label "site" ;
  rdfs:subClassOf fibo-fnd-pty-rl:ThingInRole ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass fibo-fnd-plc-loc:Location ;
      owl:onProperty fibo-fnd-plc-loc:isLocatedAt ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
      owl:someValuesFrom [
          rdf:type owl:Restriction ;
          owl:allValuesFrom owl:Thing ;
          owl:onProperty fibo-fnd-plc-fac:situates ;
        ] ;
    ] ;
  skos:definition "A site is a place, setting, or context in which something is situated." ;
.
fibo-fnd-plc-fac:Venue
  rdf:type owl:Class ;
  rdfs:label "venue" ;
  rdfs:subClassOf fibo-fnd-plc-fac:Site ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty fibo-fnd-rel-rel:hasContext ;
    ] ;
  skos:definition "A place where something happens, described in the context of the event or activity that occurs there" ;
.
fibo-fnd-plc-fac:hasLatitude
  rdf:type owl:DatatypeProperty ;
  rdf:type owl:FunctionalProperty ;
  sm:directSource "The National Atlas, nationalatlas.gov/articles/mapping/a_latlong.html" ;
  rdfs:label "has latitude" ;
  rdfs:range xsd:string ;
  skos:definition "Latitude values indicate the angular distance between the Equator and points north or south of it on the surface of the Earth." ;
.
fibo-fnd-plc-fac:hasLongitude
  rdf:type owl:DatatypeProperty ;
  rdf:type owl:FunctionalProperty ;
  sm:directSource "The National Atlas, nationalatlas.gov/articles/mapping/a_latlong.html" ;
  rdfs:label "has longitude" ;
  rdfs:range xsd:string ;
  skos:definition "Lines of longitude, called meridians, run perpendicular to lines of latitude, and all pass through both poles. Longitude values indicate the angular distance between the Prime Meridian and points east or west of it on the surface of the Earth." ;
.
fibo-fnd-plc-fac:isSituatedAt
  rdf:type owl:ObjectProperty ;
  rdfs:label "is situated at" ;
  skos:definition "indicates that something has been positioned, located or placed at some site, or in some setting, situation, or context" ;
.
fibo-fnd-plc-fac:situates
  rdf:type owl:ObjectProperty ;
  rdfs:label "situates" ;
  owl:inverseOf fibo-fnd-plc-fac:isSituatedAt ;
  skos:definition "indicates the place, setting, or context in which something is situated" ;
.
fibo-fnd-plc-loc:
  terms:license "http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense"^^xsd:anyURI ;
  sm:contentLanguage "http://www.omg.org/spec/ODM/"^^xsd:anyURI ;
  sm:contentLanguage "http://www.w3.org/standards/techs/owl#w3c_all"^^xsd:anyURI ;
  sm:copyright """Copyright (c) 2013-2014 EDM Council, Inc.
Copyright (c) 2013-2014 Object Management Group, Inc.""" ;
  sm:dependsOn "https://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"^^xsd:anyURI ;
  sm:fileAbbreviation "fibo-fnd-plc-loc" ;
  sm:fileAbstract "This ontology provides a placeholder for use in mapping geographic location-oriented concepts to the appropriate standards." ;
  sm:filename "Locations.rdf" ;
  rdfs:label "Locations Ontology" ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/AboutFND/"^^xsd:anyURI ;
  rdfs:seeAlso "https://spec.edmcouncil.org/fibo/FND/Places/AboutPlaces/"^^xsd:anyURI ;
  owl:versionIRI <https://spec.edmcouncil.org/fibo/FND/20141101/Places/Locations/> ;
  skos:changeNote "The http://www.omg.org/spec/EDMC-FIBO/FND/20130801/Places/Locations.rdf version of this ontology was modified per the issue resolutions identified in the FIBO FND 1.0 FTF report and in http://www.omg.org/spec/EDMC-FIBO/FND/1.0/AboutFND-1.0/." ;
.
fibo-fnd-plc-loc:Location
  rdf:type owl:Class ;
  rdfs:label "location" ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "Anything that can be defined as the answer to a question of the form, Where is...?" ;
  skos:scopeNote "This includes physical places such as towns, cities, physical addresses, buildings etc. and also abstract places such as on-line trading venues or financial marketplaces." ;
.
fibo-fnd-plc-loc:PhysicalLocation
  rdf:type owl:Class ;
  rdfs:label "physical location" ;
  rdfs:subClassOf fibo-fnd-plc-loc:Location ;
  skos:definition "A location in physical space" ;
.
fibo-fnd-plc-loc:RealEstate
  rdf:type owl:Class ;
  rdfs:label "real estate" ;
  rdfs:subClassOf fibo-fnd-plc-loc:PhysicalLocation ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-acc-cur:MonetaryAmount ;
      owl:onProperty fibo-ln-ln-up:cur-hasEstimatedValue ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass fibo-fnd-plc-adr:PostalAddress ;
      owl:onProperty fibo-fnd-rel-rel:has ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass loan:RealPropertyAppraisal ;
      owl:onProperty fibo-fnd-rel-rel:has ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:isClassifiedBy ;
      owl:someValuesFrom fibo-ln-ln-up:ConstructionType ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty fibo-fnd-rel-rel:isClassifiedBy ;
      owl:someValuesFrom fibo-ln-ln-up:DwellingCapacity ;
    ] ;
  owl:equivalentClass fibo-fnd-plcx-re:RealEstate ;
  skos:definition "land plus anything permanently fixed to it, including buildings, sheds and other items attached to the structure" ;
  fibo-fnd-utl-av:definitionOrigin "http://www.investopedia.com/terms/r/realestate.asp"^^xsd:anyURI ;
  fibo-fnd-utl-av:explanatoryNote "Although media often refers to the \"real estate market\" from the perspective of residential living, real estate can be grouped into three broad categories based on its use, namely residential, commercial and industrial. Examples of real estate include undeveloped land, houses, condominiums, townhomes, office buildings, retail store buildings and factories." ;
.
fibo-fnd-plc-loc:isLocatedAt
  rdf:type owl:ObjectProperty ;
  rdfs:label "is located at" ;
  rdfs:range fibo-fnd-plc-loc:Location ;
  skos:definition "a property linking something to a location or place, which might be physical or virtual" ;
.
