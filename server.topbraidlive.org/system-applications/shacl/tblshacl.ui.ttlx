tblshacl:AbstractPage
  rdf:type ui:ViewElement ;
  rdfs:label "Abstract page" ;
  rdfs:subClassOf tblui:Elements ;
.
tblshacl:ValidationPage
  rdf:type ui:ViewElement ;
  ui:prototype """
<tblui:PageContainer arg:title=\"SHACL Validation\">
    <tblui:Section>
        <form action=\"shaclValidation\" enctype=\"multipart/form-data\" id=\"theForm\" method=\"post\">
            <table>
                <tr>
                    <td>
                        <b>Data Graph:</b>&nbsp;</td>
                    <td>
                        <input name=\"dataGraph\" title=\"The file of the data graph to upload for validation\" type=\"file\"/>
                    </td>
                    <td>or URL: <input class=\"form-control\" name=\"dataGraphURL\"/>
                    </td>
                </tr>
                <tr>
                    <td/>
                    <td>
                        <span>Format: </span>
                        <input checked=\"checked\" name=\"dataGraphFormat\" type=\"radio\" value=\"ttl\">
                            <swa:Space/>
                            <span>Turtle</span>
                        </input>
                        <swa:Space/>
                        <swa:Space/>
                        <input name=\"dataGraphFormat\" type=\"radio\" value=\"json\">
                            <swa:Space/>
                            <span>JSON-LD</span>
                        </input>
                        <swa:Space/>
                        <swa:Space/>
                        <input name=\"dataGraphFormat\" type=\"radio\" value=\"rdfxml\">
                            <swa:Space/>
                            <span>RDF/XML</span>
                        </input>
                    </td>
                </tr>
                <tr>
                    <td/>
                    <td>
                        <div class=\"checkbox\">
                            <input checked=\"true\" name=\"dataGraphImports\" type=\"checkbox\" value=\"true\"/>
                            <span>Process owl:imports</span>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <swa:Space/>
                    </td>
                </tr>
                <tr>
                    <td>
                        <b>Shapes Graph:</b>&nbsp;</td>
                    <td>
                        <input name=\"shapesGraph\" title=\"The file of the shapes graph to upload for validation\" type=\"file\"/>
                    </td>
                    <td>or URL: <input class=\"form-control\" name=\"shapesGraphURL\"/>
                    </td>
                </tr>
                <tr>
                    <td/>
                    <td>
                        <span>Format: </span>
                        <input checked=\"checked\" name=\"shapesGraphFormat\" type=\"radio\" value=\"ttl\">
                            <swa:Space/>
                            <span>Turtle</span>
                        </input>
                        <swa:Space/>
                        <swa:Space/>
                        <input name=\"shapesGraphFormat\" type=\"radio\" value=\"json\">
                            <swa:Space/>
                            <span>JSON-LD</span>
                        </input>
                        <swa:Space/>
                        <swa:Space/>
                        <input name=\"shapesGraphFormat\" type=\"radio\" value=\"rdfxml\">
                            <swa:Space/>
                            <span>RDF/XML</span>
                        </input>
                    </td>
                </tr>
                <tr>
                    <td/>
                    <td>
                        <div class=\"checkbox\">
                            <input checked=\"true\" name=\"shapesGraphImports\" type=\"checkbox\" value=\"true\"/>
                            <span>Process owl:imports</span>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <swa:Space/>
                    </td>
                </tr>
                <tr>
                    <td class=\"swa-align-top\">
                        <b>Output:</b>
                    </td>
                    <td>
                        <input checked=\"checked\" name=\"output\" type=\"radio\" value=\"html\">
                            <swa:Space/>
                            <span>HTML (Report)</span>
                        </input>
                        <br/>
                        <input name=\"output\" type=\"radio\" value=\"json\">
                            <swa:Space/>
                            <span>JSON-LD</span>
                        </input>
                        <br/>
                        <input name=\"output\" type=\"radio\" value=\"ttl\">
                            <swa:Space/>
                            <span>Turtle</span>
                        </input>
                    </td>
                </tr>
                <tr>
                    <td>
                        <swa:Space/>
                    </td>
                </tr>
                <tr>
                    <td/>
                    <td>
                        <input class=\"form-control button btn-primary\" type=\"submit\"/>
                    </td>
                </tr>
            </table>
        </form>
    </tblui:Section>
</tblui:PageContainer>
"""^^ui:Literal ;
  ui:responseType ui:HTML ;
  rdfs:comment "A web interface that can be used to upload files for SHACL validation." ;
  rdfs:label "Validation page" ;
  rdfs:subClassOf tblshacl:AbstractPage ;
.
tblshacl:ValidationReport
  rdf:type ui:ViewElement ;
  spin:constraint [
      rdf:type spl:Argument ;
      spl:predicate arg:dataGraph ;
      spl:valueType rdfs:Resource ;
      rdfs:comment "The URI of the data graph." ;
    ] ;
  ui:prototype """
<tblui:PageContainer arg:title=\"SHACL Validation Report\" letrs:results=\"{#
        SELECT ?result
        WHERE {
            ?report sh:result ?result .
            ?result sh:focusNode ?focusNode .
            ?result sh:sourceConstraintComponent ?cc .
        }
        ORDER BY (?focusNode) (?cc) }\">
    <tblui:Section arg:label=\"{= spr:rowCount(?results) } Validation Results\">
        <ui:forEach ui:resultSet=\"{= ?results }\">
            <swa:ValidationResultView arg:dataGraph=\"{= ?dataGraph }\" arg:result=\"{= ?result }\"/>
        </ui:forEach>
        <p>
            <ui:createLink ui:viewClass=\"{= tblshacl:ValidationPage }\">
                <a href=\"{= ?link }\">Back to SHACL Validation form</a>
            </ui:createLink>
        </p>
    </tblui:Section>
</tblui:PageContainer>
"""^^ui:Literal ;
  ui:responseType ui:HTML ;
  rdfs:label "Validation report" ;
  rdfs:subClassOf tblshacl:AbstractPage ;
.
