tagger:currentSubjectGraphHasObject
  a spin:Function ;
  spin:body [
      a sp:Ask ;
      sp:where (
          [
            sp:object [
                sp:varName "result" ;
              ] ;
            sp:predicate tagger:subjectGraph ;
