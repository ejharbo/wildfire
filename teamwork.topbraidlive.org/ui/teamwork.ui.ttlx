  a swa:ObjectViewerClass ;
  spin:constraint [
      a spl:Argument ;
      spl:optional true ;
      spl:predicate arg:suppressPredicate ;
      spl:valueType xsd:boolean ;
      rdfs:comment "True to suppress the sh:path predicate." ;
    ] ;
  swa:objectWidgetWeight [
      a swa:WeightedExpression ;
