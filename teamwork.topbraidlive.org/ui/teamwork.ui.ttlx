teamwork:GetGraphProjectTypes
  a spin:SelectTemplate ;
  a rdfs:Class ;
  a sh:Shape ;
  spin:body [
      a sp:Select ;
      sp:distinct true ;
      sp:resultVariables (
          [
            sp:varName "graphType" ;
          ]
          [
            sp:varName "graphTypeLabel" ;
          ]
        ) ;
      sp:where (
          [
            sp:object [
                sp:varName "graphType" ;
              ] ;
            sp:predicate [
                sp:varName "predicate" ;
              ] ;
            sp:subject [
                sp:varName "projectType" ;
              ] ;
          ]
          [
            a sp:Bind ;
            sp:expression [
                a ui:label ;
                sp:arg1 [
                    sp:varName "graphType" ;
                  ] ;
              ] ;
            sp:variable [
                sp:varName "graphTypeLabel" ;
              ] ;
          ]
        ) ;
    ] ;
  spin:constraint [
