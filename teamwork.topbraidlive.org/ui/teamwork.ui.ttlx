teamwork:ChangeDetails
  a spin:SelectTemplate ;
  spin:body [
      a sp:Select ;
      sp:limit "1000"^^xsd:long ;
      sp:resultVariables (
          [
            sp:varName "Added_or_Deleted" ;
          ]
          [
            sp:varName "Subject" ;
          ]
          [
            sp:varName "Property_Name" ;
          ]
          [
            sp:varName "Property_Value" ;
          ]
        ) ;
      sp:where (
          [
            sp:object [
                sp:varName "triple" ;
              ] ;
            sp:predicate [
                sp:varName "relation" ;
              ] ;
            sp:subject [
                sp:varName "change" ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "subj" ;
              ] ;
            sp:predicate teamwork:subject ;
            sp:subject [
                sp:varName "triple" ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "property" ;
              ] ;
            sp:predicate teamwork:predicate ;
            sp:subject [
                sp:varName "triple" ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "object" ;
              ] ;
            sp:predicate teamwork:object ;
            sp:subject [
                sp:varName "triple" ;
              ] ;
          ]
