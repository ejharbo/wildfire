          ]
        ) ;
      sp:where (
          [
            sp:object [
                sp:varName "someClass" ;
              ] ;
            sp:predicate rdf:type ;
            sp:subject [
                sp:varName "resource" ;
              ] ;
          ]
          [
            a sp:Filter ;
            sp:expression [
                a sp:contains ;
                sp:arg1 [
                    a sp:str ;
                    sp:arg1 [
                        sp:varName "resource" ;
                      ] ;
                  ] ;
                sp:arg2 [
                    a sp:str ;
                    sp:arg1 [
                        sp:varName "uriPart" ;
                      ] ;
                  ] ;
              ] ;
          ]
        ) ;
    ] ;
  spin:constraint [
