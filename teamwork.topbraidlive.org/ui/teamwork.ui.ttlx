teamwork:InheritedConstraintsSection
  a ui:ResourceViewClass ;
  ui:prototype """
<ui:group letrs:ics=\"{#
        SELECT ?constraint
        WHERE {
            ?resource (rdfs:subClassOf)+ ?superClass .
            FILTER (?superClass != ?resource) .
            ?superClass sh:property ?constraint .
        } }\">
