          [
            a sp:Bind ;
            sp:expression [
                a ui:label ;
                sp:arg1 [
                    a swa:resourceType ;
                    arg:resource [
                        sp:varName "focusNode" ;
                      ] ;
                  ] ;
              ] ;
            sp:variable [
                sp:varName "typeLabel" ;
              ] ;
          ]
          [
            a sp:Bind ;
            sp:expression [
                a sp:concat ;
                sp:arg1 [
                    a ui:label ;
                    sp:arg1 [
                        sp:varName "focusNode" ;
                      ] ;
                  ] ;
                sp:arg2 "  (" ;
                sp:arg3 [
                    sp:varName "typeLabel" ;
                  ] ;
                sp:arg4 ")" ;
              ] ;
            sp:variable [
                sp:varName "result" ;
              ] ;
          ]
        ) ;
    ] ;
