                <swa:ResultSetGrid arg:exports=\"{= true }\" arg:resultSet=\"{= ?rs }\" arg:tableId=\"changeHistoryResults\"/>
                <ui:if ui:condition=\"{= spr:rowCount(?rs) = 1000 }\">
                    <div>(Showing a sample of 1000 rows only)</div>
                </ui:if>
            </ui:call>
        </ui:else>
    </ui:group>
</div>
