teamwork:cloneLabel
  a spin:Function ;
  spin:body [
      a sp:Select ;
      sp:resultVariables (
          [
            sp:varName "result" ;
          ]
        ) ;
      sp:where (
          [
            a sp:Bind ;
            sp:expression [
                a sp:lang ;
                sp:arg1 [
                    sp:varName "label" ;
                  ] ;
