                sp:varName "predicate" ;
              ] ;
          ]
        ) ;
      sp:resultVariables (
          [
            sp:varName "predicate" ;
          ]
        ) ;
      sp:where (
          [
            a sp:Union ;
            sp:elements (
                (
                  [
                    sp:object [
                        sp:varName "o" ;
                      ] ;
                    sp:predicate [
                        sp:varName "predicate" ;
                      ] ;
                    sp:subject [
                        sp:varName "subject" ;
                      ] ;
                  ]
                )
                (
                  [
                    a sp:Bind ;
                    sp:expression [
                        a teamwork:currentTeamGraph ;
                      ] ;
                    sp:variable [
                        sp:varName "teamGraph" ;
                      ] ;
                  ]
                  [
                    a sp:NamedGraph ;
                    sp:elements (
                        [
                          sp:object [
                              sp:varName "predicate" ;
                            ] ;
                          sp:predicate teamwork:predicate ;
                          sp:subject [
                              sp:varName "triple" ;
                            ] ;
                        ]
                        [
                          sp:object [
                              sp:varName "subject" ;
                            ] ;
                          sp:predicate teamwork:subject ;
                          sp:subject [
                              sp:varName "triple" ;
                            ] ;
                        ]
                        [
                          sp:object [
                              sp:varName "triple" ;
                            ] ;
                          sp:predicate [
                              sp:varName "rel" ;
                            ] ;
                          sp:subject [
                              sp:varName "change" ;
                            ] ;
