                                WHERE {
                                    rdf:nil teamwork:activatedWorkflowParticipantProperties ?predicate .
                                    FILTER EXISTS {
                                        ?resource ?predicate ?any .
                                    } .
                                    BIND (swa:labelInGraph(?predicate, ui:graphWithImports(teamwork:platformGovernanceGraph())) AS ?label) .
                                }
