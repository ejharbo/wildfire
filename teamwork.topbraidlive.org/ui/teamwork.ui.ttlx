teamwork:GraphURIPlugin
  a teamwork:GeneralPlugin ;
  teamwork:pluginIndex 5 ;
  ui:prototype """
<ui:setContext let:teamGraph=\"{= teamwork:teamGraph(?projectGraph) }\" ui:queryGraph=\"{= ?projectGraph }\">
