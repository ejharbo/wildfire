evnui:OtherVocabularySelectionEditor
  a swa:ObjectEditorClass ;
  ui:prototype """
<select class=\"form-control\" id=\"new-{= ?uid }\" name=\"new-{= ?uid }\" onchange=\"swa.load('concept-widget', { graph: $('#new-{= ?uid }').val()})\">
    <ui:if ui:condition=\"{= !bound(?object) }\">
