teamworkdoc:SPARQLFunctions
  a ui:NodeClass ;
  ui:errorPrototype """
<ui:if ui:condition=\"{= !smf:canRead(&lt;http://topbraid.org/teamwork.ui&gt;) }\">
    <swa:Section arg:title=\"Teamwork SPARQL Functions Reference\">
        <p>The logged-in user \"{= smf:currentUserName() }\" is not permitted to access this resource.</p>
    </swa:Section>
</ui:if>
"""^^ui:Literal ;
  ui:headIncludes """
<title>Teamwork SPARQL Functions Reference</title>
"""^^ui:Literal ;
  ui:prototype """
<swa:Section arg:title=\"Teamwork SPARQL Functions Reference\">
    <p>
		The following listing enumerates the public SPARQL functions from
		the teamwork namespace.  Note that many of them need to be executed
		from within an executing SWP engine, because they may access the
		current query graph or context variables.
	</p>
    <ui:setContext ui:queryGraph=\"{= ui:graphWithImports(&lt;http://topbraid.org/teamwork.ui&gt;) }\">
        <ui:group letrs:functions='{#
                SELECT ?function ?name
                WHERE {
                    {
                        ?function a spin:Function .
                    }
                    UNION
                    {
                        ?function a spin:MagicProperty .
                    } .
                    FILTER (afn:namespace(?function) = \"http://topbraid.org/teamwork#\") .
                    FILTER (!spl:hasValue(?function, spin:private, true)) .
                    FILTER (!spl:hasValue(?function, spin:abstract, true)) .
                    BIND (afn:localname(?function) AS ?name) .
                }
                ORDER BY (?function) }'>
            <ul>
                <ui:forEach ui:resultSet=\"{= ?functions }\">
                    <li let:comment=\"{= spl:object(?function, rdfs:comment) }\">
                        <b>
                            <a href=\"#function-{= ?name }\">teamwork:{= ?name }</a>
                        </b>
                        <ui:if ui:condition=\"{= bound(?comment) }\">
                            <ui:group let:dot=\"{= COALESCE(spif:indexOf(?comment, &quot;\\n&quot;), spif:indexOf(?comment, &quot;.&quot;)) }\">: {= IF((?dot &gt; 0), smf:subString(?comment, 0, (?dot + 1)), ?comment) }</ui:group>
                        </ui:if>
                    </li>
                </ui:forEach>
            </ul>
            <hr/>
            <ui:forEach ui:resultSet=\"{= ?functions }\">
                <a name=\"function-{= ?name }\"/>
                <ui:resourceView ui:resource=\"{= ?function }\"/>
                <hr/>
            </ui:forEach>
        </ui:group>
    </ui:setContext>
</swa:Section>
"""^^ui:Literal ;
  ui:responseType ui:HTML ;
  rdfs:subClassOf swa:Sections ;
.
