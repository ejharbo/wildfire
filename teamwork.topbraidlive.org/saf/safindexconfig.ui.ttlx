<http://topbraid.org/safindexconfig>
  a owl:Ontology ;
  owl:imports <http://server.topbraidlive.org/system-applications/tbladmin> ;
  owl:imports <http://topbraid.org/safindex> ;
  owl:imports <http://topbraid.org/swa> ;
  owl:versionInfo "Created with TopBraid Composer" ;
.
safindexconfig:ManageIndexPage
  a ui:ViewElement ;
  ui:prototype """
<tblui:PageContainer arg:title=\"Search the EDG Index\">
    <tblui:Section arg:label=\"Manage Search the EDG\">
        <p>
			The following list shows the graphs that are currently being tracked for the Search the EDG lucene index.
		</p>
        <p>
			Lucene Search the EDG indexing is currently <b>{= IF(safindex:isEnabled(), \"enabled\", \"disabled\") }</b>.
		</p>
        <ui:loadable ui:loadId=\"info\">
            <ui:group let:building=\"{= safindex:isBuilding() }\">
                <section let:buttonText=\"{= CONCAT(&quot;Rebuild&quot;, IF(?building, &quot;...&quot;, &quot;&quot;)) }\">
                    <ui:if ui:condition=\"{= ?building }\">
                        <p class=\"text-danger\">This index is currently building. <button class=\"btn btn-primary\">Refresh<ui:handle ui:event=\"onclick\" ui:thenLoadId=\"info\"/>
                            </button> page to track progress.</p>
                    </ui:if>
                    <ui:else>
                        <button class=\"btn btn-primary\">{= ?buttonText }<ui:handle ui:event=\"onclick\" ui:thenLoadId=\"info\">
                                <safindex:reinitialize/>
                            </ui:handle>
                        </button>
                    </ui:else>
                </section>
                <section>
                    <p>
                        <span>
                            <strong>{= safindex:updateSetSize() }</strong> resources in indexing set.</span>
                    </p>
                    <p let:resourceCount=\"{= safindex:resourceCount() }\">
                        <ui:if ui:condition=\"{= ?resourceCount &gt; -1 }\">
                            <span>Search the EDG has indexed <strong>{= safindex:resourceCount() }</strong> resources.</span>
                        </ui:if>
                        <ui:else>
                            <span class=\"alert alert-warning\">Failed to determine the number of resources that have been indexed.</span>
                        </ui:else>
                    </p>
                </section>
                <section>
                    <h3>Graphs tracked by this index</h3>
                    <ui:loadable ui:loadLater=\"{= true }\">
                        <ol>
                            <ui:forEach ui:resultSet=\"{#
                                    SELECT ?graph
                                    WHERE {
                                        rdf:nil safindex:availableGraphs ?graph .
                                    }
                                    ORDER BY (?graph) }\">
                                <li>{= ?graph }</li>
                            </ui:forEach>
                        </ol>
                    </ui:loadable>
                </section>
            </ui:group>
        </ui:loadable>
    </tblui:Section>
</tblui:PageContainer>
"""^^ui:Literal ;
  rdfs:label "Manage index" ;
  rdfs:subClassOf tbladmin:AdminPages ;
.
