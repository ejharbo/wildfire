<http://topbraid.org/safindex>
  a owl:Ontology ;
  owl:imports <http://uispin.org/ui> ;
  owl:versionInfo "Created with TopBraid Composer" ;
.
safindex:availableGraphs
  a spin:MagicProperty ;
  rdfs:label "available graphs" ;
  rdfs:subClassOf spin:MagicProperties ;
.
safindex:checkArea
  a ui:NodeClass ;
  spin:constraint [
      a spl:Argument ;
      spl:predicate arg:area ;
      spl:valueType rdfs:Resource ;
    ] ;
  rdfs:comment "Accepts a subjectArea and checks if any of the graphs in the subject area should be tracked in the index." ;
  rdfs:label "check area" ;
  rdfs:subClassOf ui:ControlElement ;
.
safindex:initialize
  a ui:NodeClass ;
  rdfs:label "initialize" ;
  rdfs:subClassOf ui:ControlElement ;
.
safindex:isBuilding
  a spin:Function ;
  spin:returnType xsd:boolean ;
  rdfs:comment "Checks if the search the edg index is currently building." ;
  rdfs:label "is building" ;
  rdfs:subClassOf spin:Functions ;
.
safindex:isEnabled
  a spin:Function ;
  spin:returnType xsd:boolean ;
  rdfs:comment "Checks if the search the edg index is enabeld." ;
  rdfs:label "is building" ;
  rdfs:subClassOf spin:Functions ;
.
safindex:isInitialized
  a spin:Function ;
  spin:returnType xsd:boolean ;
  rdfs:comment "Checks if the search the edg index is currently initialized." ;
  rdfs:label "is initialized" ;
  rdfs:subClassOf spin:Functions ;
.
safindex:reinitialize
  a ui:NodeClass ;
  rdfs:label "initialize" ;
  rdfs:subClassOf ui:ControlElement ;
.
safindex:removeTrackingStatus
  a ui:NodeClass ;
  spin:constraint [
      a spl:Argument ;
      spl:predicate arg:graph ;
      spl:valueType rdfs:Resource ;
    ] ;
  rdfs:comment "Checks the tracking status of a graph.  If the graph is being tracked it will be removed from the index otherwise there is no action." ;
  rdfs:label "check area" ;
  rdfs:subClassOf ui:ControlElement ;
.
safindex:resourceCount
  a spin:Function ;
  spin:returnType xsd:int ;
  rdfs:comment "Returns the total number of resources that have been indexed." ;
  rdfs:label "resource count" ;
  rdfs:subClassOf spin:Functions ;
.
safindex:updateSetSize
  a spin:Function ;
  spin:returnType xsd:int ;
  rdfs:comment "Returns the current number of resources waiting to be indexed." ;
  rdfs:label "update set size" ;
  rdfs:subClassOf spin:Functions ;
.
